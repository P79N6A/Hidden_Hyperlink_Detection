   <script>
  $(function() {
    if(!disableDefaultSearchEvent) {
      $(".group").click(function() {
        location.href = "/list#page=1&group=" + $("#groupSelect").val() + "&condition=" + $("#conditionSelect").val() + "&name=";
      });
      $("#search-button").click(function() {
        location.href = "/list#page=1&group=0&condition=0&name=" + encodeURIComponent($("#xlInput").val());
      });
    };
    $("#xlInput").keyup(function(event){
      if(event.which == 13) $("#search-button").click();
    });
    $(".search-in-results-input").keyup(function(event){
      if(event.which == 13) $(".search-in-results-btn").click();
    });
  });
