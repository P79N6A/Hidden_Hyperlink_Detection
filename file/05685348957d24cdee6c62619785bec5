   <script type="text/javascript">
$(function() {
  $('.type-choices').on('click', 'li', function(){
      if( $(this).hasClass('selected') ) return;
      $(this).addClass('selected').siblings('.selected').removeClass('selected');
  });

  // 关联商品
  var relative_goods_option = { duration: 300 };
  if( $(window).width() <= 480 ) {
      // 在手机版下每次只滚动一个商品，并且为了保证最后一个商品能正常显示在list最后补一个空白div
      relative_goods_option['display'] = 1;
      $('<div>').addClass('item').appendTo($('.relative-goods-list'));
  } else
      relative_goods_option['display'] = 5;

  $('.relative-goods-container').tinycarousel(relative_goods_option);

  $(".item-gallery a").each(function(i, e) {
    if($(e).data('extension') == 'gif')
      $(e).attr('href', $(e).data('file')+'.'+$(e).data('extension'));
    else if(isRetina)
      $(e).attr('href', $(e).data('file')+'_2400x1800.'+$(e).data('extension'));
    else
      $(e).attr('href', $(e).data('file')+'_1200x900.'+$(e).data('extension'));
  });
  // fancybox
  var fancybox_tpl = {
    next:'<a title="下一张" class="fancybox-nav fancybox-next" href="javascript:;"><span></span></a>',
    prev:'<a title="上一张" class="fancybox-nav fancybox-prev" href="javascript:;"><span></span></a>'
  };
  $('.item-gallery a, .toggle-mask').fancybox({
    'loop'       : false,
    'maxWidth'   : $(window).width() * .75,
    'fitToView'  : false,
    'closeBtn'   : false,
    'closeClick' : true,
    'nextEffect' : 'fade',
    'prevEffect' : 'fade',
    'tpl'        : fancybox_tpl
  });
  
  var force_deposit = false;
  var sid = "SG_042634";
      $('.add-to-cart').click(function() {
    if($('.add-to-cart').hasClass('loading'))
      return false;
    if(force_deposit && $('.payment [for=full-price]').hasClass('selected')){
      return false;
    }
    $('.add-to-cart').addClass('loading').text('正在添加');
    $.ajax({
      type: "get",
      url: "http://www.0a0a0.cc/cart/ajaxAddSpot?sid=SG_042634",
      dataType: "jsonp",
      success: function(result){
        if(result.status){
          $('.add-to-cart').removeClass('loading').text('加入购物车');
          alert('添加到购物车成功');
        }else{
          $('.add-to-cart').removeClass('loading').text('加入购物车');
          alert(result.message);
        }
      },
      error: function(){
        $('.add-to-cart').removeClass('loading').text('加入购物车');
        alert('添加失败');
      }
    });
    return false;
  });
  $('.add-to-fav').click(function() {
    if($(this).hasClass('disabled'))
      return false;
    var url = '';
    var text = '';
    if($(this).hasClass('added')) {
      url = "/Goods/ajaxDelFavorite?pid=1829850";
      text = "收藏这件商品";
      msg = "已取消收藏";
    }else{
      url = "/Goods/ajaxAddFavorite?pid=1829850";
      text = "取消收藏";
      msg = "已收藏";
    }
    $.ajax({
      type: "get",
      url: url,
      dataType: "json",
      success: function(result){
        $('.add-to-fav').removeClass('disabled');
        if(result.status){
          $('.add-to-fav').toggleClass('added');
          $('.add-to-fav').text(text).attr('title', text);
          alert(msg);
        }else{
          alert(result.message);
        }
      },
      error: function(){
        $('.add-to-fav').removeClass('disabled');
        alert('添加失败');
      }
    });
    return false;
  });
});
$('.main-image').on('load', function(){
  if( $(this).height() < 240 ) {
    $('.toggle-mask').hide();
  }
});
</script> 
