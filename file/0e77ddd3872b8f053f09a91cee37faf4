    $(function () {
        $("#register-btns").click(function () {
            $("#register-btns").addClass("active");
            $("#login-btns").removeClass("active");
            $("#login-module").hide();
            $("#register-module").show();
            $("#MultipleRegisterTab").show();
            $("#MultipleRegisterImg").show();
        })

        $("#login-btns").click(function () {
            $("#login-btns").addClass("active");
            $("#register-btns").removeClass("active");
            $("#login-module").show();
            $("#register-module").hide();
            $("#MultipleRegisterTab").hide();
            $("#MultipleRegisterImg").hide();
        })
    })
