    <script type="text/javascript">
// 生成升级的内容提示到文档中
insertContent();

function showComment(id) {
    old_get(id+'_show').style.display='';
    old_get(id+'_hidden').style.display='none';
}
function hiddenComment(id) {
    old_get(id+'_hidden').style.display='';
    old_get(id+'_show').style.display='none';
}
function delComments_confirm() {
	var cks = document.getElementsByName('cid[]');
	for (i = 0; i < cks.length; i++) {
		if (cks[i].checked == true) {
			return true;
		}
	}
	alert('请先选择想要删除的帖子');
	return false;
}
var favFwindow;
var favBwindow;
var fromClick = false;
function VC_popup(dom){
    try {
        VeryCD.Track("/stat/miniShow/");
    }catch(e){}

	fromClick=true;
	  path_to_root_dir = "http://v4.vcimg.com/";
   		var loginBox = new GlassBox();

		if (navigator.userAgent.toLowerCase().indexOf("msie") != -1) {
			//loginBox.lbo( false, 0.5 );
			loginBox.init( 'favWindow', '480px', '250px', 'false', 'facebookstyle', true, false );
			loginBox.apos(old_get("addFav").cumulativeOffset().left-350+'px',old_get("addFav").cumulativeOffset().top+12+'px')
		}else{
			//loginBox.lbo( false, 0.5 );
			loginBox.init( 'favWindow', '480px', '235px', 'false', 'facebookstyle', true, false );
			loginBox.apos(old_get("addFav").cumulativeOffset().left-350+'px',old_get("addFav").cumulativeOffset().top-186+'px')
		}

        old_get("favIframe").src = dom;
		return false;
}

function addPicClass() {
    if(old_get("cancelFav").className == "") {
        old_get("cancelFav").className = "cancelFav";
        old_get("cancelFav").title = "取消收藏";
       	new JP.mouseover.auto({
                css_mouseover_class: 'cancelFav',
                element_types: ['img', 'input'],
                root_node: 'weluser',
                show_broken_img: true
            });
    }
}
function add2fav(){
    //加为收藏的统计
    try{
        VeryCD.Track('/stat/commentFavAdd/');
    }catch(e){}
	if(!fromClick) return false;
  	var thisUrl = '/topics/2725976/favor';
	new Ajax.Request (thisUrl, {
            method:'post',
            onComplete: function(transport){
				var re = YAHOO.lang.JSON.parse(transport.responseText);
                if (re.status == 0 || re.status == 4) {
                    old_get('addFavModule').innerHTML ='<img src=http://v4.vcimg.com/images/favorite/myFavorite.gif class=\'\' onMouseout=\'addPicClass()\' id=\'cancelFav\' title=\'取消收藏\'/>';
					if(re.status == 0) {
                        popoupNotemsg('该资源收藏成功.');
					} else if(re.status == 4) {
                        popoupNotemsg('该资源已经被收藏过.');
					}
					_enableBtn();
                } else if(re.status == 1) {
                          VC_popup(formHtml);
                          popoupNotemsg('该资源收藏失败.');
                }
           }
          });
}
function del4fav(){
    //取消收藏的统计
    try{
        VeryCD.Track('/stat/commentFavCancel/');
    }catch(e){}
    var thisUrl = '/topics/2725976/unfavor';
    new Ajax.Request (thisUrl, {
        method:'post',
        onComplete: function(transport){
        	var re = YAHOO.lang.JSON.parse(transport.responseText);
            if (re.status == 0) {
                old_get('addFavModule').innerHTML ='<img src=http://v4.vcimg.com/images/favorite/favorite.gif class=\'addFav\' id=\'addFav\' title=\'加为收藏\' />';
                	popoupNotemsg('取消收藏成功.');
        			_enableBtn();
            } else if(re.status == 1) {
                      VC_popup(formHtml);
                      popoupNotemsg('取消收藏失败.');
            }
        }
    });
	}

function _enableBtn(){
       var _favinterval = setInterval(function() {
	       try{
				if(old_get("addFav")!=null){
					fromClick=true;
					old_get("addFav").onclick=add2fav;
				}
				if(old_get("cancelFav")!=null){
					old_get("cancelFav").onclick=del4fav;
				}
			}catch(e){
				alert(e);
			}
	  	}, 100);
       window.setTimeout(function () {
         clearInterval(_favinterval);
       },3000);
}
</script> 
    <script language="javascript" type="text/javascript">
var postEntry = {
    commentId 		: 0,
    upNumber	: 0,
    downNumber	: 0,
    upNumberHt	: null,
    downNumberHt: null,
    postScore  : 0
}
function upNumberStat(commentId) {
    var hotUrl = "http://stat.verycd.com/counters/folder_cmt_diggup/"+commentId;
    var object = new JSONscriptRequest(hotUrl);
    object.buildScriptTag();
    object.addScriptTag();
}
function upNumber(commentId) {
    hiddenVoteButtons(commentId);
    postEntry.commentId = commentId;
    upNumberComplete();
    var thisUrl = 'http://www.verycd.com/topics/2725976/digup/' + commentId;
    new Ajax.Request (thisUrl, {
        method:'post',
        onComplete: function(transport) {
            if ('failure' == transport.responseText) {
                popoupNotemsg('请不要重复投票.');
            }
        }
    });
    upNumberStat(commentId);
}
function downNumber(commentId) {
    hiddenVoteButtons(commentId);
    postEntry.commentId = commentId;
    downNumberComplete();
    var thisUrl = 'http://www.verycd.com/topics/2725976/digdown/' + commentId;
    new Ajax.Request (thisUrl, {
        method:'post',
        onComplete: function(transport) {
            if ('failure' == transport.responseText) {
                popoupNotemsg('请不要重复投票.');
            }
        }
    });
}

function upNumberComplete() {
    var uvb = old_get('uvb_' + postEntry.commentId);
    if (uvb){
        var upNumber= uvb.getAttribute("score");
    }
    upNumber = parseInt(upNumber) + parseInt(1);
    if (old_get('post_score_up_'+postEntry.commentId)) {
		if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){
       		old_get('post_score_up_'+postEntry.commentId).innerHTML = '&nbsp;[+'+upNumber+']&nbsp;';
			var now= "&nbsp;"+old_get('post_score_down_'+postEntry.commentId).childNodes[0].innerHTML;
		}else{
       		old_get('post_score_up_'+postEntry.commentId).innerHTML = '[+'+upNumber+']';
			var now= old_get('post_score_down_'+postEntry.commentId).childNodes[1].innerHTML;
		}
		old_get('post_score_down_'+postEntry.commentId).innerHTML = now;
    }
}

function downNumberComplete(){
    var dvb = old_get('dvb_' + postEntry.commentId);
    if (dvb){
        var downNumber 	= dvb.getAttribute("score");
        downNumber = parseInt(downNumber) - parseInt(1);;
    }

    if(old_get('post_score_down_'+postEntry.commentId)){
		if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){
        	old_get('post_score_down_'+postEntry.commentId).innerHTML = '&nbsp;['+downNumber+']&nbsp;';
			var now= "&nbsp;"+old_get('post_score_up_'+postEntry.commentId).childNodes[0].innerHTML+"&nbsp;";
		}else{
        	old_get('post_score_down_'+postEntry.commentId).innerHTML = '['+downNumber+']';
			var now= old_get('post_score_up_'+postEntry.commentId).childNodes[1].innerHTML;
		}
		old_get('post_score_up_'+postEntry.commentId).innerHTML = now;
    }
}

function hiddenVoteButtons(commentId) {
    var uvb = old_get('uvb_' + commentId);
    var dvb = old_get('dvb_' + commentId);
    uvb.innerHTML = '<img src="http://v4.vcimg.com/images/file/gotop01.gif" class="gobot" />';
    dvb.innerHTML = '<img src="http://v4.vcimg.com/images/file/gobot01.gif" class="gobot" />';
}
</script> 
