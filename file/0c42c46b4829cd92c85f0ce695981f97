// client time when loaded
var ldc = new Date();
// server time when loaded
var lds = new Date(i10("2017"), i10("03")-1, i10("29"), i10("04"), i10("02"), i10("09"));
// start clock
var clk = document.getElementById("clock");
var d=new initArray(
 " 星期日",
 " 星期一",
 " 星期二",
 " 星期三",
 " 星期四",
 " 星期五",
