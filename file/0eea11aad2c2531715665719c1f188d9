   <!-- Baidu Button END --> 
  </div> 
  <script type="text/javascript">
document.body.oncopy = function () { setTimeout( function () { var text = clipboardData.getData("text"); if (text) { text = text + "\r\n本篇文章来源于 麦商订货 原文链接："+location.href; clipboardData.setData("text", text); } }, 100 ) }
</script> 
