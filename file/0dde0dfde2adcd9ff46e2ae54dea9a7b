    <script type="text/javascript">
	var insenz_status = 0;
	var insenz_availableadvs = {'headerbanner' : '1', 'thread3_0' : '2', 'thread2_0' : '3', 'thread1_0' : '4', 'interthread' : '5', 'footerbanner1' : '6', 'footerbanner2' : '7', 'footerbanner3' : '8', 'footerbanner4' : '9','thread3_5':'10','footerbanner6' : '11','footerbanner7' : '12','footerbanner8' : '13','footerbanner10' : '14','footerbanner11' : '15','footerbanner12' : '16','footerbanner13' : '17','footerbanner14' : '18','footerbanner15' : '19','footerbanner16' : '20'};
	var insenz_adkeys = [];
	var ad_divs = document.getElementById('ad_none').getElementsByTagName('div');
	var insenz_cookie = document.cookie.indexOf('insenz_cookie') == -1 ? null : getcookie('insenz_cookie');
	var ad_obj = null; 

	if(!insenz_status || insenz_cookie) {
		for(var i = 0; i < ad_divs.length; i++) {
			if(ad_divs[i].id.substr(0, 3) == 'ad_' && (ad_obj = document.getElementById(ad_divs[i].id.substr(0, ad_divs[i].id.length - 5))) && ad_divs[i].innerHTML) {
				if(insenz_cookie) {
					re = new RegExp(insenz_availableadvs[ad_divs[i].id.substr(3, ad_divs[i].id.length - 8)] + "\:(0" + "|" + gid + "|" + fid + ")", "ig");
					if(re.exec(insenz_cookie) != null) continue;
				}
				ad_obj.innerHTML = ad_divs[i].innerHTML;
				ad_obj.className = ad_divs[i].className;
			}
		}
	}
</script> 
    <script type="text/javascript">
	if(insenz_status) {
		if(typeof insenz_usedadkeys != 'undefined' && typeof insenz_advs != 'undefined' && insenz_usedadkeys.length && insenz_advs.length) {
			insenz_cookie = '';
			for(var j in insenz_advs) {
				for(var k in insenz_advs[j]) {
					insenz_cookie += j + ':' + k + 'D';
				}
			}
			setcookie('insenz_cookie', insenz_cookie, 86400);
		} else {
			insenz_cookie = 1;
			setcookie('insenz_cookie', '', -86400);
			var insenz_usedadkeys = new Array();
		}
		if(insenz_cookie) {
			for(var i = 0; i < ad_divs.length; i++) {
				if(ad_divs[i].id.substr(0, 3) == 'ad_' && (ad_obj = document.getElementById(ad_divs[i].id.substr(0, ad_divs[i].id.length - 5))) && ad_divs[i].innerHTML && !in_array(insenz_availableadvs[ad_divs[i].id.substr(3, ad_divs[i].id.length - 8)], insenz_usedadkeys)) {
					ad_obj.innerHTML = ad_divs[i].innerHTML;
					ad_obj.className = ad_divs[i].className;
				}
			}
		}
	}
	document.getElementById('ad_none').parentNode.removeChild(document.getElementById('ad_none'));
</script> 
