	function closePop(obj, value, cookieName, issetcookie, time) {
		$(obj).onclick = function() {
			this.parentNode.style.display = 'none';
			if (issetcookie) {
				setcookie(cookieName, value, time, '', '.55bbs.com');
			}
		}
	}
