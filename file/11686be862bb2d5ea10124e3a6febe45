var marquee_product2KAc6cc = document.getElementById("marquee_product2KAc6cc");
var marquee_product1KAc6cc = document.getElementById("marquee_product1KAc6cc");
var marquee_demoKAc6cc = document.getElementById("marquee_demoKAc6cc");

var showwidthKAc6cc;
var loopwidthKAc6cc;
var iKAc6cc;
var mKAc6cc,nKAc6cc;

showwidthKAc6cc=marquee_demoKAc6cc.offsetWidth*1;
loopwidthKAc6cc=marquee_product1KAc6cc.offsetWidth*1;

if(loopwidthKAc6cc<=1){loopwidthKAc6cc=1;}

mKAc6cc=Math.ceil(showwidthKAc6cc/loopwidthKAc6cc);

for(nKAc6cc=0;nKAc6cc<mKAc6cc;nKAc6cc++){
marquee_product2KAc6cc.innerHTML+=marquee_product1KAc6cc.innerHTML;
}

marquee_demoKAc6cc.scrollLeft=0;
iKAc6cc=0;

function MarqueeKAc6cc(){

if(iKAc6cc>=loopwidthKAc6cc){
 iKAc6cc=0;
 }else{
 iKAc6cc++;
}
marquee_demoKAc6cc.scrollLeft=iKAc6cc;

}
var MyMarKAc6cc=setInterval(MarqueeKAc6cc,speed);
marquee_demoKAc6cc.onmouseover=function(){clearInterval(MyMarKAc6cc);}
marquee_demoKAc6cc.onmouseout=function(){MyMarKAc6cc=setInterval(MarqueeKAc6cc,speed);}

