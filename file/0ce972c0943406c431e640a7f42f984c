  <script>
	jQuery(".detail_b").slide({mainCell:".detail_img ul",titCell:".detail_imgbtn img",titOnClassName:"on",effect:"fade"});
	function rightNow(){
	   var sellcondition="1";
	   var goodId="7";
	   var count=$(".num").val();
	   var goods_stock="0";
	   var price_sell="6.00";
	   //alert(sellcondition);alert(goods_stock);alert(count);return;
	  if(parseInt(count)<=0||parseInt(count)>parseInt(goods_stock)||parseInt(count)<parseInt(sellcondition)){
	  alert("您的商品数量不能为0或者您的购买量超出最大库存或者您购买的低于最低购买量");return;
	  }
	  if(price_sell<=0){
	    alert("销售价格必须大于0，或者网站商品还没有添加完成！");return;
	  }
	   $.post("/index.php/Home/Car/paramj.html",{goodId:goodId,count:count,sellcondition:sellcondition,goods_stock:goods_stock},function(dat){
	   if(dat.status==1){
	   if(dat.type==4){
	    window.location.href="/index.php/Home/Car/nowgo1/params/"+dat.param;
	   }else{
	    window.location.href="/index.php/Home/Car/nowgo/params/"+dat.param;
	   }
	  
	   }else if(dat.status==2){
	    alert("请先进行登录");
		window.location.href="/index.php/Home/User/login.html";
	   }else if(dat.status==4){
	   alert("您在进行非法操作！");
	   window.location.href="{U('Public/error1')}";
	   }else if(dat.status==5){
	   alert("该商品最低起步出售为"+sellcondition);return;
	   }else if(dat.status==6){
	   alert("您购买的数量为0");return;
	   }else if(dat.status==7){
	   alert("您购买的商品数量超过了最大的库存！");return;
	   }else{
	   alert("请耐心等待，网络有点慢！");
	   }
	   },"JSON")
	   //window.location.href="/index.php/Home/Car/nowgo.html";
	}
	//加入购物车
	function jiaru(){
	var goodId="7";
	var count=$(".num").val();
	var sellcondition="1";
	var goods_stock="0";
	var price_sell="6.00"
	if(parseInt(count)<=0||parseInt(count)>parseInt(goods_stock)||parseInt(count)<parseInt(sellcondition)){
	  alert("您的商品数量不能为0或者您的购买量超出最大库存或者您购买的低于最低购买量");return;
	  }
	if(price_sell<=0){
	alert("销售价格必须大于0，或者网站商品还没有添加完成！");return;
	}
	  //alert(count);return;
	$.post("/index.php/Home/Car/addcar.html",{goodId:goodId,count:count,sellcondition:sellcondition,goods_stock:goods_stock},function(dat){
	 if(dat.status==1){
	   alert("加入购物车成功！");
	   window.location.href="/index.php/Home/Car/cart";
	 }else if(dat.status==2){
	 alert("请先进行登录！");
	 window.location.href="/index.php/Home/User/login.html";
	 }else if(dat.status==4){
	 alert("购买商品数量不能小于0或低于最低购买量！");
	 }else if(dat.status==5){
	 alert("您的输入非法！");
	 }else if(dat.status==6){
	 alert("超过了最大库存量！");
	 }else{
	 alert("网络有点开小差，请稍后进行操作");
	 }
	},'JSON')
	}
