			function keynum(){
			var sellcondition="1";
			var keynum=$("#nums").val();
			var goods_storage="0";
			//var reg=/^[\u4e00-\u9fa5]+$/;
			var reg=/^[0-9]*$/;
			if(!reg.test(keynum)){
			if(parseInt(sellcondition)>0){
			$("#nums").val(sellcondition);
			}else{
			$("#nums").val(1);
			}
