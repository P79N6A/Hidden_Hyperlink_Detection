			}
		}
	});
	var name='%E7%AC%91%E6%AD%BB%E4%B8%8D%E5%81%BF%E5%91%BD%EF%BC%9A%E7%A5%9E%E5%81%B7%EF%BC%81%E7%9B%98%E7%82%B9%E9%82%A3%E4%BA%9B%E5%81%B7%E5%90%83%E9%AB%98%E6%89%8B%E7%9A%84%E6%B1%AA%E6%98%9F%E4%BA%BA';
	function update_integral(){
		$.ajax({
	        type:'get',  
	 	    url: _memberUrl+'/Credit/update_integral/vid/'+7428070+'/name/'+name,
	        dataType : 'jsonp',  
	        jsonp:"jsoncallback", 
	        success: function(data) {
	            if(data == 'yes'){
					return false;
				}
	        }
    	});
    }
	
	// 指定视频积分 +10
    function storage2(){
		$.ajax({
	        type:'post',  
	 	    url: _memberUrl+'/Credit/storage2/vid/'+7428070+'/name/'+name,
	        dataType : 'jsonp',  
	        jsonp:"jsoncallback", 
