      <script type="text/javascript">
			function changeData(year,month,_id,t){
				var _cid = '39';
				$.get(
					'/Play/ajax_list',
					{vod_userid : _id, y : year, mon : month, cid : _cid},
					function(data){
						$('#tab_con_div').html(data.con);
						$('#tab_con_div a').click(djtj); //点击统计
						$('#7428070').addClass('ws_current').append('<div class="playing">正在播放</div>');
						scrollbar1.update();
					},'json'
				)
				$('.hover').removeClass('hover');
				if(t == 0){
					$('#now_month').addClass('hover');
				}else{
					$(t).addClass('hover');
				}
			}
			function changeYear(year,month,_id){
				var _cid = '39';
				$.get(
					'/Play/ajax_list',
					{vod_userid : _id, y : year, mon : month, cid : _cid},
					function(data){
						$('#tab_nav_ul').html(data.nav);
						$('#tab_nav_ul a').click(djtj); //点击统计
						$('#tab_con_div').html(data.con);
						$('#tab_con_div a').click(djtj); //点击统计
						$('#7428070').addClass('ws_current').append('<div class="playing">正在播放</div>');
						scrollbar1.update();
					},'json'
				)
			}
            </script> 
      <script type="text/javascript">
		    var scrollbar1;
           	$(function(){
				$("#scrollbar1").tinyscrollbar();
				scrollbar1 = $("#scrollbar1").data("plugin_tinyscrollbar");
				if($("#r_wide").is(":visible")){
						scrollbar1.update();
					};
				
			})
            </script> 
