<script>
var SLPGER = {
    root: "http://19.offcn.com/",
    uid: "" 
};
/*微信关注*/
$("#wxIco").click(function(){
	$(".wxPopup").css("display","block");
	$(".meng").css("display","block");
});
$(".meng").click(function(){
	$(".wxPopup").css("display","none");
	$(".meng").css("display","none");
})
</script>
<script type="text/javascript">
/*备案号*/
$(function(){
  var day = new Date();
  var year = day.getFullYear();
  document.getElementById('year').innerHTML=year;
  GetAjaxShareInfo();//获取今日直播
  _IsIOS();
})
/*判断是安卓还是ios*/
function _IsIOS() {
    var ua = navigator.userAgent.toLowerCase();
    if (ua.match(/iPhone\sOS/i) == "iphone os") {
        $('.download').attr('href','https://itunes.apple.com/cn/app/19ke-tang-zhong-gong-jiao/id1163589777?mt=8');
    } else {
        $('.download').attr('href','http://dl.app.offcn.com/19kt-app/19kt.apk');
    }
}

/*考试类型切换*/
$('.etinfo').click(function(){
	var etinfoname = $(this).html();
	var etinfoid = $(this).attr('id');
	$('.qbks').html(etinfoname);
	$('.qbks').attr('id',etinfoid);
	$(".fix_qbks").css("display","none");
	$(".meng").css("display","none");
	/*考试类型和地区存储*/
	$.cookie('etinfoid', etinfoid, { expires: 365, path: '/' }); 
	$.cookie('etinfoname', etinfoname, { expires: 365, path: '/' }); 
	GetAjaxShareInfo();
});
/*地区切换*/
$('.areainfo').click(function(){
	var showarea = $(this).html();
	var areainfoid = $(this).attr('id');
	$('.qbdq').html(showarea);
	$('.qbdq').attr('id',areainfoid);
	$(".fix_qbdq").css("display","none");
	$(".meng").css("display","none");
	$.cookie('areaId', areainfoid, { expires: 365, path: '/' }); 
	$.cookie('showarea', showarea, { expires: 365, path: '/' }); 
	GetAjaxShareInfo();
});

/*今日直播*/
function GetAjaxShareInfo(){
	var etinfoid = $.cookie('etinfoid'); 
	var area_id = $.cookie('areaId');
	var showarea = $.cookie('showarea');
	var etinfoname = $.cookie('etinfoname');
	$('.qbdq').html(showarea);
	$('.qbks').html(etinfoname);
	if(!area_id){
		$('.fix_qbdq').css("display","block");
	}
	$('.today_live_main ').html('<p>直播信息加载中......</p>');
    if(area_id){
        $.ajax({
            type : 'POST',
            cache : false,
            dataType : 'JSON',
            url  : SLPGER.root + 'home/newGetZbInfo/',
            data : {
                area_id : area_id,
                etinfoid : etinfoid
            }, //form表单序列化
            success:function(result){ //用ajax方法发送信息到当前Action中的main方法
                if(result.status==1){
					$('.today_live_main').html(result.data);
                }else{
					$('.today_live_main').html('<p>该地区暂无直播，”切换考试类型“试试看！</p>');
                }
            }
        });	
    }
}
</script>

	</body></html>