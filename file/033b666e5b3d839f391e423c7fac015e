  <script type="text/javascript">

    $(function(){
        /**
     * 游戏收藏
     */
    if($('.game_like').length){
        $('.game_like').on('tap',function(){
            $.ajax({
                url: '/game/like.html',
                data: 'id='+$('.game_like').attr('game_id')+'&backurl='+window.location.href,
                type: 'POST',
                dataType: 'json',
                success: function (res) {
                    if(res.type==0){
                        // 失败
                        $(this).AlertDialog({
                            msg:res.msg,
                            callback:function(){
                                if(res.url){
                                    window.location.href=res.url;
                                }else{
                                    window.location.reload();
                                }
                            }
                        });
                    }else{
                        // 成功
                        $(this).AlertDialog({
                            msg:res.msg,
                            title:'提示',
                            callback:function(){
                                if(res.url){
                                    window.location.href=res.url;
                                }else{
                                    window.location.reload();
                                }
                            }
                        });
                    }
                }
            });
        });
    }
    /**
     * 游戏取消收藏
     */
    if($('.game_unlike').length){
        $('.game_unlike').on('tap',function(){
            $.ajax({
                url: '/game/unlike.html',
                data: 'id='+$('.game_unlike').attr('game_id')+'&backurl='+window.location.href,
                type: 'POST',
                dataType: 'json',
                success: function (res) {
                    if(res.type==0){
                        // 失败
                        $(this).AlertDialog({
                            msg:res.msg,
                            callback:function(){
                                if(res.url){
                                    window.location.href=res.url;
                                }else{
                                    window.location.reload();
                                }
                            }
                        });
                    }else{
                        // 成功
                        $(this).AlertDialog({
                            msg:res.msg,
                            title:'提示',
                            callback:function(){
                                if(res.url){
                                    window.location.href=res.url;
                                }else{
                                    window.location.reload();
                                }
                            }
                        });
                    }
                }
            });
        });
    }
    
        $('#pullrefresh').LoadMore({
            container:$('#table-list1'),
            url:'/game/54547-page.html'
        });
    });
</script> 
