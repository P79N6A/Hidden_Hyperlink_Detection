var _pic = "http%3A%2F%2Fwww.sucaitv.com%2Fforum.php%3Fmod%3Dimage%26aid%3D81635%26size%3D400x400%26key%3D22a81935b10f2564%26nocache%3Dyes%26type%3Dfixnone|http%3A%2F%2Fwww.sucaitv.com%2Fforum.php%3Fmod%3Dimage%26aid%3D81636%26size%3D400x400%26key%3D55a412e8dbea95e8%26nocache%3Dyes%26type%3Dfixnone";
