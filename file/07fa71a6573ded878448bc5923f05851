  <script>
function AddFavorite(sURL, sTitle) {
    try {
        window.external.addFavorite(sURL, sTitle);
    }
    catch (e) {
        try {
            window.sidebar.addPanel(sTitle, sURL, "");
        }
        catch (e) {
            alert("加入收藏失败，请使用Ctrl+D进行添加");
        }
    }
}
$(function(){
	$(".s_btn").click(function(){
		var $txt=$("#s_txt").val();
		if($txt==""){
			alert("请输入搜索关键字！");
			return false;
		}
	});
	$(".head_city").click(function (event) {
		if (!$(this).hasClass("h-hover")) {
			$(this).addClass("h-hover");
			$(".h_city li a").click(function (event) {
				$(".head_city span strong").text($(this).text());
				$(".head_city").removeClass("h-hover");
				event.stopPropagation();
			})
			event.stopPropagation();
		}else {
			$(this).removeClass("h-hover")
		}
	});
	$("body").click(function () {
		$(".head_city").removeClass("h-hover")
	});
	$("#SearchUl li a").click(function () {
		rem = $(this).parent("li");
		var rel = rem.attr("rel");
		var name = rem.attr("name");
		$("#s_txt").attr("name",name);
		document.getElementById("searchline").action = rel;
	});
	//home--nav
    $(".nav-ul li").mouseenter(function () {
        if ($(this).hasClass("flag")) {
            $(".nav-child").slideUp(200);
            $(".nav-ul li").removeClass("active");
        } else {
            $(this).addClass("active").siblings().removeClass("active");
            $(".nav-child").stop(true,true).slideDown(200);
            var index = $(this).index() - 1;
            $(".nav-child p").eq(index).addClass("active").siblings().removeClass("active");
        }
    });
    $(".home-nav").mouseleave(function () {
        $(".nav-child").stop(true,true).slideUp(200);
        setTimeout('$(".nav-ul li").removeClass("active")', 200);
    });
	//登录--
    $(".login-reg").mouseenter(function () {
        $(".login-reg-box").stop(true,true).slideDown(200);
    });
    $(".login-reg").mouseleave(function () {
        $(".login-reg-box").stop(true,true).slideUp(200);
    });
})
</script> 
