  <script type="text/javascript">
$(function(){
	$(".SBtn").click(function(){
		var rel=$(this).attr("rel");
		var rev=$(this).attr("rev");
		$("#"+rel).val(rev);
		$("#PG").val(1);
		$("#searchForm").submit();
	});
	var pg=parseInt($("#PG").val());
	$(".PAGE").click(function(){
		if($(this).hasClass("disabled")){
			return false;	
		}
		if($(this).hasClass("next")){
			pg=pg+1;
		}
		if($(this).hasClass("prev")){
			pg=pg-1;
		}
		if(pg>0 && pg<=TotalPage){
			$("#PG").val(pg);
			$("#searchForm").submit();
		}
	});
	
	$("#TotalPage").text(TotalPage);
	if(pg==1){
		$(".search-page").find(".prev").addClass("disabled");
	}
	if(pg==TotalPage){
		$(".search-page").find(".next").addClass("disabled");
	}
	$(".search-list").each(function(){
		var l=$(this).find("ul>li").length;
		if(l>8){
			$(this).find(".search-content-more").show();
		}
	});
	$(".search-filter-more").click(function(){
		if($(this).hasClass("expanded")){
			$(this).removeClass("expanded").find("span").text("展开更多筛选条件")	;
			$(".search-lists-hidden").removeClass("search-lists-hidden-show");
		}else{
			$(this).addClass("expanded").find("span").text("收起更多筛选条件")	;
			$(".search-lists-hidden").addClass("search-lists-hidden-show");
		}
	})
	$(".search-content-more").click(function(){
		if($(this).closest(".search-list").hasClass("expanded")){
			$(this).closest(".search-list").removeClass("expanded")
			$(this).find("span").text("更多")	;
		}else{
			$(this).closest(".search-list").addClass("expanded")
			$(this).find("span").text("收起")	;
		}
	})	
	var ReCount=$(".page").attr("rev");
	$("#ReCount").text(ReCount);
	
	var IDS="";
	$(".Lines").each(function(){
		if (IDS==""){
			IDS=$(this).attr("rev");
		}else{
			IDS+=","+$(this).attr("rev");
		}
	});
	$.get("/ajax/getplans2.asp?str="+IDS,function(str){
		var A=str.split("|");
		var i,j,B,C;
		for(i=0;i<A.length;i++){
			B=A[i];
			C=B.split(",");
			a=C[0];
			b=C[1];
			c=C[2];
			$(".Lines[rev="+a+"]").html(function(){
				if (b=="无"){
					return "暂无出团计划";
				}else{
					return b;
				}
			}).addClass(function(){
				if (b=="无"){
					return "gray";
				}else{	
					return "orange";
				}
			});
		}
	});
	
});
</script>   
