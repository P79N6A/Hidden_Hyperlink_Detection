    text-decoration:none;
    color:#000;
    display:block;
    width:50px;
}
p#back-to-top a span{
    background:#ddd ;
    border-radius:6px;
    display:block;
    height:30px;
    width:50px;
    font-size: 70px;
    padding-top: 30px;
    color: #999;
}
#back-to-top a:hover span{
    background:#ddd;
}
ul {
    list-style:none;
