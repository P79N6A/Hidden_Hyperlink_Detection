<script type="text/javascript">
	var _show = document.getElementById('active_go_oldlink');
	var _hide = document.getElementById('active_stay_aicai');
	var _close = document.getElementById('active_gg_close');
	
	_show.onclick = function(e){
		document.getElementById('display_shadow').style.display = 'block';
		document.getElementById('display_popupBox_gg').style.display = 'block';
	}
	
	_close.onclick = _hide.onclick = function(e){
		document.getElementById('display_shadow').style.display = 'none';
		document.getElementById('display_popupBox_gg').style.display = 'none';
	}
   window.COMM_LOGIN = {
					 $Id : function(id){
						 return document.getElementById(id);	
					},
					getXmlHttpRequest : function(){
						 var xmlHttp = null;
						 if(window.ActiveXObject)
				             xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
				         else if(window.XMLHttpRequest)
				             xmlHttp = new XMLHttpRequest();
						 return xmlHttp;
					 },
					 sendRequest : function(url,callback,queryString){
						 var tempqueryString = queryString || "";
						 
						 var xmlHttp = this.getXmlHttpRequest();
						 var callbackFn = function(){
							 if(xmlHttp.readyState==4){
				                if(xmlHttp.status == 200){
				                	var jsonObj = eval("("+xmlHttp.responseText+")");
					                callback(jsonObj);
				                }else{
				                	//alert("系统发生异常,请稍后再试!");
				                	return false;
				                }
					         }
						};
						url += (url.indexOf('?')>=0)?'&':'?';
						url += 'noCache='+new Date().getTime();
			            xmlHttp.open("POST",url);
			            xmlHttp.onreadystatechange = callbackFn;
			            xmlHttp.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
			            xmlHttp.send(tempqueryString);
					},
					loginsuccess : function(allname,subname,memLevelImg,hascertNo){
						var h_uname = this.getCookieVal('hupu_uname_at_aicai');
						//this.$Id("login_username_inner_span").title = allname;
						this.$Id("login_username_inner_span").innerHTML = h_uname;
						//this.$Id("_not_full_info_login_username_inner_span").title = allname;
						this.$Id("_not_full_info_login_username_inner_span").innerHTML = h_uname;
				  		var tempFullInfoObj = this.$Id("login_user_logined_div");
				  		var loginednotfullinfodivObj = this.$Id("_not_not_full_info");
			  			this.$Id("login_user_unlogin_div").style.display = 'none';
				  		if(hascertNo){
				  			tempFullInfoObj.style.display = '';
				  			loginednotfullinfodivObj.style.display = 'none';
				  		}else{
				  			tempFullInfoObj.style.display = 'none';
				  			loginednotfullinfodivObj.style.display = '';
				  			this.$Id("login_user_unlogin_div").style.display = 'none';
				  			
				  		}
					},
					refreshInfo : function(){
						var loginVal = this.getCookieVal('CPLOGIN');
						var loginInfoVal = this.getCookieVal('CPLOGIN_INFO');
						if(loginVal!=null){
							if(loginInfoVal!=null){
								var jsonObj = eval("("+loginInfoVal+")");
								this.loginsuccess(jsonObj.allname, jsonObj.subname, jsonObj.memLevelImg, jsonObj.hascertNo);
							}
						 }
					},
					refreshInfoForOutPageQuit : function(){
						var loginVal = this.getCookieVal('CPLOGIN');
						var loginInfoVal = this.getCookieVal('CPLOGIN_INFO');
						if(loginVal == null){
							window.location.reload();
						 }
					},
					setCookie : function(name, value, domain, expires) {
						domain = domain || document.domain;
						if (typeof (expires) == 'undefiend' || expires == null || expires == '') {
							document.cookie = name + "=" + encodeURIComponent(value)
									+ "; path=" + "/" + "; domain=" + domain ;
						} else {
							var expTimes = expires * 1000;
							var expDate = new Date();
							expDate.setTime(expDate.getTime() + expTimes);
							document.cookie = name + "=" + encodeURIComponent(value)
									+ ";expires=" + expDate.toGMTString() + "; path=" + "/"
									+ "; domain=" + domain ;
						}
					},
					getCookieVal : function(name){
						var cookieArr = document.cookie.replace(/\s/g, "").split(';');
						for(var i=0;i<cookieArr.length;i++){
							var tempObj = cookieArr[i].split('=');
							if(tempObj[0]==name)
								return decodeURIComponent(tempObj[1]);
						}
						return null;
					},
					showMoney : function(){
						var walletObj = this.$Id("login_wallet_leftId");
						var showmoneyObj = this.$Id("login_show_moneyId");
						if(showmoneyObj.innerHTML.indexOf("显示余额")>=0){
					    	var tempurl = "http://caipiao.hupu.com/xpassport/hupu/user/showMoney";
					    	var _this = this;
					    	//改为jsonp的请求，解决走势图、即时比分跨域请求
					    	jQuery.ajax({
								url : tempurl,
								cache : false,
								dataType : "jsonp",
								jsonp : "callback",
								success : function(jsonObj) {
									if(jsonObj.success){
						    			if(jsonObj.isLogin){
						    				walletObj.innerHTML = "<font color='red'>"+jsonObj.money+"元</font>";
						    				showmoneyObj.innerHTML = '隐藏';
						    			}else{
						    				var logineddivObj = _this.$Id("login_user_logined_div");
									  		var unlogineddivObj = _this.$Id("login_user_unlogin_div");
									  		logineddivObj.style.display = 'none';
									  		unlogineddivObj.style.display = '';
						    			}
						    		}else{
						    			alert(jsonObj.msg);
						    		}
						    		return false;
								}
							});
					    	
					    	/* this.sendRequest(tempurl,function(jsonObj){
					    		if(jsonObj.success){
					    			if(jsonObj.isLogin){
					    				walletObj.innerHTML = "<font color='red'>"+jsonObj.money+"元</font>";
					    				showmoneyObj.innerHTML = '隐藏';
					    			}else{
					    				var logineddivObj = _this.$Id("login_user_logined_div");
								  		var unlogineddivObj = _this.$Id("login_user_unlogin_div");
								  		logineddivObj.style.display = 'none';
								  		unlogineddivObj.style.display = '';
					    			}
					    		}else{
					    			alert(jsonObj.msg);
					    		}
					    		return false;
					    	}); */

						}else{
							walletObj.innerHTML = '';
							showmoneyObj.innerHTML = '显示余额';
						}
						return false;
					},
					dropUrlSsid : function(){
						var ssidparam = '';
						if(parentUrl.indexOf('ssid') >= 0){
							if(parentUrl.split('ssid')[1].indexOf('&') != -1){
								ssidparam = parentUrl.substring(parentUrl.indexOf('ssid'),parentUrl.split('ssid')[0].length + 5 + parentUrl.split('ssid=')[1].indexOf('&'));
								parentUrl = parentUrl.replace(ssidparam + '&','')
							}else{
								ssidparam = 'ssid=' + parentUrl.split('ssid=')[1];
								parentUrl = parentUrl.replace('?' + ssidparam,'');
							}
						}
						parentUrl = parentUrl.replace(ssidparam,'');
					},
					loginout : function(){
						var _this = this;
						jQuery.ajax({
							url : "http://caipiao.hupu.com/xpassport/hupu/login/loginOut",
							cache : false,
							dataType : "jsonp",
							jsonp : "callback",
							success : function(jsonObj) {
								if(jsonObj.success){
							  		var logineddivObj = _this.$Id("login_user_logined_div");
							  		var loginednotfullinfodivObj = _this.$Id("_not_not_full_info");
							  		var unlogineddivObj = _this.$Id("login_user_unlogin_div");
							  		logineddivObj.style.display = 'none';
							  		loginednotfullinfodivObj.style.display = 'none';
							  		unlogineddivObj.style.display = '';
								}else{
									alert(jsonObj.msg);
								}
								
								_this.dropUrlSsid();
								var jumpurl = parentUrl;
								if(jumpurl.indexOf('uc') >= 0 || jumpurl.indexOf('xpassport') >= 0 || jumpurl.indexOf('cashier') >= 0){
									jumpurl = 'http://caipiao.hupu.com/';
								}
								var reUrl = 'http://passport.hupu.com/logout?jumpurl=' + jumpurl;
								window.location.href = reUrl;
							}
						});
					},
					
					loginoutForSwitchAccount : function(){
						var tempurl = "http://caipiao.hupu.com/xpassport/hupu/login/loginOut";
						var _this = this;
						this.sendRequest(tempurl,function(jsonObj){
							if(jsonObj.success){
						  		var logineddivObj = _this.$Id("login_user_logined_div");
						  		var unlogineddivObj = _this.$Id("login_user_unlogin_div");
						  		logineddivObj.style.display = 'none';
						  		unlogineddivObj.style.display = '';
							}else{
								alert(jsonObj.msg);
							}
					  	});
					},
					//虎扑手机号码绑定
					hupuMobileBind : function(currentUrl){
						this.refreshInfoForOutPageQuit();
						currentUrl = currentUrl || parentUrl;
						var link = "http://caipiao.hupu.com/cashier/hupuMoneyPhoneCheck?currentUrl=" + currentUrl;
						webAlert({title:'虎扑绑定手机', url:link, width:480, height:280});//url参数其实就是iframe
					},
					//虎扑修改手机号码
					hupuMobile : function(currentUrl){
						this.refreshInfoForOutPageQuit();
						currentUrl = currentUrl || parentUrl;
						var link = "http://caipiao.hupu.com/xpassport/hupu/login/hupu/hupuMobile?currentUrl=" + currentUrl;
						webAlert({title:'虎扑修改手机', url:link, width:560, height:330});//url参数其实就是iframe
					},
					//跳转到完善资料页面
					perfectInfo : function(currentUrl){
						this.refreshInfoForOutPageQuit();
						currentUrl = currentUrl || parentUrl;
						var link = "http://caipiao.hupu.com/xpassport/hupu/login/hupu/hupuAuth?currentUrl=" + currentUrl;
						webAlert({title:'虎扑实名认证', url:link, width:480, height:280});//url参数其实就是iframe
					},
		        	charger :function(){
		        		this.refreshInfoForOutPageQuit();
		        		setTimeout(function(){
							var charger_link = 'http://caipiao.hupu.com/cashier/payment/toCharge/charger?chargeType=charger&version=new';
			        		//var link = 'http://caipiao.hupu.com/xpassport/hupu/login/hupu/hupuAuth?currentUrl=' + escape(charger_link);
							webAlert({title:'虎扑实名认证', url:charger_link, width:480, height:280});//url参数其实就是iframe
						},0);
		        	},
		        	drawMoney :function(){
		        		this.refreshInfoForOutPageQuit();
		        		setTimeout(function(){
		        			var charger_link = 'http://caipiao.hupu.com/cashier/drawMoney';
			        		//var link = 'http://caipiao.hupu.com/xpassport/hupu/login/hupu/hupuAuth?currentUrl=' + escape(charger_link);
							webAlert({title:'虎扑实名认证', url:charger_link, width:480, height:280});//url参数其实就是iframe
						},0);
		        	},
					hupuLogin : function(currentUrl){
						/* 
						if(typeof $('.panel-tool-close') != 'undefined'){
							$('.panel-tool-close').click();
						}
						if(typeof $('.ui_close') != 'undefined'){
							$('.ui_close').click();
						}
						setTimeout(function(){
							currentUrl = currentUrl || parentUrl;
							var link = "http://caipiao.hupu.com/xpassport/hupu/login/hupu/index?currentUrl=" + currentUrl;
							webAlert({title:'虎扑登录', url:link, width:640, height:320});//url参数其实就是iframe
						},0); 
						*/
						currentUrl = currentUrl || parentUrl;
						var link = "http://caipiao.hupu.com/xpassport/hupu/login/hupu/index?currentUrl=" + currentUrl;
						window.location.href = link;
					},
					getQueryString : function(name) {
					    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
					    var r = parent.window.location.search.substr(1).match(reg);
					    if (r != null) return unescape(r[2]); return null;
					},
					getDomainHost : function(){
						var reg = /(([^.]+\.(com|cn|net|org|biz|info|cc|tv))|((\d+\.){3}(\d+)))((?=:)|)/;
						var domain = window.location.host;
						var domainHost = reg.exec(domain)[0];
						if (typeof (domainHost) == 'undefined' || domainHost == null
								|| domainHost == '') {
							domainHost = domain;
						}
						return domainHost;
					},
					//获取虎扑ssid参数，走授权流程
					hupuSsid : function(){
						var ssid = this.getQueryString('ssid');
						var referer = this.getQueryString('referer') || '';
						
						var _this = this;
						_this.sendRequest('http://caipiao.hupu.com/xpassport/hupu/login/hupu/hupuSsid',function(jsonObj){
							if(jsonObj.ssid != '' && jsonObj.ssid != ssid){
								ssid = jsonObj.ssid;
							}
							if(ssid == null) return ;
							//如果ssid为空字符或者-1就退出当前登录信息
							if(ssid == '' || ssid == '-1'){
								_this.loginoutForSwitchAccount();
								return ;
							}
							var loginVal = _this.getCookieVal('CPLOGIN');
							if(loginVal != null) return;
							_this.loginoutForSwitchAccount();
							//设置ssid的cookie记录
							_this.dropUrlSsid();
							var link = 'http://caipiao.hupu.com/xpassport/hupu/login/hupu/index?currentUrl=' + parentUrl + '&referer=' + referer;
							window.location.href = link;
							//webAlert({title:'虎扑登录', url:link, width:640, height:320});//url参数其实就是iframe
					  	});
					},
					isBlank : function(str){
						if(str == null || str == 'undefined' || str == ''){
							return true;
						}
						return false;
					}
			   };
			   var parentUrl = this.parent.window.location.href;
			   //初始化页面，根据cookie刷新登录信息
			   COMM_LOGIN.refreshInfo();
			   setTimeout(function(){COMM_LOGIN.hupuSsid();},0);
			   
			   	//收藏本站
				function addFavorite() {
					var title = "虎扑彩票";
					var url = subsystem_webclient;
					try {
				       window.external.addFavorite(url, title);
					} catch (e) {
						try {
							window.sidebar.addPanel(title, url, "");
						} catch (e) {
							alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请使用Ctrl+D进行添加");
						}
					}
				}
			   	
</script>
