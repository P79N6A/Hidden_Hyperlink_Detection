	$(function(){
function move(menu,cur){
	$(menu).mouseover(function(){
	$(menu).removeClass(cur);
	$(this).addClass(cur);
	if(menu=='#tbc_01'){
	$("#tbc_02 li").eq(0).addClass(cur);
	$("#tbc_03 li").eq(0).addClass(cur);
	}else if(menu=='#tbc_02'){
	$("#tbc_01 li").eq(0).addClass(cur);
	$("#tbc_03 li").eq(0).addClass(cur);
	}else if(menu=='#tbc_03'){
	$("#tbc_01 li").eq(0).addClass(cur);
	$("#tbc_02 li").eq(0).addClass(cur);
}
})
}
	move("#tbc_01 li","h");
	move("#tbc_02 li","h");
	move("#tbc_03 li","h");
	$("#tbc_01 li:first").addClass("h");
	$("#tbc_02 li:first").addClass("h");
	$("#tbc_03 li:first").addClass("h");
})

</script>

