            var regx = /^((https|http)?:\/\/).*\.fang\.com(.+)?\/?$/;
            var flag = regx.test(location);
            if (flag && (location.indexOf("/chushou/") > -1 || location.indexOf("/shou/") > -1 || location.indexOf("/chuzu/") > -1 || location.indexOf("/zu/") > -1 || location.indexOf("/chengjiao/") > -1)) {
                if (location.indexOf('.htm') == (location.length - 4)) {//以".htm"结尾
                    jQuery(this).attr("href", location + "?xiaoqu_uuid=pc." + cityshort + "." + newcode);
                } else {
                    jQuery(this).attr("href", location + "&xiaoqu_uuid=pc." + cityshort + "." + newcode);
                }
            }
        });
    });
</script>
    </div>


<script>
    $('.apply-btn').click(function () {
        $(".layer").show();
        $(".tancengbg").show();
    })

    $('.layer-close').click(function () {
        $(".layer").hide();
        $(".tancengbg").hide();
    })

    $('.bottom-close').click(function () {
        $(".bottom-txt").hide();
    })
</script>
<script type="text/javascript">
    if (jQuery) {
        jQuery("#orginalNaviBox li a").each(function () {
            if (jQuery(this).html() == "小区成交") {
                jQuery(this).parent().attr("class", "li-on");
            } else {
                jQuery(this).parent().attr("class", "");
            }
        });
    }
</script>
</div></body></html>