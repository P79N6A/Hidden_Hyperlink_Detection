    <script type="text/javascript">
        function intscrbar(maxsize){
            if($("#numsort li").length>maxsize){
                --maxsize;
                $("#numsort").append($('<a class="more" id="panelctr" href="javascript://" title="更多相关类别"></a>'));
                $("#numsort li:gt("+maxsize+")").each(function(){$(this).hide();});
                $("#panelctr").toggle(function(){
                    $("#numsort li:gt("+maxsize+")").each(function(){
                        $(this).show();
                    });
                    $("#panelctr").removeClass("more");
                    $("#panelctr").addClass("less");
                    $("#panelctr").attr("title","收起相关类别");
                    $("#numsort li.cur").addClass("nobg")
                }, function(){
                    $("#numsort li:gt("+maxsize+")").each(function(){
                        $(this).hide();
                    });
                    $("#panelctr").removeClass("less");
                    $("#numsort li.cur").removeClass("nobg")
                    $("#panelctr").addClass("more");
                    $("#panelctr").attr("title","更多相关类别");
                });
            }else{return false}
        }
        intscrbar(5);
    </script>
