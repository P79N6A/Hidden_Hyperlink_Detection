        <script type="text/javascript">
            $(function(){

//商圈直达
                var html = '<span class="tosqa"><a href="javascript:void(0);">商圈直达</a></span><div class="hc" id="tosqBox"><dl class="hotarea" id="areazf">';
                if($('#hotarea').html()!=null)
                {
                    html += $('#hotarea').html().replace(/二手房/g, '');
                    html += '</dl><i class="shadow"></i></div>';
                }
                document.getElementById('tosq').innerHTML = html;


                $("#tosq>.tosqa").click(function(){
                    //隐藏
                    if($("#tosq").hasClass('tosqhover')){
                        $("#tosq").removeClass('tosqhover');
                        if($('#price_secitem1').is(':visible')){
                            $('#filter_huansuan1').show();
                        }else{
                            $('#filter_huansuan2').show();
                        }
                    }
                    else{
                        clickLog('from=fc_list_bj_shangquanzhida');

                        $("#tosq").addClass('tosqhover');
                        if($('#price_secitem1').is(':visible')){
                            $('#filter_huansuan1').hide();
                        }else{
                            $('#filter_huansuan2').hide();
                        }
                        if($('#areazf dd:visible').size() == 0)
                            $('#areazf dd:eq(0)').show();
                    }
                    return false;
                });

                $("#tosqBox").click(function(){return false;});

                $(document.body).click(function(){
                    $("#tosq").removeClass('tosqhover');
                    if($('#price_secitem1').is(':visible')){
                        $('#filter_huansuan1').show();
                    }else{
                        $('#filter_huansuan2').show();
                    }
                });

                $("#tosq dt").hover(function(){
                    $("#tosq dd").hide();
                    $("#tosq dt").removeClass("select");
                    $(this).next("dd").show();
                    $(this).addClass("select");
                }, function(){
                    //$("#tosq dd").hide();
                    //$("#tosq dt").removeClass("select");
                });
                $("#areazf a").click(function(){
                    window.location.href=this.href;
                })


            });


            $("#selection .subarea .nearsq input:checkbox").click(function(){
                if(!this.checked)return;
                var sc = $('#selection .subarea .nearsq input:checkbox[checked]');
                var count =  sc.length;
                if(count > 4) {
                    alert('最多选择4个商圈。');
                    return false;
                }
            });
            var subsq = function(){
                boot.require('util.uri', function(Frame, uri){
                    var sc = $('#selection .subarea .nearsq input:checkbox[checked]'),
                        url = new uri(),
                        local = url.path.local;
                    if(local) {
                        local = local.split('_').slice(0,1);
                    } else {
                        local = [];
                    }

                    for(var i = 0, leni = sc.length; i < leni; i ++) {
                        local.push(sc[i].value);
                    }
                    url.path.local = local.join('_');
                    location.href = url.toString();
                });
            };

        </script>




        <script type="text/javascript">
            $('.subwayline').mouseenter(function(){
                $(this).find('dt').addClass('active').end()
                    .find('dd').show();
                clickLog("from=fc_subline_cx_ay")
            }).mouseleave(function(){
                $(this).find('dt').removeClass('active').end()
                    .find('dd').hide();
            });

            function changeMode(type) {
                boot.require('dom', function(Frame, dom){
                    if (type == 'single') {
                        dom.show("single_line");
                        dom.hide("muti_line");
                        $("#muti_line input:checkbox[checked]").each(function(){
                            this.checked = false;
                        });
                    } else {
                        dom.hide("single_line");
                        dom.show("muti_line");
                    }
                    if(type=="muti"){
                        $("#muti_line label").removeClass('select');
                    }
                });
            }
            function onCheck() {
                if (this.checked) {
                    return true;
                }
                if($("#muti_line input:checkbox[checked]").length > 5) {
                    alert('最多选择5个站点。');
                    return false;
                }
            }
            function goMutiLine() {
                boot.require('dom', function(Frame, dom){
                    var div = dom.get('muti_line'),
                        cs = div.getElementsByTagName('input'),
                        value = [];
                    for (var i = 0, leni = cs.length; i < leni; i++) {
                        if (cs[i].type == 'checkbox' && cs[i].checked) {
                            value.push(cs[i].value);
                        }
                    }
                    if(value.length>0)
                    {
                        location.href = location.protocol + '//' + location.host + (location.pathname.replace(/\/(bus|sub)(\/l[^/]+)\/(s[0-9_]+)?(.*)/, '\/$1$2\/') +  '/s' + value.join('_') + '/' + RegExp.$4).replace(/\/+/g, '/') + location.search;
                    }
                });
            }

            boot.require('dom', function(Frame, dom){
                var singleDIV = dom.get("single_line");
                if(singleDIV) {
                    var as = dom.get("single_line").getElementsByTagName('a'),
                        html = '<a href="' + as[0].href + '" class="' + as[0].className + '" >' + as[0].innerHTML + '</a>';
                    for (var i = 1, leni = as.length - 1; i < leni; i++) {
                        html += '<span class="checkbox"><input type="checkbox" id="muti_checkbox_' + i + '" value="' + as[i].getAttribute('v') + '" ' + (as[i].className == 'select' ? 'checked="checked"' : '') + ' onclick="return onCheck();"><label htmlFor="muti_checkbox_' + i + '" for="muti_checkbox_' + i + '" ' + (as[i].className == 'select' ? 'class="select"' : '') + ' >' + as[i].innerHTML + '</label></span>';
                    }
                    //html += '<a href="javascript:void(0);" onclick="changeMode(\'single\')" >单选模式</a>';
                    html += '<a class="btn btn2 vmiddle" href="javascript:void(0)"><input type="button" onclick="goMutiLine();" value="确定"></a>';
                    html += '<a class="aorange" href="javascript:void(0);" onclick="changeMode(\'single\')">返回单选</a>';
                    html += '<div class="cl"></div>';
                    var div = dom.create("DIV", {
                        id: 'muti_line',
                        className: 'tags',
                        style : 'display:none',
                        html : html
                    });
                    dom.get("single_line").parentNode.appendChild(div);

                    Frame.init(function(){
                        if ($("#single_line .select").length > 1) {
                            changeMode('');
                            $("#single_line .select").removeClass('select');
                        }
                    });
                }
            });

        </script>

        <script type="text/javascript">
            (function($){
                $.getUrlParams = function(){
                    var ret = {},
                        seg = window.location.search.replace(/^\?/,'').replace(/%7C/g, '|').split('&'),
                        len = seg.length, i = 0, s;
                    for (;i<len;i++) {
                        if (!seg[i]) { continue; }
                        s = seg[i].split('=');
                        ret[s[0]] = s[1];
                    }
                    return ret;
                }
            })(jQuery);

        </script>
        
