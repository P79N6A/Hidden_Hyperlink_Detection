          // <!CDATA[
            function CheckPage() {
                var r=document.getElementById('txtCFTPage');
                if(r)
                {   
                    var match_value=/^[0-9]*[1-9][0-9]*$/ 
                    var input_value=r.value.replace(/\ /g,'');
                    if(input_value==null || input_value.length<=0)
                    {
                        alert('请输入数字页码。'); r.value='';r.focus(); return false; 
                    } 
                    else
                    {
                        if(!input_value.match(match_value))
                        {
                          alert('您输入的是非数字信息，请输入数字。');r.value='';r.focus();return false;
                        }
                        else
                        {
                            if( input_value>0)
                            {
                                alert('您输入的页数超过当前范围，请修改。');r.value='';r.focus();return false;
                            }
                            if( input_value<=0)
                            {
                                window.location.href='http://dlc2c.5173.com/sale/applyorder.aspx?id=DS160802-170793780&pg='+input_value;return false;
                            }
                            return false;
                        }
                    }
                    return false;
                }
                else
                {
                    return false;
                }
            }
            //debugger 
            function CFTPageIndexChang(obj,evt)
            {
              if(obj!=null)
              {
                   var evt =evt || window.event;       
                   var key_code= evt.keyCode ? evt.keyCode:(evt.which ? evt.which : evt.charCode);
                    if (key_code <48 || key_code > 57)
                    {
                      if(key_code==13)
                      {
                        key_code=9;
                        return CheckPage();
                        //return false;
                      }
                      return false;
                   }
              }
            }
