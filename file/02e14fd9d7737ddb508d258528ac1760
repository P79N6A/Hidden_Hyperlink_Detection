        function CopyUrl() {
            var url = document.location.href;
            if (document.all) {
                window.external.addFavorite(url, document.getElementById('lblTitle').innerText);
            }
            else if (window.sidebar) {
                window.sidebar.addPanel(document.getElementById('lblTitle').textContent, url, "");
            }
        }
        function showdiv(id) {
            if (g('div_content').style.display != 'none') { g('div_content').style.display = 'none'; g("li_div_content").className = ''; }
            if (g('div_FrequentlyQuestions').style.display != 'none') { g('div_FrequentlyQuestions').style.display = 'none'; g("li_div_FrequentlyQuestions").className = ''; }
