  <script type="text/javascript">
$(function(){
	if($(".nav_item").hasClass("active")==false)
	{
		$(".nav_item").eq(0).addClass("active");
	}
})
//刷新按钮
$("#refresh").click(
function() {
     document.location.reload();

});
 //新闻切换
$('.nav_item,#sub').click(function(){

	var id_attr=$(this).attr("id");
	var p="";
	var keyword="";
	var type="";

	if(id_attr=="sub")//搜索关键词
	{
		  keyword=$("#keyword").val();
		  $(".active").data("keyword",keyword);

		   p=$(".active").data("p");
		   type=$(".active").data("id");
	}
	else
	{
		$('.nav_item').removeClass('active');
		$(this).addClass('active');
		   p=$(this).data("p");
		   type=$(this).data("id");
	}
	
	//var i=$(this).index();

	 
    	var url="/index/ajax_news.html";
    	
    	 
    	  p1=parseInt(p)+1;
    	  $(this).data("p",p1); 
			 // if(type==15)
			 // {
			 // 	type="isheadline";
			 // }
 
 			 $.post(url,{"type":type,"pg":p1,"keyword":keyword},function(res){
 			   
			 	 var html=""; 
  
			 	 if(res.length!=0)
			 	 {

				 	for (var i = 0; i < res.length; i++) {
				 		 
				 		var styles="";
				 		
				 		html+='<li><div class="news_item clearfix" >';
				 		 var href="/news/article.html";
						  href+="&id="+res[i]["id"]+"&nowCat="+res[i]["type"];

				 		if(res[i]['titleimg']=="")
						  {
						  	  styles=" style='width:100%' ";
						  }
						  else
						  {
						  	html+='<div class="news_img mgr25 fl"><a  target="_blank" href="'+href+'"><img src="'+res[i]["titleimg"]+'" alt="'+res[i]["title"]+'"/></a></div>';
				 		
						  }
						 

				 		html+='<div class="news_text fl" '+styles+'><div class="news_text_tx"><a  target="_blank" href="'+href+'">'+res[i]["title"]+'</a></div><div class="news_s"><span class="mgr25">来源：'+res[i]["refer"]+'  </span><span class="mgr25">'+res[i]["addtime"]+'</span></div></div></div></li>';

				 	}
				 }
			 
				  $(".news_wrap ul").html(html);
				});
        //加载数据
       $('html,body').animate({
    scrollTop: '0px'
  }, 300);
     
});
//加载数据
var totalheight = 0;     //定义一个总的高度变量


function loadata()
{
    totalheight = parseFloat($(window).height()) + parseFloat($(window).scrollTop());     //浏览器的高度加上滚动条的高度

    if ($(document).height() <= totalheight)     //当文档的高度小于或者等于总的高度的时候，开始动态加载数据
    {
    	var url="/index/ajax_news.html";
    	 var p=$(".active").data("p");
    	 var keywords=$("#keyword").val();

    	  p1=parseInt(p)+1;
    	  $(".active").data("p",p1);
    	  
			  var type=$(".active").data("id");
  
			 if(type==15)
			 {
			 	type="isheadline";
			 }
 			 $.post(url,{"type":type,"p":p1,"keyword":keywords},function(res){
 			 	 
			 	var html=""; 
 				var styles="";
			 	 if(res.length!=0)
			 	 {
				 	for (var i = 0; i < res.length; i++) {
				 		var styles="";
				 		
				 		html+='<li><div class="news_item clearfix" >';
				 		var href="/news/article.html";
						  href+="&id="+res[i]["id"]+"&nowCat="+res[i]["type"];
				 		if(res[i]['titleimg']=="")
						  {
						  	  styles=" style='width:100%' ";
						  }
						  else
						  {
						  	html+='<div class="news_img mgr25 fl"><a href="'+href+'" target="_blank"><img src="'+res[i]["titleimg"]+'" alt="'+res[i]["title"]+'"/></a></div>';
				 		
						  }
						  

				 		html+='<div class="news_text fl" '+styles+'><div class="news_text_tx"><a href="'+href+'" target="_blank">'+res[i]["title"]+'</a></div><div class="news_s"><span class="mgr25">来源：'+res[i]["refer"]+'  </span><span class="mgr25">'+res[i]["addtime"]+'</span></div></div></div></li>';

				 	}
				 }
				  $(".news_wrap ul").append(html);
				});
        //加载数据
       
    }
}
$(window).scroll( function() {
	var specialtype=0;
	if(specialtype!=1)
	{
		loadata();
	}
    
});
</script> 
