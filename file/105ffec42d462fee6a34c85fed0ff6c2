        <script>
		function foldList(obj){
	var obj = $('#'+obj);
	$('li h3',obj).bind('mouseenter',function(){
		$(this).parents('li').siblings().removeClass('unfold')	;
		$(this).parents('li').addClass('unfold');
	})
}
/*标签切换*/
function cgTab(id){
	var obj = $('#'+id);
		tab = $('li',obj);
	tab.bind('mouseover',function(){
		var idx = $(this).index();
		if($(this).hasClass('cur')){
			return	
		}
		$("div[id^="+id+"]").hide();
		$('#'+id+'_content_'+idx).show();
		tab.removeClass('cur');
		$(this).addClass('cur');
	})
}

$(function(){
	cgTab('focustab');
	
})
		</script>
