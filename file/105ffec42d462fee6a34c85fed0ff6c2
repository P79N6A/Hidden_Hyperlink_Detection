<script>
//隐藏html5hide的内容20150514
$(document).ready(function() {
	
	var setlurl=$(".sel").attr("href");
	var selnow_now=$(".page_c").find(".sel").html()-0;
	var mobilectol=false;
//	if((navigator.userAgent.match(/(phone|pad|pod|iPhone|iPod|ios|iPad|Android|Mobile|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i))) {
		
	if((navigator.userAgent.match(/(phone|pod|iPhone|iPod|ios|Android|Mobile|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i))) {
		mobilectol=true;
		
	}
	
	if(mobilectol&&($(window).width()<740)){
		$(".html5hide").html("");
	}
	var sum = $("ul.page_c li").eq($("ul.page_c li").length - 2).text();
	if($(window).width()<740){		
		$("ul.page_c li:eq(0)").children("a").text("上一页");
		$("ul.page_c li:eq(1)").children("a").text(selnow_now+"/"+sum);
		$(".sel").attr("href","#")
		for(var uu=2;uu<$("ul.page_c li").length-1;uu++){
			$("ul.page_c li:eq("+uu+")").addClass("html5hide");
		}
		$("ul.page_c li:last").children("a").text("下一页");
	}else{
		$("ul.page_c li:eq(0)").children("a").text("<");
		$("ul.page_c li:eq(1)").children("a").text(1);
		$(".sel").attr("href",setlurl);
		for(var uu=2;uu<$("ul.page_c li").length-1;uu++){
			$("ul.page_c li:eq("+uu+")").removeClass("html5hide");
		}
		$("ul.page_c li:last").children("a").text(">");
	}
	
	$(window).resize(function(){
		if($(window).width()<740){		
			$("ul.page_c li:eq(0)").children("a").text("上一页");
			$("ul.page_c li:eq(1)").children("a").text(selnow_now+"/"+sum);
			$(".sel").attr("href","#")
			for(var uu=2;uu<$("ul.page_c li").length-1;uu++){
				$("ul.page_c li:eq("+uu+")").addClass("html5hide");
			}
			$("ul.page_c li:last").children("a").text("下一页");
		}else{
			$("ul.page_c li:eq(0)").children("a").text("<");
			$("ul.page_c li:eq(1)").children("a").text(1);
			$(".sel").attr("href",setlurl);
			for(var uu=2;uu<$("ul.page_c li").length-1;uu++){
				$("ul.page_c li:eq("+uu+")").removeClass("html5hide");
			}
			$("ul.page_c li:last").children("a").text(">");
		}
	})
	
	$('ul.page_c').show();
	
});

</script>
