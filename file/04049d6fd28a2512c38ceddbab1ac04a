//切换
function show_tb(id,num,box,btn){
	for(var i= 1;i < num;i++){
		$('#'+box+i).hide();
		$('#'+btn+i).parent('li').removeClass("navon");
	}
	$('#'+box+id).show();
	$('#'+btn+id).parent('li').addClass("navon");
}
function showhidden(id,i){
	var dis = (i == 1) ? 'block' : 'none';
	document.getElementById(id).style.display = dis;
}

function loaddaohang(){
	var dh = document.getElementById('daohang');
	if(dh){
		if(dh.src != 'http://www.xcar.com.cn/bbs/header/daohangindex.php'){
			dh.src = 'http://www.xcar.com.cn/bbs/header/daohangindex.php';
		}
	}
	showhidden('listmenu_menu','1');
}
function loaddaohang_cs(){
	showhidden('listmenu_menu_cs','1');
}
var timer;
function msgFlicker(){
    obj = document.getElementById('header_msg');
    if(obj.className == 'menutb glint'){
        obj.className = 'menutb'
    }else{
        obj.className = 'menutb glint'
    }
    timer = setTimeout("msgFlicker()",500)
}

(function($){
    $('#listmenu_menu_cs').html('<iframe scrolling="no" frameborder="0" target="_blank" marginheight="0" marginwidth="0" border="0" style="width:545px;height:320px;" src="http://newcar.xcar.com.cn/inc_htm/all_cheshi_list.htm" id="daohang_cs"></iframe>');
    $('#listmenu_menu').html('<iframe scrolling="no" frameborder="0" id="daohang" style="width:803px;height:559px;" border="0" marginwidth="0" marginheight="0"></iframe>');
    $('#klist li').hover(function(){
        $(this).attr('class','active');
    },function(){
        $(this).removeAttr('class');
    })
    $('#k').focusin(function(){
        if($(this).val() == '高尔夫'){
            $(this).val('');
        }
    })
    $('#k').focusout(function(){
        if($(this).val() == ''){
            $(this).val('高尔夫');
        }
    });
    $('body').bind('click',function(e){
        var e = e ? e : window.event;  
        var tar = e.srcElement || e.target;
        if(false === $(tar).hasClass('menutb')){
            $('.mbbs').removeClass('hover');
        }
    })
    $(".menubox").hover(function(){
        $(this).addClass('hover');
        $(this).children('.muser_show').css('width',$(this).width());
        var _rel = $(this).attr("rel");
        if(_rel && _rel == 'xheaderbbsnavBox'){
            var xheaderbbsNav = document.getElementById("xheaderbbsNav");
            var iframe = xheaderbbsNav.children.item(0);
            if(iframe.src == ''){
                var _myDate = new Date();
                var _version = '';
                _version += _myDate.getFullYear();
                _version += _myDate.getMonth()+1;
                _version += _myDate.getDate();
                _version += _myDate.getHours();
                _version += _myDate.getMinutes();
                iframe.src = 'http://www.xcar.com.cn/bbs/header/bbsnav.htm?action=bbsnav&domain='+document.domain+'&v='+_version;
            }
        }
        if(_rel && _rel == "xheaderchishi"){
            var xheadcsdaohang = document.getElementById("xheadcsdaohang");
            var iframe = xheadcsdaohang.children.item(0);
            if(iframe.src==""){
                var url_nav = "http://"+window.location.host;
                iframe.src = "http://info.xcar.com.cn/ches_nav.php?navul="+url_nav;
            }
        }
    }, function(){
        $(this).removeClass("hover");
    });

    /*相关列表*/
    $("#aboutul li").hover(function(){
        var ids=[];
        ids=$(this).attr("id").split("_");
        var id=ids[1];
        if(id){
            $("#aboutul").children().removeClass();
            $(this).addClass("navon");
            var content=$("#aboutinfo"+id).html();
            if(content!=""){
                $("#showinfo").html(content);
            }
        }
    });
    $('#club_tab li').bind('mouseover',function(){
        var _this = $(this);
        var index = _this.attr('data');
        $('#club_tab_'+index).fadeIn('fast').siblings('ul').hide();
        _this.find('a').addClass('lxh_bbs_hot_title_active').parent('li').siblings('li').find('a').removeClass('lxh_bbs_hot_title_active');
    });
        window.lreload = function(){
        window.location.reload();
    }
    //登陆操作
    $('#xlogin,.keywords_focuson').bind('click',function(){
        $(this).unbind('click');
        $(this).xLoginBox({show:true,func:lreload});
        return false;
    })
        $(function(){
        $('#control_feedback').remove();
        $('#CollectOpinions').one('click',function(){
            $(this).attr('href','http://my.xcar.com.cn/total_station_feedback.php');
            $(this).attr('target','_blank');
        });
    })
})(jQuery)
