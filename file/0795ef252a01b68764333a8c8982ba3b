							//chooselis(1);
							function reurl(){
							   var keyword = $("#keyword").val();
							   if( keyword == '' ){window.location = "http://www.yichongwu.com";}
							   else {
								   window.location = "/k!"+keyword+"_t.html";
							   }		   
							}

							function entersearch(){
								 //alert(dd);
								var event = window.event || arguments.callee.caller.arguments[0];
								if (event.keyCode == 13)
								{
									reurl();
								}
							} 
							</script> 
