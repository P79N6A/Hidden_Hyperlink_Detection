function getDate(){ 
var d = new Date(); 
var year = d.getFullYear(); 
var month = d.getMonth()+1; 
var date = d.getDate(); 
var hour= d.getHours(); 
var minutie= d.getMinutes(); 
var second= d.getSeconds();

document.write(year+"-"+month+"-"+date+" "+hour+":"+minutie);

} 

getDate(); 

</script> 
