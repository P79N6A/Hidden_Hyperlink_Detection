      <script type="text/javascript">
$(function(){
	var contentIndex = function () {
	    var e;
	    return e = function (article, article_index) {
	        var target_article, headline, target_articleIndex, u, d, location_hash;
	        target_article = $(article);
	        target_articleIndex = $(article_index);
	        headline = target_article.find("h2, h3");
	        if(headline.size()===0) $('#ml-u').append('<li><span class="g-bg glyphicon glyphicon-info-sign padding-r-2" aria-hidden="true"></span>没有章节</li>');
	        d = 1;
	        headline.each(function (e) {
	            var n, r, i;
	            i = $(this).text(),
	            i=i.replace(/^(\s|\u00A0)+/,'').replace(/(\s|\u00A0)+$/,''),
	            "" !== i && ($(this).attr("id", "articleHeader" + e),
	                r = parseInt($(this)[0].tagName.slice(1)),
	                n = null,
	                0 === e || r === d ?
	                    (n = $('<li><span class="g-bg glyphicon glyphicon-triangle-right" aria-hidden="true"></span><a href="#articleHeader' + e + '"></a></li>'), n.find("a").text(i), target_articleIndex.append(n)) :
	                    r > d ?
	                        (n = $("<li><ul class='ml-sub'><li><span class='g-bg glyphicon glyphicon-triangle-right' aria-hidden='true'></span><a href=\"#articleHeader" + e + '"></a></li></ul></li>'), n.find("a").text(i), target_articleIndex.append(n), target_articleIndex = n.find("ul")) :
	                    d > r && (1 === r ? (n = $('<li><span class="g-bg glyphicon glyphicon-triangle-right" aria-hidden="true"></span><a href="#articleHeader' + e + '"></a></li>'), n.find("a").text(i), $(article_index).append(n), target_articleIndex = $(article_index)) :
	                        (n = $("<li><ul class='ml-sub'><li><span class='g-bg glyphicon glyphicon-triangle-right' aria-hidden='true'></span><a href=\"#articleHeader" + e + '"></a></li></ul></li>'), n.find("a").text(i), target_articleIndex.parents("ul").parents("ul").length ? (target_articleIndex.parents("ul").parents("ul").append(n), target_articleIndex = n.find("ul")) : (n = $('<li><span class="g-bg glyphicon glyphicon-triangle-right" aria-hidden="true"></span><a href="#articleHeader' + e + '"></a></li>'), n.find("a").text(i), $(article_index).append(n), target_articleIndex = $(article_index)))), d = r)
	        }),location_hash = location.hash, location_hash && $(window).scrollTop($(location_hash).offset().top)
	    }
	};
	var c = new contentIndex();
	c(".m-bg", "#ml-u");
	if('book'==''){
		$('#ml-u a').click(function(){$("html,body").animate({scrollTop:$($(this).attr('href')).offset().top-55},200);$('#ml-u li').removeClass('active');$(this).parent().addClass('active');return false;});
	}
	$("#ml-zj").click(function(){
		$zj_span=$(this).find('span');
		if($zj_span.attr('class')=='glyphicon glyphicon-plus'){
			$zj_span.attr('class','glyphicon glyphicon-minus');
		}else{
			$zj_span.attr('class','glyphicon glyphicon-plus');
		}
		var r_window_h=$(window).height()-252;
	    $("#ml").toggle(300,function(){
	    	if($('#ml-u').height()>r_window_h){
	    		r_window_h='overflow: auto;height:'+r_window_h+'px';
	    		$('#ml-u').attr('style',r_window_h);
		   }
		});
	});
});
</script>
