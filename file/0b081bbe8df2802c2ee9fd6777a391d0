	$(".randCodeImage").attr('width','90px');
	$(".randCodeImage").attr('height','36px');
	$(function(){
		$('.tabs-nav').on('click','li a',function(){
			
			var self=$(this);
			self.addClass('tabulous_active').parent()
				.siblings().find('a').removeClass('tabulous_active')
			var index=$(this).parent().index()
			$('.tabs-content').eq(index).removeClass('tabs-hide').addClass('showsip')
				.siblings().addClass('tabs-hide').removeClass('showsip')
			
		})
		
		
		
		
		
		$('.getSnscaptcha').on('click',function(){//获取短信
			$(this).attr('')
			if($.trim($('#phone').val())==''){
				alert('请输入手机号')
				return
			}
			
			
			if($("#phone").val().length == 11 && $("#image_captcha").val().length == 4){
				var ajaxurl = 'http://www.ixingmei.com/member/index.php?act=connect_sms&op=get_captcha&nchash='+$("#sms_codeimage").attr("nchash")+'&type=2';
				ajaxurl += '&captcha='+$('#image_captcha').val()+'&phone='+$('#phone').val();
				$.ajax({
					type: "POST",
					url: ajaxurl,
					async: false,
					dataType: 'jsonp',
					xhrFields: {
						withCredentials: true
					},
					crossDomain: true,
					success: function(rs){
						if(rs == 'true') {
							$(".getSnscaptcha").html('短信动态码已发出');
						} else {
							alert(rs);
						}
					}
				});
			}
			
			
		})
		
		//验证用户登录
		
		$('#login_form').submit(function(){
			
			if($.trim($('#user_name').val())==''){
				alert('请输入手机号')
				return false;
			}else if($('#password').val()==''){
				alert('密码不能为空')
				return false;
			}else if($('input[name="captcha"]').val()==''){
				alert('验证码不能为空')
				return false;
			}else{
				return true;
			}
			
		})
		
		
		
		//验证手机登录
		
		$('#post_form').submit(function(){
			
			if($.trim($('#phone').val())==''){
				alert('请输入手机号')
				return false;
			}else if($('#image_captcha').val()==''){
				alert('验证码不能为空')
				return false;
			}else if($('#sms_captcha').val()==''){
				alert('动态验证码不能为空')
				return false;
			}else{
				return true;
			}
			
		})
		
	})
