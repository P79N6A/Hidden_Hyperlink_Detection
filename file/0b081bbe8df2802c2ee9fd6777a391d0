  <script>
 $(function(){

 var nav='';
  	nav=4;
 
 
  var tablist=$('.site-menu').find('.navlist')
  var pathName=window.location.pathname;

  if(pathName.match(/^\/index.php$/) || pathName.match(/^\/index.html$/) || pathName.match(/^\/$/) || pathName.match(/^\/shop\/$/)){
   
    addCookie('currentid',0,30,{path:"/"})
    tablist.removeClass('current')
    tablist.eq(0).addClass('current');
  
   }else{

     var index=getCookie('currentid')
        // console.log(index)
         if(index && index!='undefined'){
          tablist.removeClass('current')
          tablist.eq(index).addClass('current');
         }
      }


  $('.site-menu').on('click','li',function(){
    
     var _this=$(this)
     var tabsName=_this.find('.navlist');
     if(tabsName.attr('target')=='_blank') return
     var i=tabsName.attr('currid');
     addCookie('currentid',i,30,{path:"/"})
  })

})
</script> 
