	function showMoreContent() {
		if ($("#gameinfo_content").hasClass("ginfo-item-box-desc")) {
			$("#gameinfo_content").removeClass("ginfo-item-box-desc");
			$("#gameinfo_content").addClass("ginfo-item-box-all");
			$("#game_content_more").removeClass('am-icon-chevron-down');
			$("#game_content_more").addClass('am-icon-chevron-up');
		} else {
			$("#gameinfo_content").removeClass("ginfo-item-box-all");
			$("#gameinfo_content").addClass("ginfo-item-box-desc");
			$("#game_content_more").removeClass('am-icon-chevron-up');
			$("#game_content_more").addClass('am-icon-chevron-down');
		}
	}

	function setsptag(tags){
		//
		if("86"=="86"){
			$("#sptag-lan").find(".sptitle").text("中文");
		}else if("86"=="81"){
			$("#sptag-lan").find(".sptitle").text("日文");
		}
		if(tags=='') return '';
		var taglist = tags.split(',');
		var count = static_gamesptag.length;
		for(var i=0;i<taglist.length;i++){
			for(var j=0;j<count;j++){
				if(taglist[i]==static_gamesptag[j]['tagid']){
					$("#sptag-"+taglist[i]).addClass("active");
					$("#sptag-"+taglist[i]).find(".sptitle").text(static_gamesptag[j]['tagtitle']);
					break;
				}
			}
		}
	}
