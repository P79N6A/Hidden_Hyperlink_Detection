		}
		function MoeAppDonwload(){
			if(isWeiXin()){
				window.location.href = "http://app.guopan.cn/?channelid=10591";
			}else{
				window.location.href = "http://app.guopan.cn/?channelid=10591";
			}
		}
		function isWeiXin() {
			var ua = window.navigator.userAgent.toLowerCase();
			if (ua.match(/MicroMessenger/i) == 'micromessenger') {
				return true;
			} else {
				return false;
			}
		}
		function isAndroid() {
			var ua = window.navigator.userAgent.toLowerCase();
			if (ua.indexOf("android")>0) {
				return true;
			} else {
				return false;
			}
		}
		function v20_app_status(parames) {
			if (appstatus_count == 0) return;
			var _cntid = parames["cntid"];
			var _obj = $("#appstatus_" + _cntid);
			if (_cntid == "226127") {
				$("#appstatus_" + _cntid).hide();
				return;
			}
			var native_downstatus = parames["downstatus"];
			if (native_downstatus == 0) {
				_obj.removeClass("app_btdown_active");
				_obj.text('下 载');
				_obj.unbind();
				_obj.bind("click", function() {
					MOE.call_native("download", {
						userid: MOE.userid(),
						cntid: _cntid,
						packid: $(this).data("apppackid"),
						areatype: 0
					});
					//
					setTimeout(function() {
						MOE.call_native("event_dispatch", {
							type: "appstatus",
							cntid: _cntid
						});
					}, 2000);
					return false;
				});
			} else if (native_downstatus == 4) {
				_obj.addClass("app_btdown_active");
				_obj.text('下载中');
				_obj.unbind();
				_obj.bind("click", function() {
					MOE.call_native("diag_download", new Object());
					return false;
				});
			} else if (native_downstatus == 5) {
				// 比对版本 是否需要更新 version
				_obj.unbind();
				_obj.addClass("app_btdown_active");
				if (parames["version"] == _obj.data("appversion")) {
					_obj.text('打 开');
					_obj.bind("click", function() {
						MOE.call_native("open_app", {
							packid: _obj.data("apppackid")
						});
						return false;
					});
				} else {
					_obj.text('更 新');
					_obj.bind("click", function() {
						MOE.call_native("diag_mygame", {});
						return false;
					});
				}
			} else {
				_obj.addClass("app_btdown_active");
				_obj.text('下载中');
				_obj.unbind();
				_obj.bind("click", function() {
					MOE.call_native("diag_download", new Object());
					return false;
				});
			}
		}
		(function($) {
			'use strict';
			$(function() {
				var ret = MOE.init(); // 初始化
				// 滚动检测
				$(window).scroll(function() {
					//						if ($(window).scrollTop() > 100) {
					//							$("#amz-toolbar").fadeIn(500);
					//						} else {
					//							$("#amz-toolbar").fadeOut(1000);
					//						}
					//
					var windowHeight = document.documentElement.clientHeight;
					var scrollTop = document.body.scrollTop;
					var scrollHeight = document.body.scrollHeight;
					var scrollY = scrollHeight - windowHeight - scrollTop;
					if (scrollY < 10) {
						if (typeof(loadMore) == "function") {
							loadMore();
						}
					}
				});
			});
		})(jQuery);
