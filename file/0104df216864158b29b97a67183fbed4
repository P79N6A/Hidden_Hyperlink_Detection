		function MoeAppDonwload1(){
			if(isWeiXin()){
				window.location.href = "https://mp.weixin.qq.com/mp/profile_ext?action=home&scene=110&__biz=MzI0MjA1MjY5Ng==#wechat_redirect";
			}else{
