function get_linkword(keyword)
{
    document.getElementById("popwin").innerHTML="正在读取数据";
    x=document.documentElement.scrollLeft+event.clientX;
    y=document.documentElement.scrollTop+event.clientY;
      var paper=document.getElementById("popwin");
    paper.style.border = "1px window-inset #015fbf";
    //paper.style.filter = "alpha(opacity=50)";
    paper.style.height="140px";
    paper.style.width="320px";
    paper.style.background="#dfffff";
    paper.style.position="absolute";
    paper.style.left=x+30+ "px";
    paper.style.top=y+ "px";
    paper.style.display="";
    document.getElementById("getNewsScript").src="http://news.hf.house365.com/get_news_link.php?keyword="+keyword;
}
