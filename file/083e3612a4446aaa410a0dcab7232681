		 var news_id='001330840';
		 var url='';
		 if(n_page<=10)
			url='%!news_url!%'.replace(news_id+"_0"+(n_page-1),news_id);
		 else
			url='%!news_url!%'.replace(news_id+"_"+(n_page-1),news_id);
		 var strclass="";
		 var url2='';
		 var daohang_l='';
		 var daohang_r='';
