$(function(){ 
$("#pindao_nav>ul>li").mouseover(function(){ 
	$("#pindao_nav>ul>li").each(function(i){ 
	$(this).removeClass("hover"); 
	}); 
	$(this).addClass("hover"); 
}).mouseout(function(){ 
	$(this).removeClass("hover"); 
}); 
}); 
</script> 
