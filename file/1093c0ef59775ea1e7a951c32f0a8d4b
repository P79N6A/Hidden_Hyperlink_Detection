   <script type="text/javascript">
//DD_belatedPNG.fix('.kefu_bj');
//用户中心效果
$(".youce .ucenter").mouseover(function(){
	$(".youce .ucenter .hy_bj").addClass("hy_bjhover");
	$(this).children(".xs_user").css({ "display":"block",opacity:1});
});
$(".youce .ucenter").mouseout(function(){
	$(".youce .ucenter .hy_bj").removeClass("hy_bjhover");
	$(this).children(".xs_user").css({ "display":"none",opacity:0});
});
//动画菜单效果
var idx=9999;
//客服
function r_mouse_over(thisid,id,style){
	$("."+id).css({ "display":"none",opacity:0});
	$("."+style).addClass(style+"hover");
	$("."+thisid).next().css({ "display":"block",opacity:1,right:"70px"});
	$("."+thisid).next().animate({ right:"35px"},'fast');
}
function r_mouse_out(thisid,id,style){
	$("."+thisid).next().css({ "display":"none",opacity:0});
	$("."+style).removeClass(style+"hover");
}
function r_mouse_over_none(thisid,style){
	$("."+thisid).css({ "display":"block",opacity:1,right:"35px"});
	$("."+style).addClass(style+"hover");
}
function r_mouse_out_none(thisid,style){
	$("."+thisid).css({ "display":"none",opacity:0});
	$("."+style).removeClass(style+"hover");
}

//免运费

<!--搜索-->
$(".searchbtn").click(function(){
	var s=$.trim($(this).prev("input").val());

	if(s=="" || s.length<2)
	{
		alert('请输入搜索内容且不得少于2字符');
		return false;
	}
