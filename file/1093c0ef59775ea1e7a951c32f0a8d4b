function chgchkcd()
{
	$("#yzmimg").attr("src",'/checkCode.php?tm='+Math.random()).css({ "display":"block"});
}
$("#llogin").submit(function(){
	if($.trim($("#uname").val())=="")
	{
		alert('用户名不能为空');
		return false;
	}
	if($("#upwd").val()=="")
	{
		alert('密码不能为空');
		return false;
	}
	if($.trim($("#yzm").val())=="")
	{
		alert('验证码不能为空');
		return false;
	}
});
