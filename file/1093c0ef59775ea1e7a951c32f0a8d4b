  <script type="text/javascript">
function onover(){
	//$(this).children(".pinlei").removeAttr("style");
	$("#pinlei").css("display","block")
}
function onout(){
	$("#pinlei").css("display","none")
}
//$(".nav_fl").mouseover(function(){
//	$(this).children(".pinlei").removeAttr("style");
//});
//$(".nav_fl").mouseout(function(){
//	$(this).children(".pinlei").css({ "display":"none"});
//});

$("#pinlei ul li").mouseover(function(){
	$("#pinlei ul li").removeClass("hover");
	$("#pinlei").css("display","block")
	$("#pinlei ul li .xianshi").css({ "display":"none"});
	$(this).addClass("hover").children(".xianshi").removeAttr("style").css({ "top":"-"+$(this).children(".xianshi").attr("name")+"px"});	
});
$("#pinlei ul li").mouseout(function(){
	$("#pinlei ul li .xianshi").css({ "display":"none"});
	$("#pinlei ul li").removeClass("hover");
	$("#pinlei").css("display","none")
});

</script> 
