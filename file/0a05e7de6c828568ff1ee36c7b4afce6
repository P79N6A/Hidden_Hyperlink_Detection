			var speed3=25//速度数值越大速度越慢
			var demo2_jctj = document.getElementById("demo2_jctj");
			var demo1_jctj = document.getElementById("demo1_jctj");
			var demo_jctj = document.getElementById("demo_jctj");
			demo2_jctj.innerHTML=demo1_jctj.innerHTML
			function Marquee_jctj(){
			if(demo2_jctj.offsetWidth-demo_jctj.scrollLeft<=650)
			demo_jctj.scrollLeft-=demo1_jctj.offsetWidth
			else{
			demo_jctj.scrollLeft++
			}
			}
			var MyMar_jctj=setInterval(Marquee_jctj,speed3)
			demo_jctj.onmouseover=function() {clearInterval(MyMar_jctj)}
			demo_jctj.onmouseout=function() {MyMar_jctj=setInterval(Marquee_jctj,speed3)}
