	$(window).load(function() {
		var $avatar = $('.postauthor .avatar img');
		if($avatar.length){
			$avatar.each(function(){
				var avatarwidth = $(this).width();
				if(avatarwidth<=120){
					$(this).width(avatarwidth);
				}else{
					$(this).width(120);
				}
			});
		}
	});
})(jQuery);
