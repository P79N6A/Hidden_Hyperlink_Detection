function weChat(){
jQuery("#mcover").css("display","none");  // 点击弹出层，弹出层消失
}
jQuery(function(){
setTimeout(function () {
jQuery("#mcover").hide(); }, 8000);    //8000毫秒是隐藏层
 })
</script> 
