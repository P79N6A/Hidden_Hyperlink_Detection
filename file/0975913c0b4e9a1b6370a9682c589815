             <td align="left" style="color:#999">来源： &nbsp;&nbsp;&nbsp;&nbsp;作者：&nbsp;&nbsp;&nbsp;&nbsp;发布时间：2011-04-15&nbsp;&nbsp;&nbsp;&nbsp;浏览数：<span id="ajaxElement_1"></span> <script type="text/javascript" language="javascript">
function getQueryString()
{
	var retval = '';
	var queryString = document.location.search;
	if (queryString == null || queryString.length <= 1) return '';
	return decodeURI(decodeURI(queryString.substring(1)));
}
