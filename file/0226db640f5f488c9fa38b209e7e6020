  <script language="javascript" type="text/javascript">
function glog(){
    try{           
                linkname="149";
                address="149"+"wangyongw";
                wlink=linkname+"+"+"2015723115347870.jpg"+"+"+"面议"+"+"+"HYE-2000微机电液伺服压力试验机"+"+"+"52"+"+"+address;    
                old_info=getCookie("history_info");
                var insert=true;
                if(old_info==null){//判断cookie是否为空
                    insert=true;
                }
                else{    
                    var old_link=old_info.split("wangyongw");
                    for(var j=0;j<=5;j++){
                        if(old_link[j].indexOf(linkname)!=-1)
                            insert=false;
                            if(old_link[j]=="null")
                            break;
                    }
                }
                if(insert){
                    wlink+=getCookie("history_info");
                    setCookie("history_info",wlink);
                    //history_show().reload();
                    //break;
                }           
    }
    catch(e){}
    return true;
}
     glog();
</script> 
