  <script>
    function changeZimu_area (){
        $('.zimu_active').mousemove(function () {
            var zimu = $(this).attr('zimu-rel');
            var className=".zimuzuhe-" + zimu;
            var div=$(".zimuzuhe-con").find(className);
            $(".zimuzuhe-con div").css('display','none');
            if(div){
                div.css('display','');
            }
        }).click(function(){
            return false;
        });
    }
    changeZimu_area();
	$("#t_min, #t_max").focus(function(){
		$("#shai_price").show();
	});
	function shai (price) {
		var p_1 = parseInt($('.'+price+'_1').val());
		var p_2 = parseInt($('.'+price+'_2').val());
        if(isNaN(p_1) && isNaN(p_2)) {
            return false;
        }
		if (p_1 ==0 && isNaN(p_2)) {
			p_2 = 99999
		}
		if (p_2 ==0 && isNaN(p_1)) {
			p_1 = 0
		}
		if (p_1 > 0 && !p_2) {
			p_2 = 99999
		}
		if (p_2 > 0 && !p_1) {
			p_1 = 0
		}
		if (p_1 >= 0 && p_2 >= 0 && p_1 > p_2) {
			var k = p_2;
			p_2 = p_1;
			p_1 = k
		}

        var url = window.location.href;
        var uri = url.split("?");
        if (uri.length < 2)
        {
            window.location = url + '?minmax=' + p_1 + '_' + p_2;
        }
        var cond = uri[1].split("&");

        url = url.replace(eval('/'+price+'\\d{1,2}/g'),'');
        url = url.replace(eval('/\\?.*/g'), '');
        if(url.substr(-2,1) == url.substr(-1,1)) url = url.substr(0,url.length-1);

        var new_url = url + '?minmax=' + p_1 + '_' + p_2;
        var new_uri = '';
        for (var i=0; i<cond.length; i++)
        {
            if (cond[i].indexOf("minmax") >= 0)
            {
                continue;
            }
            new_uri = new_uri + '&' + cond[i];
        }
        window.location = new_url + new_uri;
	}
    //搜索
    function search(){
        var val = $("#search_kw").val();
        var urlpath = new Array();
        var url = "";
        if(location.pathname.indexOf("html")){
             urlpath = location.pathname.split("/");
            for(var i=0;i<(urlpath.length-1);i++){
                url = url + urlpath[i] + "/";
            }
        }
        var url =  url + "?query=" + val;
        $("#lbform").attr("action",url);
        $("#lbform").submit();
    }
    function gosearch(e){
        var keycode = 0;
        //IE浏览器
        if(CheckBrowserIsIE()){
            keycode = event.keyCode;
        }else{
            //火狐浏览器
            keycode = e.which;
        }
        if (keycode == 13 ) //回车键是13
        {
            search();
        }
    }
    //判断访问者的浏览器是否是IE
    function CheckBrowserIsIE(){
        var result = false;
        var browser = navigator.appName;
        if(browser == "Microsoft Internet Explorer"){
            result = true;
        }
        return result;
    }
</script> 
  <script>
    var query = "惠州 休闲 度假村";
    var keys = query ? query.split(' ') : [];
    var txts = [];
    for (var i = 0; i < keys.length; i++) {
        if (keys[i] == '') {
            continue;
        }
        txts.push(keys[i]);
    }
    $('.text-highlight').highlight(txts);
    $(".info").highlight(txts);
</script> 
  <script>
    $(function(){
        $('.icon3').hover(function(){
            $(this).children().children().addClass('current').siblings().removeClass('current');
        },function(){
            $(this).children().children().removeClass('current');
        });
    });
</script> 
  <script>
    $(function(){
        $('.dxtg_url').click(function(){
            var clickfrom = $(this).attr('clickfrom');
            var url = $(this).attr('href') + '?clickfrom=' + clickfrom
            window.open(url);
            return false;
        });
        $('.dxtg .detail').click(function(){
            var dxtg_url_obj = $(this).find('.title .name a');
            var clickfrom = dxtg_url_obj.attr('clickfrom');
            var url = dxtg_url_obj.attr('href') + '?clickfrom=' + clickfrom;
            window.open(url);
        });
    });
</script> 
  <script>
    $(".list-ul>li").on('mousemove',function(){
        if ($(this).children('ul').get(0)){
            $(this).find('ul').css("display","block");
            $(this).children('a').addClass("a-hover");
        }else {
            $(this).children('a').addClass('ahover');
        }
    }).on('mouseout',function(){
        $(this).find('ul').css("display","none");
        $(this).children('a').removeClass("a-hover ahover");
    });
</script> 
  <script>
    $(function(){
        $('.yuyue_vertop .click_btn').click(function(e){
            e.stopPropagation();
            $('.content_jumpDiv').click(function(e){
                e.stopPropagation();
            });
            $('.yuyue_vertop').css('z-index',99);
            $(this).parents('.yuyue_vertop').css('z-index',999);

            $('.content_jumpDiv').fadeOut('fast');
            $(this).parents('.yuyue_vertop').find('.content_jumpDiv').fadeIn("");
            var check = 0;
            var postid = $(this).attr("id");
            var phone = $(this).attr("data-phone");
            var obj1 = ".weixinqrcode_" + postid;
            $(obj1).each(function (){
                if ($(this).attr("src"))
                {
                    check = 1;
                }
            });
            if (check)
            {
                return false;
            }
            var url = "/ajax/weixinqrcode/";
            var cityname = "惠州";
            $.ajax({
                type: "post",
                url: url,
                data: {postid:postid, phone:phone},
                dataType: "json",
                success: function(data){
                    $(obj1).each(function (){
                        $(this).attr("src", data.url);
                    });
                    var obj2 = ".location_" + postid;
                    $(obj2).each(function (){
                        if (data.location)
                        {
                            $(this).text(data.location);
                        }
                        else
                        {
                            $(this).text(cityname);
                        }
                    });
                    if (data.location)
                    {
                        var obj3 = ".xiegang_" + postid;
                        $(obj3).each(function (){
                            $(this).show();
                        });
                    }
                }
            });
        });
        $('.yuyue_vertop .close_btn,body').click(function(){
            $('.content_jumpDiv').fadeOut('')
        })
    })
</script> 
