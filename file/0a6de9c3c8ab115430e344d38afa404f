<script>
function mover(object,place){
	var pl=new Array("home","projects","tendercenter","infocenter","businessbase");
	for (var i=0;i<10;i++){
		if(document.getElementById("m_d_" + i)){
			document.getElementById("m_d_" + i).style.display = i == object ? "block" : "none";
		}
		if(document.getElementById("m_a_" + i)){
			document.getElementById("m_a_"+i).className=i == object ? "daohangs_a02_20140611" : "daohangs_a01_20140611";
			if(pl[i]==place){		
				document.getElementById("m_a_"+i).className="daohangs_a03_20140611";
			}
		}
	}
	if(pl[object] && object!=0){
		$('#xialadh_bar').show();
	}
}
//鼠标离开时
function mleave(object,place){
	var pl=new Array("home","projects","tendercenter","infocenter","businessbase");
	for (var i=0;i<10;i++){
		if(document.getElementById("m_d_" + i)){
			document.getElementById("m_d_" + i).style.display = "none";
		}
		if(document.getElementById("m_a_" + i)){
			document.getElementById("m_a_"+i).className="daohangs_a01_20140611";
			if(pl[i]==place){		
				document.getElementById("m_a_"+i).className="daohangs_a03_20140611";
				/*if(document.getElementById("m_d_" + i)){
					document.getElementById("m_d_" + i).style.display = "block";
				}*/
			}
		}
	}
	$('#xialadh_bar').hide();
}
</script>

