<script type="text/javascript">
function mima(){
	$("#isbj1487").val(2);
	$("#infopass2").hide();
	$("#infopass2").val('');
	$("#infopass").show();
	$("#infopass").val('');
	$("#infopass").focus();
}
//显示logindiv
function showIndexLogin(){
	$("#index_loginid").show();
}

//离开登录时判断
function fun_1487_dl_hide(){
	var iid=document.getElementById('autodl1487');
	if(iid.checked || document.getElementById('isbj1487').value==2){
		$("#inde_denglu_20140709").show();
	}else{
		$("#inde_denglu_20140709").hide();
	}
}
//首页登录
function shouydl_on(){
	var SDLkuang = document.getElementById("inde_denglu_20140709");
	SDLkuang.style.display = "block";
}
function shouydl_off(){
	var SDLkuang = document.getElementById("inde_denglu_20140709");
	SDLkuang.style.display = "none";
//	var SDLkuangFj = document.getElementById("inde_denglu02_20140709");
//	SDLkuangFj.style.display = "none";
}

$(function(){
	$("#inde_denglu_20140709").hide();
     $('body').click(function(evt) {
         if($(evt.target).parents("#inde_denglu_20140709").length==0 && 
             evt.target.id != "inde_denglu_20140709" && evt.target.id != "loginid14717") {
             $('#inde_denglu_20140709').hide();
         }
     });
}); 
//首页-我关注的项目
function mygzdxmu(){
	var myDengLu = document.getElementById("mygzxm_uld_20140714");
	//myDengLu.style.display = "block";
	if(myDengLu.style.display == "block"){
		myDengLu.style.display = "none";
	}else{
		myDengLu.style.display = "block";
	}
}
//首页离开用户名
function infonameleave(obj){
	if(obj.value==''){
		obj.value='用户名';
	}
}
function yanzhengleave(obj){
	if(obj.value==''){
		obj.value='验证码';
	}
}
//新首页登录用户名
function checkcode_infoname(obj){
	$("#isbj1487").val(2);
	if(obj.value=='用户名'){
		obj.value='';
	}
	var sreurl = "/php/checkUser/code.php?" + new Date();
	document.getElementById("code1").src = sreurl;
}
//新首页登录验证码
function checkcode_yanzheng(obj){
	$("#isbj1487").val(2);
	if(obj.value=='验证码'){
		obj.value='';
	}
	var sreurl = "/php/checkUser/code.php?" + new Date();
	document.getElementById("code1").src = sreurl;
}
//验证登陆
window.onload = initnew;
function initnew(){
	check_sign_innew();
	updateGongnengLianjie();
	check_sign_bs();
}
function check_sign_innew(){
	$.post('/php/homenew/check_sign_in.php', {}, function(result) {
		if(result == "-1") {
			window.location = "/busy.html";
		}
		var denglu = document.getElementById("denglu");
		if(denglu) denglu.innerHTML = result;
	});
}
function check_sign_bs(){
	$.post('/php/homenew/check_sign_bsxz.php', {}, function(result) {
		var denglu = document.getElementById("bs14728");
		if(denglu) denglu.innerHTML = result;
	});
}
</script>

