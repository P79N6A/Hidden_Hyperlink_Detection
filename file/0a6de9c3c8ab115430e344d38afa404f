<script>
//导出html
function fun_1489_html(){
	var foldarr="";
	$("input[name='daochu1488[]']").each(function(){
		if(this.checked == true){
			foldarr=foldarr+','+$(this).val();
		}
	});
	if(foldarr == ''){
		alert('请选择要导出的记录');
	}else{
		foldarr=foldarr.substr(1);
		$("#excel_1488_uniseq").val(foldarr);
		$("#excel_form_id1488").attr("action","/php/new_site_center/excel_word/wordout.php?html=html");
		$("#excel_form_id1488").submit();
	}
}
//导出word
function fun_1489_word(){
	var foldarr="";
	$("input[name='daochu1488[]']").each(function(){
		if(this.checked == true){
			foldarr=foldarr+','+$(this).val();
		}
	});
	if(foldarr == ''){
		alert('请选择要导出的记录');
	}else{
		foldarr=foldarr.substr(1);
		$("#excel_1488_uniseq").val(foldarr);
		$("#excel_form_id1488").attr("action","/php/new_site_center/excel_word/wordout.php");
		$("#excel_form_id1488").submit();
	}
}
function fun_1489_excel(){
	var foldarr="";
	$("input[name='daochu1488[]']").each(function(){
		if(this.checked == true){
			foldarr=foldarr+','+$(this).val();
		}
	});
	if(foldarr == ''){
		alert('请选择要导出的记录');
	}else{
		foldarr=foldarr.substr(1);
		$("#excel_1488_uniseq").val(foldarr);
		//$("#excel_form_id1488").attr("action", "/php/saveExcelop_all.php");
		$("#excel_form_id1488").attr("action", "/php/new_site_center/excel_word/excel.php");
		$("#excel_form_id1488").submit();
	}
	//alert(foldarr);
}
function fun_1488_select(){
	if($("#a_n_1488").attr('checked')==true){
    $("[name='daochu1488[]']:checkbox").attr("checked", true); 
   }else{
    $("[name='daochu1488[]']:checkbox").attr("checked", false);
   }
}
