<script>
	$(document).ready(function(){
		var istruezhuce = true;
		var istruezhucefirst = true;
		var selectCurrentHeight=$('#info-zhuce-html').offset().top; 
		//滚动条事件  
		$(window).scroll(function(){  
			if(istruezhucefirst){
			//获取滚动条的滑动距离  
				var scroH1 = $(this).scrollTop(); 
				if(scroH1> selectCurrentHeight){
					if(istruezhuce){
						$('#info-zhuce-space').show();
						istruezhuce = false;
					}
				}
			}
		});
	});
	function hidedInfoZhuce(){
		$('#info-zhuce-space').hide();
		istruezhucefirst = false;
	}
</script>

