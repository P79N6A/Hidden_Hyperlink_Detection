var isphone = true;
function setFontsize(type){
    var whichEl = document.getElementById("doc_content");
	var typebig = document.getElementById("type-big");
	var typesmall = document.getElementById("type-small");
    if (whichEl!=null) {
        if (type==1){
				typebig.style.display="none";
				typesmall.style.display="block";
                whichEl.style.fontSize='18px';
                whichEl.style.lineHeight='30px';
        }else {
                whichEl.style.fontSize='16px';
                whichEl.style.lineHeight='28px';
				typebig.style.display="block";
				typesmall.style.display="none";
        }
    }
}

</script> 
