    $.ajax({
        type:'get',
        url:"http://guangzhou.liebiao.com/ajax/postclicks/?postid=149161259",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
