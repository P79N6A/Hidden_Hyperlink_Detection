;$(function() {
    var commuexp_getmore = '/v3/ajax/jiedu/random/';
    var comm_id = '0';
    if(jiedu_switch+''=='1'){
        $.ajax({
            url: commuexp_getmore,
            type: 'get',
            data: {comm_id: comm_id},
            dataType: 'json',
            success: function (res){
                if(res.status+'' == '0'&&res.data.jiedu.length>0){
                    var _d = res.data.jiedu;
                    var str = '<a class="commuexp-mod" href="'+res['data']['url']+'" data-soj="communitypro_more"><h3>小区专家<em><i class="commuexp-line"></i>为您解读该小区</em></h3><ul>';
                    var _list = '';
                    for(var i=0,len = _d.length;i<len;i++){
                        _list+='<li class="comuexp-item">';
                        _list+='<img src="'+_d[i]['user_photo']+'" alt="经纪人头像">';
                        _list+='<div class="commuexp-title"><h4>'+_d[i]['true_name']+'</h4></div>';
                        _list+='<p>'+_d[i]['comm_speciality']+'</p>';
                        _list+='</li>';
                    }
                    str+=_list;
                    str+='</ul>';
                    str+='<span class="commuexp-more"><i class="iconfont">&#xE084</i>查看全部解读</span></a>';
                    $('.commu-expert').html(str);

                    // $('.commu-expert .commuexp-mod').on('click', function (){
                    //     $('.commuexp-more').trigger('click');
                    // });
                }
            },
            error: function (){

            }
        });
    }

});
