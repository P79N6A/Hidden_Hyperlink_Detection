url:obj.attr('href') + '&handlekey=favbtn&inajax=1',
data:{'favoritesubmit':'true', 'formhash':'58303e95'},
dataType:'xml',
})
.success(function(s) {
window.location.reload();
//popup.open(s.lastChild.firstChild.nodeValue);
//evalscript(s.lastChild.firstChild.nodeValue);

})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
});
return false;
});
</script> 
   <script type="text/javascript">
$('.deletefavbtn').on('click', function() {
var obj = $(this);
$.ajax({
type:'POST',
url:obj.attr('href') + '&handlekey=favbtn&inajax=1',
data:{'deletesubmit':'true', 'formhash':'58303e95'},
dataType:'xml',
})
.success(function(s) {
window.location.reload();
})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
});
return false;
});
</script> 
