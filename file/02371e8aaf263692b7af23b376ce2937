  function check(){
    // alert("");
    // if(""){
    //   alert('您提交的过于频繁，请稍后进行评论！');
    //   return false;
    // }

    var url = "http://www.xmcm.com/zh/message/code.php"
    var code = $('input[name="code"]').val();  //验证码
     $.ajax({type:"POST",async:false,dataType:'json',url:url,data:{code:code},success:function(data){
          if(data == 0){
            alert("验证码有误！");
            phonedata = false;
          }else if(data == 3){
            alert("您提交的过于频繁，请稍后进行评论！");
            phonedata = false;
          }else{
            phonedata = true;
          }
    }});
  }
</script>  
