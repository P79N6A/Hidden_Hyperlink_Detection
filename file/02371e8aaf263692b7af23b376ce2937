$('.language a').click(function(){
	var _this = $(this);
	$('.language a').each(function(){
		$(this).removeClass('cursel');
	});
	_this.addClass('cursel');
});


$(function(){
	$("#subbtn").click(function(){
		var word = $("#word").val();
		if($.trim(word)==""){
			alert('请输入搜索关键词');
			return false;
		}
	});
	
	// console.log(getCookie('zh_choose'));
	if(getCookie('zh_choose')=='s'){
		$('.zh_tran_s').addClass('cursel');
	}else if(getCookie('zh_choose')=='t'){
		$('.zh_tran_t').addClass('cursel');
	}else{
		$('.zh_tran_s').addClass('cursel');
	}

});
</script> 
