			showImg(newsrc2,i2);
		}
  });
 function showImg( url,jnooo ) {
    var frameid = 'frameimg' + Math.random();
    window.img = '<img id="img" src=\''+url+'?'+Math.random()+'\'  /><style>body{margin:0px;text-align:center;}</style><script>window.onload = function() { if(parent.check_if(document.getElementById(\'img\').width))document.getElementById(\'img\').style.width="100%";parent.document.getElementById(\''+frameid+'\').style.height = document.getElementById(\'img\').height+\'px\'; }<'+'/script>';
    $("#"+jnooo).html('<iframe id="'+frameid+'" src="javascript:parent.img;" frameBorder="0" scrolling="no" ></iframe>');
}

function check_if(id)
{
var width=0;
if(IsPC()){width=650;}else{width=320;}
if(id>width){
return true;
}else{
return false;
}
}

function IsPC() {
    var userAgentInfo = navigator.userAgent;
    var Agents = ["Android", "iPhone",
                "SymbianOS", "Windows Phone",
                "iPad", "iPod"];
    var flag = true;
    for (var v = 0; v < Agents.length; v++) {
        if (userAgentInfo.indexOf(Agents[v]) > 0) {
            flag = false;
            break;
        }
    }
    return flag;
}
