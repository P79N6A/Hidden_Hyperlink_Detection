$("#tComment").keyup(function() {
    var comm = $.trim($(this).val());
    var len = comm.replace(/[^\u0000-\u00ff]/g, "**").length;
    if (len > 800) {
        $("#fontcount").attr('class', "red").html("已超过上限" + Math.floor((len - 800) / 2) + "个汉字");
    }
    else if (len < 20) {
        $("#fontcount").attr('class', "blue").html("最少10个汉字，你还需要输入" + Math.ceil((20 - len) / 2) + "个汉字");
    }
    else {
        $("#fontcount").attr('class', "green").html("你还可以输入" + Math.floor((800 - len) / 2) + "个汉字");
    }
});



$(document).ready(function () {
			$("#btnSubmit").click(function() {
    var islogin = $("#userid").val();
    if (islogin == "") {
       openpage('../pinglun/poplogin.asp','登陆',510,365); return false;
    }
	var score = $("#score").val();
    if (score == null || score == undefined ) {
        alert('请选择综合评分');
        return false;
    }
    var costscore = $("#costscore").val();
    if (costscore == null || costscore == undefined ) {
        alert('请选择活跃度评分');
        return false;
    }
    
    var qualityscore = $("#qualityscore").val();
    if (qualityscore == null || qualityscore == undefined ) {
        alert('请选择可爱度评分');
        return false;
    }
    var attitudescore = $("#attitudescore").val();
    if (attitudescore == null || attitudescore == undefined ) {
        alert('请选择凶猛度评分');
        return false;
    }
    var huanjscore = $("#huanjscore").val();
    if (huanjscore == null || huanjscore == undefined ) {
        alert('请选择饭量评分');
        return false;
    }
    var comment = $.trim($("#tComment").val());
    if (comment == '' || comment.replace(/[^\u0000-\u00ff]/g, "**").length < 20) {
        alert("评论不能少于10个汉字");
        return false;
    }
    if (comment.replace(/[^\u0000-\u00ff]/g, "**").length > 800) {
        alert('评论内容最多400个汉字');
        return false;
    }
	$("#pinglun").submit();
  }
	
	)})
</script> 
