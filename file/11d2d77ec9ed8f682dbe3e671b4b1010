        //右侧广告随滚动条显示
        $(window).bind("scroll", scrollfun);
        function scrollfun() {

            if ($(document).scrollTop() > 138) {
                if (!window.XMLHttpRequest) {
                    $("#divad5").css({ position: "absolute" });
                    $("#divad5").css("top", $(document).scrollTop() + "px");
                }
                else {
                    $("#divad5").css({ position: "fixed", top: "55px" });
                }
            }
            if ($(document).scrollTop() < 138) {
                $("#divad5").css({ position: "" });
            }
        }
        //关闭左侧跟右侧滚动广告
        function closead(type) {
            if (type == 7) {
                $("#divad1").hide();
            }
            else if (type == 8) {
                $("#divad2").hide();
            }
        }
    </script> 
