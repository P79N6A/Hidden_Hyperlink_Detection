  <script type="text/javascript">
	var timeplus = 0;
	var _djs = null;
	function ShowCountDown(end) {
		var endDate = new Date();
			endDate.setTime(end);
		var now = new Date();
			now.setTime(_rush_nowtime);
		// var endDate = new Date(year, month-1, day);
		var leftTime=endDate.getTime()-now.getTime();
		var leftsecond = parseInt(leftTime);
		var day1=Math.floor(leftsecond/(60*60*24));
		var hour=Math.floor((leftsecond-day1*24*60*60)/3600);
		var minute=Math.floor((leftsecond-day1*24*60*60-hour*3600)/60);
		var second=Math.floor(leftsecond-day1*24*60*60-hour*3600-minute*60);
		hour+= day1*24
		//cc.innerHTML = hour+"小时"+minute+"分"+second+"秒";
		if(hour==0&&minute==0&&second==0){
			location.reload();
		}
		$("#rush-djs").html(hour+":"+minute+":"+second);
		_djs = setTimeout(function(){ShowCountDown(end)},1000);
	}
	function rushButton(stock){
		if(stock!=0){
			$("#rush-button").hide();
	    	$("#add2Cart").show();
		}else{
			$("#rush-button").show();
	    	$("#add2Cart").hide();
		}
	}
	function show_rush(rushEndTime,rushStock){
		if(rushEndTime && rushStock!=null){
			$("#showPrice").addClass("rush-buy-red");
			var _stock = '';
			if(rushStock>0){
				_stock = '还剩'+rushStock+'件';
			}
			$("#rush-warp").html('<div class="forecast-detail clearfix">'+
								'	        			<span class="forecast-detail-name">抢购</span>'+
								'	        			<span class="forecast-detail-price">'+
																_stock+
								'								 <em id="rush-djs"></em>后恢复原价'+
								'	        			</span>'+
								'	        		</div>').show();
			clearTimeout(_djs);
			ShowCountDown(rushEndTime);
			rushButton(rushStock);
		}else{
			$("#rush-warp").html("");
			$("#showPrice").removeClass("rush-buy-red");
			rushButton(1000);
		}
	}
	var _rush_endtime = null;
	var _rush_nowtime = null;
	var _rush_stock = null;
	
</script> 
