              console.log(data)
              if(data.success){
                  var _len = data.retObj.length
                  for(var i=0;i<_len;i++){
                      var _price = ''
                      if(data.retObj[i].discountPrice
                          && data.retObj[i].discountPrice<data.retObj[i].price){
