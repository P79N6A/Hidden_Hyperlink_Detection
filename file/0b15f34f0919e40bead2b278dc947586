  <script>
$(document).ready(function(){
	  $('#top .show_table .topmenu ul').each(function(){
	    $(this).hover(function(){
	      var len = $(this).find('li').length;
	      $(this).stop().animate({height: 40*len+'px'}, 200);
	    },function(){
	      $(this).stop().animate({height: '40px'}, 200);
	    });
	  });


	  $("#top .login").find("a:first-child").mouseover(function(){
		  $("#login_top").fadeIn();
	  });
	  $("#login_top").mouseleave(function(){
		  $("#login_top").hide();
	  });
	  $("#top .cart a").mouseover(function(){
		  $("#cart_top").fadeIn();
	  });
	  $("#cart_top").mouseleave(function(){
		  $("#cart_top").hide();
	  });

	  $("#skeyword").keydown(function(event){
	        if(event.keyCode == 13){
	        	serchItems();
	        }
	  });

	//登录后面板js
	  $("#loginin-til, #account_top").hover(function(){
		  $("#account_top").fadeIn();
	  }, function(){
         $("#account_top").hide();
      });

});

function serchItems(){
	var skeywordVal = document.getElementById('skeyword').value;
	if(skeywordVal!=''){
        _tj.type='mv';
        _tj.handleName = 'pc-search';
        _tj.searchInner = encodeURIComponent(skeywordVal);
        _tj.push(_tj);

		window.location.href="/app/item/search.htm?searchKeyword="+skeywordVal;
	}else{
		alert('请输入搜索词');
	}
}

//weibo login
function wbLogin(){
	$.ajax({
		type:"post",
	    url:"/user/wbLogin.htm",
	    dataType:"json",
	    success:function(data){
	    	if(data.success==true){
                _tj.type='mv';
                _tj.handleName = 'pc-weibo-login';
                _tj.push(_tj);
	    	//	window.open(data.retObj,'_blank , width=800 , height=400');
	    	$("#wblogin").attr("href",data.retObj);
	    	}

	    }
	});
}

//load miniCart data
$(document).ready(function(){
	loadMinicart();
});

//load minicart data function
function  loadMinicart(){

	$.ajax(
			{
				type:"get",
				url:"/app/cart/packs/size",
				dataType:"json",
				success:function(data){
					if(data.code==200&&data.data>0){
						$("#cart_count").html(data.data+"件商品");
						$("#cart_count").attr("rel",data.data);
					}else{
						$("#cart_count").html("");
					}
				},
				error:function(){
					$("#cart_count").html("");
//					$("#cart_count").attr("rel",0);
				}
			}
	)
	$.ajax({
		type:"get",
	    //url:"/cart/minicart.htm",
	    url:"/app/cart/minicart.htm",
	    dataType:"json",
	    success:function(data){
	    	//clean element before load
	        $("#cart-sidebar").empty();
 	    	$("p[class='subtotal']").empty();
 	    	$("p[class='subtotal']").remove();
 	    	$("div[class='actions']").empty();
 	    	$("div[class='actions']").remove();
 	    	$("a[id='checkout']").empty();
 	    	$("a[id='checkout']").remove();
	    	if(data.code==200){
	    		var packs = data.data.packs;
	    		if(packs.length>0){
	    			// $("#cart_count").attr("href","/app/cart/viewCart.htm");
	    			$.each(packs,function(i,pack){
	    				var li=$("<li class=\"item odd\"></li>");

	    				var a=$("<a class=\"product-image\"></a>").attr("href","/app/item/detail/"+pack.productId+".htm").attr("title",pack.spv.name);
	    				var img=$("<img width=\"80\" height=\"83\"/>").attr("src",pack.spv.image+'@2e_0o_0l_721h_690w_90q.src');
	    				var div_product_details=$("<div class=\"product-details\"></div>");
	    				var p_product_name=$("<p class=\"product-name\"></p>");
	    				var a_title=$("<a>"+pack.spv.name+"</a>").attr("href","/app/item/detail/"+pack.productId+".htm");
	    				var span_quantity=$("<span>数量:"+pack.count+"</span>");
	    				var span_price=$("<br><span>价格: <span class=\"price\">"+pack.spv.price+"</span></span>");
	    				li.append(a.append(img));
	    				div_product_details.append(p_product_name.append(a_title)).append(span_quantity).append(span_price);
	    				li.append(div_product_details);
	    				$("#cart-sidebar").append(li);

	    			});

	    			var subtotal=$("<p class=\"subtotal\"> <span class=\"label\">商品总计：</span> <span id=\"totalPrice\" class=\"price\">"+'06'+data.data.price+"</span> </p>");
	    			// var subtotalPoint=$("<p class=\"subtotal\"> <span class=\"label\">积分总计：</span> <span id=\"totalPoint\" class=\"price\">"+totalPoint+"</span> </p>");

	    			var div_actions=$("<div class=\"actions\"><br/></div>");

	    			var button_checkout=$("<button onclick=\"_tj.type='mv',_tj.handleName = 'pc-click-cart', _tj.push(_tj);window.location='/app/cart/viewCart.htm';\" title=\"查看购物车\" class=\"button J_ClickCart\"><span><span>查看购物车</span></span></button>");
					$("#block-content").append(subtotal);
					$("#block-content").append(div_actions.append(button_checkout));
	    		}else{
	    			// $("#cart_count").html("");
	    			var span=$("<span>您的购物车中没有商品。</span>");
	    			$("#cart-sidebar").append(span);
	    		}
	    	}
	    }

	});
}
//top dologin
function top_doLogin(){
	var username=$("#top_username_id").val();
	var password=$("#top_password_id").val();
	if(username==""||password==""){
		location.href="/user/login.htm";
		return;
	}
	$.ajax({
		type:"post",
		async:false,
	    url:"/user/dologin.htm",
	    data:{"loginName":username,"password":password},
	    dataType:"json",
	    success:function(data){
	    	if(data.success==false){
	    		window.location="/user/login.htm";
	    	}else{
                _tj.type='mv';
                _tj.handleName = 'pc-login';
                _tj.push(_tj);
	    		if(data.retObj!=null&&data.retObj!=""){
	    			window.location=data.retObj;
	    		}else{
	    		  window.location="/";
	    		}
	    	}
	    } ,
	    error: function(){
	        alert('请求失败!');
	    }
	});
}

//logout
function top_logout(){
	$.ajax({
		type:"post",
	    async:false,
	    url:"/user/logout.htm",
	    dataType:"json",
	    success:function(data){
	    	if(data.success==false){
	    		window.location="/user/login.htm";
	    	}else{
	    		window.location="/";
	    	}
	    } ,
	    error: function(){
	        alert('请求失败!');
	    }
	});
}

//统计
$('.J_ClickCart').bind(function(){
    !!'' && (_tj.type='mv',_tj.handleName = 'pc-click-cart', _tj.push(_tj));
});
</script> 
