function selSubItemChange(subitem){
	//var selSubItemVal = $('#selSubItem').val();


	var id =  $(subitem).val();

	if(id==-1){
		$(".rush-tips").show();
		$("#showPrice").html("<span> 06129.00</span>");
		show_rush(_rush_endtime,_rush_stock);
		return;
	}
	$(".rush-tips").hide();
	var _select = $(subitem).children('option:selected');
    var planedDeliveryDateStr =  _select.attr("planedDeliveryDateStr");
    var discountPrice =  _select.attr("discountPrice");
    var _minA =  _select.attr("minAmount");
    var price = _select.attr("normalprice");
    var ygprice = _select.attr("ygprice");
    var rushStock = _select.attr("rushStock");
    var rushEndTime = _select.attr("rushEndTime");
    // console.log(rushStock+'||'+rushEndTime)
	if(planedDeliveryDateStr==undefined){
		planedDeliveryDateStr="";
	}

	$("#planedDeliveryDateStr").html(planedDeliveryDateStr);
	var balance = parseFloat(price)-parseFloat(discountPrice);
	$("#showPrice").html("");
	setminAmount(_minA);
	// if(discountPrice!='' && balance!=0){
	// 	$("#showPrice").html("原价：<span class='delline'>06"+price+"</span><br><span style='margin-left:39px;'>06"+discountPrice+"</span>");
	// }else{
	// 	$("#showPrice").html("原价：06"+discountPrice);
	// }
	if(ygprice){
		$("#forecast-detail-price").html('06'+ygprice);
		$("#forecast-detail").show();
	}else{
		$("#forecast-detail").hide();
	}
	var _discountPrice = discountPrice;
	if(rushStock&&rushEndTime){
		_discountPrice+= '<em>抢购价</em>';
	}
	show_rush(rushEndTime,rushStock);
	if(discountPrice!='' && balance!=0){
		$("#showPrice").html("<span >06"+_discountPrice+"</span><span class='delline'>06"+price+"</span>");
	}else{
		$("#showPrice").html("<span >06"+_discountPrice+"</span>");
	}


}


function hiden(){
    _tj.type='mv',_tj.handleName = 'pc-goShopping',_tj.push(_tj);
	 $("#cart_add").fadeOut();
	  $("#meng").fadeOut();
}
function shengclick(shengid, shengname){
	jQuery("#address_shi").html("");
	jQuery("#address_qu").html("");
	//jQuery("#datediv").html("");
	jQuery("#datediv").fadeOut();
	jQuery("#canaddcart").val("false");
	jQuery("#stockoutDiv").fadeOut();

	var skuId='PROD001018508';//2016 情人节配送
	//情人节特殊判断。北京2 、天津26、河北9 出现送达提醒
	var pickScopeType = $("#pickScope").val();
	var pickScope ="";
	if(pickScopeType==1 || pickScopeType==8){
		pickScope="SELF";
	}else if(pickScopeType==2){
		pickScope="FEDEX24";
	}
	jQuery.ajax({
	    type: "post",
	    async : false,
	    url : '/pcs/city.htm?proviceOeId='+shengid+'&pickScope='+pickScope+'&skuId='+skuId,
	    dataType:'json',
	    success: function(jsonData) {    //回传函数实体，参数为XMLhttpRequest.responseText
	    	if(jQuery(jsonData.success)){
	    		jQuery(jsonData.retObj).each(function() {
	    			jQuery("#address_shi").append("<li onclick=\"javascript:shiclick("+this['oeId']+", '"+this['name']+"')\">"+this['name']+"</li>");
	    		});
	    	}else{
	    		//错误信息提示
	    	}
        },
        error: function(){
	        //alert('加载菜单数据失败!');
	    }
    });

	jQuery("#d_sheng").css("border-bottom-color","#000");
	jQuery("#d_shi").css("border-bottom-color","#fff");
	jQuery("#d_qu").css("border-bottom-color","#000");

	jQuery("#address_sheng").hide();
	jQuery("#address_shi").fadeIn();
	jQuery("#address_qu").hide();
	jQuery("#d_sheng").text(shengname);
}

function shiclick(shiid, shiname){
	jQuery("#address_qu").html("");
	//jQuery("#datediv").html("");
	jQuery("#datediv").fadeOut();
	jQuery("#canaddcart").val("false");
	jQuery("#stockoutDiv").fadeOut();
	var pickScopeType = $("#pickScope").val();
	var pickScope ="";
	if(pickScopeType==1 || pickScopeType==8){
		pickScope="SELF";
	}else if(pickScopeType==2){
		pickScope="FEDEX24";
	}

	var skuId='PROD001018508';//2016 情人节配送

	jQuery.ajax({
	    type: "post",
	    async : false,
		url : '/pcs/district.htm?cityOeId='+shiid+'&pickScope='+pickScope+'&skuId='+skuId,
	    dataType:'json',
	    success: function(jsonData) {    //回传函数实体，参数为XMLhttpRequest.responseText
	    	if(jQuery(jsonData.success)){
	    		jQuery(jsonData.retObj).each(function() {
	    			jQuery("#address_qu").append("<li onclick=\"javascript:quclick("+this['id']+", '"+this['name']+"')\">"+this['name']+"</li>");
	    		});
	    	}else{
	    		//错误信息提示
	    	}
        },
        error: function(){
	        //alert('加载菜单数据失败!');
	    }
    });

	jQuery("#d_sheng").css("border-bottom-color","#000");
	jQuery("#d_shi").css("border-bottom-color","#000");
	jQuery("#d_qu").css("border-bottom-color","#fff");

	jQuery("#address_sheng").hide();
	jQuery("#address_shi").hide();
	jQuery("#address_qu").fadeIn();
	jQuery("#d_shi").text(shiname);

}

function quclick(quid, quname){
	var pickScope = '4';
	jQuery("#d_qu").text(quname);

	jQuery.ajax({
	    type: "post",
	    async : false,
		url : '/pcs/districtCanPost.htm?districtId='+quid,
	    dataType:'json',
	    success: function(jsonData) {    //回传函数实体，参数为XMLhttpRequest.responseText
	    	if(jQuery(jsonData.success)){
	    		var flowerDeliveryType = jsonData.retObj.flowerDevType;
	    		//var hasDdays = '';
	    		//alert(flowerDeliveryType + "  " + pickScope);
	    		if(flowerDeliveryType==1){//地区支持8小时
	    			if(pickScope==2){
	    				jQuery("#canaddcart").val("true");
	    				//if(hasDdays=='1')
	    					//jQuery("#datediv").fadeIn();
	    			}
					if(pickScope==1 || pickScope==8){
						jQuery("#canaddcart").val("true");
						//if(hasDdays=='1')
							//jQuery("#datediv").fadeIn();
	    			}
	    		}else if(flowerDeliveryType==2){//地区支持24小时
					if(pickScope==2){
						jQuery("#canaddcart").val("true");
						//if(hasDdays=='1')
							//jQuery("#datediv").fadeIn();
	    			}
					if(pickScope==1 || pickScope==8){
						jQuery("#canaddcart").val("notsupport");
						jQuery("#stockoutDiv").fadeIn('slow');
					}
	    		}else if(flowerDeliveryType==0){
	    			jQuery("#canaddcart").val("notsupport");
	    			jQuery("#stockoutDiv").fadeIn('slow');
	    		}
	    		jQuery(".address_nei").fadeOut();
                $('#d_qu').css({'borderBottomColor': '#000'});
	    	}else{
	    		//错误信息提示
	    	}
        },
        error: function(){
	        //alert('加载菜单数据失败!');
	    }
    });
}
function add2CartLogin(skuId){
	var redirectUrl = "/app/item/detail/"+skuId+".htm";
	location.href="/user/login.htm?redirectUrl="+escape(redirectUrl);
}
//加入购物车
var timerCart = null;
function add2Cart(smallImg,itemId,skuId,quantity){
	var selectId = $("#selSubItem").val();
	if(selectId==-1){
		alert('请选择');
		return;
	}
	var itemName=$('.product_name').html();

	if(document.getElementById('canaddcart')) {
		var canaddcart = jQuery("#canaddcart").val();
		if(canaddcart=="notsupport"){
			alert('您选择的区域暂时缺货');
			return;
		}
		if(canaddcart=="false"){
			alert("请先选择送达省市区，确认是否可以配送");
			return;
		}
	}
    $("#cart_add .in").css({top: 0, left: 0, width: '515px', height: 'auto', opacity: 1});

	var _json = {
		"data":[{
			/*"skuCode":selectId,*/
			 "spvId":selectId,
			"count":minAmount,
			"source":"RAW"
		}]
	}
	$.ajax({
		type:"post",
		url:"/app/cart/packs",
		data:JSON.stringify(_json),
		dataType:"JSON",
		contentType:"application/json",
		success:function(data){
			console.log(data);
			if(data.code>=200&& data.code<300){
				_tj.type='mv';
				_tj.handleName = 'pc-addCart';
				_tj.sku = selectId;
				_tj.push(_tj);
				loadMinicart();
				$("#last_add_item_img").attr("src",smallImg+'@2e_0o_0l_136h_130w_90q.src');
				$("#last_add_item_name").html(itemName+"已经成功加入您的购物车。");
//				var relateItemsHtml="";
//				for(var i=0;i<data.retObj.length;i++){
//					var item=data.retObj[i];
//					relateItemsHtml=relateItemsHtml+"<p><a href='" +"/app/item/detail/"+item.itemId+".htm'"+"><img width=\"92\" height=\"96\" src='"+item.cartImgUrl+ "@2e_0o_0l_721h_690w_90q.src'/><br>"+item.title+"</a></p>";
//				}
//				$("#relateItems").html("");
//				relateItemsHtml=relateItemsHtml+"<div class='clear h15'></div>";
//
//				$("#relateItems").html(relateItemsHtml);

				$("#cart_add").fadeIn();
				$("#meng").fadeIn();
				timerCart = setTimeout(function(){
					$("#cart_add .in").css({position: 'relative'}).animate({
						top: '-160px',
						left: '480px',
						width:0,
						height:0,
						opacity: 0
					}, 400);
					$("#meng").fadeOut();
				},5000);
			}else{
				alert(data.message);
				//location.reload();
			}
		},
		error:function(){
			console.log(123);
		}

	});




}

//加入收藏夹
function add2Fav(itemId,skuId){
	$.ajax({
		type:"post",
		url:"/favorite/add.htm",
		data:{"itemId":itemId,"skuId":skuId},
		dataType:"json",
		success:function(data){
			if(data.success==true){
                _tj.type='mv';
                _tj.handleName = 'pc-addFav';
                _tj.push(_tj);
                alert('已加入收藏!');
				//location.href="/favorite/list.htm";
			}else{
				if(data.message=="noLogin"){//未登录用户要求登陆
					location.href="/user/login.htm?redirectUrl="+escape("/favorite/list.htm?type=add");
				}else{
					alert(data.message);
				}
			}
		}
	});
}
function setminAmount(_minAmount){
	minAmount = _minAmount;
	//最低起售判断
	if(minAmount && minAmount>1){
		$("#minAmount").html(minAmount+"件起售");
	}else{
		$("#minAmount").html("");
	}
}


