  <script type="text/javascript">
    var HT_Upload = (function(HU){
        HU.data = {
            name:'',
            id:'',
            isreload:1,
            imgf:'',
            imgb:'',
            isCreateOrder:0,
            uploadStatus:0,
            planStatus:null,
            planNum:0,
            cardId:0
        };
        HU.init = function(){
            jQuery("#J_CloseUpload").bind('click',function(){
                jQuery(".id-upload-warp").hide();
                HU.planCancel();
            })
            jQuery("#J_ImgSubmit").bind('click',function(){
                console.log(HU.data.name);
                console.log(HU.data.id);
                console.log('正面'+HU.data.imgf);
                console.log('背面'+HU.data.imgb);
                if(HU.data.imgf==''){
                	 alert("请上传身份证正面");
                	 return;
                }
                if(HU.data.imgb==''){
               	 alert("请上传身份证背面");
               	 return;
               }

                var url = "";
                if(!HU.data.isCreateOrder){
                	url = "/idcard/saveIdCardImg.htm";
                }else{
                	url = "/idcard/uploadImgAndImgStatus.htm";
                }
                $.ajax({
                    type:"post",
                    url:url,
                    data:{
                    	name:HU.data.name,
                        idCard:HU.data.id,
                        frontImg:HU.data.imgf,
                        backImage:HU.data.imgb
                    },
                    dataType:"json",
                    success:function(data){
                        if(data.success==true){
                            HU.data.uploadStatus =1;
                            $("#imgUpload").hide();
                            alert('身份证上传成功正在等待审核，赶紧去下单吧！');
                            HU.data.isreload?location.reload():jQuery(".id-upload-warp").hide();
                        }else{
                           alert(data.message);
                        }
                       }
                    });
            })
        }
        HU.plusXing = function(str,frontLen,endLen) {
            var len = str.length-frontLen-endLen;
            var xing = '';
            for (var i=0;i<len;i++) {
                xing+='*';
            }
            return str.substr(0,frontLen)+xing+str.substr(str.length-endLen);
        }

        HU.planStart = function(){
            HU.data.planStatus = setTimeout(function(){
                console.log(HU.data.planNum);

                if(HU.data.planNum>=100||HU.data.uploadStatus==1){
                    HU.planCancel();
                    return false;
                }
                HU.data.planNum++;

                if(HU.data.isCreateOrder==0){
                    $.ajax({
                        type:"get",
                        url:"/idcard/getIdCardByOrderNo.htm?orderNo="+HU.data.orderNo,
                        success:function(data){
                            if(data.retObj.auditStatus==2  || data.retObj.auditStatus ==1){
                                HU.planCancel();
                                if(HU.data.uploadStatus==1){
                                    return false;
                                }else{
                                    HU.data.uploadStatus=1;
                                    $("#imgUpload").hide();
                                    HU.data.isreload?location.reload():jQuery(".id-upload-warp").hide();
                                }
                            }else{
                                HU.planStart();
                            }
                        },
                        error:function(){
                            HU.planStart();
                        }
                    });
                }else{
                    $.ajax({
                        type:"get",
                        url:"/idcard/searchIdCardById.htm?idCard="+HU.data.cardId,
                        success:function(data){
                            if(data.retObj.imgStatus==2 || data.retObj.imgStatus ==1){
                                HU.planCancel();
                                if(HU.data.uploadStatus==1){
                                    return false;
                                }else{
                                    HU.data.uploadStatus=1;
                                    HU.planCancel();
                                    $("#imgUpload").hide();
                                    if(data.retObj.imgStatus ==1){
                                    	 alert('身份证上传成功正在等待审核，赶紧去下单吧！');
                                    }
                                   
                                    HU.data.isreload?location.reload():jQuery(".id-upload-warp").hide();
                                }
                            }else{
                                HU.planStart();
                            }
                        },
                        error:function(){
                            HU.planStart();
                        }
                    });
                }



            },3000)
        }

        HU.planCancel = function(){
            HU.data.planNu=0;
            clearTimeout(HU.data.planStatus);
        }

        HU.show = function(name,id,isreload,orderNo,isCreateOrder,cardId){
            this.data.name=name;
            this.data.id=id;
            this.data.isreload = isreload;
            this.data.orderNo=orderNo;
            this.data.isCreateOrder = isCreateOrder?isCreateOrder:0;
            this.data.cardId=cardId;
            $("#J_IdInfo_pop").html(name+'&nbsp;'+this.plusXing(id+'',4,4));

            if(isCreateOrder == 1){
                var html = '<img id="idcardImg" alt="" src="/idcard/idCardImgUploadScan.htm?cardId='+cardId+'&memberCode=" />';
                $("#erweima").html(html);
                $(".id-scan-code").removeClass("none");
                $(".id-upload-pop").css({
                    height:'600px',
                    margin:'-300px 0 0 -250px'
                })
                $("#J_ReturnCart").show();
            }else{
                var html = '<img id="idcardImg" alt="" src="/idcard/idcardcode.htm?orderNo='+orderNo+'&memberCode=" />';
                $("#erweima").html(html);
                $(".id-scan-code").removeClass("none");
                $(".id-upload-pop").css({
                    height:'600px',
                    margin:'-300px 0 0 -250px'
                })
                $(".id-upload-pop").css({
                    height:'560px',
                    margin:'-280px 0 0 -250px'
                });
            }
            jQuery(".id-upload-warp").show();
            HU.planStart();
        };
        return HU;
    })(window.HT_Upload||{});
    HT_Upload.init();
    $("#J_ReturnCart").on('click',function(){
        window.location.href="/app/cart/viewCart.htm";
    })
    KindEditor.ready(function(K) {
                var uploadbutton = K.uploadbutton({
                    button : K('#J_FrontID')[0],
                    fieldName : 'FrontID',
                    url : '/common/uploadIdCard.htm',
                    afterUpload : function(data) {
                        if (data.error === 0) {
                            htloading(0,0);
                            var url = K.formatUrl(data.url, 'absolute');
                            HT_Upload.data.imgf=data.url;
                            showidimg(0,url);
                        } else {
                            htloading(0,0);
                            alert(data.message);
                        }
                    },
                    afterError : function(str) {
                        alert('自定义错误信息: ' + str);
                    }
                });
                var uploadbutton1 = K.uploadbutton({
                    button : K('#J_BackID')[0],
                    fieldName : 'BackID',
                    url : '/common/uploadIdCard.htm',
                    afterUpload : function(data) {
                        if (data.error === 0) {
                            htloading(1,0);
                            var url = K.formatUrl(data.url, 'absolute');
                            HT_Upload.data.imgb=data.url;
                            showidimg(1,url);
                        } else {
                            htloading(1,0);
                            alert(data.message);
                        }
                    },
                    afterError : function(str) {
                        alert('自定义错误信息: ' + str);
                    }
                });
                uploadbutton.fileBox.change(function(e) {
                    var ffile = $("input[name='FrontID']").val();
                    if(!/\.(gif|jpg|jpeg|png|GIF|JPG|PNG)$/.test(ffile))
                    {
                      //$("#J_FrontID .id-upload-loading").hide();
                      alert("请上传图片文件");
                      return false;
                    }
                    htloading(0,1);
                    uploadbutton.submit();
                });
                
                uploadbutton1.fileBox.change(function(e) {
                    var bfile = $("input[name='BackID']").val();
                   if(!/\.(gif|jpg|jpeg|png|GIF|JPG|PNG)$/.test(bfile))
                    {
                      //$("#J_FrontID .id-upload-loading").hide();
                      alert("请上传图片文件");
                      return false;
                    }
                    htloading(1,1);
                    uploadbutton1.submit();
                });
            });
    
    function htloading(data,flag){
        var loading = jQuery(".file-input-loading:eq("+data+")");
        flag?loading.show():loading.hide();
    }
    function showidimg(data,url){
        jQuery(".id-img:eq("+data+") img").attr('src',url);
        jQuery(".id-img:eq("+data+")").show();
    }
    //刷新验证码
    function flushVcode(){
        $("#vcodeImg").attr("src","/auth/vcode.htm?t="+Math.random());
    }

    jQuery(document).ready(function($){
        var areaArray = ['+86','+1','+7','+20','+27','+30','+31','+32', '+33','+34','+36','+39','+40','+41','+43','+44','+45','+46','+47','+48','+49','+51','+52','+53','+54','+55','+56','+57','+58','+60','+61','+62','+63','+64','+65','+66','+81','+82','+84','+90','+91','+92','+93','+94','+95','+98','+211','+212','+213','+216','+218','+220','+221','+222','+223','+224','+225','+226','+227','+228','+229','+230','+231','+232','+233','+234','+235','+236','+237','+238','+239','+240','+241','+242','+243','+244','+245','+247','+248','+249','+250','+251','+252','+253','+254','+255','+256','+257','+258','+260','+261','+262','+263','+264','+265','+266','+267','+268','+269','+297','+298','+299','+350','+351','+352','+353','+354','+355','+356','+357','+358','+359','+370','+371','+372','+373','+374','+375','+376','+377','+378','+380','+381','+382','+385','+386','+387','+389','+420','+421','+423','+501','+502','+503','+504','+505','+506','+507','+508','+509','+590','+591','+592','+593','+594','+595','+596','+597','+598','+599','+670','+673','+675','+676','+677','+678','+679','+680','+682','+685','+686','+687','+689','+852','+853','+855','+856','+880','+886','+960','+961','+962','+963','+964','+965','+966','+967','+968','+970','+971','+972','+973','+974','+975','+976','+977','+992','+993','+994','+995','+996','+998','+1242','+1246','+1264','+1268','+1284','+1340','+1345','+1441','+1473','+1649','+1664','+1671','+1684','+1721','+1758','+1767','+1784','+1787','+1809','+1868','+1869','+1876'],
        phoneAreaPop = $('#J_PhoneAreaPop');

        //只有首页显示绑定手机弹窗
        !function(){
            var show = '';
            if(location.pathname == '/' && show=='0'){
                $('.u-mask').removeClass('none');
            }
        }();
        //弹窗获取验证码
        jQuery('#J_GetPhoneCode').bind('click', function(){
            cleanErr();
            var $verp = $('#J_PhoneNum'),
                verifyPhone = $verp.val(),
                $that = $(this),
                vcode = $("#J_PhonePic").val();
            var areaCode=$('#J_PhoneAreaPop').val();
            $.ajax({
                type:"post",
                url:"/user/sendValidateCode.htm",
                dataType:"json",
                data:{"mobile":verifyPhone,"vcode":vcode,"areaCode":areaCode},
                beforeSend: function(){
                if(vcode==''){
                    $('.J_ErrTips').text('请输入图形验证码！');
                    $("#J_PhonePic").addClass('err');
                    return false;
                }else if(verifyPhone==''){
                    $('.J_ErrTips').text('手机号不能为空！');
                    $verp.addClass('err');
                    return false;
                }else if($.inArray(phoneAreaPop.val(), areaArray)<0){
                    $('.J_ErrTips').text('暂不支持此国际区号！');
                    $verp.addClass('err');
                    return false;
                }
            },
            success: function(data){
                if(data.success==true){
                    flushVcode();
                    $that.prop({disabled: true}).text('重新获取(60)');
                    countSec(60, function(s){
                        $that.text('重新获取('+s+')');
                    }, function(){
                        $that.text('重新获取').prop({disabled: false});
                        $that=null;
                    });
                }else{
                    $('.J_ErrTips').text(data.message);
                }

            }
            });
            });

        //验证绑定手机号
        jQuery('.J_PhoneBind').on('keyup', function(){
            cleanErr();
            if(checkEmpty($('.J_PhoneBind'))){
                $('#J_PhoneCheck').prop({disabled: false});
            }else{
                $('#J_PhoneCheck').prop({disabled: true});
            }
        });


        //验证
        jQuery('#J_PhoneCheck').bind('click', function(){
            cleanErr();
            var $verp = $('#J_PhoneNum'),
                verifyPhone = $verp.val(),
                $that = $(this);
            var mcode = $("#J_Code").val();
            var areaCode=$('#J_PhoneAreaPop').val();
            $.ajax({
                type:"post",
                url:"/user/bindCellphone.htm",
                dataType:"json",
                data:{"mobile":verifyPhone,"mcode":mcode,"areaCode":areaCode},
                beforeSend: function(){
                    if($.inArray(phoneAreaPop.val(), areaArray)<0){
                        $('.J_ErrTips').text('暂不支持此国际区号！');
                        $verp.addClass('err');
                        return false;
                    }else {
                        cleanErr();
                    }
                },success: function(data){
                    if(data.success==true){
                        var $pop = $('#J_PhonePop');
                        $pop.html('<p class="scs">绑定成功！<br>窗口会在<span>5</span>秒钟后关闭！</p>');
                        countSec(5, function(s){
                            $pop.find('span').text(s);
                        }, function(){
                            $(".u-mask").addClass("none");
                        });
                    }else{
                        $('.J_ErrTips').text(data.message);
                    }
                }
            });
        });

        jQuery('#J_OpenBind').bind('click', function(){
            var isLogin ='';
            if(!!isLogin){
                $('.u-mask').removeClass('none');
            }else{
                location.href= '/user/login.htm';
            }
        });


        $('#J_CliclClose').bind('click', function(){
            $.ajax({
            type:"post",
            url:"/user/closeVerifiedMobile.htm",
            dataType:"json",
            success:function(data){
                if(data.success==true){
                    $(".u-mask").addClass("none");
                }else{
                    $(".u-mask").addClass("none");
                }
                }
            });
        });



        //验证是否为空
        function checkEmpty($ele){
            var isEmpty = true;
            $ele.each(function(){
                ($(this).val() == '') && (isEmpty=false);
            });
            return isEmpty;
        }

        //60秒倒计时
        function countSec(s, fn1, fn2){
            if(s>=1){
                --s;
                setTimeout(function(){
                    fn1(s);
                    countSec(s, fn1, fn2);
                }, 1000);
            }else {
                fn2();
            }
        }

        function cleanErr(){
            $("#J_PhonePic").removeClass('err');
            $('#J_PhoneNum').removeClass('err');
            $('#J_PhoneAreaPop').removeClass('err');
            $('#J_Code').removeClass('err');
            $('.J_ErrTips').text("");
        }


        //生日花
        $('#birthdayflower').load('/beasttimes/birthday.php');
    });
    </script> 
