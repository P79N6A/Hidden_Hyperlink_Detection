     <script type="text/javascript">
document.onreadystatechange = subSomething;
function delay1()
{
document.getElementById('circle1').style.cssText="display:none;";
setTimeout('delay2()', 50);
}
function delay2()
{
document.getElementById('circle').style.cssText="display:none;";
setTimeout('delay3()', 40);
}
function delay3()
{
document.getElementById('circletext').style.cssText="display:none;";
}
function subSomething() 
{ 
if(document.readyState == "complete")
{	
document.getElementById('circletext').innerHTML="完成鸟";
setTimeout('delay1()', 100);

}
} 

</script> 
