    $.ajax({
        type:'get',
        url:"http://shanghai.liebiao.com/ajax/postclicks/?postid=341889951",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
