var url = location.href;
var startIndex = url.indexOf("#");
var endIndex = url.indexOf("/*/");
if(url.substring(startIndex + 1,endIndex) == 404 && endIndex > 0) {
    document.getElementById("noExitTopic").innerHTML = "您所访问的页面 " + url.substring(endIndex + 3) + " 已被删除或暂时不可用。";
