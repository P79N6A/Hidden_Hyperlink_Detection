function get_baby_info(){
    var el_baby = $('#babyQueryAttr'), bb_birthday,bb_gender=0,bb_height='',bb_weight='',birthday_input=el_baby.find(".txt-birthday"), now = new Date();
    if( !el_baby.length ) {return;}
    if( !birthday_input.attr('data-init') ) {
        birthday_input.Jcal({
            chosendate: (now.getMonth()+1)+'/'+now.getDate()+'/'+now.getFullYear(),
            startdate: (now.getMonth()+1)+'/'+now.getDate()+'/'+(now.getFullYear()-14),
            enddate: (now.getMonth()+1)+'/'+now.getDate()+'/'+(now.getFullYear()+1),
            outputFormat: '{y}-{m}-{d}',
            zIndex: 10, x:0, y:0
        });
        birthday_input.attr('data-init','1'); //防止重复绑定
    }
    function gen_tip(birthday) {
        var birthday = new Date(birthday*1), res={'tip':''}, t1,t2;
        t1 = birthday<now ? birthday : now;
        t2 = birthday<now ? now : birthday;
        var y=t2.getYear()-t1.getYear(), m=t2.getMonth()-t1.getMonth(), d=t2.getDate()-t1.getDate();
        if( d<0 ) {d+=30; m-=1;}
        if( m<0 ) {m+=12; y-=1;}
        if( !y && !m && !d ) {
            res = {'tip':'即将出生', 'status':'now'};
        } else if (birthday<now) {
            res['status'] = 'past';
            if( y ) {
                res.tip = y+'岁' + (m ? m+'个月': '' );
            } else {
                if( m ) {res.tip = m+'个月';}
                if( d ) {res.tip += d+'天';}
            }
        } else {
            res['status'] = 'future';
            if( y ) {
                res.tip = y+'年' + (m ? m+'个月': '' );
            } else {
                if( m ) {res.tip = m+'个月';}
                if( d ) {res.tip += d+'天';}
            }
        }
        if( res['status']=='now' ) {
            el_baby.find('.tc-info').html('您的宝宝<strong>即将出生</strong>啦！<a href="javascript:;" class="tci-change" clstag="search|keycount|search|updatebabyinfo">[ 修改信息 ]</a>');
        } else if( res.status=='past' ) {
            el_baby.find('.tc-info').html('您的宝宝已经<strong>'+res.tip+'</strong>啦！<a href="javascript:;" class="tci-change" clstag="search|keycount|search|updatebabyinfo">[ 修改信息 ]</a>');
        } else if( res.status=='future' ) {
            el_baby.find('.tc-info').html('您的宝宝即将在<strong>'+res.tip+'</strong>后出生！<a href="javascript:;" class="tci-change" clstag="search|keycount|search|updatebabyinfo">[ 修改信息 ]</a>');
        }
        el_baby.find('.tc-info a').click(function(){
            el_baby.find('.query-con').removeClass('hide');
            el_baby.find('.tips-con').addClass('hide');
        });
        $('#babyQueryAttr').removeClass('hide');
    }
    function filtUrl(d, c) {
        var e, f;
        if (!d && !c) {
            return ""
        } else {
            if (!c) {
                e = window.location.pathname + window.location.search;
                f = d
            } else {
                e = d;
                f = c
            }
        }
        return e.replace(new RegExp("(^|\\?|&)" + f + "=([^&]*)", "gi"), "")
    }
    function set_bb_info(bb_birthday, bb_gender) {
        var param = bb_birthday+'-'+bb_height+'-'+bb_weight+'-'+bb_gender;
        $.ajax({
            url:'http://uprofile.jd.com/u/baby/setinfo?babyinfo='+param,
            async:true,
            dataType: 'jsonp',
            success:function(data){
                if(!data||data.flag!=2){alert('Joy,没记住您的选择...'); return;}
                gen_tip(bb_birthday);
                el_baby.find('.query-con').addClass('hide');
                el_baby.find('.tips-con').removeClass('hide');
                if( $('#for_my_bb').hasClass('tc-btn-selected') ) {
                    var url = filtUrl(window.location.href,'baby');
                    window.location.href= url;
                }
            }
        });
    }
    //1：代表男，2：代表女
    $.ajax({
        url:'http://uprofile.jd.com/u/baby/getinfo?callback=?',
        async:true,
        dataType:'jsonp',
        success:function(data){
            var data = data.babyinfo.split('-');
            if( !data || !data[0] ) {el_baby.removeClass('hide').find('.query-con').removeClass('hide'); return ;}
            bb_birthday=data[0];
            bb_height=data[1];
            bb_weight=data[2];
            bb_gender=data[3]||0;
            el_baby.find('.tips-con').removeClass('hide');
            var bd = new Date(bb_birthday*1);
            el_baby.find('.query-con .txt-birthday').val(bd.getFullYear()+'-'+(bd.getMonth()+1)+'-'+bd.getDate());
            if(bb_gender=='1') {el_baby.find('.query-con .boy').addClass('selected');}
            if(bb_gender=='2') {el_baby.find('.query-con .girl').addClass('selected');}
            gen_tip(data[0]);
        }
    });
    $('#for_my_bb').click(function(){
        var url = window.location.href
        if( !$(this).hasClass('tc-btn-selected') ) {
            url = filtUrl(url,'JL');
            url += '&baby='+bb_birthday+','+bb_gender;
            url +="&JL=7_1_0";
        } else {
            url = filtUrl(window.location.href,'baby');
            url = filtUrl(url,'JL');
        }
        window.location.href = url;
    });
    el_baby.find('.boy').click(function(){
        $(this).addClass('selected');
        el_baby.find('.girl').removeClass('selected');
    });
    el_baby.find('.girl').click(function(){
        $(this).addClass('selected');
        el_baby.find('.boy').removeClass('selected');
    });
    el_baby.find('.btn-submit').click(function(){
        var birthday = $.trim(el_baby.find('.txt-birthday').val());
        if( !birthday ) {alert('请选择宝宝出生日期'); return;}
        if( !/^(\d{4})-(\d{1,2})-(\d{1,2})$/.test(birthday) ) {
            alert('Joy看不懂您写的日期，请按照"年-月-日"格式书写');
            return;
        }
        bb_birthday = new Date(RegExp.$1,RegExp.$2-1,RegExp.$3).getTime();
        if( el_baby.find('.boy').hasClass('selected') ) {bb_gender=1;}
        if( el_baby.find('.girl').hasClass('selected') ) {bb_gender=2;}
        jdModelCallCenter.settings.fn=function(){
            set_bb_info(bb_birthday, bb_gender);
        }
        $.getJSON("http://passport.jd.com/loginservice.aspx?method=Login&callback=?", function(r) {
            if (r && r.Identity && r.Identity.IsAuthenticated) {
                set_bb_info(bb_birthday, bb_gender);
            } else {
                jdModelCallCenter.login();
            }
        });
        $('.Jcalendar').remove();
    });
}
get_baby_info();
/* 母婴 end */
</script> 
   <script>/**
 * 图片滚动
 */
(function($) {
    $.fn.imgScroll = function(options, callback) {
        // 默认参数
        var defaults = {
            // 事件类型=click,mouseover
            evtType: 'click',
            // 可见图
            visible: 1,
            // 方向x,y
            direction: 'x',
            // 按钮-下一张，默认为元素选择器字符串，也可以是jQuery对象
            next: '#next',
            // 按钮-上一张，默认为元素选择器字符串，也可以是jQuery对象
            prev: '#prev',
            // 滚动到头按钮class
            disableClass: 'disabled',
            // 滚动到头按钮class是否加方向前缀prev-, next-
            disableClassPerfix: false,
            // 滚动速度
            speed: 300,
            // 每次滚动图片个数
            step: 1,
            // 无法(不足以)滚动时是否显示控制按钮
            showControl: false,
            // 每个滚动元素宽度，默认取li的outerWidth
            width: null,
            // 每个滚动元素宽度，默认取li的outerHeight
            height: null,
            // 是否显示滚动当前状态(1,2,3,4,...)
            navItems: false,
            // 包围元素的class，默认为'scroll-nav-wrap'
            navItmesWrapClass: 'scroll-nav-wrap',
            // 当前项目高亮class
            navItemActivedClass: 'current'
        };
        // 继承 初始化参数 - 替代默认参数
        var settings = $.extend(defaults, options);
        // 实例化每个滚动对象
        return this.each(function() {
            var that = $(this),
                ul = that.find('ul').eq(0),
                lis = ul.find('li'),
                len = lis.length,
                liWidth = null,
                liHeight = null,
                btnNext = typeof settings.next == 'string' ? $(settings.next) : settings.next,
                btnPrev = typeof settings.prev == 'string' ? $(settings.prev) : settings.prev,
                current = 0,
                step = settings.step,
                visible = settings.visible,
                total = Math.ceil((len-visible)/step)+1,
                dir = settings.direction,
                evt = settings.evtType,
                disabled = settings.disableClass,
                prevDisabled = settings.disableClassPerfix ? 'prev-' + disabled : disabled,
                nextDisabled = settings.disableClassPerfix ? 'next-' + disabled : disabled,
                nav = settings.navItems,
                navWrap = settings.navItmesWrapClass,
                navClass = settings.navItemActivedClass,
                last = false,
                first = true,
                perfect = (len-visible)%step == 0;
            /**
             * direction 滚动方向
             */
            function resetStyles(direction) {
                // 重置按钮样式
                if(len >= step + visible) {
                    btnPrev.addClass(prevDisabled);
                    btnNext.removeClass(nextDisabled);
                } else {
                    btnNext.addClass(nextDisabled);
                }
                // 重置每个滚动列表项样式
                if(lis.eq(0).css('float') !== 'left') {
                    lis.css('float', 'left');
                }
                // 重新设置滚动列表项高宽
                liWidth = settings.width || lis.eq(0).outerWidth();
                liHeight = settings.height || lis.eq(0).outerHeight();
                // 重置最外层可视区域元素样式
                that.css({
                    'position': that.css('position') == 'static' ? 'relative' : that.css('position'),
                    'width': direction == 'x' ? liWidth * visible : liWidth,
                    'height': direction == 'x' ? liHeight : liHeight * visible,
                    'overflow': 'hidden'
                });
                // 重置滚动内容区元素样式
                ul.css({
                    'position': 'absolute',
                    'width': direction == 'x' ? liWidth * len : liWidth,
                    'height': direction == 'x' ? liHeight : liHeight * len,
                    'top': 0,
                    'left': 0
                });
            }
            /**
             * index 切换到第几页滚动
             * isPrev 是否点击上一张
             */
            function switchTo(index, isPrev) {
                // 是否正在动画中
                if (ul.is(':animated')) {
                    return false;
                }
                // 是否滚动到头或者尾
                if ( first && isPrev || last && !isPrev ) {
                    return false;
                } else {
                    index = isPrev ? --current : ++current
                }
                ul.animate(
                        dir=='x' ? {left: index >= (total-1) ? -(len-visible)*liWidth : -index*step*liWidth} : {top: index >= (total-1) ? -(len-visible)*liHeight : -index*step*liHeight},
                    options.speed,
                    function() {
                        // 滚动尾
                        if ( len-index*step <= visible ) {
                            btnNext.addClass(nextDisabled);
                            last = true;
                        } else {
                            btnNext.removeClass(nextDisabled);
                            last = false;
                        }
                        // 滚动头
                        if ( index <=0 ) {
                            btnPrev.addClass(prevDisabled);
                            first = true;
                        } else {
                            btnPrev.removeClass(prevDisabled);
                            first = false;
                        }
                        // 显示导航数字
                        if ( nav ) {
                            setNavCurrent(index);
                        }
                        // 每次滚动后回调参数
                        if ( typeof callback == 'function' ) {
                            /**
                             * index 当前滚动到第几页
                             * total 一共有多少页
                             * 可视区域内的滚动li jQuery对象集合
                             */
                            callback.apply(that, [
                                index,
                                total,
                                // 每次可视区li的总集合
                                lis.slice(index*step, index*step+visible),
                                // 每次滚动到可视区li的集合
                                lis.slice(index*step+visible-step, index*step+visible)
                            ]);
                        }
                    }
                );
            }
            /**
             * nav 数字导航外层div的class
             * 数字导航当前页高亮class
             */
            function showNavItem(nav, actived) {
                var nav = $('<div class="'+nav+'"></div>');
                for ( var i = 0; i < total; i++ ) {
                    nav.append('<em '+ (i==0?' class='+actived : '') +' title="'+ (i+1) +'">'+ (i+1) +'</em>');
                }
                that.after(nav);
            }
            function setNavCurrent(ind) {
                if ( $('.'+navWrap).length > 0 ) {
                    $('.'+navWrap).find('em').removeClass(navClass).eq(ind).addClass(navClass);
                }
            }
            function bindEvent() {
                btnPrev.unbind(evt).bind(evt, function() {
                    switchTo(current, true);
                });
                btnNext.unbind(evt).bind(evt, function() {
                    switchTo(current, false);
                });
            }
            // 初始化滚动
            if ( len > visible && visible >= step ) {
                // 可以滚动
                resetStyles(dir);
                bindEvent();
                if ( nav ) {
                    showNavItem(navWrap, navClass);
                }
            } else {
                // 无法滚动
                if ( settings.showControl ) {
                    btnNext.add(btnPrev).show();
                } else {
                    btnNext.add(btnPrev).hide();
                }
                btnPrev.addClass(prevDisabled);
                btnNext.addClass(nextDisabled);
            }
        });
    }
})(jQuery);
//收集skuId
var skuIds = [];
$('ul.list-h li[sku]').each(function(i){
    skuIds.push($(this).attr('sku'));
