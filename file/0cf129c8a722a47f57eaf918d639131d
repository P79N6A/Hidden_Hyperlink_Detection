   <script>
   window.addEvent('domready',function(){
	   this.addEvent('scroll',function(){
	        if(window.getScrollTop()>200){
	         $('botto').show();	
	         }else{
		     $('botto').hide();
		   }
	   });
   });
</script> 
   <script>
    function botto(){
    var obj = document.getElementById("botto"); 
    var toppx = 100;
    if (typeof document.body.style.maxHeight==="undefined") {
        toppx = (document.body.scrollTop||document.documentElement.scrollTop) + toppx;
    }
    var rtpx = document.body.offsetWidth > 1355 ? (document.body.offsetWidth - 1200)/2 - 80 : 0;
    if (obj.style.top != toppx || obj.style.right != rtpx){
	    obj.style.top = toppx + 'px';
	    obj.style.right = rtpx + 'px';
	}
}
	window.setInterval("botto()",1); 
</script> 
