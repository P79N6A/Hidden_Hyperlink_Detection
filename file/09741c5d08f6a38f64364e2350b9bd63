    var mail_list_orig_value = $("#mailing-list-subscription-form-1 input[name='mail']").val();
    $("#mailing-list-subscription-form-1 input[name='mail']").focus(function(){
	  if (mail_list_orig_value == $(this).val()) {
	    $(this).val("");
	  }
    }).blur(function(){
	  if ($(this).val() == "") {
	    $(this).val(mail_list_orig_value);
	  }
