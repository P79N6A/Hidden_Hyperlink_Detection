function getip(){
	$.ajax({   
		url:"http://api.go2map.com/engine/api/ipcity/json?cb=?",   
		dataType:"json",   
		success:function(data,status){   
			if(data.status == "ok")   
				document.getElementById('ipDetect').innerHTML=data.response.ip;  
			else  
				document.getElementById('ipDetect').innerHTML=''; 
		}   
	});
}
getip();
