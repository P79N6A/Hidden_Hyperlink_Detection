   <script type="text/javascript">

function buy(buy_type)

{

    if (goodsspec.getSpec() == null)

    {

        alert(lang.select_specs);

        return;

    }

    var spec_id = goodsspec.getSpec().id;



    var quantity = $("#quantity").val();

    if (quantity == '')

    {

        alert(lang.input_quantity);

        return;

    }

    if (parseInt(quantity) < 1)

    {

        alert(lang.invalid_quantity);

        return;

    }

    add_to_cart(spec_id, quantity,buy_type);

}



function add_to_cart(spec_id, quantity,buy_type)

{

    var url = SITE_URL + '/index.php?app=cart&act=add';

    $.getJSON(url, {'spec_id':spec_id, 'quantity':quantity}, function(data){

        if (data.done)

        {

			if(buy_type==1){

				alert(data.msg);

			}else{

				location.href = SITE_URL +"/index.php?app=cart";	

			}

        }

        else

        {

            alert(data.msg);

        }

    });

}



var specs = new Array();


specs.push(new spec(55941, '嫩白保湿', '500ml', 22.80,23.00, 99));


specs.push(new spec(55942, '嫩白保湿', '300ml', 19.80,20.00, 99));


var specQty = 2;

var defSpec = 55941;

var goodsspec = new goodsspec(specs, specQty, defSpec);

</script> 
