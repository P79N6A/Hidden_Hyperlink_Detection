   <script type="text/javascript">

$(document).ready(function(){

	var curVal = $("#quantity");

	var stock = $("#stock");

	var temp;  //得到文本框当前的数据

	var temp1=1;

	$(".jianOne").click(function(){

		temp = curVal.val();

		if(temp==2){

		    $(this).removeClass('L_zb1_l');

			$(this).addClass('L_zb1');	

		}

		if(temp==1){

			curVal.val(1);

			return false;

		}

		curVal.val(--temp); //数量减1

		$('.addOne').addClass('L_zb2_l');

	});

	$(".addOne").click(function(){

		temp = parseInt(curVal.val());

		number = parseInt(stock.val());

		    if(temp+1>=number){

		      $(this).removeClass('L_zb2_l');

			  $(this).addClass('L_zb2');

			}

		    if(temp>=number){

				return false;

			}

			curVal.val(++temp);  //数量加1

			$('.jianOne').addClass('L_zb1_l');

	});

});

function changePrice()

{

	var curVal = $("#number");

	var goods_number = $("#goods_number");

	temp = parseInt(curVal.val());

		number = parseInt(goods_number.val());

		if(temp>number){

				alert('库存不足');

				curVal.val(number);

				return false;

			}	

}

</script> 
