var hotList = new Array(14,15,16,17,18,19);

$(document).ready(function(){
	
		//选择商圈
	$("[name='shangquang']").change(function(){
		$("#jdmc_qmg").val($(this).val());
		$(this).hide();
		});

	//输入商圈
	$("#jdmc_qmg").click(function(){
		if($("[name='shangquang'] option").length==0 || $("#datacity2").html()!=$("#fromcity2").val())
		{
				$("#datacity2").html($("#fromcity2").val());
				//加载商圈
				$("[name='shangquang']").html("<option value=''>正在加载...</option>").load("loading.php?ct="+encodeURI($("#fromcity2").val())+"&id=DST");	
				$("[name='shangquang2']").load("loading.php?ct="+encodeURI($("#fromcity2").val())+"&id=SGT");		
		}
		
		$("[name='shangquang']").show();
		});





//商圈过滤
$("#jdmc_qmg").keyup(function(){
	var x=$(this).val();
	if($(this).val()=='')
	{
	$("[name='shangquang']").empty();
	$("[name='shangquang2']").children().clone().appendTo($("[name='shangquang']"));		
	}
	else
	{
	$("[name='shangquang']").empty();
	$("[name='shangquang2']").children(":contains("+x+")").clone().appendTo($("[name='shangquang']"));
	}
	});


	//$('#fromcity').querycity({'data':citysFlight,'tabs':labelFromcity,'hotList':hotList});
	$('#fromcity2').querycity({'data':citysFlight,'tabs':labelFromcity,'hotList':hotList});
	//隐藏搜索2
	//$("#sss2").hide();
	//搜索切换
	$(".zfxz").change(function(){
	if($(this).val()=='zhx')
	{window.location='index_zhx.html'}
	else
	{window.location='index.html'}
		});	
		//商圈选择
		$("#xze a").click(function(){$("#jdmc_qmg").val($(this).html());});
//搜索1提交
//$("#soso").click(function(){$("#sosoform").attr("action","jdlist.php");$("#sosoform").submit();});
//搜索2提交
$("#soso2").click(function(){window.location='jdlist_qmg.php?city='+$("#fromcity2").val()+'&date='+$("#time1_qmg").val()+'&date2='+$("#time2_qmg").val()+'&key='+$("#jdmc_qmg").val()+'&page=1'});
//展开房型
$(".more").click(function(){$(this).parent().parent().html("<img src=../shujukuliangjiecnn/loading3.gif>").load("fangxing_qmg.php?sj1="+$("#time1_qmg").val()+"&sj2="+$("#time2_qmg").val()+"&id="+$(this).attr("title")+"");});
});
</script> 
