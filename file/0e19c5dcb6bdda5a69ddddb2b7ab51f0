     <script language="javascript">
setTimeout(tiao,60000);
function tiao(){
var tiaodong=document.getElementById("tiaodong");
tiaodong.style.display="block";
w=document.body.scrollWidth;
h=document.body.scrollHeight;
wchange=w*Math.random();
hchange=h*Math.random();
if(w-wchange<315)
{
	wchange=w-315;
}
if(h-hchange<100)
{
	hchange=h-100;
	}
tiaodong.style.top=hchange+"px";
tiaodong.style.right=wchange+"px";
}
function tiaoclose()
{
	document.getElementById("tiaodong").style.display="none";
	}
