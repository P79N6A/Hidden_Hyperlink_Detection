            function initLoginRegM(open) {

                var cont = renderTemplate('#temp-modal');
                cont.find('.login-selsect').on('click', function () {
                    $('#mlogin-form').attr('action', '/login/')
                    $('.login-selsect').addClass('login-underline');
                    $('.register-select').removeClass('register-underline');
                    $('.login-popup').removeClass('hidden');
                    $('.register-popup').addClass('hidden');
                });
                cont.find('.register-select').on('click', function () {
                    $('#mlogin-form').attr('action', '/register/')
                    $('.register-select').addClass('register-underline');
                    $('.login-selsect').removeClass('login-underline');
                    $('.login-popup').addClass('hidden');
                    $('.register-popup').removeClass('hidden');

                });
//                cont.find(".login-enter").keyup(function (event) {
//                    if (event.keyCode == 13) {
//                        $(".login-button").click();
//                        //$(".register-button").click();
//                    }
//                });
//
//                cont.find("#textfield, .fix").keyup(function (event) {
//                    if (event.keyCode == 13) {
//                        $(".register-button").click();
//                        //$(".register-button").click();
//                    }
//                });
                Popup.open(cont);
                open = open || 'login';
                if (open == 'login')
                    cont.find('.login-selsect').trigger('click');
                else
                    cont.find('.register-select').trigger('click');
            }
        </script>




                <script>
            $(document).ready(function () {
                var home = $('.menuhome').hasClass('home-active');
                var cat = $('.maenucat').hasClass('categories-active');
                $('.menusearch').on('click', function () {
                    $(this).addClass('search-active').removeClass('search');
                    $('.menuhome').removeClass('home-active').addClass('home');
                    $('.maenucat').removeClass('categories-active').addClass('categories');
                    if ($('#search-open').hasClass('hidden')) {
                        $('#search-open').removeClass('hidden');
                        $('header').css("height", '307px');
                    } else {
                        if (home == true) {
                            $('.menuhome').removeClass('home').addClass('home-active');
                        }
                        if (cat == true) {
                            $('.maenucat').removeClass('categories').addClass('categories-active');
                        }
                        $('#search-open').addClass('hidden');
                        $('.menusearch').addClass('search').removeClass('search-active');
                        $('header').css("height", '252px');
                    }
                });
            });</script>
