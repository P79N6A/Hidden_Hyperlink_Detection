        }
        $("#status").click(function(){
            if($(this).html()=='点击查看更多'){
                $("#status").text('点击收起');
                $("#tab1 tr:gt(10)").show();
            }else{
                $("#status").text('点击查看更多');
                $("#tab1 tr:gt(10)").hide();
            }

        });
    })

$(function(){
	(function(){
		var obj1 = $('#tex_hov');
		var _txt1 = obj1.val();
		obj1.focus(function(){
			if(obj1.val()== _txt1){
				obj1.val('');
				obj1.addClass('texon1');
			}
		}).blur(function(){
			if(obj1.val()==''){
				obj1.val(_txt1);
				obj1.removeClass('texon1');
			}
		});
	})();
});	


$(document).ready(function(){
	$(".nayae").click(function(){
		$(".adrs").toggle();

		})
	})

//点击高校分数线搜索
function gxfsx(){
	var gxfsx_area1=  $(".gxfsx_area1 option:selected").val(),  //考生所在地
		brandch_id=  $(".gxfsx_type option:selected").val(),  //类型
    	gxfsx_area2=  $(".gxfsx_area2 option:selected").val(),  //高校所在地
   		college_name=  $(".collge option:selected").val();

	var str = '';
	if(gxfsx_area2){
		str = str +"/"+gxfsx_area2;
    }else{str=str+"/0";}
    if(gxfsx_area1){
		str = str +"/"+gxfsx_area1;
    }else{str=str+"/0";}
    if(brandch_id){
		str = str +"/"+brandch_id;
    }else{str=str+"/0";}
    if(college_name){
		str = str +"/0/0/want/"+college_name;
    }else{
    	str = str +"/0/0/want/";
    }
   
	window.location.href="http://gaokao.koolearn.com/college/schline"+str;
}

//根据地区，ajax请求获得学校
function getcollege(){
	id = $(".gxfsx_area2").val();
	if(id){
		$.getJSON("http://gaokao.koolearn.com/college/pro/getcollege",{'id':id},function(e){
	         if(e['status']){
	          	$(".collge").html(e['msg']);
	         }else{
	        	 alert(e['msg']);
	         }
		})
		
	}
	
}
$(function(){
	//getmin();
	getcollege();
});
 
	
</script>
