  <script type="text/javaScript">
  $('#top').click(function(){$('html,body').animate({scrollTop: '0px'}, 800);return false;});
  $('#bottom').click(function(){var bottom = parseInt($(document).height()-120)+"px";$('html,body').animate({scrollTop: bottom}, 800);return false;});  
  $(window).scroll(function() {var st = $(window).scrollTop(); $('#toolBackTop').hide();if(st > 30) $('#toolBackTop').show();});
  $(document).ready(function(){$('.gall-list').cycle({fx: 'fade',pager: '.pagination-area',timeout: 5500});}); 
  document.getElementById('type_side_body').contentEditable=false;
  function openOrClose(img){
      var ul = img.parentNode.parentNode;var src = img.src;var filename  = src.split('/image/')[1];
      var arrli = ul.getElementsByTagName('li');var len = arrli.length+1;
      if(filename == 'open_li.gif'){img.src = img.src.replace('open_li','close_li');for(var i=1;i<len;i++){arrli[i].style.display = 'none';}
      }else{img.src = img.src.replace('close_li','open_li');for(var i=1;i<len;i++){arrli[i].style.display = 'block'; }	
      }
  }
</script>  
