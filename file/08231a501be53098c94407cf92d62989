$(function(){

	$max_width = (WIN_WIDTH-20)/2;

	$max_height = (WIN_WIDTH-20)/2/110*135;

	$(".oe_indexuser dl").css({"width":$max_width});

	$(".oe_indexuser dl dt img").css({"width":$max_width});

	$(".oe_indexuser dl dt img").css({"height":$max_height});

});

