  <div class="oe_index_changebtn">
   <span onclick="_oeChangeNext();"><i class="iconfont">G</i>换一组</span>
  </div> 
  <script type="text/javascript">

$(function(){

	
});

function _oeChangeNext() {

	$.ajax({

		type: "GET",

		url: "/webapp.php?c=index&a=changenext&apptype=&appmapx=&appmapy=",

		cache: false,

		data: {r:get_rndnum(8)},

		dataType: "json",

		success: function($data) {

			$json = eval($data);

			$response = $json.response;

			$result = $json.result;

			if ($response == "1") {

				if ($result.length > 0) {

					$("#change_next_box").html($result);

				}

			}

		},

		error: function() {

		

		}

	});

}

</script> 
