			key=key.replace(/[ ]/g,""); 
			if(key!='')
			{
				$.get('/index/search/',{keyword:key,actiontishi:'tishi'},function(data){
					if(data)
					$('#tishimenu').html(data).show();
				})
			}
			else
			$('#tishimenu').hide();
