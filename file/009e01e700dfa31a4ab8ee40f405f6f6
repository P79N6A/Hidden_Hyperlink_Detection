  <script type="text/javascript">   
document.onkeydown = chang_page;function chang_page(e) {   
    var e = e || event,   
    keycode = e.which || e.keyCode;      
    if (keycode == 33 || keycode == 37) location = 'http://www.gkoo.net/apollools/life-style/assembly/';      
    if (keycode == 34 || keycode == 39) location = 'http://www.gkoo.net/show-expo/kekoukelegongsichengweizhongguo2010nianshanghaishibohuiquanqiuhezuohuoban-2/';      
}      
</script> 
