    <script>
$(function(){
	$('.mainoj .contente ul').width(300*$('.mainoj .contente li').length+'px');
	$(".mainoj .tab a").mouseover(function(){
		$(this).addClass('on').siblings().removeClass('on');
		var index = $(this).index();
		number = index;
		var distance = -300*index;
		$('.mainoj .contente ul').stop().animate({
			left:distance
		});
	});
	
	var auto = 0;  //等于1则自动切换，其他任意数字则不自动切换
	if(auto ==1){
		var number = 0;
		var maxNumber = $('.mainoj .tab a').length;
		function autotab(){
			number++;
			number == maxNumber? number = 0 : number;
			$('.mainoj .tab a:eq('+number+')').addClass('on').siblings().removeClass('on');
			var distance = -300*number;
			$('.mainoj .contente ul').stop().animate({
				left:distance
			});
		}
		var tabChange = setInterval(autotab,3000);
		//鼠标悬停暂停切换
		$('.mainoj').mouseover(function(){
			clearInterval(tabChange);
		});
		$('.mainoj').mouseout(function(){
			tabChange = setInterval(autotab,3000);
		});
	  }  
});
</script> 
