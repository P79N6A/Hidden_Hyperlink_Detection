        var offsetTop = $(window).scrollTop();
        if (2300 > offsetTop && offsetTop > 1200) {
            if (delta > 0) {
                ani_node_3_phone.css('top', '480px');
            }
            else {
                ani_node_3_phone.css('top', '431px');
            }
        }
    });
  
