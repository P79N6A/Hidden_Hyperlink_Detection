     $(document).mousewheel(function (evt, delta) {
          var offsetTop = $(window).scrollTop();
          var xx=0;
          if (delta > 0) {      
             xx = offsetTop - 3500;
          }
          else {   
             xx = offsetTop - 5460;
          }
          if (xx < 50) { xx = 50; }
          if (xx > 90) { xx = 90; }
          $("#maxui_threess_1").css("margin-top",xx+"px");
       
    });

})()
  
