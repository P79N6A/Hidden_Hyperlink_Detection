    <script type="text/javascript">
function feedback(commentId, floor) {
    var msg;
    msg ="http://www.verycd.com/topics/112603/comments/comment"+commentId;
    msg += "\n";

    if(-1 != floor) {
        msg += "举报: 第"+floor+"楼\n";
    } else {
        msg += "举报:热门评论\n";
    }
    msg += "原因: \n";
    old_get('feedback_contents').value = msg;
    old_get('feedback_contents').name  = 'contents';
    old_get('feedback_form').submit();
    old_get('feedback_contents').name  = '';
}
</script> 
