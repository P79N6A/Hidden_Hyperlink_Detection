                        } else {
                           fromClick=true;
                           add2fav();
                        }
                        if (!e) var e = window.event;
                        e.cancelBubble = true;
                        if (e.stopPropagation) e.stopPropagation();
                    }
                 }
                 if(old_get("cancelFav")!=null){

                     old_get("cancelFav").onclick=function(){
