<script type="text/javascript">
    function GetImg(first) {
        var isget = 0;
        if (first == 0) {
            if (jQuery("#imgcode").html().length == 0) {
                isget = 1;
            }
        }
        else {
            isget = 1;
        }
        if (isget == 1) {
            jQuery("#imgcode").html("<span id='loadimgtxt'>正在显示验证码..</span><img id='loadvcodeimg' src='/default.aspx?at=img&r=" + Math.random() + "&guid=c8a772cc-624e-479d-be58-85a3882cd7fa' />");
            //jQuery("#clickshow").text("看不清");
            jQuery("#imgcode").on("click", function () { GetImg(); })
        }
        
        jQuery("#loadvcodeimg").bind("load", function () {
            loadimgtxt(0);
        })

        jQuery("#loadvcodeimg").bind("error", function () {
            loadimgtxt(1);
        })
    }
    function loadimgtxt(var1) {
        if (var1 == 0) {
            jQuery("#loadimgtxt").text("正在显示验证码..");
            jQuery("#loadimgtxt").addClass("autohide");
            jQuery("#loadvcodeimg").removeClass("autohide");
            jQuery("#loadvcodeimg").bind("error", function () {
                loadimgtxt(1);
            })
        }
        if (var1 == 1) {
            jQuery("#loadimgtxt").removeClass("autohide");
            jQuery("#loadimgtxt").text("加载失败，点击重试");
            jQuery("#loadvcodeimg").addClass("autohide");
            jQuery("#loadvcodeimg").off("error");
        }
    }
</script>
