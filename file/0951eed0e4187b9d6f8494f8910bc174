    //    GetImg(0);
        jQuery("#vcode").hide();
    
    jQuery(function () {

        if(request("s")!=""&&request("sc")==1){
            if(1){
                jQuery("#vcode").show();
            }else{
                jQuery("#vcode").hide();
            }      
            jQuery("#checkcode").click();
            jQuery("#checkcode").val("1");
            jQuery("#divcheck").hide();
            jQuery("#code").val('').focus();
            jQuery("#havecode").val("1");
            GetImg();
        }
        jQuery("#checkcode").on("click",function(){
            if(jQuery(this).val()=="0"){
                jQuery(this).val("1");
                jQuery("#vcode").show();
                jQuery("#code").val('').focus();
                jQuery("#divcheck").hide();
                GetImg();
            }else{
                jQuery(this).val("0");
                jQuery("#divcheck").show();
                jQuery("#vcode").hide();
            }
        });

        var mp;
        jQuery("#result_table .Now").each(function (index) {
            mp = jQuery(this).text().split(' ');
            if (mp.length > 1) {
                jQuery(this).html("");
                for (var i = 0; i < mp.length; i++) {
                    jQuery(this).append("<span>" + mp[i] + "</span>");
                }
            }
        });
        jQuery("#saveDat").on("click", function () {
            var _num = new Array();
            var _name = new Array();
            var _url = new Array();
            var _time = new Array();
            _num[0] = jQuery("#first li:eq(2)").find("p").text();
            _name[0] = jQuery("#first li:eq(3)").find("p").text();
            _url[0] = jQuery("#first li:eq(4)").find("p").text();
            _time[0] = jQuery("#first li:eq(5)").find("p").text();
            jQuery("#result_table tr").each(function (index) {
                _num[index + 1] = jQuery(this).children().eq(0).text();
                _name[index + 1] = jQuery(this).children().eq(1).text();
                _url[index + 1] = jQuery(this).children().eq(2).text();
                _time[index + 1] = jQuery(this).children().eq(3).text();
            });
            jQuery("#_num").val(_num);
            jQuery("#_name").val(_name);
            jQuery("#_url").val(_url);
            jQuery("#_time").val(_time);
            jQuery("#frm").attr("action", "saveExc.ashx");
        });

        if (jQuery('#result_table tr').length <= 20) {
            jQuery('#page').hide();
        } else {
            jQuery('#page').show();

            total_page = Math.ceil(jQuery('#result_table tr').length / 20);
            var pagehtml = '<span id="page_num"><a href="javascript:" onclick="turn_page(-1)"> < </a>';
            for (var i = 1; i <= total_page; i++) {
                if (i == 1) {
                    pagehtml += '<a href="javascript:" class="bg-blue03 col-white" onclick="turn_page(' + i + ')">' + i + '</a>';
                } else {
                    pagehtml += '<a href="javascript:" onclick="turn_page(' + i + ')">' + i + '</a>';
                }
            }
            //if(total_page!=1){
                pagehtml += '<a href="javascript:" onclick="turn_page(-2)"> > </a></span><span class="col-gray02">共' + total_page + '页，到第</span><input id="tpn" class="pagewrite" /><span class="col-gray02">页</span><a id="t_tpn" href="javascript:" class="bg-blue03 col-white">确定</a>';
            //}
            jQuery('#page').html(pagehtml);
            if (jQuery('#page_num .bg-blue03').text() == "1") {
                jQuery('#page_num a').first().hide();
            }
            show_hide(1);

            jQuery("#t_tpn").on("click", function () {
                if(isInt(jQuery("#tpn").val())){
                    turn_page(jQuery("#tpn").val());
                }else{
                    alert("请输入大于零的正整数");
                    return;
                }                
            });
        };
        jQuery("#showVcode").on("click", function () {
            jQuery(".Tool-IcpMainCent").hide();
            jQuery("#vcode").show();
            jQuery("#code").val('').focus();
            jQuery("#divcheck").hide();
            jQuery("#checkcode").val("1");
            jQuery("#checkscreen").hide();
            jQuery("#havecode").val("1");
            GetImg(0);
        });
        jQuery(".updateByVcode").on("click",function(){
           jQuery(this).parent().hide(); 
           jQuery(".Tool-IcpMainCent").hide();
            jQuery("#vcode").show();
            jQuery("#code").val('').focus();
            jQuery("#checkscreen").hide();
            jQuery("#checkcode").click();
            jQuery("#checkcode").val("1");
            jQuery("#havecode").val("1");
            jQuery("#divcheck").hide();
            GetImg(0);
        });

        jQuery("#search").on("click",function(){
            if(jQuery("#havecode").val()=="1"&&jQuery("#code").val()==""){
                alert("请输入验证码");
                jQuery("#code").val('').focus();
                return false;
            } 
        });
        
                    jQuery.ajax({
                type: "post",
                url: "/safeImg.ashx",
                data: { url: 'zhongtai-ex.com'},
                success: function (data) {
                    jQuery("#safe").html(data);
                },
                complete: function (XHR, TS) {
                    XHR = null;
                }
            })
            });
    function turn_page(num) {
        if(num!=0){
            if(num<=total_page){
                if (num == -1) {
                    if (jQuery('#page_num .bg-blue03').text() != "1") {
                        jQuery('#page_num .bg-blue03').prev().addClass("bg-blue03").addClass("col-white").siblings().removeClass("bg-blue03").removeClass("col-white");
                    }
                } else if (num == -2) {
                    if (jQuery('#page_num .bg-blue03').text() != total_page.toString()){
                        jQuery('#page_num .bg-blue03').next().addClass("bg-blue03").addClass("col-white").siblings().removeClass("bg-blue03").removeClass("col-white");
                    }
                } else {
                    jQuery('#page_num a').eq(num).addClass("bg-blue03").addClass("col-white").siblings().removeClass("bg-blue03").removeClass("col-white");
                }
                if(jQuery('#page_num .bg-blue03').text()=="1"){
                    jQuery('#page_num a').first().hide();
                    if(total_page>1){
                        jQuery('#page_num a').last().show();
                    }
                }else if(jQuery('#page_num .bg-blue03').text() == total_page.toString()){
                    jQuery('#page_num a').last().hide();
                    if(total_page>1){
                        jQuery('#page_num a').first().show();
                    }
                }else{
                    jQuery('#page_num a').first().show();
                    jQuery('#page_num a').last().show();
                }
                show_hide(jQuery('#page_num .bg-blue03').text());
            }else{
                alert("超出页面范围，请输入小于最大页码的正整数");
                return;
            }
        }else{
            alert("请输入大于零的正整数");
            return;
        }
    };
    function show_hide(num){
        jQuery('#result_table tr').each(function (index) {
            if ((num - 1 == 0 ? jQuery(this).index() >= (num - 1) * 19 : jQuery(this).index() >= (num - 1) * 19 + (num - 1)) && jQuery(this).index() <= num * 19 + (num - 1)) {
                jQuery(this).show();
            } else {
                jQuery(this).hide();
            }
        });
    }
    function request(paras) {
        var url = location.href;
        var paraString = url.substring(url.indexOf("?") + 1, url.length).split("&");
        var paraObj = {}
        for (i = 0; j = paraString[i]; i++) {
            paraObj[j.substring(0, j.indexOf("=")).toLowerCase()] = j.substring(j.indexOf("=") + 1, j.length);
        }
        var returnValue = paraObj[paras.toLowerCase()];
        if (typeof (returnValue) == "undefined") {
            return "";
        } else {
            return returnValue;
        }
    };
    function isInt(input)  
    {
        var re = /^[1-9]+[0-9]*]*$/;

        if (!re.test(input)) {
            return false;
        } else {
            return true;
        }  
    }  
</script>

