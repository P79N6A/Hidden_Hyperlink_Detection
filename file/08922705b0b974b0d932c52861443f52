  <script type="text/javascript">
$(function(){
    var w = $("#mid").width();//容器宽度
    $("#mid img").each(function(){
        //如果有很多图片，我们可以使用each()遍历
        var img_w = $(this).width();//图片宽度
        var img_h = $(this).height();//图片高度
        if(img_w>w){
            var height = (w*img_h)/img_w;
            $(this).css({"width":w,"height":height});//设置缩放后的宽度和高度
        }
    });
});
$(function(){
    $('.stfc_1').mouseenter(function(){
       $(this).find('.stfc_1_pos').slideDown();
    }).mouseleave(function(){
       $(this).find('.stfc_1_pos').slideUp();

    })
})

$(function(){
    $('.lwkai').click(function(){
       $('.gifts').show("500");
    })
})
if($.cookie("gift_send_ts")!=11){
$(function(){
    $('.gift_send_ts').show("1000");
    $('.gift_send_ts dd').click(function(){
        $('.gift_send_ts').hide("1000");
    });
    $('.gift_send_ts dl').click(function(){
        $('.gift_send_ts').hide("1000");
        $.cookie('gift_send_ts', '11',{ expires:1}); // 存储一个带7天期限的 cookie 
    });
})
}
</script> 
  <script type="text/javascript">
$(document).ready(function(){
var obj = $('#sticky');
var offset = obj.offset();
var topOffset = offset.top;
var leftOffset = offset.left;
var marginTop = obj.css("marginTop");
var marginLeft = obj.css("marginLeft");

$(window).scroll(function() {
var scrollTop = $(window).scrollTop();

    if (scrollTop >= topOffset){

        obj.css({
            top: '60px',
            marginLeft: '778px',
            position: 'fixed',
        });
    }

    if (scrollTop < topOffset){

        obj.css({
            top: marginTop,
            marginLeft: marginLeft,
            position: 'relative',
        });
    }
});
});

</script> 
