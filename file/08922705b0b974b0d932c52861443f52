   <div class="cd-popup-container"> 
    <p class="xtitlex"></p> 
    <a href="javascript:;" id="hides" class="cd-popup-close img-replace clo">Close</a> 
   </div> 
  </div> 
  <script>
    function show(content,status,url,time){
        content = arguments[0] || "";
        status = arguments[1] || 0;
        url = arguments[2] || "";
        time = arguments[3] || 1;
        $('.xtitlex').html(content);
        $('.cd-popup').fadeIn(1000);
        $("#hides").click(function(){
            $('.cd-popup').hide();
            if (status == 1) {
                window.setTimeout("window.location='"+url+"'",time*0);
            }
        });
        $(".cd-popup").click(function(){
            $('.cd-popup').hide();
            if (status == 1) {
                window.setTimeout("window.location='"+url+"'",time*0);
            }
        });
        if (status == 0) {
        	$('.cd-popup').delay(time*800).fadeOut(0);
        }else{
        	$('.cd-popup').delay(time*800).fadeOut(0);
        	window.setTimeout("window.location='"+url+"'",time*500);
        }
    }

</script> 
  <script>
$(".addfav").click(function(){
    var obj = $(this);
    $.ajax({
        url:'/Share/favorite',
        data:{aid:22207,title:"泳装",type:1,url:"/Pic/p/22207.html"},
        dataType:"text",
        type:"post",
        success:function(da){
            if (da == 'nologin') {
                show("请先登陆");
                return false;
            }else if (da == 'exists') {
                show("已经收藏过了，不能重复收藏！");
                return false;
            }else if (da == 'error') {
                show("收藏失败，请稍后重试！");
                return false;
            }else if (da == 'success') {
                $(obj).find("num").text(parseInt($(obj).find("num").text())+1);
                show("收藏成功！");
                return false;
            }else{
                show("操作失败!");
                return false;
            }
        }
    });
});
$(".dianzan").click(function(){
    var obj = $(this);
    $.ajax({
        url:'/Share/dianzan',
        data:{aid:22207,title:"泳装",type:1,url:"/Pic/p/22207.html"},
        dataType:"text",
        type:"post",
        success:function(da){
            if (da == 'nologin') {
                show("请先登陆");
                return false;
            }else if (da == 'often') {
                show("亲，您今天已经投过票了，明天再来吧");
                return false;
            }else if (da == 'error') {
                show("亲，操作失败(⊙o⊙)");
                return false;
            }else if (da == 'success') {
                //
                $(obj).find("num").text(parseInt($(obj).find("num").text())+1);
                show("谢谢支持");
                return false;
            }else{
                show("操作失败!");
                return false;
            }
        }
    });
});

</script> 
  <script type="text/javascript">
    $(function(){
        $('.songlw').click(function(){
            $('.mask').show();
            $('.floatLayerslw').show();
        })
        var w =$('.floatLayerslw').width();
        var h =$('.floatLayerslw').height();
        $('.floatLayerslw').css({marginLeft:""+(-w/2)+"px",marginTop:""+(-h/2)+"px"});

        $('.closely').click(function(){
            $('.mask,.floatLayerslw').hide();
        })
    })
</script> 
