     <script type="text/javascript">
//查看结果
function replace_em(str){
    str = str.replace(/\[em_([0-9]*)\]/g,"<img src=\"http://static1.lvgutou.com/static/script/face/arclist/$1.gif\" border=\"0\" />");
    return str;
}
$(document).keydown(function(event){
    if($("#msg").val()==""){
        $("#pinglun").css('background','#999999');
    }else{
        $("#pinglun").css('background','#91c233');
    }
})
$(document).keyup(function(event){
    if($("#msg").val()==""){
        $("#pinglun").css('background','#999999');
    }else{
        $("#pinglun").css('background','#91c233');
    }
})
</script> 
