  <script type="text/javascript">
    $('.gift_img').hide();
    $('.list li').mousemove(function(e){
        $('.gift_img').eq($(this).index()).show().css({
            "top": e.pageY-70,
            "left": e.pageX-160
        }).siblings("div").hide();
    });
    $('.list li').mouseleave(function(){
        $('.gift_img').hide();
    }); var ul=$("#liwulists li"); ul.click(function(){ var str = $(this).attr("id");$("#lwimgs").attr("src","http://static1.lvgutou.com/static/images/liwuico/"+str+".png");document.getElementById('liwuname').value=str; $(".gifts").hide("500");});
function modi(obj){
    if (obj.value == 0) {
        $(obj).replaceWith('<input type="text" class="seednum modi" id="total">');
        modicss();
    }
}
function modicss(){
    $(".modi").keyup(function(){
        if (isNaN($(this).val())) {
            $(this).val(1);
            show("赠送数量只能是正整数");
            return false;
        }
    });
}
$(document).ready(function(){$("#total").val(1);});
</script>  
