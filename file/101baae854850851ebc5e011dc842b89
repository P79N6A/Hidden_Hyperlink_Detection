    $(function () {
    	$("#cy-form").Validform({
        	btnSubmit:"#cy-submit",
    		tiptype:2,
    		datatype:{
    			"carno":function(gets,obj,curform,regxp){
    				var error = true;
    				var reg = /^[\u4E00-\u9FBF][A-Za-z][A-Za-z0-9]{4}[A-Za-z0-9港澳挂教]$/;
    				if(!reg.test(gets)){
    					error = "请输入正确的车牌号码";
    				}
                    return error;
    			},
    			"frameno":function(gets,obj,curform,regxp){
    				var framenoLength = gets.length;
    				if(framenoLength == 6){
    					return true;
    				}else{
	    				return "请输入车架号后六位";
    				}
    			},
    			"engineno":function(gets,obj,curform,regxp){
    				var enginenoLength = gets.length;
    				if(enginenoLength == 6){
    					return true;
    				}else{
	    				return "请输入发动机后六位";
    				}
    			},
    			"truename":function(gets,obj,curform,regxp){
    				var result = true;
    				var reg = /[\u4E00-\u9FA5\uF900-\uFA2D]/;
    				if(gets){
    					if(!reg.test(gets)){
    						result = "请输入中文姓名";
    					}
    				}
    				return result;
    			},
    			"mobile":function(gets,obj,curform,regxp){
    				var result = true;
    				var reg = /^13[0-9]{9}$|14[0-9]{9}|15[0-9]{9}$|18[0-9]{9}$/;
    				if(gets){
    					if(!reg.test(gets)){
    						result = "请填写正确的手机号码";
    					}
    				}
    				return result;
    			},
    			"vcode":function(gets,obj,curform,regxp){
    				var vcodeLength = gets.length;
    				if(vcodeLength == 4){
    					return true;
    				}else{
	    				return "请输入四位验证码";
    				}
    			}
    		},
    		beforeSubmit:function(curform){
    			if($("#nobaseservice").find("div.form-item").size() >= 1 && $("#nobaseservice").attr("jt-base") == "true"){
                	/*window.scrollTo(0,0);
                	$("#nobaseservice").show().attr("jt-base","false");
                	$("#baseservice").hide();
                    $("[jt-control-type=summany] tr.hide").removeClass("hide").addClass("show");
                	$(this).before('<button class="btn btn-big btn-red" type="button" id="before_button">上一步</button>');
                	$("#before_button").click(function(){
