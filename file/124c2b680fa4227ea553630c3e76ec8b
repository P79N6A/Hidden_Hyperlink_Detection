        function showModalMessage(tf, messageStr) {
            if (tf) {
                $('#ModalMessage .modal-body').html('<span class="glyphicon glyphicon-ok"></span>&nbsp;' + messageStr);
            } else {
                $('#ModalMessage .modal-body').html('<span class="glyphicon glyphicon-remove"></span>&nbsp;' + messageStr);
            }
            $('#ModalMessage').modal({});
        }
    </script> 
