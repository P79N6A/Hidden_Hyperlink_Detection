		{
			jQuery('.categoryMegaMenu li')
				.bind('mouseenter', function()
				{
					var li = jQuery(this);
					if (li.children('.subMenu').length > 0) {
						li.addClass('slided selected');
						var listItems = li.attr('id').split('-');
						if(!app.isiPad() || (app.isiPad() && megamenuOnIpad.indexOf(listItems[1]) > -1))
						{
							li.children('.subMenu').css('z-index', '9999').stop(true, true).slideDown(200, function()
							{
								jQuery('.categoryMegaMenu li').not('.slided').children('.subMenu').hide();
								li.removeClass('slided');
							});
						}
					} else {
						jQuery('.categoryMegaMenu li').children('.subMenu').hide();
					}
				})
				.bind('mouseleave', function()
				{
					jQuery(this).removeClass('selected').children('.subMenu').css('z-index', '1');
				});


			if (enableLogoBehaviour) {
				menuObject = jQuery('.categoryMegaMenu');
			} else {
				menuObject = jQuery('.categoryMegaMenu ul');
			}
			menuObject
				.bind('mouseenter', function() {
					jQuery(this).addClass('hovered');
				})
				.bind('mouseleave', function() {
					jQuery(this).removeClass('hovered');
					jQuery('.categoryMegaMenu li').children('.subMenu').hide();
				});
		},
		checkForSubmenu: function()
		{
			var submenu = jQuery('#submenuWrapper ul li');
			if (submenu.length > 0) {
				jQuery('#menuWrapper').css({
					'border-bottom' : '2px solid #ffffff'
				});
				jQuery('#menuWrapper').addClass('noBorder');
			} else {
				jQuery('#submenuWrapper').hide();
			}
		}
	};

	jQuery(window).load(function()
	{
		megamenu.init();
		app.containerPosition()
	});

	jQuery(function()
	{
		megamenu.checkForSubmenu();

		var prevItem = '';
		jQuery('#headerSubMenu li')
			.bind('touchstart', function(e)
			{
				if((prevItem == '') || (prevItem.attr('id') != jQuery(this).attr('id')))
				{
					if(jQuery(this).find('.outter').length > 0){
						jQuery(this).addClass('hovered');
						prevItem = jQuery(this);
						return false;
					}
				}

			})
			.bind('touchend', function(e)
			{
				jQuery(this).removeClass('hovered');
			});

	});

	// check if this menu should be opened on page load
	function handleMenuAutoOpen(categoryid)
	{
		var categorymenuLI = jQuery('#categorymenu-' + categoryid);
		if (jQuery(categorymenuLI).hasClass('autoopen')) {
			// menu is set to be auto-opened, check if this is the first time (auto-open it only once)
			var preventAutoOpen = (app.util.readCookie('menuPreventAutoOpen-' + categoryid) == null ? false : true);
			if (!preventAutoOpen) {
				// append menu close button
				var closeButton = jQuery('<a>').html('&nbsp;');
				jQuery(closeButton).addClass('categorymenu_close allcapslink');
				jQuery(closeButton).attr('title', 'Close');

				var hideTimeout = null;

				jQuery(closeButton).unbind('click').bind('click', function()
				{
					// hide menu
					jQuery(categorymenuLI).find('div.outter').css('visibility', 'hidden');
					jQuery(categorymenuLI).hover(function()
					{
						jQuery(this).find('div.outter').css('visibility', '');
						jQuery(this).unbind('hover');
					});

					clearTimeout(hideTimeout);

					// remove the close button
					jQuery(this).remove();
				});

				jQuery(categorymenuLI).find('div.menucontentholder').prepend(closeButton);

				// set cookie to prevent auto-opening this menu again
				var days = 1.0;
				app.util.createCookie('menuPreventAutoOpen-' + categoryid, '1', days);

				// open the menu
				jQuery(categorymenuLI).find('div.outter').css('visibility', 'visible');

				// auto close after a few seconds
				hideTimeout = setTimeout(function()
				{
					jQuery(categorymenuLI).find('a.categorymenu_close').remove();

					jQuery(categorymenuLI).find('div.outter').css('visibility', 'hidden');
					jQuery(categorymenuLI).find('div.outter').css('visibility', '');

				}, (5 * 1000));
			}
		}
	}
//--></script>





