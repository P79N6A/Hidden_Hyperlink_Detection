		function dialogLinkClickLIstener() {
			jQuery('a.dialogLink').on('click', function(e)
			{
				e.preventDefault();
				var popInId = jQuery(this).attr('href');
				var popInId = popInId.substring(1);
				loadDialogContent(popInId);
			});
		}

		function loadDialogContent (popInId) {

			//var textPopInId = popInId.substring(1);
			var textPopInId = popInId;
			jQuery('.ui-dialog-content').not('.jrDialog').dialog('close');

			if (ajaxAssets[textPopInId] != null) {
				if (jQuery('#' + popInId).html() != '') {
					jQuery('#' + popInId).dialog('open');
				} else {
					jQuery.ajax({
					type: 'GET',
					url: '/on/demandware.store/Sites-cfgint-Site/en/Page-Include',
					data: {'cid' : ajaxAssets[textPopInId]}
					}).done(function(html) {
						jQuery('#' + popInId).html(html);
						jQuery('#' + popInId).dialog('open');
					})
					.always(function() {dialogLinkClickLIstener();});
				}
			} else {
				jQuery('#' + popInId).dialog('open');
			}
		}

		var dialogParam = location.search.split('showDialog=')[1]
		loadDialogContent(dialogParam);

		dialogLinkClickLIstener();

		jQuery('#privateContent button').on('click', function(e)
		{
			e.preventDefault();
			jQuery('.ui-dialog-content').dialog('close');
		});
	});
//--></script>



<script type="text/javascript">
	jQuery(window).load(function() {
		jQuery('.newsletter-email').each(function(index, element){
			var newsletter_default_text = jQuery(element).val();
			jQuery(element).focus(function(){
				if (jQuery(element).val() == newsletter_default_text) { jQuery(this).val(''); }
				jQuery(element).parents('.outter').addClass('shown');
			});
			jQuery(element).blur(function(){
				jQuery(element).parents('.outter').removeClass('shown');
				if (jQuery(element).val() == '') { jQuery(this).val(newsletter_default_text); }
			});
		});

		var bodyHeight = jQuery('.newsletter-legal-body').height()+parseInt(jQuery('.newsletter-legal-body').css("padding-top"))+parseInt(jQuery('.newsletter-legal-body').css("padding-bottom"));
		var nameHeight = jQuery('.newsletter-legal-name').height()+parseInt(jQuery('.newsletter-legal-name').css("padding-top"))+parseInt(jQuery('.newsletter-legal-name').css("padding-bottom"));
		var arrowHeight = parseInt(jQuery('.newsletter-legal-body>.arrow').css("height"));

		var top = (bodyHeight - nameHeight)/2;
		var arrowTop = (bodyHeight-arrowHeight)/2;

		if (!isNaN(top) && !isNaN(arrowTop)) {
			jQuery('.newsletter-legal-body').css("top","-"+top+"px");
			jQuery('.newsletter-legal-body>.arrow').css("top",arrowTop+"px");
		}

		jQuery('.newsletter-legal-footer').mouseenter(function(){
			jQuery('.newsletter-legal-body').css("display","block");
		});
		jQuery('.newsletter-legal-footer').mouseleave(function(){
			setTimeout(function () {
				jQuery('.newsletter-legal-body').css("display","none");
			}, 3000);
		});
	});

	var browser = app.util.getBrowser();
		var isIE7 = ((browser.name == 'msie') && (parseInt(browser.version, 10) < 9));
		
			baseURLassets = "http://demandware.edgesuite.net/aafs_prd/on/demandware.static/Sites-cfgint-Site/-/en/v1482474120904/images/assets/";
			var url="http://youtube-origin.clarins.com/coverflow/thumbs/";
			if(document.location.toString().indexOf('https://') != -1) {
				url=url.replace("http://","https://");
			}
			baseURLthumbnail=url;
		
		jQuery(window).load(function()
		{
			
			
			if (typeof(InitYTPlayers) == "function")
				InitYTPlayers();
			
			

			var languagesContainer = jQuery('#footer .languagesContainer');

			jQuery('#footer div.languages div.current').click(function () {
				if (jQuery(languagesContainer).is(':visible')) {
					jQuery(languagesContainer).hide('fast');
					jQuery(this).removeClass('clicked');

					
					if (isIE7) {
						jQuery('#footer').css('z-index', 1);

					}
				} else {
					jQuery(languagesContainer).attr('style', 'top: -' + (jQuery(languagesContainer).outerHeight() -25) + 'px')
					jQuery('#footer .languageBottom .languageBottomCenter').attr('style', 'width:' + (jQuery(languagesContainer).outerWidth() - jQuery(this).outerWidth() - (jQuery(window).height() < jQuery(document).height()? 5 : 4)) + 'px');
					jQuery(languagesContainer).show('fast');
					jQuery(this).addClass('clicked');

					
					if (isIE7) {
						jQuery('#footer').css('z-index', 99999);

					}
				}

			});
			jQuery('.menucontentholder').each(function(index){
				var tallest = 0;
				var menuItems = jQuery(this).find('.delimiterholder .categorylistholder, .categoryslot1holder-refinement');
				menuItems.each(function(){
					var thisHeight = jQuery(this).outerHeight();
					if ( thisHeight > tallest ) { tallest = thisHeight; }
				});
				jQuery.each(menuItems, function () {
					var el = jQuery(this);
					var paddingMargin = jQuery(el).outerHeight(true) - el.height()
					el.css("height", (tallest - paddingMargin) +"px");
				});
				jQuery(this).find('.categorylistholder:last').css({"background":"none"});
			});
		});
		jQuery(document).ready(function(){

			if (browser.name == 'msie' && parseInt(browser.version) <= 7 && jQuery('.headerbar').length > 0 && jQuery("div.pt_beautyconsult, .pt_guidedsearch, .pt_staticpagewide").length < 1) {
				if ( jQuery('.pt_checkout').length > 0 ) { jQuery('.headerbar').css({top: "87px", position: "absolute"}); }
				else {
					var headerbarHtml = jQuery('.headerbar').html();
					jQuery('.headerbar').remove();
					jQuery('<div class="headerbar">'+headerbarHtml+'</div>')
						.css({"float": "right", "position": "static", "width": "auto"})
						.prependTo('div#main');
				}
			}
			
		});
		jQuery(document).ajaxComplete(function(){
			try {
				if ( typeof FB != 'undefined' ) FB.XFBML.parse();
			} catch(ex) {}
		});
