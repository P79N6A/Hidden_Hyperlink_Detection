<script type="text/javascript">
	var minicartFixWidth = 0;
	var minicartFixTimer;
	function fixMinicart () {
		clearTimeout(minicartFixTimer);
		var width = jQuery('.minicarttotal').width()
		if (minicartFixWidth != width) {
			if (width > 140) {
				jQuery('.headercustomerinfo').css('right', width + 10);
			}
			minicartFixWidth = width;
		}
		minicartFixTimer = setTimeout('fixMinicart()', 500);
	}
	fixMinicart();
</script>
