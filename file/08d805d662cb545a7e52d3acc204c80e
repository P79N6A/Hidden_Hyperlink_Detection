<script type="text/javascript"><!--
	// this is used for identifying Clarins
	app.resources["isClarins"]	= false;

	jQuery(function() {
		jQuery('#samplesList > .clear').filter(':first').css('height','0');
		jQuery('#samplesList > .clear').filter(':last').css('height','0');
		
		var browser = app.util.getBrowser();
		if (browser.name == 'mozilla' && parseFloat(browser.version) == 2) {
			jQuery('.breadcrumb').css({'margin-top': '13px'});
