	//Quick-buy functionality
	function quickBuy(pid, e) {
		jQuery.ajax({
			method: "POST",
			data: {pid: pid},
			url: "/on/demandware.store/Sites-cfgint-Site/en/Product-QuickBuy"
		}).done(function(msg) {
			jQuery("#dialogQuickBuy").show().html(msg);
			e && jQuery("#dialogQuickBuy").dialog({
				open: function(event, ui) {
					jQuery(".ui-widget-overlay").css('background', 'none').bind("click", function() {
						jQuery("#dialogQuickBuy").dialog("close");
					});
				},
				dialogClass: 'uiDialogQuickBuy',
				resizable: false,
				draggable: false,
				closeText: '',
				minHeight: 150,
				width: 313,
				modal: true
			});
			e && jQuery("#dialogQuickBuy").parent().css({
				"position": "absolute",
				"top": e.pageY - jQuery('.uiDialogQuickBuy').height(),
				"left": e.pageX - jQuery('.uiDialogQuickBuy').width() / 2
			});
			jQuery(".close-button-mobile").hide();
		});
	}

	function addToCartQuickBuy(e) {
		e.preventDefault();
		var pid = $("#pid").val();
		jQuery.ajax({
				method: "POST",
				url: app.minicart.url,
				data: {
					Quantity: 1,
					cartAction: "add" ,
					pid: pid
				}
			}).done(function( req ) {
				jQuery("#dialogQuickBuy").dialog('close');
				jQuery('#minicart').html(req);
			});
	}
</script>
