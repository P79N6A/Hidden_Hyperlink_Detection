			jQuery(this).addClass('prev-horizontal-hover');
		}, function() { jQuery(this).removeClass('prev-horizontal-hover'); });
		jQuery('#addtocartpopup .jcarousel-next-horizontal, .drContent.workswellwith .jcarousel-next-horizontal').hover(function() {
			jQuery(this).addClass('next-horizontal-hover');
		}, function() { jQuery(this).removeClass('next-horizontal-hover'); });
	});

	jQuery(window).ajaxStop(function()
	{
		jQuery('#samplesList > .clear').filter(':first').css('height','0');
		jQuery('#samplesList > .clear').filter(':last').css('height','0');
	});

	jQuery(window).load(function()
	{
		var jCarouselVertical = function(){
			if (jQuery(".productdetail .maywerecommend .jcarousel-prev-vertical").hasClass('jcarousel-prev-disabled') == false) {
				jQuery(".productdetail .maywerecommend .jcarousel-prev-vertical").addClass('prev-vertical-normal').hover(function() {
					jQuery(this).addClass('prev-vertical-hover');
				}, function() {
					jQuery(this).removeClass('prev-vertical-hover');
				});
			} else {
				jQuery(".productdetail .maywerecommend .jcarousel-prev-vertical").removeClass('prev-vertical-normal prev-vertical-hover');
			}

			if (jQuery(".productdetail .maywerecommend .jcarousel-next-vertical").hasClass('jcarousel-next-disabled') == false) {
				jQuery(".productdetail .maywerecommend .jcarousel-next-vertical").addClass('next-vertical-normal').hover(function() {
					jQuery(this).addClass('next-vertical-hover');
				}, function() {
					jQuery(this).removeClass('next-vertical-hover');
				});
			} else {
				jQuery(".productdetail .maywerecommend .jcarousel-next-vertical").removeClass('next-vertical-normal next-vertical-hover');
			}
		};

		jQuery(".productdetail .maywerecommend .jcarousel-prev-vertical, .productdetail .maywerecommend .jcarousel-next-vertical").bind('click', function(){
			setTimeout(function(){ jCarouselVertical(); 	}, 500);
		});

		jCarouselVertical();
	});
//--></script>


