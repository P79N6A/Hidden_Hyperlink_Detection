<script type="text/javascript" charset="utf-8">
jQuery(document).ready(function() {
	jQuery('#mega-footer-container').find('.footer-content-block:last').css('background-image', 'none');
	jQuery('#mega-footer-bottom').find('.bottom-links').find('li:first').css({
		"padding-left": "0px",
		"background": "none"
	});
	jQuery('#mega-footer-bottom').find('.social-links').find('a:first').css({"margin-left": "0px"});

	//get highest element in the super footer on last row to align a triman logo with it

	var maxElementHeight = 0;
	jQuery('.floating-data .trimanSection').find(".inner-content").each(function(){
		var currentElHeight = jQuery(this).height();
		if(currentElHeight > maxElementHeight){
			maxElementHeight = currentElHeight;
		}
	});

	jQuery('.trimanLogo').height(maxElementHeight);

	jQuery('.mega-footer-countryselector').find('.languageSelectorTopCorner').remove();
	jQuery('.mega-footer-countryselector').find('.languageBottom').remove();
	jQuery('.mega-footer-countryselector').find('.current').find('.clear').remove();
	var currentHTML = jQuery('.mega-footer-countryselector').find('.current').find('.textContainer').html();
	jQuery('.mega-footer-countryselector').find('.current').remove();
	jQuery('.mega-footer-countryselector').find('.languagesContainer').css({'position': 'static', 'display': 'inline'});
	jQuery('.mega-footer-countryselector').find('.languageSelector').wrap('<li class="dropmenuContainer" />');
	jQuery('.mega-footer-countryselector').find('.dropmenuContainer').prepend('<a href="javascript:;" class="selected">'+currentHTML+'</a>');
	jQuery('.mega-footer-countryselector').find('.dropmenuContainer').wrap('<ul />');

	jQuery('.dropmenuContainer').click(function(){
		jQuery('.mega-footer-countryselector').find('.languageSelector').css({'top': jQuery('li.dropmenuContainer').position().top - jQuery('.languageSelector').height() - 6});
		var displayValue = jQuery(this).children('.languageSelector').css('display');
		if (displayValue == 'none') {
			jQuery(this).children('.languageSelector').css('display', 'block');
			jQuery(this).children('.selected').addClass('clicked');
		} else {
			jQuery(this).children('.languageSelector').css('display', 'none');
			jQuery(this).children('.selected').removeClass('clicked');
		}
	});

	var dropmenuContainerWidth = 220;

	if (typeof(countrySelectorWidth) != 'undefined' && !isNaN(countrySelectorWidth)) {
		dropmenuContainerWidth = countrySelectorWidth;
	}

	jQuery('.mega-footer-countryselector .languageSelector').css('width', (dropmenuContainerWidth-10)+'px');
	jQuery('.mega-footer-countryselector li.dropmenuContainer').css('width', dropmenuContainerWidth+'px');
	jQuery('.mega-footer-countryselector .selector-container').css('width', dropmenuContainerWidth+'px');

	var newsletterLegalBody = jQuery('.mega-footer-newsletter').find('.newsletter-legal-body');
	if ( newsletterLegalBody.length > 0 && newsletterLegalBody.html() != '' ) {
		newsletterLegalBody.appendTo('.mega-footer-newsletter .newsletter-title');
		jQuery('.mega-footer-newsletter .newsletter-title').mouseenter(function(){
			jQuery('.newsletter-legal-body').css("display","block");
		});
		jQuery('.mega-footer-newsletter .newsletter-title').mouseleave(function(){
			jQuery('.newsletter-legal-body').css("display","none");
		});
	}

	jQuery('#mega-footer-container')
		.find('.floating-data')
		.find('.container')
		.filter(function (index) {return (index % 3 == 2);}).each(function() {
			jQuery(this).addClass('lastContainer');
	});
});
jQuery(window).load(function(){
	if (jQuery('.newsletter-legal.newsletter-validation').length > 0) {
		var allContentWidth = jQuery('.mega-footer-content').width();
		var dropdownWidth = jQuery('.mega-footer-countryselector').width();
		var availabW = allContentWidth - dropdownWidth - 10;
		jQuery('.mega-footer-newsletter').width(availabW);
	}

	var newsletterInputCnt = jQuery(".mega-footer-content .li-newsletter-email");
	var newsletterInput = jQuery(".mega-footer-content .newsletter-email");
	if (newsletterInput.length == 1) {
		var inputNlWidth = newsletterInput.width();
		var nlWidth = newsletterInput.parents(".mega-footer-newsletter").width();
		var csWidth = jQuery(".mega-footer-countryselector").width();
		var sfContainerWidth = newsletterInput.parents(".mega-footer-content").width();
		var newWidth = sfContainerWidth - (nlWidth+csWidth) - 20 + inputNlWidth;
		newsletterInput.width(newWidth);
		var cntNewWidth = newWidth + parseInt(newsletterInput.css('padding-left')) + parseInt(newsletterInput.css('padding-right'));
		newsletterInputCnt.width(cntNewWidth);
	}
	var signupIncentiveEnabled = jQuery('input.enableSignupIncentive').val();
	if (signupIncentiveEnabled == 'true') {
		if (false) {
			jQuery('.li-newsletter-email input.newsletter-email')
			.mouseenter(function() {
				jQuery('.newsletter-validation label').css('opacity','0.5');
				jQuery('.mega-footer-newsletter .newsletterpromotioncalloutmsg').fadeIn(500).css({'position':'absolute','top':'52px'});
			})
			.mouseleave(function() {
				jQuery('.mega-footer-newsletter .newsletterpromotioncalloutmsg').fadeOut('slow');
				jQuery('.newsletter-validation label').css('opacity','1');
			});
		} else {
			jQuery('.mega-footer-newsletter .newsletterpromotioncalloutmsg').show();
		}
	}
	$('html').click(function() {
		$('ul.languageSelector').css('display','none');
	});
	$('.languagesContainer').click(function(event){
		event.stopPropagation();
	});
});

</script>

<!--[if lte IE 8]>
<script type="text/javascript" charset="utf-8">
jQuery(document).ready(function(){
	var columnsHeight = [];
	jQuery('.mega-footer-content .floating-data .container').each(function(i, element){
		columnsHeight.push(jQuery(element).height());
	});
	var maxHeight = Math.max.apply(Math, columnsHeight);
	if (maxHeight > 0) {
		jQuery('.mega-footer-content .floating-data .container').each(function(i, element) {
			jQuery(element).height(maxHeight);
		});
	}
});
</script><![endif]-->

















	 

	






































































































































































	



































































