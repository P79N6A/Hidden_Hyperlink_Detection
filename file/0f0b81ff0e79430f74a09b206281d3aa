                       function AddShopping2(proID) {
                        
		                   var requestData = {
			                   source: "packet",
			                   Quantity: "1",
			                   ProID: proID,
			                   areaID: "",
			                   time: new Date().getMilliseconds()
		                   };
		                   $.ajax({
			                   url: "/Ajax/Order/AddCart.ashx",
			                   data: requestData,
			                   cache: false,
			                   success: function (result) {
				                   if (result.indexOf('ErrorMessage') != -1) {
					                   whir.shop.alert(result.split("|")[1]);
				                   } else {
					                   showCart();
					                   whir.shop.alert("商品成功添加到购物车");
				                   }
			                   },
			                   error: function () {

			                   }
		                   });
	                   }
