    //整站无图处理
    jQuery.each(jQuery("img"), function (i, n) { jQuery(n).error(function () { n.src = '/res/images/nopic.jpg'; }); n.src = n.src; });
    //
    $("#newsDL").find("dd").each(function () {
        var numindex = $(this).find("span").length;
        if (numindex != 0) {
            var href = $(this).find("span").eq(0).find("a").attr("href");
            $(this).find("a").eq(0).attr("href", href);
        }
    });
</script> 
