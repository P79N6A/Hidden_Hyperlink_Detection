  <script type="text/javascript">
    $(".speclist dd span a").bind("click", function () {
        var title = $(this).attr("title");
        $(this).parent().parent().prev().html(title);
        $(".operate .buy").show();
    })

    $(".speclist dd span a").each(function () {
        var cla = $(this).attr("class");
        var title = $(this).attr("title");
        if ("select" == cla) {
          $(this).parent().parent().prev().html(title);
        }

    })

</script> 
  <script type="text/javascript">

    $(".mainnav li").each(function () {
        if ($.trim($(".location a:eq(1)").text()) == $.trim($(this).text())) {
            $(this).attr("class", "onnav");
        }
    })


</script>  
