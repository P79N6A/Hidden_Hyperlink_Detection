	    jQuery(function() {

				
		    //var json = jQuery.parseJSON(data);
		    //$("#areaSelTxt").html(json.data.country + '-' + json.data.area + '-' + json.data.region + '-' + json.data.city + '-' + json.data.county);

		    if (jQuery("#areaSelTxt").length > 0) {
			    jQuery.get("../ajax/GetAreaByIP.aspx", { dd: new Date().getMilliseconds() }, function(data) {
				    var aid = 0;
				    try {
					    var json = jQuery.parseJSON(data);
					    aid = json.AreaID;
                        if(aid!="0")
                        {
					        jQuery("#areaSelTxt").html(json.AreaName);
					        jQuery("#areaSelTxt").attr("value", aid);
                        }
				    } catch(e) {
					    aid = jQuery(".arrow[value]").attr("value");
				    }
				    GetStockState(aid);
			    });

		    }				


		    GetPromInfo(type, prom, pkey);

		    //导航定位及面包屑
		    switch (type) {
		    case "groupbuy":
			    //nav = '1';
			    locationUrl = "<a href='/groupbuy/groupbuy.aspx'>团购</a> > <span>" + $(".detail .name").html() + "</span>";
			    break;
		    case "rushbuy":
			   // nav = '3';
			    locationUrl = "<a href='/rushbuy/rushbuy.aspx'>抢购</a> > <span>" + $(".detail .name").html() + "</span>";
			    break;
		    case "auction":
			   // nav = '5';
			    locationUrl = "<a href='/auction/auction.aspx'>竞拍</a> > <span>" + $(".detail .name").html() + "</span>";
			    break;
		    case "integral":
			    //nav = '4';
			    locationUrl = "<a href='/integral/integral.aspx'>积分换购</a> > <span>" + $(".detail .name").html() + "</span>";
			    break;
		    }

		   // jQuery(".mainnav li").eq(nav).attr("class", "onnav");
		    if (locationUrl != "")
			    jQuery(".location").html(locationUrl);

		    jQuery(".location").show();
		    $(".sidebar_history").each(function() {
			    if ($(this).find('dl').length < 1) {
				    $(this).hide();
			    }
		    });
	    });
    
