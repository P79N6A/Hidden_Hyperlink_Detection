    jQuery(".category div[id^='proCategory']").mouseover(function () {        
        jQuery(this).find("div").eq(1).show();
       // jQuery(this).addClass("show");
    });
    jQuery(".category div[id^='proCategory']").mouseleave(function () {
        $(this).find("div").eq(1).hide();
     //   jQuery(this).removeClass("show");
    });
	
