  <script type="text/javascript">
	    jQuery(".product_intro_tab img").each(function (i, item) {
		    var src = jQuery(item).attr("src");
		    if (src != "" && src.indexOf('/') < 0)
			    jQuery(item).attr("src", '/' + src.substring(1));
	    })

