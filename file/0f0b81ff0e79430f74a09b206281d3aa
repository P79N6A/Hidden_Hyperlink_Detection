    <script type="text/javascript">
    $("#Fid2517").addClass("on");
    $("#Fid2517").siblings("dl").show();

    //点击展开收缩
    $(".menu_category li h5").click(function () {
        if($(this).hasClass("on")){
            $(this).removeClass("on");
            $(this).next("dl").find("dl").hide();
            $(this).next("dl").find("h6").removeClass("on");
        }else{
            $(this).addClass("on");
        }
        $(this).next("dl").slideToggle();
    });

    $(".menu_category li h6").each(function () {
        var num = $(this).next("dl").find("dd").length;
        if (num != 0) {
            $(this).click(function () {
                $(this).next("dl").slideToggle();
                if ($(this).hasClass("on")) {
                    $(this).removeClass("on");
                } else {
                    $(this).addClass("on");
                }
            });
        } else {
            $(this).find("a").addClass("all");
            $(this).next("dl").hide();
        }

	$(this).next("dl").find("dd a").click(function () {
            var parentId = $(this).parent().parent().prev().attr("id");
            var hidhref = $(this).attr("hidhref");
            var value = $(this).attr("value");
            var ids = "";
            $(".menu_category li h6").each(function () {
                if ($(this).attr("id") != parentId) {
                    var id = $(this).next("dl").find("dd a[class='on']").attr("value");
                    if (id != undefined) {
                        ids += $(this).next("dl").find("dd a[class='on']").attr("value") + ",";
                    }
                }
            });
            if (value == undefined) {
                if (ids.length > 0) {
                    ids = ids.substring(0, ids.length - 1);
                }
            }
            else {
                ids += value;
            }
            location.href ="product.aspx" + hidhref + "&qst=" + ids;
        });

    });

</script> 
