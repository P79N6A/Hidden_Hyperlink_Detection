    <script type="text/javascript">
            //分类漂浮             
	        var offset = $("#H_piaofu").offset();
	        var height = offset.top;

	        $(window).scroll(function () {
		        var scrolled = $(window).scrollTop();
		        if (scrolled > height) {
					
			        jQuery("#H_piaofu").attr("class", "tab piaofu");
		        }
		        else {
			        jQuery("#H_piaofu").attr("class", "tab");
