       <!--提示加入购物车弹出层--> 
       <script type="text/javascript">
	              var LoginURL='/Member/login.aspx';
	              var requestUrl = "/Ajax/Order/AddCart.ashx";
	              function AddShopping() {
				        
                        var NewProID= GetSpecId();


		              var Quantity = $("#txtQuantity").val();
		              var r = /^[0-9]*[1-9][0-9]*$///正整数

		              if (!r.test(Quantity)) {
			              whir.shop.alert("购买数量输入有误！");
			              return;
		              }
		              if (parseInt(Quantity)<=0) {
			              whir.shop.alert("购买数量至少要为1");
			              return;
		              }
		              var areaValue=$("#areaSelTxt").attr("value");
		              var requestData = {
			              
			              source: "info",
			             // ProID: "10231",
                          ProID: NewProID,
			              
			              Quantity: Quantity,
			              areaID: areaValue,
			              time: new Date().getMilliseconds()
		              };
		              $.ajax({
			              url: requestUrl,
			              data: requestData,
			              cache: false,
			              success: function (result) {
				              if(result.indexOf('NotLogin')>-1){
					              LoginURL+='?backurl='+escape(location.href);
					              location.href=LoginURL;
					              return false;
				              }
				              if (result.indexOf('ErrorMessage') != -1) {
					              whir.shop.alert(result.split("|")[1]);
				              } else {
					              showCart();
					              $(".AddtoCart").show();
				              }
			              },
			              error: function () {

			              }
		              });
	              }

                  function GetSpecId()
                  {
                    	var arrayObj = new Array();//已选则规格数组
								$(".parameter .select[value]").each(function (i, item) {
									arrayObj[arrayObj.length]= parseInt($(item).attr("value"),10);
								});
						 var specid='';
 
						
                                 
                                if( ( ',' + arrayObj.join(",") + ',' ) ==',507,')
								{ 
									specid=10231;
								}
						
                                else 
                                if( ( ',' + arrayObj.join(",") + ',' ) ==',828,')
								{ 
									specid=10232;
								}
						
                                else 
                                if( ( ',' + arrayObj.join(",") + ',' ) ==',798,')
								{ 
									specid=10233;
								}
						
                                else 
                                if( ( ',' + arrayObj.join(",") + ',' ) ==',502,')
								{ 
									specid=10234;
								}
						
                        return  specid;
                  }

              </script> 
