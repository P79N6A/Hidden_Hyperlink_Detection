    var datapid = 0;
    function dorecomm(obj){
        if(obj.find('.icon_praise_gray').hasClass('praised')){
            return ;
        }
        var islogin = 0;
        if(!islogin){
            window.location.href = 'member.php?mod=logging&action=login&mobile=2';
            return ;
        }

        var url = '';
        datapid = obj.attr('data-pid');
        if(datapid){
            url = 'forum.php?mod=misc&action=postreview&do=support&tid=1962&pid='+datapid+'&hash=a0fe88f3&inajax=1&isappinstalled=1';
        }else{
            url = 'forum.php?mod=misc&action=recommend&do=add&tid=1962&hash=a0fe88f3&inajax=1&isappinstalled=1';
        }

        $.ajax({
                type: 'GET',
                url: url,
                dataType: 'xml'
            })
            .success(function (s) {
                evalscript(s.lastChild.firstChild.nodeValue);
            })
            .error(function () {
                popup.close();
            });
    }

    function succeedhandle_fastpost(locationhref, message, param) {
        sucalert();
        var pid = param['pid'];
        var tid = param['tid'];
        if(pid) {
            $.ajax({
                    type:'POST',
                    url:'forum.php?mod=viewthread&tid=' + tid + '&weban=1&viewpid=' + pid + '&mobile=2&isappinstalled=1',
                    dataType:'xml'
                })
                .success(function(s) {
                    $('#js_cmt_mylist').prepend(s.lastChild.firstChild.nodeValue);
                })
                .error(function() {

                    popup.close();
                });
        } else {
            if(!message) {
                message = '本版回帖需要审核，您的帖子将在通过审核后显示';
            }
            popup.open(message, 'alert');
        }
                setTimeout(function(){
            window.location.href = locationhref.replace(/page=\d+/, '');
        }, 1000);
                $('#fastpostmessage').val('');
//        if(param['sechash']) {
//            $('.seccodeimg').click();
//        }
    }
    function errorhandle_(message, param){
        if(param['recommendv']) {
            var likeNum3 = $('#likeNum3');
            $('#like3').find('.icon_praise_gray').addClass('praised');
            likeNum3.text(parseInt(likeNum3.text())+1);
        }else if(message=='投票成功 '){
            var like4 = $('#datapid-'+datapid);
            like4.find('.icon_praise_gray').addClass('praised');
            like4.find('.praise_num').text(parseInt(like4.find('.praise_num').text())+1);
        }else{
            popup.open(message, 'alert');
        }
    }

    function errorhandle_fastpost(message, param) {
        popup.open(message, 'alert');
    }
    function sucalert(){
