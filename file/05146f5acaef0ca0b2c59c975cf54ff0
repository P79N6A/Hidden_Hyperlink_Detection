    var filter_url = JSON.parse(filter_url_json);
    function changeList(field){
        VeryCD.Track('/stat/baseList' + ucfirst(field.name) + 'Zongyi/');
        var option = field.options[field.selectedIndex].value;
        var url = filter_url[field.name][option];
        if(url){
            document.location = url;
        }else{
            document.location = filter_url[field.name]['__default'];
        }
    }

    function ucfirst(value){
        return value.charAt(0).toUpperCase() + value.slice(1);
    }
</script> 
