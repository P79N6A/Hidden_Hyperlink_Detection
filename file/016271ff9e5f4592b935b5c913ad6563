    $(".pic > a > img").toggle(
        function () {
            if ($(this).attr("src").match(/thumbnail/)){
                $(this).attr("src", $(this).attr("src").replace("thumbnail", "bmiddle"));
            }
            else{
                $(this).attr("src", $(this).attr("src").replace(/\/160$/,"/460"));
            }

            $(this).removeClass("imgSmall").addClass("imgBig");
        },
        function () {
            if ($(this).attr("src").match(/bmiddle/)){
                $(this).attr("src", $(this).attr("src").replace("bmiddle", "thumbnail"));
            }
            else{
