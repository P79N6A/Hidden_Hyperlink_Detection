  <script language="javascript" type="text/javascript">
			function Checkpostdata(theform){
				//cont=IframeID.document.body.innerText;
				cont=IframeID.document.body.innerHTML;
				IframeID.document.body.innerHTML=cont;
				IframeID.focus()
	            var con=OSWEBXHTML.GetXHTML(IframeID.document.body);
	            document.getElementById('Body').value = con.replace(/99/gi,".");					
                var ischeck=true;
                document.Dvform.Submit.disabled=true;
					if (theform.codestr!=null){
						if (theform.codestr.value==''){
							document.getElementById('errinfo').innerHTML='<li><font color="red">你忘记输入验证码了！</font></li>';
							theform.codestr.focus();
							ischeck=false;
						}
					}
					if (theform.Body.value.length==0){
						document.getElementById('errinfo').innerHTML='<li><font color="red">你忘记填写回复内容了！</font></li>';
						IframeID.focus();
						ischeck=false;
					}else{
						if (theform.Body.value.length >16240) {
							document.getElementById('errinfo').innerHTML='<li><font color="red">回复内容不能大于 16240 字节！</font></li>';
							IframeID.focus();
							ischeck=false;
						}

						if (theform.Body.value.length <0) {
							document.getElementById('errinfo').innerHTML='<li><font color="red">回复内容不能小于 0 字节！</font></li>';
							IframeID.focus();
							ischeck=false;
						}
					}
                                           
                    if (ischeck==false){document.Dvform.Submit.disabled=false;return false}else {return true;}
                 }
</script>
