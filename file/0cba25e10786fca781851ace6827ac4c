//RedColor("zhuanchang");
RedColor("tz");
function RedColor(p){ 
  document.getElementById(p).style.color="#ff0000"; 
  setTimeout('BlueColor("'+p+'")',400);  
} 

function BlueColor(p){ 
  document.getElementById(p).style.color="#0000ff"; 
  setTimeout('RedColor("'+p+'")',400);  
} 
