   <script type="text/javascript">
         function hiddenDiv()//隐藏右边信息
    {
        var divId=document.getElementById("divShowRight");   
        var divShow=document.getElementById("divShow");
        var divHidden=document.getElementById("divHidden");
        divId.style.display="none";
        divShow.style.display="";
        divHidden.style.display="none";
        
    }
    function showDiv()//显示右边信息
    {
        var divId=document.getElementById("divShowRight");   
        var divShow=document.getElementById("divShow");
        var divHidden=document.getElementById("divHidden");
        divId.style.display="";
        divShow.style.display="none";
        divHidden.style.display="";
    }
     //点击搜索按钮事件    
        var enter_flag=false;  
        function trim(str){
            for(var i=0;i<str.length&&str.charAt(i)==" ";i++);
            for(var j=str.length;j>0&&str.charAt(j-1)==" ";j--);
            if(i>j)return "";
            return str.substring(i,j);
        }
        function enter_click(evt,t)
        {
            evt=window.event||evt;
            if(evt.keyCode==13)
            search_click(t);
        }
        function search_click(t)
        {
            var textfield=document.getElementById(t);
            var keyword='';
            
            if(textfield.value=="问卷关键词或ID"){textfield.value=""; }
            if(trim(textfield.value)!="") 
                keyword=escape(trim(textfield.value)).replace("+","%2b");

            if (keyword != '') {
                var pattern = /^\d{4,8}$/;
                if (pattern.exec(keyword))
                    window.location = "http://www.sojump.com/jq/" + keyword + ".aspx";
                else
                    window.location = "http://www.sojump.com/publicsurveys.aspx?keyword=" + keyword;
            }
            else
                window.location = "http://www.sojump.com/publicsurveys.aspx";
        }  
      
        </script> 
