       <script type="text/javascript">
    var selVal = "";
    var ddlFirst = document.getElementById("ddlFirst");
    var divChoice = document.getElementById("divChoice"); //题目选项div
    var btnAdd = document.getElementById("btnAdd");
    var btnDelFenLei = document.getElementById("btnDelFenLei");
    var fenLeiJoinArray = ["省份", "城市", "来源"];
    var ddlConds = [];//题目下拉框数组
    var isNew=1;
    function defaultReport(){
        var loc='/report/'+activityId+'.aspx';
        window.location=loc;
        return false;
    }
    //选项改变
    function itemClicked(cond) {
        var sconds = cond.split(",");
        var conddd = "";
        if (sconds[0] == "省份"||sconds[0]=="城市"||sconds[0]=="来源") {
            //var choices = qs[ddlFirst.selectedIndex];
            var choices = getChoices(ddlFirst);
            var objSel = { "省份": "7", "城市": "9", "来源": "8" };
            var val = choices.split("§")[sconds[1] - 1];
            if (sconds[0] == "来源")
                val = val.split("[")[0];
            cond = objSel[sconds[0]] + "┋0┋" + val;
            conddd = "?aec=" + encodeURIComponent(cond);
        }
        else
            conddd = "?arc=" + encodeURIComponent(cond);
        var loc = "/report/" + activityId + ".aspx" + conddd;
       
        if (prevLi && prevLi.getAttribute("isCeping")) loc = "/ceping/" + activityId + ".aspx" + conddd;
        window.location.href = loc + hideU;
        if (window.event)
            window.event.returnValue = false;
    }
    function getChoices(ddl){
      var val=ddl.value;
       if (val == "省份") {
                 if (!htJoinData[1]) loadData(1);
                 choices = htJoinData[1];
             }
             else if (val == "城市") {
                 if (!htJoinData[2]) loadData(2);
                 choices = htJoinData[2];
             }
             else if (val == "来源") {
                 if (!htJoinData[3]) loadData(3);
                 choices = htJoinData[3];
             }
             else {
                 var index = ddl.selectedIndex;
                 choices = qs[index];
             }
             return choices;
    }
    //条件改变
    function ddlChange() {
        var condsArray = new Array();
        for (var i = 0; i < ddlConds.length; i++) {//遍历题目
            var val = ddlConds[i].value;
             var choices=getChoices(ddlConds[i]);
            
            if (!choices)
                return;
            var cArray = choices.split("§");
            condsArray.push(cArray);
        }
        var sb = "";
        if (ddlConds.length == 1) {//只有一个题目
            var cArray = condsArray[0];
            var val = ddlConds[0].value;
            for (var i = 0; i < cArray.length; i++) {
                var ci = i + 1; var cond = val + "," + ci; var title = cArray[i];
                var dval = title;
                if (val == "来源") dval = title.split("[")[0];
                var sel = "";
                if (selVal == ci || selVal==dval) {
                    sel = " class='selected' ";
                    reportName = title; selVal = "";
                    //if(!isCeping) document.getElementById("hrefAnalyze").style.display = "";
                }
                sb += "<a "+sel+" href='javascript:void(0);' onclick=\"itemClicked('" + cond + "',this);\">" + title + "</a>";
            }
        }
        else if (ddlConds.length == 2) {//有两个题目，需要进行交叉
            
            var cArray1 = condsArray[0];
            var val1 = ddlConds[0].value;
            var cArray2 = condsArray[1];
            var val2 = ddlConds[1].value;
            if (val1 == val2) {//两个值相等
                this.value = this.prevValue;
                //if(!isCeping) document.getElementById("hrefAnalyze").style.display = "";
                return;
            }
            if (fenLeiJoinArray.indexOf(val2) > -1 || fenLeiJoinArray.indexOf(val1) > -1) {
                this.value = this.prevValue;
                if (confirm("题目不能与“省份”，“城市”，“来源”进行组合，请使用自定义查询功能。是否立即前去自定义查询页面？")) {
                    changeTab('liQuery');
                }
                return;
            }
            for (var i = 0; i < cArray1.length; i++) {
                var ci1 = i + 1;
                if (i > 0)
                    sb +="<div style='margin-top:10px;'></div>";
                for (var j = 0; j < cArray2.length; j++) {
                    var ci2 = j + 1;
                    var title = cArray1[i] + "―" + cArray2[j];
                    var cond = val1 + "," + ci1 + "" + val2 + "," + ci2;
                    var sel = "";
                    if (selVal == (ci1 + "," + ci2)) {
                        sel = " class='selected' ";
                        reportName = title;
                    }
                    sb += "<a "+sel+" href='javascript:void(0);' onclick=\"itemClicked('" + cond + "');\">" + title + "</a>";
                }

            }
        }
       
        divChoice.innerHTML = sb;
        this.prevValue = this.value;
    }
    
    
    function addCond() {//添加题目
        if (ddlConds.length >= 2) {
            if (confirm("分类统计最多只允许2个筛选条件。如需组合更多条件，请使用自定义查询功能。是否立即前去自定义查询页面？")) {
                changeTab('liQuery')
            }
            return;
        }
        if (fenLeiJoinArray.indexOf(ddlConds[0].value)>-1) {
            if (confirm("题目不能与“省份”，“城市”，“来源”进行组合，请使用自定义查询功能。是否立即前去自定义查询页面？")) {
                changeTab('liQuery');
            }
            return;
        }
        var divNewCond = document.createElement("div");
        divNewCond.style.margin="5px 0 0 74px";
         var newCond = ddlFirst.cloneNode(true);
         newCond.onchange = ddlChange;
         var sIndex = ddlFirst.selectedIndex;
         var nIndex = 0;
         if (sIndex + 1 < ddlFirst.options.length)
             nIndex = sIndex + 1;
         else if (sIndex - 1 > 0)
             nIndex = sIndex - 1;
         newCond.selectedIndex = nIndex;

        divNewCond.appendChild(newCond);

        var newBtn = btnAdd.cloneNode(true);
        divNewCond.appendChild(newBtn);
        newBtn.onclick = addCond;

        var imgJian = document.createElement("img");
        imgJian.src = "/images/Mysojump/QuestionnaireMng/Design/jian.gif";
        imgJian.style.cursor = "pointer";
        imgJian.style.verticalAlign="middle";
        imgJian.style.paddingLeft="1px";
        imgJian.align = "middle";
        imgJian.onclick = function () {
            ddlFirst.parentNode.parentNode.parentNode.removeChild(divNewCond);
            ddlConds.remove(newCond);
            ddlFirst.onchange();
        }
        divNewCond.appendChild(imgJian);
        ddlFirst.parentNode.parentNode.parentNode.appendChild(divNewCond);
        ddlConds.push(newCond);
        newCond.onchange();
    }
    function doAnalyze() {
        if (fenLeiJoinArray.indexOf(ddlFirst.value) > -1) {
            var objSel = { "省份": "-1", "城市": "-2", "来源": "-3" };
            ddlListX.value = objSel[ddlFirst.value];
        }
        else {
            ddlListX.value = ddlFirst.value;
            if (ddlListX.value == ddlListY.value)
                ddlListY.value = "-1";
        }
        if (ddlConds.length == 2) {
            btnAddX.onclick();
            topicX[1].value = ddlConds[1].value;
        }
        
        changeTab(1);document.getElementById("divTipY").style.display = "";
    }
    function initCond() {//分类统计，初始化
        ddlConds.push(ddlFirst);
        ddlFirst.onchange = ddlChange;
        btnAdd.onclick = addCond;
        if (reportType == 3 && qCond) {//有条件时，初始化
            var ccc = qCond.split("");
            if (ccc.length >= 1) {
                var cdetail = ccc[0].split(",");
                ddlFirst.value = cdetail[0];
                if (ccc.length == 2) {
                    addCond();
                    var c1Deatil = ccc[1].split(",");
                    ddlConds[1].value = c1Deatil[0];
                    selVal = cdetail[1] + "," + c1Deatil[1];
                    ddlConds[1].onchange();
                }
                else {
                    selVal = cdetail[1];
                    ddlFirst.onchange();
                }
            }
        }
        else if (reportType == 3 && eCond) {
            var conds = eCond.split("┋");
            switch (conds[0]) {
                case "7":
                    ddlFirst.value = "省份"; selVal = conds[2];
                    break;
                case "9":
                    ddlFirst.value = "城市"; selVal = conds[2];
                    break;
                case "8":
                    ddlFirst.value = "来源"; selVal = conds[2];
                    break;
            }
            ddlFirst.onchange();
        }
        else {
            ddlFirst.selectedIndex = 0;
            for (var i = 0; i < ddlFirst.options.length; i++) {
                var optText = ddlFirst.options[i].text;
                if (isSample(optText)) {
                    ddlFirst.selectedIndex = i;
                    break;
                }
            }
            ddlFirst.onchange();
        }
    }
    initCond();
</script> 
       <script type="text/javascript">
    var topicX = new Array();
    var topicY = new Array();
    var ddlListX = document.getElementById("ddlListX");
    var ddlListY = document.getElementById("ddlListY");
    var divMsg = document.getElementById("divMsg");
    var btnAddX = document.getElementById("btnAddX");
    var btnAddY = document.getElementById("btnAddY");
    //var tempArray;
    var diffArray;

    var regconds = new Array();
    if (reportType == 2 && qCond)
        regconds = qCond.split(";");
    btnAddX.onclick = function () {
        AddCondition(1);
    }
    btnAddY.onclick = function () {
        AddCondition(2);
    }
    var ddlJiaoCha = document.getElementById("ctl02_ContentPlaceHolder1_ViewReport1_ddlJiaoCha");
    var btnDelJiaoCha = document.getElementById("btnDelJiaoCha");
    if (ddlJiaoCha) {
        ddlJiaoCha.onchange = function () {
            if (this.value)
                window.location = this.value;
            btnDelFenLei.style.display = this.value ? "" : "none";
        }
    }
    if (btnDelJiaoCha) {
        btnDelJiaoCha.style.display = ddlJiaoCha.value ? "" : "none";
        btnDelJiaoCha.onclick = function () {
            var deleteUrl = "/wjx/report/deletereport.aspx?activity=" + activityId + "&reportid=" + reportid;
            if (confirm('此操作无法恢复，确认删除吗？'))
                PDF_launch(deleteUrl, 400, 100);
        }
    }
    function saveJiaoCha() {
        if (topicX.length > 0 && topicY.length > 0) {
            var qCond = getRCond();
            var name = topicX[0].options[topicX[0].selectedIndex].text;
            PDF_launch("/wjx/activitystat/savereport.aspx?activity=" + activityId + "&type=2&qCond=" + qCond + "&name=" + encodeURIComponent(name), 420, 100);
        }
    }
    function ddlListChanged() {//防止重复
        var val = this.value;
        divMsg.innerHTML = "";
        for (var i = 0; i < topicX.length; i++) {
            if (this != topicX[i] ) {
                if (val == topicX[i].value) {
                    this.value = "0";
                    divMsg.innerHTML = "题目不能重复";
                    return;
                }
                else if ((val < 0 || topicX[i].value < 0) && topicX.indexOf(this) > -1) {
                    var txt = "";
                    if (topicX[i].value < 0) {
                        txt = topicX[i].options[topicX[i].selectedIndex].text; topicX[i].value = "0";
                    }
                    else {
                        txt = this.options[this.selectedIndex].text; this.value = "0";
                    }
                    divMsg.innerHTML = "很抱歉，" + txt + "只能单独作为自变量，不能跟其它自变量组合。";
                    return;
                }
            }
            
        }
        for (var i = 0; i < topicY.length; i++) {
            if (this != topicY[i] && val == topicY[i].value) {
                this.value = "0"; divMsg.innerHTML = "题目不能重复";
                return;
            }
        }
        this.prevValue = this.value;
    }
    Init();
    function Init() {
        topicX.push(ddlListX);//下拉框数组
        topicY.push(ddlListY);
        ddlListX.onchange = ddlListChanged;
        ddlListY.onchange = ddlListChanged;
        if (regconds[0] && regconds[1]) {
            var topics = regconds[0].split(",");
            ddlListX.value = topics[0];
            for (var i = 1; i < topics.length; i++) {
                var topic = topics[i];
                if(topic&&topic!="-1")
                  AddTopic(topic,1);
            }
            var topics = regconds[1].split(",");
            ddlListY.value = topics[0];
            for (var i = 1; i < topics.length; i++) {
                var topic = topics[i];
                if (topic && topic != "-1")
                    AddTopic(topic, 2);
            }
        }
        else{
            for (var i = 1; i < ddlListX.options.length; i++) {
                var optText = ddlListX.options[i].text;
                if (isSample(optText)) {
                    ddlListX.selectedIndex = i;
                    break;
                }
            }
            
        }
        if (topicX.length == 1 && topicY.length == 1) {
            document.getElementById("hrefSwapXY").style.display = "";
        }

    }
    var reg = '';
    if (reg == "time") {
        changeTab(1);topicX[0].value = "-4";
    }
    function AddCondition(type) {
        divMsg.innerHTML = "";
        AddTopic("0", type);
    }
    function AddTopic(topic, type) {//添加条件，topic表示题号，text表示题目标题,type=1表示自变量
        var ddlList;
        var tempArray = null;
        if (type == 1) {
            tempArray = topicX;
            diffArray = topicY;
            ddlList = ddlListX;
        }
        else {
            tempArray = topicY;
            diffArray = topicX;
            ddlList = ddlListY;
            document.getElementById("divTipY").style.display = "none";
        }
        if (tempArray.indexOf(topic) > -1) {
            divMsg.innerHTML = "您已选择了该题目";
            return;
        }
        //不包含此题目
        if (diffArray.indexOf(topic) > -1) {
            divMsg.innerHTML = "因变量不能与自变量相同";
            return;
        }
        if (tempArray == topicX && topicX.length >= 2) {
            divMsg.innerHTML = "自变量的个数最多为两个";
            return;
        }
        else if(tempArray==topicY && topicY.length>=10){
            divMsg.innerHTML = "因变量的个数最多为10个";
            return;
        }

        var divNew = document.createElement("div");
        var newDdl = ddlList.cloneNode(true);
        //新下拉框索引
        newDdl.value = topic;
        newDdl.onchange = ddlListChanged;
        newDdl.prevValue = null;
        divNew.appendChild(newDdl);

        var newBtn = btnAddX.cloneNode(true);
        divNew.appendChild(newBtn);
        newBtn.onclick = function () {
            AddCondition(type);
        };

        var imgJian = document.createElement("img");
        imgJian.src = "/images/Mysojump/QuestionnaireMng/Design/jian.gif";
        imgJian.style.cursor = "pointer";
        imgJian.style.verticalAlign="middle";
        imgJian.style.paddingLeft="1px";
        imgJian.align = "middle";
        imgJian.onclick = function () {
            tempArray.remove(newDdl);
            ddlList.parentNode.parentNode.removeChild(divNew);
            divMsg.innerHTML = "";
        }
        divNew.appendChild(imgJian);
        ddlList.parentNode.parentNode.appendChild(divNew);
        tempArray.push(newDdl);


        if (topicX.length > 1 || topicY.length > 1)
            document.getElementById("hrefSwapXY").style.display = "none";
    }
    function swapXY() {
        var valX = topicX[0].value;
        var valY = topicY[0].value;
        if (valX - 1 > 0) {
            topicX[0].value = "-1";
            topicY[0].value = valX;
            topicX[0].value = valY;
        }
        btnAnalyzeClick();
    }
    function getRCond() {
        var xx = ""; var yy = "";
        var index = 0;
        var noSameTopic = new Array();
        for (var i = 0; i < topicX.length; i++) {
            var val=topicX[i].value;
            if (val == "0")
                continue;
            if (noSameTopic[val])
                continue;
            if (index > 0)
                xx += ",";
            xx += val;
            noSameTopic[val] = "1";
            index++;
        }
        index = 0;
        for (var i = 0; i < topicY.length; i++) {
            var val = topicY[i].value;
            if (topicY[i].value == "-1")
                continue;
            if (noSameTopic[val])
                continue;
            if (index > 0)
                yy += ",";
            yy += val;
            noSameTopic[val] = "1";
            index++;
        }
        var rcond = xx + ";" + yy;
        return rcond;
    }
    var ddlReportSource = document.getElementById("ctl02_ContentPlaceHolder1_ViewReport1_ddlReportSource");
    
    function btnAnalyzeClick() {
        if (topicX.length == 2) {
            for (var i = 0; i < topicX.length; i++) {
                var q = parseInt( topicX[i].value);
                if (q < 0) {
                    var txt = topicX[i].options[topicX[i].selectedIndex].text;
                    divMsg.innerHTML = "很抱歉，" + txt + "只能单独作为自变量，不能跟其他变量组合。" ;
                    return;
                }
            }
        }
        if (topicX.length == 1 && topicX[0].value == "0") {
            divMsg.innerHTML = "请选择自变量";
            return;
        }
        if (topicY.length == 1 && topicY[0].value == "0") {
            divMsg.innerHTML = "请选择因变量";
            return;
        }
        if (topicX.length > 0 && topicY.length > 0) {
            var rcond = getRCond();
            var regloc="viewregnew.aspx";
            var loc = "/wjx/activitystat/"+regloc+"?activity=" + activityId + "&rrc=" + encodeURIComponent(rcond);

            var newSid = "";
            if (ddlReportSource && ddlReportSource.value) newSid = "&sid=" + ddlReportSource.value;
            loc += newSid;
            window.location = loc+hideU;
            return true;
        }
        else {
            divMsg.innerHTML = "自变量跟因变量不能为空";
            return false;
        }
    }
    function addSource() {
        changeTab("liQuery");
        var spanCrossTip = document.getElementById("spanCrossTip");
        spanCrossTip.style.display = "";
    }
    var sid = '';
    if (ddlReportSource) {
         if(sid)ddlReportSource.value = sid;
         ddlReportSource.onchange = function () {
             if (this.value == "-1") {
                 addSource();
             }
         }
    }
</script> 
