	$('.search_btn').click(function(){
		var type = $('input[name=type]').val();
		var keyword = $('.ui-autocomplete-input').val();
		if (keyword == '输入信息关键词')
		{
			keyword = '';
			layer.msg('输入信息关键词');
			return false;
		}else{
			window.open('http://www.52zjk.cn/Search?type='+ type +'&keyword=' + keyword,'_self');
		}
	})
	$('.ui-autocomplete-input').focus(function(){
		var val = $(this).val() == '输入信息关键词' ? '' : $(this).val();
		$(this).val(val);
	}).blur(function(){
		var val = $(this).val() == '' ? '输入信息关键词' : $(this).val();
		$(this).val(val);
	})
		
	$('.ui-autocomplete-input').keypress(function(e) {
		if (e.keyCode==13) {
			$('.search_btn').eq(0).click();
		}
	});
})
function addBookmark(title,url) { 
	if (window.sidebar) { 
		window.sidebar.addPanel(title,url,""); 
	} else if( document.all ) { 
		window.external.AddFavorite(url,title); 
	} else if( window.opera && window.print ) { 
		return true; 
	} 
}
function setHomepage(){
	if (document.all){
		var url=window.location.href;
		document.body.style.behavior='url(#default#homepage)';
		document.body.setHomePage(url);
	}else if(window.sidebar){
		if(window.netscape){
			try{
				netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
			}catch (e){
				layer.alert( "亲爱的用户你好：\n你使用的不是IE浏览器，此操作被浏览器阻挡了，你可以选择手动设置为首页！\n给你带来的不便，本站深表歉意。" );
			}
		}
	}
}
