jQuery(document).ready(function(){
var leftheight=jQuery(".ct_left").height()+jQuery(".comment").height();
var rightheight=jQuery(".ct_right").height();
if(leftheight<rightheight-57){
if(jQuery(".comment")){
jQuery(".comment").css('min-height',rightheight-78-jQuery(".ct_left").height()+"px")
}
else{
jQuery(".ct_left").css('min-height',rightheight-77-jQuery(".comment").height()+"px");
}
}
});
</script> 
