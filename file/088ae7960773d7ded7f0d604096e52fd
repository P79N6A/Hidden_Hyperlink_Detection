function delcomment(id){
art.dialog({content: '确定删除吗？',yesFn:function(){
var url = 'comment.php';
var pars = "delcomment=delcomment&comid="+id;
jQuery.ajax({
   type: "POST",
   url: url,
   data: pars,
   dataType :'html',
   success: function(msg){
if(msg!="error"){
window.location.reload();
}
else
{
art.dialog({content: '系统出错，请联系管理员',yesFn:function(){}});
}
   }
});
},
noFn:function(){},
icon:"warning"
});
}
function comhuifu(id){
jQuery("#comhuifufup").val(id);
jQuery("#comhuifumsg").html(jQuery("#commessage"+id).html());
art.dialog({
title:"回复评论：",
content: document.getElementById('comhuifu'),
yesFn:function(){
jQuery("#comhuifuform").submit();
comsub(1);
},
noFn:function(){},
id:"comhuifu_pop"
});
}
function comsup(id){
var url = 'comment.php';
var pars = "comzhichi=comzhichi&comid="+id;
jQuery.ajax({
   type: "POST",
   url: url,
   data: pars,
   dataType :'html',
   success: function(msg){
if(msg!="error"){
jQuery("#comzhichi"+id).html("支持["+msg+"]");
}
else
{
art.dialog({content: '对不起，您12小时内只能支持同一评论一次',yesFn:function(){}});
}
   }
});
}
function comsub(type){
var theform=jQuery("#comsubform"+type);
if(type==0){
var text=jQuery("#comtext").val();	
}
else if(type==1){
var text=jQuery("#comhuifutext").val();
}
else if(type==2){
var text=jQuery("#comtext2").val();
}
var url = 'comment.php';
var pars = "checkmsg=checkmsg&comtext="+text;
jQuery.ajax({
   type: "POST",
   url: url,
   data: pars,
   dataType :'json',
   success: function(msg){
if(msg.message=="ok"){
if(text==""|| text =="文明上网，理性发言" ){
return false;
}
else{
theform.submit();
}
}
else
{
art.dialog({
content: msg.message ,
yesFn:function(){
if(msg.censor_type=="1"){
return;
}
else{
if(text==""|| text =="文明上网，理性发言" ){
return false;
}
else{
theform.submit();
}
}
},
noFn:function(){return;}
});
}
   }
});
}
jQuery(document).ready(function() {
jQuery(".comment .textarea").bind("focus",function(){
if(jQuery(this).val()=="文明上网，理性发言"){
jQuery(this).val("");
}
jQuery(this).addClass("txton");
jQuery(this).removeClass("txtblur");
});
jQuery(".comment .textarea").bind("blur",function(){
jQuery(this).addClass("txtblur");
jQuery(this).removeClass("txton");
if(jQuery(this).val()==""){
jQuery(this).val("文明上网，理性发言");
}
});
});
function showaddpl2()
{
jQuery("#tmp_fbpl").addClass("tmp_pl2");
jQuery("#tmp_fbpl").removeClass("tmp_pl");
jQuery(".gotocom").removeClass("bdbottom");
jQuery("#tmp_body").show();
jQuery("#comtext2").focus();

}
function hideaddpl2(){
jQuery("#tmp_fbpl").addClass("tmp_pl");
jQuery("#tmp_fbpl").removeClass("tmp_pl2");
jQuery("#tmp_body").hide();
jQuery(".gotocom").addClass("bdbottom");

}
</script> 
