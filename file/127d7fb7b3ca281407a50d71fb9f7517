          content: '<p class="text-muted small">把播放器贴到博客或论坛，用ctrl+c复制</p> <input class="input" type="text">'
        }).on('shown.bs.popover', function(){
          var inputEmbed = $(this).data('bs.popover').tip().find('input');
          inputEmbed.val(embedText).get(0).select();
