    $.ajax({
        type:'get',
        url:"http://xingtai.liebiao.com/ajax/postclicks/?postid=341229834",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
