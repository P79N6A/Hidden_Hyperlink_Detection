 function checksc(){
		  
            $.ajax({
                type: "get",
                url : "favorite.php",
                dataType:'json',//返回的值以 JSON方式 解释
                data: 'sc_id='+1035+'&type='+3,
                success: function(data){
                   //alert(data['msg']+'\n'+data['type1']);
                    if(data['msg']==0){
                        alert("您还未登录,请先登录!");
					}
                    else{
                    $("#sfsc").html("已收藏");
                    alert(data['msg']);
					}
                }
            });
        }
  function checkddtj(){
        var lytime=$.trim($("#start_time").val());
