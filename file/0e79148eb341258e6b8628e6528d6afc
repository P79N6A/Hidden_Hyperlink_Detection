                    <script type="text/javascript">
                        jQuery(".banner").slide({mainCell: ".bd ul", effect: "leftLoop", trigger: "click", interTime: 3000, autoPlay: true});//头部轮播
                        jQuery(".mingjia_banner").slide({mainCell: ".bd ul", effect: "leftLoop", trigger: "click", vis: 6});//名家讲座

                        var screen_width = window.screen.width;//当前显示器的宽度
                        var body_height = document.body.scrollHeight;//网页正文的高度（包括未见区域的高度）
                        if (screen_width >= 1366) {//判断，显示器分辨率大于等于1366的时候显示竖向的导航，小于的时候显示横向的导航
                            $(".window_change").removeClass("window_nav_heng");
                            $(".window_change").addClass("window_nav");
                        } else {
                            $(".window_change").addClass("window_nav_heng");
                            $(".window_change").removeClass("window_nav");
                            $(window).scroll(function () {
                                var scroll_height = $(this).height();//可视窗口的高度
                                var scroll_top = $(this).scrollTop();//偏移量
                                var foot_height = $(".foot").height();
                                if (scroll_top + scroll_height > parseInt(body_height - foot_height)) {//-158是减去脚部的高度
                                    $(".window_nav_heng").hide();
                                } else {
                                    $(".window_nav_heng").show();
                                }
                            })
                        }


                    </script>
                    <script>
                        //投票结果和按钮
                        $(".btn1").click(function () {

                            var checked = $(".check").is(":checked");
                            if (checked == true) {

                                $(".tp_now").css("display", "block");
                                $(".close_btn,.qr_btn").click(function () {
                                    $(".tp_now").css("display", "none");
                                });
                            } else {
                                $(".tp_now").css("display", "none");
                            }

                        });
                        $(".btn2").click(function () {
                            $(".tp_result").css("display", "block");
                            $(".close_btn").click(function () {
                                $(".tp_result").css("display", "none");
                            })
                        })
