                  <script>
                        $(".change_testword").click(function () {
                            var timestamp = Date.parse(new Date());
                            $("#captcha").attr('src', 'http://xf.faxuan.net/service/gc.html?timestamp=' + timestamp);
                        })
                        function writeCookie(name, value) {
                            var Days = 7; //此 cookie 将被保存 30 天
                            var exp = new Date();
                            exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);
                            document.cookie = name + "=" + escape(value) + ";expires=" + exp.toGMTString();
                        }
                        function readCookie(name) {
                            var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
                            if (arr != null)
                                return unescape(arr[0]);
                        }
                        $(".login_button").click(function () {
    if ($("#user_name").val() == null || $("#user_name").val() == "") {
        $("#user_name").focus();
    } else {
        var newTab = window.open('about:blank');
      	var user_name = $("#user_name").val();
      	var user_pass = $("#user_pass").val();
        $.ajax({
            type: "POST", cache: false, dataType: "json", url: "/shop/api/xf_login",
            data: {
                user_name: $("#user_name").val(),
                user_pass: $("#user_pass").val(),
                code: $("#code").val(),
                rid: readCookie("rid").replace('rid=', '').replace(";", "").replace(" ", "")

            }, success: function (data) {
                if (data.code == 200) {
                    base.addCookie("RmbUser", {
                        "RmbUser": false,
                        "userAccount": user_name,
                        "userPassword": user_pass
                    }, {path: "/", expires: 7, domain: "faxuan.net"});
                    if ($('.remeber_checkbox').is(":checked")) {
                        writeCookie("merberUsername", $("#user_name").val());
                        writeCookie("merberPassword", $("#user_pass").val());
                    }
                    base.addCookie("bindType", '', {path: "/", expires: -1, domain: "faxuan.net"});
                    base.addCookie("passwordLevel", '', {path: "/", expires: -1, domain: "faxuan.net"});

                    if ((parseInt(data.data.userBindingType) == 0 || typeof (data.data.userBindingType) == "undefined") && parseInt(data.data.passwordLevel) == 0 && /^1\d{10}$/.test(data.data.user_name) == false) {
                        base.addCookie("bindType", 1, {
                            path: "/",
                            expires: bps.MillisecondToDate(),
                            domain: "faxuan.net"
                        });
                        base.addCookie("passwordLevel", 1, {
                            path: "/",
                            expires: bps.MillisecondToDate(),
                            domain: "faxuan.net"
                        });
                    } else if (parseInt(data.data.userBindingType) > 0 && parseInt(data.data.passwordLevel) == 0) {
                        base.addCookie("bindingFog", 1, {
                            path: "/",
                            expires: bps.MillisecondToDate(),
                            domain: "faxuan.net"
                        });
                        base.addCookie("passwordLevel", 1, {
                            path: "/",
                            expires: bps.MillisecondToDate(),
                            domain: "faxuan.net"
                        });
                    } else if ((parseInt(data.data.userBindingType) == 0 || typeof (data.data.userBindingType) == "undefined") && /^1\d{10}$/.test(data.data.user_name) == true) {

                        base.addCookie("isPhone", 1, {
                            path: "/",
                            expires: bps.MillisecondToDate(),
                            domain: "faxuan.net"
                        });
                    } else if ((parseInt(data.data.userBindingType) == 0 || typeof (data.data.userBindingType) == "undefined") && /^1\d{10}$/.test(data.data.user_name) == false && parseInt(data.data.passwordLevel) == 1) {

                        base.addCookie("bindType", 1, {
                            path: "/",
                            expires: bps.MillisecondToDate(),
                            domain: "faxuan.net"
                        });
                    }
                    var loginUser = {
                        userAccount: data.data.userAccount,
                        userName: data.data.userName,
                        userType: data.data.userType,
                        roleId: data.data.roleId,
                        domainCode: data.data.domainCode,
                        rankId: data.data.rankId,
                        lockFlag: data.data.lockFlag,
                        areaCode: data.data.areaCode,
                        domainName: data.data.domainName,
                        sid: data.data.sid,

                        id: data.data.id,
                        domainType: data.data.domainType
                    }

                    base.addCookie("loginUser", loginUser, {
                        path: "/",
                        expires: bps.MillisecondToDate(),
                        domain: "faxuan.net"
                    });
					
                  	if(parseInt(data.data.userType) == 1){
                      add_use_lpoint(data.data.userAccount,data.data.domainCode,data.data.sid);
                    }
                  	
                    if (parseInt(data.data.userType) == 1) {
                        newTab.location.href = "http://xf.faxuan.net/bps/frame/t/frame.html?loginUserAccount=" + data.data.userAccount+"&sid="+data.data.sid;
                    } else {
                        newTab.location.href = "http://xf.faxuan.net/bpms/frame/t/frame.html?loginUserAccount=" + data.data.userAccount+"&sid="+data.data.sid;
                    }
                } else {
                  newTab.close();   
                  alert(data.msg);
                    
                }
            }
        });
    }

})

						//add_use_lpoint('1666666660010','101006001000000','');

						function add_use_lpoint(userAccount,domainCode,sid){
                          var timestamp = Date.parse(new Date());
                            $.ajax({
                                type: "POST", cache: false, url: "/shop/api/add_use_lpoint",
                                data:{
                                  userAccount:userAccount,
                                  domainCode:domainCode,
                                  sid:sid

                                }, 
                                success: function (data) {	


                                }
                            });
                          
                        }

                        var user_name = readCookie('merberUsername')
                        if (typeof user_name != undefined && user_name != null) {

                            user_name = user_name.replace('merberUsername=', '')
                            user_name = user_name.replace(';', '')
                            $('#user_name').val(user_name)
                        }

                        var password = readCookie('merberPassword')
                        if (typeof password != undefined && password != null) {
                            password = password.replace('merberPassword=', '')
                            $('#user_pass').val(password)
                        }

                              if (typeof user_name != undefined && user_name != null && typeof password != undefined && password != null) {
            user_name = user_name.replace('merberUsername=', '')
            user_name = user_name.replace(';', '')
            $('.remeber_checkbox').attr('checked',true)
        }


                    </script>
