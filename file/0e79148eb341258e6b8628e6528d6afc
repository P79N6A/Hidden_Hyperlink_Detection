                        <script type="text/javascript">
                            $(".bd_pop_close").click(function () {
                                var date = new Date();
                                var expiresDays = 365;
                                date.setTime(date.getTime() + expiresDays * 24 * 3600 * 1000);
                                $.cookie('the_cookie', 'the_value', {expires: date})
                                $('.bd_pop').css('display', 'none');
                                $('.shade').css('display', 'none');
                            })
                            if ($.cookie('the_cookie') == 'the_value') {
                                $('.bd_pop').css('display', 'none');
                                $('.shade').css('display', 'none');
                            } else {
                                popShow();
                            }
                            function popShow() {
                                // 遮罩
                                $('.shade').height($(document).height());
                                window.onresize = function () {
                                    $('.shade').height($(document).height());
                                }
                                // 弹出
                                $('.bd_pop').css('display', 'block');
                                $('.shade').css('display', 'block');
                            }





                        </script>-->
                        <script type="text/javascript">
                          $.ajax({
                            url: "/shop/api/province_sort?domainCode=100000000000000",

                                type: 'get',
                                cache:false,
                                success: function(data){
                                    var data = eval('('+data+')')
                                    var top_unit = data['top_unit'];
                                    
                                    var unit_str = ''
                                    for(var i in top_unit){
                                        unit_str += '<li><span class="num num'+(parseInt(i)+1)+'">'+(parseInt(i)+1)+'</span><span class="ph_main2" title="'+top_unit[i]["DOMAIN_NAME"]+'">'+top_unit[i]["DOMAIN_NAME"]+'</span><span class="ph_num">'+top_unit[i]["SCORE"]+'</span><div class="clear"></div></li>';
                                    }
                                    var top_score = data['top_score']
                                    
                                    var score_str = ''
                                    for(var j in top_score){
                                        score_str+= '<li><span class="num num'+(parseInt(j)+1)+'">'+(parseInt(j)+1)+'</span><span class="ph_name">'+top_score[j]["USER_NAME"]+'</span><span class="ph_main" title="'+top_score[j]["DOMAIN_NAME"]+'">'+top_score[j]["DOMAIN_NAME"]+'</span><span class="ph_num">'+top_score[j]["SCORE"]+'</span><div class="clear"></div></li>';
                                    }
                          $("#unit_str").html(unit_str)
                          $("#score_str").html(score_str)
                                }
                            })
                        </script>
                        <script type="text/javascript">
                          $.ajax({
                              url: "/shop/api/get_center_data?site_id=10",
                              type: 'get',
                              cache:false,
                              success: function(data){
                              var data = eval('('+data+')')
                              var list_con = "";
                              for(var i in data){          
                                      if (data[i]["title"] != undefined){
			  list_con += '<li>'+data[i]["title"]+'<span>'+data[i]["count"]+'</span>'+data[i]["unit"]+'</li>';
		  }
                                  }
                              $("#law_main_url").html(list_con)
                              }
                          })
                          $(document).ready(function () {
                          var d=new Date();
                            $("#captcha").attr("src","http://xf.faxuan.net/service/gc.html?tt="+d);
                       
                          });
