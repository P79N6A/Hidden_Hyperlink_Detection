  <script type="text/javascript">
    $(function () {


        RefreshVerifyCode();

        $("#changeVerifyCode").click(function () {
            RefreshVerifyCode();
        });

        var $div_li = $(".TabMenu ul li");
        $div_li.click(function () {
            $(this).addClass("selected")
                .siblings().removeClass("selected");
            var index = $div_li.index(this);
            $(".TabBox > div")
                .eq(index).show()
                .siblings().hide();
        });

        //登录
        $(".login").click(function () {

            if (objLogin.form()) {
                $("#formLogin").ajaxSubmit(function (data) {
                    if (data != undefined && data.url != null && data.url != "") {

                        if (data.message == 'success' && $("#email").val().toLowerCase().indexOf("@yahoo") != -1) {
                            $("#floaterWins").hide();
                            heibgYahoo();
                            $("#editEmail").show();
                            return;
                        }

                        window.location.href = data.url;
                    }
                    else {
                        alert(data.message);
                    }
                });
                return false;
            }
        });
        var objLogin = $("#formLogin").validate({
            rules: {
                email: { required: true, email: true },
                password: { required: true }
            },
            messages: {
                email: {
                    required: "<font style='color:red'>*请输入您的邮件地址</font>",
                    email: "<font style='color:red'>*邮件格式不正确</font>"
                },
                password: {
                    required: "<font style='color:red'>*请输入密码</font>"
                }
            }
        });


        $("#password").keypress(function (event) {
            if (event.which == 13) {
                $(".login_ok").trigger("click");
            }
        });

        //注册
        $(".reg").click(function () {

            // 排除yahoo邮箱
            if ($("#formRegister [name='reg_email']:eq(0)").val().toLowerCase().indexOf("@yahoo") != -1) {
                alert('不支持yahoo邮箱，请更换其他类型邮箱！');
                return false;
            }

            if (objRegister.form()) {
                $("#formRegister").ajaxSubmit(function (data) {
                    if (data != undefined && data.url != null && data.url != "") {
                        window.location.href = data.url;
                    }
                    else {
                        alert(data.message);
                    }
                });
                return false;
            }
        });

        var objRegister = $("#formRegister").validate({
            rules: {
                reg_nick: { required: true, userName: true, userNameRangelength: true, remote: "/Account/NickNameIsExist" },
                reg_email: { required: true, email: true, remote: "/Account/EmailIsExist" },
                reg_password: { required: true, minlength: 6, maxlength: 12 },
                reg_confrim_password: { required: true, minlength: 6, maxlength: 12, equalTo: '#reg_password' },
                verifyCode: { required: true },
                agree: "required"
            },
            messages: {
                reg_nick: {
                    required: "<font style='color:red'>*昵称不能为空</font>",
                    remote: "<font style='color:red'>*昵称不能重复</font>",
                    userName: "<font style='color:red'>用户名只能包括中文字、英文字母、数字和下划线</font>",
                    userNameRangelength: "<font style='color:red'>*4-16个字符</font>"
                },
                reg_email: {
                    required: "<font style='color:red'>*请输入您的邮件地址</font>",
                    email: "<font style='color:red'>*邮件格式不正确</font>",
                    remote: "<font style='color:red'>*邮件地址已经存在</font>"
                },
                reg_password: {
                    required: "<font style='color:red'>*请输入密码</font>",
                    minlength: "<font style='color:red'>*至少{0}个字符，区分大小写</font>",
                    maxlength: "<font style='color:red'>*密码长度最多为{0}字符</font>"
                },
                reg_confrim_password: {
                    required: "<font style='color:red'>*请再次输入密码</font>",
                    minlength: "<font style='color:red'>*至少{0}个字符，区分大小写</font>",
                    equalTo: "<font style='color:red'>*重复密码与密码不相等</font>",
                    maxlength: "<font style='color:red'>*密码长度最多为{0}字符</font>"
                },
                verifyCode: {
                    required: "<font style='color:red'>*请输入验证回答</font>"
                },
                agree: "<font style='color:red'>*</font>"
            },
            event: "blur"

        });

        jQuery.validator.addMethod("userName", function (value, element) {
            return this.optional(element) || /^[\u0391-\uFFE5\w]+$/.test(value);
        }, "format error");

        jQuery.validator.addMethod("userNameRangelength", function (value, element) {
            var length = 0;
            var schar;
            for (var i = 0; schar = value.charAt(i); i++) {
                length += (schar.match(/[^\x00-\xff]/) != null ? 2 : 1);
            }
            return length >= 4 && length <= 16;
        }, "format error");
    });

    function RefreshVerifyCode() {
        $.post("/Public/GenerateCheckCode2", { rnd: Math.random() }, function (datas) {
            $("#lblVerifyCode").text(datas);
        });
    }
</script> 
