  <script type="text/javascript">
    $(function () {

        // 确定事件
        $("#snsBind_5").click(function () {
            var email = $("#email_5").val();
            var pwd = $("#pwd_5").val();
            var pwd2 = $("#pwd_again_5").val();


            $.post("/Account/SnsBindInfoEdit", { email: email, pwd: pwd, pwd2: pwd2,snsType:1 }, function (data) {

                if (!data) {
                    alert("绑定成功！");
                    location.href = "/Customer/Edit";
                } else {
                    alert(data);
                }
            });
            return false;
        });
    })
