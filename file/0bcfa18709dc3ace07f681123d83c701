  <script type="text/javascript">

    //背景方法，如调用方法带有任意参数，则为删除，反之添加。
    function heibgYahoo(remove) {
        var docu = document;
        if (remove && docu.getElementById("heidiv")) {
            docu.body.removeChild(docu.getElementById("heidiv"));
        } else if (!docu.getElementById("heidiv")) {
            if (remove) return;
            var heidiv = docu.createElement("div");
            heidiv.id = "heidiv";
            (function ($) {
                docu.body.style.position = "relative";
                $.height = jQuery(document).height() + "px";
                $.width = "100%";
                $.position = "absolute";
                $.left = "0";
                $.top = "0";
                $.background = "#000";
                $.opacity = "0.6";
                $.filter = "alpha(opacity=60)";
                $.zIndex = "10";
            })(heidiv.style);
            docu.body.insertBefore(heidiv, docu.body.children[0]);
        }
    }

    $(function () {

        // 排除登陆/注册页
        var tmpUrl = location.href.toLowerCase();
        if (tmpUrl.indexOf('/register') != -1 || tmpUrl.indexOf('/login') != -1) {
            return false;
        }

        // yahoo edit

        // 添加到body
        $("#editEmail").appendTo("body");

        // yahoo邮箱修改
        var yahooError = "不能是yahoo邮箱!";
        function updateEmail() {

            if ($("#email").val().toLowerCase().indexOf("@yahoo") != -1) {

                // 弹输入框，绑定事件emailClick(targetEmail)
                heibgYahoo();
                $("#editEmail").show();

            } else {
                location.href = "/home";
            }
        }

        // email 点击确认
        function emailClick(targetEmail) {

            if (!targetEmail) {
                alert("内容不能为空！");
                return false;
            }
            if (targetEmail.toLowerCase().indexOf("@yahoo") != -1) {
                alert(yahooError);
                return false;
            }

            var myreg = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
            if (!myreg.test(targetEmail)) {
                alert('请输入有效的Email！');
                return false;
            }


            $.get("/Account/UpdateEmail", { email: targetEmail }, function (statu) {
                if (statu == '0') {
                    alert('修改失败！');
                } else {
                    alert('修改成功！');
                    window.location.reload();
                }
            });
        }

        $("#yahooOk").click(function () {
            emailClick($("#newEmail").val());
        });
        $("#yahooCancel,#emailClose").click(function () {
            window.location.reload();
            heibgYahoo("1");
            $("#editEmail").hide();
        });

        // end yahoo



    })
</script> 
