   <script type="text/javascript">
    $(function () {

        $(".more").click(function () {
            $(this).closest("td").prev().find("ul").eq(0).hide();
            $(this).closest("td").prev().find(".More_overflowY").show();
            $(this).hide();
            $(this).next().show();
        });
        $(".hideMore").click(function () {
            $(this).closest("td").prev().find("ul").eq(0).show();
            $(this).closest("td").prev().find(".More_overflowY").hide();
            $(this).hide();
            $(this).prev().show();
        });

        //添加/取消关注
        $(".AttentionPr,.cancel_Attention").live("click", function () {
            if (isInBlacklist == "1") {
                alert("对不起，您的账号已被停用，如有疑问请联系service@i2ya.com");
                return false;
            }
            var devObjectDetailID = $(this).attr("id"); //产品ID
            var attentionType = "Product";
            var CurrentObj = $(this);

            var currentCss = $(this).attr("class");
            if (currentCss == "AttentionPr") {
                $.post("/Public/AddOrCancleAttention", { flag: "add", attentionType: attentionType, devObjectDetailID: devObjectDetailID, rad: Math.random() }, function (data) {
                    if (data != undefined && data.url == "") {
                        $(CurrentObj).removeClass("AttentionPr").addClass("cancel_Attention");
                    }
                    else {
                        showFloater('#floaterWins .floaterLR');
                    }
                });
            }
            else {
                new Confirm({ msg: "确认取消关注？", left: $(this).offset().left, top: $(this).offset().top }, function (r) {
                    $.post("/Public/AddOrCancleAttention", { flag: "", attentionType: attentionType, devObjectDetailID: devObjectDetailID, rad: Math.random() }, function (data) {
                        if (data != undefined && data.url == "") {
                            $(CurrentObj).removeClass("cancel_Attention").addClass("AttentionPr");
                        }
                        else {
                            showFloater('#floaterWins .floaterLR');
                        }

                    });
                });
            }
        });

        $(".prodPicPath").live("mouseenter", function () {
            var id = $(this).attr("productID");
            var attentionObj = $(this).closest("li").find("#attention" + id).find("span");
            var currentCss = $(this).closest("li").find("#attention" + id).find("span").attr("class");

            var obj = ProductPopup({ productID: id, left: $(this).offset().left, top: $(this).offset().top + 100, lazyObject: $(this) }, function () {
                if (currentCss == "AttentionPr") {
                    $(attentionObj).removeClass("AttentionPr").addClass("cancel_Attention");
                }
                else {
                    $(attentionObj).removeClass("cancel_Attention").addClass("AttentionPr");
                }
            }).show();
        });

        $(".prodLink").live("mouseenter", function () {
            var id = $(this).attr("productID");
            var attentionObj = $(this).closest("li").find("#attention" + id).find("span");
            var currentCss = $(this).closest("li").find("#attention" + id).find("span").attr("class");

            var obj = ProductPopup({ productID: id, left: $(this).offset().left, top: $(this).offset().top + 10, lazyObject: $(this) }, function () {
                if (currentCss == "AttentionPr") {
                    $(attentionObj).removeClass("AttentionPr").addClass("cancel_Attention");
                }
                else {
                    $(attentionObj).removeClass("cancel_Attention").addClass("AttentionPr");
                }
            }).show();
        });

        $("#brandName").autocomplete(
            {
                source: function (request, response) {
                    $.ajax({
                        url: "/search/SerachBrandByKeyWord",
                        dataType: "json",
                        data: {
                            top: 8,
                            k: function () { return $("#brandName").val(); }
                        },
                        success: function (data) {
                            var d = data;
                            response($.map(data, function (item) {
                                return {
                                    label: item.ChineseName,
                                    valueId: item.BrandID
                                }
                            }));
                        }
                    });
                },
                select: function (event, ui) {
                    var url = $(this).attr("url") + "&brandID=" + ui.item.valueId;
                    window.location.href = url;
                },
                max: 5, width: "200",
                minChars: 1
            });

        //排序
        $("#ProductSort_Current").hover(function () {
            $(".ProductSort_More").show();
        });
        $('#jsddm li').bind('mouseover', jsddm_open);
        $('#ProductSort_Current').bind('mouseout', jsddm_timer);


        //添加收藏
        $(".addAttention").click(function () {
            if (isInBlacklist == "1") {
                alert("对不起，您的账号已被停用，如有疑问请联系service@i2ya.com");
                return false;
            }
            var devObjectDetailID = $(this).attr("prodID"); //产品ID
            var attentionType = "Product";
            var flag = $(this).attr("flag");
            if (flag == "") flag = "add";
            $this = $(this);
            $.post("/Public/AddOrCancleFavorites", { flag: 'add', attentionType: attentionType, devObjectDetailID: devObjectDetailID, rad: Math.random() }
        , function (data) {
            if (data != undefined && data.url == "") {
                if (flag == "add") {
                    $this.html("取消收藏(" + data.message + ")");
                    $this.attr("flag", "1");
                }
                else {
                    $this.html("收藏(" + data.message + ")");
                    $this.attr("flag", "");
                }
            }
            else {
                showFloater('#floaterWins .floaterLR');
            }
        });
        });

        // 显示品牌名称
        $(".share").each(function () {
            var $this = $(this);
            $.post("/Brand/GetBrandName", { productId: parseInt($(this).attr("proid")) }, function (data) {
                $this.attr("brandName", data);
            });
        });

        //分享到
        $(".share").live("mouseenter", function () {
            if (isInBlacklist == "1") {
                alert("对不起，您的账号已被停用，如有疑问请联系service@i2ya.com");
                return false;
            }
            var $this = $(this);
            var ProductID = $this.attr("proid");
            var ProName = $this.attr("proname");
            var proDesp = $this.attr("productdesp");
            var brandName = $this.attr("brandname");


            var rating = ""; //$this.attr("rating");
            var nickName = ""; // $this.attr("nickName");

            var picUrl = $this.closest("li").find(".ProductList_left a img:eq(0)").attr("src");

            BindJiaThis(ProductID, ProName, proDesp, rating, nickName, brandName, picUrl);
        });

        //分享
        function BindJiaThis(ProductID, ProName, proDesp, rating, nickName, brandName, picUrl) {
            var Url = "http://www.i2ya.com/Product/" + ProductID;
            jiathis_config = {
                siteNum: 5,
                sm: "tsina,tqq,kaixin001,renren,douban",
                url: Url,
                summary: "",
                title: "#二丫点评#亲们，我正在@二丫网美妆分享社区 分享" + brandName + "/" + ProName + "哦，想要分享给大家！" + proDesp + "...",
                pic: picUrl,
                hideMore: true
            };
        }


        // 关键词
        var idList = "";
        $(".Brand_ProductList li .ProductList_left a").each(function () {
            idList += $(this).attr("productid") + ",";
        });
        if (idList != "") {
            idList = idList.substr(0, idList.length - 1);
        }
        $.getJSON("/Public/GetKewWordList", { productId: idList, rnd: Math.random() }, function (data) {
            if (data) {

                $(".Brand_ProductList li .ProductList_right .ProductList_Keyword").empty().each(function (i) {

                    var sb = "";
                    $(data[i].split(",")).each(function (idx, v) {
                        sb += String.format("<a href='/Search/Index?keyWord={0}' target='_blank'>{1}</a> ", encodeURI(v), v);
                    });

                    $(this).html("关键词： " + sb);

                });
            }

        });

        //
    });

    var timeout = 100;
    var closetimer = 0;
    var ddmenuitem = 0;

    function jsddm_open() {
        jsddm_canceltimer();
        jsddm_close();
        ddmenuitem = $(this).find('ul').eq(0).css('visibility', 'visible');
    }

    function jsddm_close()
    { if (ddmenuitem) ddmenuitem.css('visibility', 'hidden'); }

    function jsddm_timer()
    { closetimer = window.setTimeout(jsddm_close, timeout); }

    function jsddm_canceltimer() {
        if (closetimer) {
            window.clearTimeout(closetimer);
            closetimer = null;
        }
    }
    document.onclick = jsddm_close;

</script> 
