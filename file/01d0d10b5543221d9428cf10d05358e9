function load_down_addr1(file_id){
$('#addr_list').html("<img src=\"images/ajax_loading.gif\" align=\"absmiddle\" border=\"0\" /><span class=\"f14\">加载中...</span>");
	$.ajax({
		type : 'post',
		url : 'ajax.php',
