function save_as(file_id){
	$.ajax({
		type : 'get',
		url : 'ajax.php',
		data : 'action=save_as&file_id='+file_id+'&t='+Math.random(),
		dataType : 'text',
		success:function(msg){
			if(msg == 'true'){
				atips('另存文件操作成功。');
			}else if(msg =='ufile'){
				atips('此文件已在您的网盘中，不需另存。');
			}else{
				atips(msg);
			}
		},
		error:function(){
		}

	});
}
