                        <script>
                            function getC(name) {
                                var parts = document.cookie.split(name + "=");
                                if (parts.length == 2) {
                                    return parts.pop().split(";").shift();
                                }
                                else {
                                    return "";
                                }
                            }
                            function goUCenter(module) {
                                document.location.href = "/ucenter/" + getC("username") + "/" + module;
                            }
                        </script>
