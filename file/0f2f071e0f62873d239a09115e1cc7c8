   <script type="text/javascript">
        if (navigator.userAgent.match(/mobile/i)) {
            $(".botbox").hide();
        }
        var pmh = $(window).height();
        if (!window.XMLHttpRequest && document.all) {
            $(window).scroll(function () {
                var srh = $(window).scrollTop();
                $(".botbox").css("top", pmh + srh - 130 + "px");
            })
        }
