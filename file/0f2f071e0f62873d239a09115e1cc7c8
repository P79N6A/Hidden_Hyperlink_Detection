   <script src="/js/jquery.pin.js"></script> 
   <!-- PIN ALL THE THINGS! --> 
   <script>
        //$(".pinned").pin({ containerSelector: ".container", minWidth: 940 });
    </script> 
   <!-- That's all - pretty easy, right? --> 
   <script type="text/javascript">
        //默认16px_start
        var size=16;
        var divBody = document.getElementById("core-info");
        var divBody2 = document.getElementById("idcomment");
        divBody.style.fontsize = size + "PX";
        var divChildBody = divBody.childNodes;
        for (var i = 0; i < divChildBody.length; i++) {
            if (divChildBody[i].nodeType == 1) {
                divChildBody[i].style.fontSize = size + "PX";
            }
        }
        //默认16px_end

        //tags链接
        

        function add_div(id, replyTxt) {
            var o = document.createElement("div");
            o.className = "wc_list_nod clearfix";
            document.getElementById("feedList").insertBefore(o, document.getElementById("feedList").childNodes[3]);
            var strHTML = detail.GetUserComment(id, replyTxt).value;
            o.innerHTML = strHTML;
            //评论次数增加
            
            document.getElementById("list_titleHidden1").value = parseInt(document.getElementById("list_titleHidden1").value) + 1;
            document.getElementById("idlist_title").innerHTML = document.getElementById("list_titleHidden1").value.toString() +"条评论";
        }
        function showCommet(id, NickName) {
            hideCommet();
            document.getElementById("txtReply" + id).style.display = '';
            document.getElementById("txtReplyContent" + id).value = "@" + NickName.replace(/[ ]/g, "") + "：";
            $("#replyerror" + id).hide();
            //将光标移至文档末尾
            var objRCon = document.getElementById("txtReplyContent" + id);
            var len = objRCon.value.length;
            objRCon.focus();
            if (document.selection) {
                var sel = objRCon.createTextRange();
                sel.moveStart('character',len);
                sel.collapse();
                sel.select();
            } else if (typeof objRCon.selectionStart == 'number' && typeof objRCon.selectionEnd == 'number') {
                objRCon.selectionStart = objRCon.selectionEnd = len;
            }
        }
        function hideCommet() {
            $("blockquote[id^='txtReply']").hide();
        }
        function checkLen(obj,i) {

            var maxChars = 150; //最多字符数  

            if (obj.value.length > maxChars) obj.value = obj.value.substring(0, maxChars);

            var curr = maxChars - obj.value.length;

            document.getElementById("count" + i).innerHTML = curr.toString();

        }
        function clicksubmit(id, NickName) {
            var replyTxt = $("#txtReplyContent" + id).val().toString();
            replyTxt = ModifyContent(replyTxt, NickName);

            if (replyTxt == "") {
                $("#replyerror" + id).html("请输入回复内容");
                $("#replyerror" + i).show();
                return;
            }
            var str = '173121173121';
            var result = detail.AddReply(id, replyTxt, str).value;
            var dt = new Date();
            var year = dt.getFullYear();
            var month = dt.getMonth() + 1;
            var day = dt.getDate();
            var hours = dt.getHours();
            var minutes = dt.getMinutes();
            if (month.toString().length < 2) {
                month = "0" + month;
            }
            if (day.toString().length < 2) {
                day = "0" + day;
            }
            if (hours.toString().length < 2) {
                hours = "0" + hours;
            }
            if (minutes.toString().length < 2) {
                minutes = "0" + minutes;
            }
            var replyTime = year + "-" + month + "-" + day + " " + hours + ":" + minutes;
            if (result != "failure") {

                $("#content_txt_" + id).append("<div class='content_txt'><a href='javascript:void(0);' style='color: #ff4f00;'>" + result + ":" + replyTxt + "</a><span class='pub_time'>今天 " + hours + ":" + minutes + "</span></div>");
                $("#txtReplyContent" + id).val("");
                hideCommet();
            }
        }
        $("#txtcontent").focus(function () {
            $("#errorDetail").hide();
        });
        //添加评论
        $("#comment").click(function () {

            if ($("#txtcontent").val() == "") {
                $("#errorDetail").text("请输入评论内容").show();
                return;
            }

            var str = '173121173121';
            var result = detail.AddComment(str, $("#txtcontent").val()).value;
            if (result != "failure") {
                add_div(result, $("#txtcontent").val());
                document.getElementById("txtcontent").value = "";
                //location.reload();
            }
        });


        $("#idcomment").click(function () {
            $("#txtcontent").focus();
        });

        //文章字体设置
        function checkFontSize(size) {
            var divBody = document.getElementById("core-info");
            var divBody2 = document.getElementById("idcomment");
            if (!divBody) {
                return;
            }
            divBody.style.fontsize = size + "PX";
            var divChildBody = divBody.childNodes;
            for (var i = 0; i < divChildBody.length; i++) {
                if (divChildBody[i].nodeType == 1) {
                    divChildBody[i].style.fontSize = size + "PX";
                }
            }
        }

        function ModifyContent(strContent, NickName) {
            NickName = "@" + NickName.replace(/[ ]/g, "") + "：";
            if (strContent.indexOf(NickName) == -1) {
                return strContent;
            }
            else {
                return strContent.substr(strContent.indexOf(NickName) + NickName.length);
            }
        }
    </script> 
