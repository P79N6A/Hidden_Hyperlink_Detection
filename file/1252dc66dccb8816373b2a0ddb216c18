  <script>
        $("#footerClose").click(function () {
            $("#footerbanner").css('display', 'none');
        });

        function is_weixin() {
            var ua = navigator.userAgent.toLowerCase();
            if (ua.match(/MicroMessenger/i) == "micromessenger") {
                return true;
            } else {
                return false;
            }
        }

        var browser = {
            versions: function () {
                var u = navigator.userAgent, app = navigator.appVersion;
                return {
                    trident: u.indexOf('Trident') > -1,
                    presto: u.indexOf('Presto') > -1,
                    webKit: u.indexOf('AppleWebKit') > -1,
                    gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1,
                    mobile: !!u.match(/AppleWebKit.*Mobile.*/),
                    ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/),
                    android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1,
                    iPhone: u.indexOf('iPhone') > -1,
                    iPad: u.indexOf('iPad') > -1,
                    webApp: u.indexOf('Safari') == -1
                }
            }(),
            language: (navigator.browserLanguage || navigator.language).toLowerCase()
        };

        $(".appopen").click(function (e) {
            if (is_weixin()) {
                e.stopPropagation();
                if (browser.versions.ios || browser.versions.iPhone || browser.versions.iPad) {
                    $("#topbanner").css('display', 'block');
                    $("#topbanner").html("<img src=\"http://www.soupu.com/images/bg.png\" style=\"max-width:100%;\" />");
                } else {
                    $("#topbanner").css('display', 'block');
                    $("#topbanner").html("<img src=\"http://www.soupu.com/images/bg2.png\" style=\"max-width:100%;\" />");
                }
            } else {
                location.href = "http://fusion.qq.com/app_download?appid=1105025140&platform=qzone&via=QZ.MOBILEDETAIL.QRCODE&u=262966824";
            }
        });

        $(document).ready(function () {
            if (!is_weixin()) {
                //$("#topbanner").css('display', 'none');
                //$("#topbanner2").css('display', 'none');
                //$("#footerbanner").css('display', 'none');
                if (!navigator.userAgent.match(/(iPhone|iPod|iPad|Android|ios|Windows Phone)/i)) {
                } else {
                    //location.href = "http://fusion.qq.com/app_download?appid=1105025140&platform=qzone&via=QZ.MOBILEDETAIL.QRCODE&u=262966824";
                }
            }
        });

        $(document).click(function () {
            if ($("#topbanner").css("display") == "block") {
                $("#topbanner").css('display', 'none');
            }
        });
    </script> 
