<script>
/*游戏介绍*/
$(function(){
    var str = $(".game-intro").text()
    if(str.indexOf("...")>0){
        $(".game-intro .description").click(function(){
            $(".game-intro").css({'display':'none'})
            $("#desc").css({'display':'block'})
            handlePosition();
        })
    }
    $("#description").click(function(){
        $(".game-intro").css({'display':'block'})
        $("#desc").css({'display':'none'})
        handlePosition();
    })
});

function handlePosition(){
    var posInfo = $("#copylinkli").position();
    $("#copybtnParent").css({left:posInfo.left+370+'px',top:posInfo.top+6+'px'});
};
handlePosition();

