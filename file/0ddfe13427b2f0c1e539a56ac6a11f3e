<script>
$(function(){
    //小编推荐
    var wxTime = setInterval("img_left('qy-scroll-cont')",5000);
    $(".qy-scroll").hover(function(){
        clearInterval(wxTime)
    },function(){
        wxTime=setInterval('img_left(\'qy-scroll-cont\')',5000)
    })
    $("#qy-left-btn").click(function(){img_left('qy-scroll-cont')})
    $("#qy-right-btn").click(function(){img_right('qy-scroll-cont')})

    //桥页右侧中部的广告高度调整
    var leftHeight = $("#wrapLeft").css("height");
    var newHeight = parseInt(leftHeight)-570;
    var rightNewHeight = newHeight>0 ? parseInt(newHeight/27)*27 : 0;
    $("#wrapRight").css("height",rightNewHeight+"px");
    var vaec = $("#wrapRight").css("height");
})

function img_left(obj){
    $("#"+obj+" ul:first").stop().animate({marginLeft:"-696px"},'slow',function(){$(this).detach().appendTo("#"+obj).css("margin-left","0px")})
}
function img_right(obj){
    $("#"+obj+" ul:last").stop().css("margin-left","-696px").detach().prependTo("#"+obj).animate({marginLeft:"0px"})
}

    /*排行切换*/
function ShowTag2(id){
    if(id=="zph"){
    document.getElementById("zph").className = "rt-a-d";
    document.getElementById("yph").className = "rt-b";
    document.getElementById("zphl").style.display = "block";
    document.getElementById("yphl").style.display = "none";
    }else{
    document.getElementById("zph").className = "rt-a";
    document.getElementById("yph").className = "rt-b-d";
    document.getElementById("zphl").style.display = "none";
    document.getElementById("yphl").style.display = "block";
    }
}

