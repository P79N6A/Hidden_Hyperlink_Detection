 var url_spos="http://down.51cto.com/service/index_down_ajax.php";
$("#smbtn_yj").bind('click',function(){ 
 $("#cl_red").hide(); 
 $("#c1_red").hide(); 
  if($("#area").val()== '' ){
    $("#cl_red").show();
	$("#cl_red").text("意见或建议不能为空");	
	return false;
  }  
  if(strlenlig($("#area").val()) <4 ){
	$("#cl_red").text("意见或建议不能少于2个汉字");
	$("#cl_red").show();
	return false;
  }
  if(strlenlig($("#area").val()) >600){
	$("#cl_red").text("意见或建议不能大于300个汉字");
	$("#cl_red").show();
	return false;
  }
   if($("#ord").val()== '' ){
    $("#c1_red").show();
	$("#c1_red").text("联系方式不能为空");	
	return false;
  }
  var emailformat = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+(\.|-)+[-_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;  
  if($("#ord").val().indexOf("@")==-1){
     var patten = /^[1-9][0-9]{4,15}$/;
	 if(!patten.test($("#ord").val())){
		 $("#c1_red").show();
		 $("#c1_red").text("请输入正确的QQ格式");return false;		
	  }
  }else{
	  if(!emailformat.test($("#ord").val())){
		 $("#c1_red").show();
		 $("#c1_red").text("请输入正确的邮箱格式");return false;		
	  } 
  } 
  jQuery.ajax({
		type: "POST",		
		data: {'content':$("#area").val(),'contact':$("#ord").val()},
		url: url_spos+"?do=feedback&time="+Math.random(),		
		dataType:'json',
		success: function(data){ 
			 if(data.message==1){				
                $(".popyjdatil").hide();
                $(".popsuctext").show();       				
			 }else{
				 $(".fail_tj").html(data.message);								
			}		 
		 }
	}); 
})
function strlenlig(str){  
  var len = 0;
    for (var i=0; i<str.length; i++) { 
     var c = str.charCodeAt(i); 
    //单字节加1 
     if ((c >= 0x0001 && c <= 0x007e) || (0xff60<=c && c<=0xff9f)) { 
       len++; 
     } 
     else { 
      len+=2; 
     } 
    } 
    return len;
} 
