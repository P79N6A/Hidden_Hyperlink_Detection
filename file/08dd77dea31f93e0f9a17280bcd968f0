<script src="/app1/templates/js/hammer.js"></script>
<script src="/app1/templates/js/index.js"></script>
<script src="/app1/templates/js/free_trail.js"></script>
<script src="/app1/templates/js/productDetail.js"></script>
<script>
    $(function () {
        function showFreeTrialDialog(fromDingDing) {
            if ($(window).width() < 768 && navigator.userAgent.indexOf("Safari") > 0) {
                window.scrollTo(0, 0);
            }

            if (fromDingDing) {
                $('#normalTitle').hide();
                $('#dingdingTitle').show();
                $('#dingdingQuestion').show();
                $('#normalPrompt').hide();
                $('#dingdingPrompt').show();
            } else {
                $('#normalTitle').show();
                $('#dingdingTitle').hide();
                $('#dingdingQuestion').hide();
                $('#normalPrompt').show();
                $('#dingdingPrompt').hide();
            }
            
            $('#free').modal('show');
        }

        $('#free_button').on('click', function () {
            showFreeTrialDialog();
            _czc.push(["_trackEvent","首页","产品购买","尊享版免费试用"]);
        });

        window.setTimeout(showFreeTrialDialog, 3000);

        var urlLink = window.location.href;
        var urlQuery = urlLink.indexOf('?') > 0 ? urlLink.substring(urlLink.indexOf('?') + 1) : null;

        if (urlQuery && urlQuery == 'fromDingDing') {
            showFreeTrialDialog(1);
        }

        if ($(window).width() < 768 && navigator.userAgent.indexOf("Safari") > 0) {
            $('#free').data('location',$('#free_button').parents('.price_main').offset().top+400).on('hidden.bs.modal', function (e) {
                window.scrollTo(0, $('#free').data('location'));
            });
        }

        if ($(window).width() >= 768) {
            document.getElementById("carousel-example-generic01").setAttribute("data-ride", "carousel");
            document.getElementById("carousel-example-generic03").setAttribute("data-ride", "carousel");
            $('.Q-main').on('mouseover', function () {
                $(this).find('.Q-last').each(function(){
                    $(this).height($(this).parents('.Q-main').height());
                });
                // TODO!!!
                $(this).find('.Q-left,.Q-right').each(function(){
                    var animation = $(this).attr('data-animation');
                    var finalStyle = $(this).attr('data-final-style');
                    var style = {
                            'animation': animation,
                            '-moz-animation': animation,/* Firefox */
                            '-webkit-animation': animation,   /* Safari 和 Chrome */
                            '-o-animation': animation
                        };
                    $.extend(style,getFinalStyle(finalStyle));
                    $(this).height($(this).parents('.Q-main').height());
                    $(this).css(style).find('.Q-infos').css({
                        'animation': "showinfos 0.6s",
                        '-moz-animation': "showinfos 0.6s",/* Firefox */
                        '-webkit-animation': "showinfos 0.6s",   /* Safari 和 Chrome */
                        '-o-animation': "showinfos 0.6s"
                    });
                });
                $(this).find('.Q-info-line,.Q-img-line').each(function(){
                    var animation = $(this).attr('data-animation');
                    var finalStyle = $(this).attr('data-final-style');
                    var style = {
                            'animation': animation,
                            '-moz-animation': animation,/* Firefox */
                            '-webkit-animation': animation,   /* Safari 和 Chrome */
                            '-o-animation': animation
                        };
                    $.extend(style,getFinalStyle(finalStyle));
                    $(this).css(style);
                });
                $(this).find('.Q-left-bottom,.Q-right-top').each(function(){
                    var animation = $(this).attr('data-animation');
                    var finalStyle = $(this).attr('data-final-style');
                    var style = {
                            'animation': animation,
                            '-moz-animation': animation,/* Firefox */
                            '-webkit-animation': animation,   /* Safari 和 Chrome */
                            '-o-animation': animation
                        };
                    $(this).height($(this).parents('.Q-main').height());
                    $.extend(style,getFinalStyle(finalStyle));
                    $(this).css(style);
                });
                $(this).find('.Q-infos').css({
                    'animation': "showinfos 0.6s",
                    '-moz-animation': "showinfos 0.6s",/* Firefox */
                    '-webkit-animation': "showinfos 0.6s",   /* Safari 和 Chrome */
                    '-o-animation': "showinfos 0.6s"
                }); 
            });
            $('.Q-details').on('mouseout', function () {
                $(this).children().hide();
            }).on('mouseenter', function () {
                $(this).children().show();
                _czc.push(["_trackEvent","首页","功能介绍详细",$(this).parents('div').children('h4:eq(0)').text()]);
            });
        } else {
            $('.Q-main').remove();
            $("#carousel-example-generic03 .carousel-control").remove();
            $("#carousel-example-generic01 .carousel-control").remove();
        }
        $('#Modal_app').on('shown.bs.modal', function(){
            _czc.push(["_trackEvent","首页","安装手机App"]);
        });
    });
    var getFinalStyle = function(finalStyle){
        var styleFinal = {}
        if(finalStyle){
            if(finalStyle.split(',').length > 1){
                for(var t=0;t<finalStyle.split(',').length;t++){
                    styleFinal[finalStyle[t].split(':')[0]] = finalStyle[t].split(':')[1];
                }
            }else{
                styleFinal[finalStyle.split(':')[0]] = finalStyle.split(':')[1];
            }
        }
        return styleFinal;
    }
    var mass_lg01 = "";
    var mass_sm01 = "";
    var mass_md01 = "";
    var mass_urls = "";
    $.ajax({
        url: "/multievent/banner/get_path.json",
        type: "post",
        data: {
            "target": "web"
        },
        dataType: "json",
        success: function (result) {

            if (result.errorInfo !== undefined && JSON.stringify(result.errorInfo) != '{}') {
                alert(result.errorInfo);
            } else {
                mass_lg01 = result.LONG;
                mass_md01 = result.WEB;
                mass_sm01 = result.MOB;
                mass_urls = result.URL;
                myslider();
            }
        },
        error: function (error) {
            //alert("获取信息失败，请刷新后重试");
        }
    });

    var getPathAndClass = function(url){
        var callback = {
            url:'javascript:;',
            more_class:'no-printer'
        }
        if(url && url.length > 0){
            callback.url = url;
            callback.more_class="printer";
        }
        return callback;
    }

    function myslider() {
        //头部轮播
        var slider01 = document.getElementById("carousel-example-generic");
        var slider01_indicators = slider01.querySelector(".carousel-indicators");
        var slider01_inner = slider01.querySelector(".carousel-inner");
        var slider01_in01 = "";
        var slider01_in02 = "";


        var img = document.querySelectorAll(".slider_img");
        var slider03 = document.getElementById("slider03");
        var items = slider03.querySelectorAll(".item");
        var foot_imgs = document.getElementById("foot_zf").querySelectorAll(".item");
        for (var i = 0; i < 10; i++) {
            if ($(window).width() >= 1920) {
                if (i < mass_lg01.length) {
                    var urlAndClass = getPathAndClass(mass_urls[i]);
                    slider01_in02 += ' <li data-target="#carousel-example-generic" data-slide-to="' + i + '"></li>';
                    slider01_in01 += '<div class="item"><a class="'+urlAndClass.more_class+'" href="javascript:;" onclick="cnzz4href('+"'"+urlAndClass.url+"','首页','banner点击','第"+(i+1)+"图'"+');"><img class="img-responsive" src="' + mass_lg01[i] + '" alt=""></a><div class="carousel-caption"></div></div>';
                }
                if (i < 4) {
                    $('.jieshao_bg').each(function (i) {
                        $(this).attr('src', '/app1/templates/images/bg/bg_0' + (i + 1) + '.jpg');
                    });
                    foot_imgs[i].innerHTML += '<img class="img-responsive" src="/app1/templates/images/bg/foot_zf' + (i + 1) + '.jpg"/>';
                }
            } else if ($(window).width() >= 768) {

                if (i < mass_md01.length) {
                    var urlAndClass = getPathAndClass(mass_urls[i]);
                    slider01_in02 += ' <li data-target="#carousel-example-generic" data-slide-to="' + i + '"></li>';
                    slider01_in01 += '<div class="item"><a class="'+urlAndClass.more_class+'" href="javascript:;" onclick="cnzz4href('+"'"+urlAndClass.url+"','首页','banner点击','第"+(i+1)+"图'"+');"><img class="img-responsive" src="' + mass_md01[i] + '" alt=""></a><div class="carousel-caption"></div></div>';
                }

                if (i < 4) {
                    $('.jieshao_bg').each(function (i) {
                        $(this).attr('src', '/app1/templates/images/bg/bg_0' + (i + 1) + '.jpg');
                    });
                    foot_imgs[i].innerHTML += '<img class="img-responsive" src="/app1/templates/images/bg/foot_zf' + (i + 1) + '.jpg"/>';
                }
            } else {

                if(i<mass_sm01.length){
                    var urlAndClass = getPathAndClass(mass_urls[i]);
                    slider01_in02+=' <li data-target="#carousel-example-generic" data-slide-to="'+i+'"></li>';
                    slider01_in01+='<div class="item"><a class="'+urlAndClass.more_class+'" href="javascript:;" onclick="cnzz4href('+"'"+urlAndClass.url+"','首页','banner点击','第"+(i+1)+"图'"+');"><img class="img-responsive" src="' + mass_sm01[i] + '" alt=""></a><div class="carousel-caption"></div></div>';
                }
                if (i < 4) {
                    foot_imgs[i].innerHTML += '<img class="img-responsive" src="/app1/templates/images/bg/smfoot_zf' + (i + 1) + '.png"/>';
                }
                if (i < 6) {
                    items[i].innerHTML += '<img class="img-responsive" src="/app1/templates/images/smbg/smbg_0' + (i + 1) + '.jpg"/>';
                }
            }

        }

        slider01_inner.innerHTML = slider01_in01;
        slider01_inner.querySelectorAll(".item")[0].setAttribute("class", "item active");
        slider01_indicators.innerHTML = slider01_in02;
        slider01_indicators.firstElementChild.setAttribute("class", "active");
        $('[data-hreflink]').on('click', function () {
            var url = $(this).attr('data-hreflink');
            if (url) {
                if(url)
                window.location.href = $(this).attr('data-hreflink');
            }
        });

    }

</script>


<div style="border-top:1px solid #ddd;padding-bottom: 50px;"></div>


<!-- Footer Begin -->
<div>
