        function AddShoppingCart() {
			
            var getUrl = "ShoppingCart.axd?methodname=ADDGOODS2CART&servicetype=1&offerid=DB047-20170324-70646606";
            $.ajax({
                url: getUrl,
                type: 'POST',
                error: function () { },
                success: function (data) { CallBack_ShoppingCart(data); }
            });
        }

        function CallBack_ShoppingCart(data) {
            try {
                if (data != null && data != "") {
                    var result = data.split("|");
                    if (result[0] == "0") {
                        Cart_Succeed_Show(result[2]);
                    } else if (result[0] == "-1") {
                        Cart_Exists_Show();
                    } else if (result[0] == "-2") {
                        Cart_UpperLimit_Show(result[2]);
                    }
                    else if (result[0] == "2" || result[0] == "4") {
                        // 未登录这里如何处理是好啊
                        //__doPostBack('Detail09v1$btnAddShoppingCart', '')
                        //$.LAYER.close();
                        window.location.href = "https://passport.5173.com/?returnUrl=http%3a%2f%2fgameid.5173.com%2fBizOffer%2fViewDetailV2.aspx%3fId%3dDB047-20170324-70646606%26IsCart%3d1";
                        return;
                    }
                    else {
                        alert("很抱歉，系统繁忙，请稍后再试…");
                    }
    } else {
        alert("很抱歉，系统繁忙，请稍后再试…");
    }
} catch (e) {
    alert("很抱歉，系统繁忙，请稍后再试…");
}
    $("lblAddGoods2CartBackReslut").text(data);
}

function Cart_Succeed_Show(slimitDays) {
    __utmTrackEvent(encodeURIComponent("购物车"), encodeURIComponent("购物车加入成功"), encodeURIComponent(GameName + "-帐号"));
    // urchin code run begin
    Cart_Urchin_Run_ShowLayer("购物车加入成功", "帐号");
    $("#p_cart_succeed_info").html("购物车中的帐号类商品将为您保存" + slimitDays + "天。");
    jQuery.LAYER.show({ id: 'layer_cart_succeed' });
}
function Cart_Exists_Show() {
    __utmTrackEvent(encodeURIComponent("购物车"), encodeURIComponent("购物车重复添加"), encodeURIComponent(GameName + "-帐号"));
    Cart_Urchin_Run_ShowLayer("购物车重复添加", "帐号");
    jQuery.LAYER.show({ id: 'layer_cart_exists' });
}
function Cart_UpperLimit_Show(limitCount) {
    __utmTrackEvent(encodeURIComponent("购物车"), encodeURIComponent("购物车已满"), encodeURIComponent(GameName + "-帐号"));
    Cart_Urchin_Run_ShowLayer("购物车已满", "帐号");
    $("#span_cart_upperlimit_limitcount").html(limitCount);
    jQuery.LAYER.show({ id: 'layer_cart_upperlimit' });
}

function Cart_Urchin_Run_ShowLayer(addType, category) {
    try {
        // 类目
        var runtype = "%e8%b4%ad%e7%89%a9%e8%bd%a6";
        var runcategory = encodeURIComponent(addType + "-" + category).toLocaleLowerCase();
        var runname = encodeURIComponent("").toLocaleLowerCase();
        __utmTrackEvent(runtype, runcategory, runname);
    } catch (e) { }
}
function Cart_Urchin_Run_CloseLayer(typeid) {
    try {
        switch (typeid) {
            case 1: // the goods add succeed layer click '我知道了'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%8a%a0%e5%85%a5%e6%88%90%e5%8a%9f', '%e5%86%8d%e9%80%9b%e9%80%9b');
                return;
            case 2: // the goods add succeed layer click '查看购物车'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%8a%a0%e5%85%a5%e6%88%90%e5%8a%9f', '%e6%9f%a5%e7%9c%8b%e8%b4%ad%e7%89%a9%e8%bd%a6');
                return;
            case 3: // the goods add succeed layer click colose button
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%8a%a0%e5%85%a5%e6%88%90%e5%8a%9f', '%c3%97');
                return;
            case 4: // the cart is full click '我知道了'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%b7%b2%e6%bb%a1', '%e5%86%8d%e9%80%9b%e9%80%9b');
                return;
            case 5: // the cart is full click '查看购物车'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%b7%b2%e6%bb%a1', '%e6%9f%a5%e7%9c%8b%e8%b4%ad%e7%89%a9%e8%bd%a6');
                return;
            case 6: // the cart is full click colose button
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%b7%b2%e6%bb%a1', '%c3%97');
                return;
            case 7: // the goods already exist click '我知道了'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e9%87%8d%e5%a4%8d%e6%b7%bb%e5%8a%a0', '%e5%86%8d%e9%80%9b%e9%80%9b');
                return;
            case 8: // the goods already exist click '查看购物车'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e9%87%8d%e5%a4%8d%e6%b7%bb%e5%8a%a0', '%e6%9f%a5%e7%9c%8b%e8%b4%ad%e7%89%a9%e8%bd%a6');
                return;
            case 9: // the goods already exist click colose button
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e9%87%8d%e5%a4%8d%e6%b7%bb%e5%8a%a0', '%c3%97');
                return;
        }
    } catch (e) { }
}

function Show5914Layer() {
    var hfUrlClientID = jQuery("#hideDeliveryPostUrlId").val();
    var url = jQuery("#" + hfUrlClientID).val();
    window.open(url);
}
//弹出QQ对话框,参数为QQ号码
function OpenQQ(QQ) {
    var o = jQuery("#BusinessConsultingQQ");
    if (QQ.length > 0) {
        o.find("a").attr("href", "http://wpa.qq.com/msgrd?v=3&amp;uin=" + QQ + "&amp;site=qq&amp;menu=yes");
        o.find("img").attr("src", "http://wpa.qq.com/pa?p=2:" + QQ + ":41");
        o.show();
    }
}

function collectionUrl() {
    //收藏的url
    var favUrl = document.location.href;
    //收藏标题(当前商品标题)
    var favTitle = $("#bizofferTitle").text();
    if ($.browser.msie) {
        window.external.addFavorite(favUrl, favTitle);
    }
    else {
        window.sidebar.addPanel(favTitle, favUrl, "");
    }
}
