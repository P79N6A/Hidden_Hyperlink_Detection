    <script type="text/javascript" language="javascript">

    $(function () {

        $(".pub_title_r").find("a").hover(function () {
            var ch = $(this).attr("Id") + "Content";
            $("#" + ch).show();
            $("#" + ch).siblings("div").hide();
            $(this).attr("class", "pub_tab_on");
            $(this).siblings().attr("class", "");
        }, function () {
            $(this).attr("class", "pub_tab_on");
            $(this).siblings("class", "");
        });
    });

    function fastChannel(id1, id2) {
        if ($(id1).is(":visible")) {
            $(id1).hide();
        } else {
            $(id1).show();
        }

        if ($(id2).is(":visible")) {
            $(id2).hide();
        } else {
            $(id2).show();
        }
    }

    $(function () {
        /*气泡*/
        $('.tq-title .tq-li').hover(function (event) {
            if (event.target.nodeName == 'A') {
                $(this).addClass("on");
                $(this).find('.tq-list').css("display", "block");
            }
        }, function () {
            $('.tq-list').css("display", "none");
            $(this).removeClass("on");
        });
        $('.tq-title .tq-li a').hover(function () {
            $(this).parent().addClass("on");
            $(this).parent().find('.tq-list').css("display", "block");
        }, function () {
            //$(this).parent().removeClass("on");
        });

        $(window).scroll(function () {
            if ($(this).scrollTop() > 500) {
                $('.tq-title a.back').css("display", "block");
            } else {
                $('.tq-title a.back').css("display", "none");
            }
        });
        $("div.tq ul.ul-1 li a").click(function () {
            var offset = $($(this).attr("data-href")).offset();
            $("html,body").animate({ scrollTop: offset.top - 40 }, 500);
        });
        $("#a_goto_top").click(function () {
            //var offset = $($(this).attr("data-href")).offset();
            $("html,body").animate({ scrollTop: 40 }, 500);
        });
    });

</script> 
