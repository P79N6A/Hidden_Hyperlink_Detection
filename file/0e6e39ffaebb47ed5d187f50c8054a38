    function get_cc_verification_code(vId) {
        var vcode = "";
        $.ajax({
            type: "Get",
            url: "/API/Validate/GetCcVerificationCode.ashx?rnd=" + Math.random(),
            data: {
                vId: vId
            },
            async: false,
            success: function (rt) {
                vcode = rt;
            }
        });
        return vcode;
    }
    var hits = parseInt($.trim($("#span_hits").html()));

    function LikeTopic(id) {
        hits = hits + 1;
        $("#span_hits").html(hits);
        $("#span_hits2").html(hits);
        $.ajax({
            url: '/Handlers/ClickHandler.ashx',
            type: 'POST',
            dataType: 'json',
            data: {
                Id: id,
                category: "Information"
            },
            success: function (data) {
                hits = parseInt($.trim(data.Count));
            },
            error: function () {
            }
        });
    }

