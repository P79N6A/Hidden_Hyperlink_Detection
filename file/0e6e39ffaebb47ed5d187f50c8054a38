      <script type="text/javascript">
    checkUrl();
    /*头部搜索*/
    $('.seach-select').hover(function () {
        $(this).find("li").show();
    }, function () {
        $(this).find("li:last-child").hide();
    });
    $('#searchBox > li').click(function () {
        var selected = $.trim($(this).find('span').html());
        var currentType = $.trim($(this).siblings().find('span').html());
        $('#searchBox').find("li:first-child > span").html(selected);
        $('#searchBox').find("li:last-child > span").html(currentType);
        $('#searchBox').find("li:first-child").show();
        $('#searchBox').find("li:last-child").hide();

    });
    //function globelQuery(e) {
    //    if (!e)
    //        e = window.event;
    //    if ((e.keyCode || e.which) == 13) {
    //        alert(2);
    //       ;
    //    }
    //} 
    function btnSearch_Click() {
        var keywords = $.trim($("#txtSearchKeywords").val());
        if (keywords == "") {
            alert("请输入您要找的内容！");
            $("txtSearchKeywords").focus();
            return;
        }

        var currentType = $.trim($("#searchBox").find("li:first-child > span").html());

        if (currentType == "课程") {
            window.open("/search/Lessons.aspx?kw=" + escape(keywords) + "&p="+3);
        }
        else if (currentType == "资讯") {
            window.open("/search/Informations.aspx?kw=" + escape(keywords) + "&p="+3);
        }
}

function txtSearchKeywords_KeyPress() {
    if (event.keyCode == 13) {
        event.keyCode = 0;
        btnSearch_Click();
    }
}
    
