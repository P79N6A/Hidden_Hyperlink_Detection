  <script language="javascript">
var menuTopMargin = 0;
var menuSpeed= 10;
var heightLimit = 0;
var closed = 0;
var folded = 1;

function mouseClick() {
	var obj = document.getElementById('top10');
	if (folded == 0) {
		document.getElementById('foldicon').src ="/bbsimg/plus16.gif";
		obj.style.display = 'none';
		obj.innerHTML ='';
		folded = 1;
	} else {
		document.getElementById('foldicon').src ="/bbsimg/minus16.gif";
		obj.style.display = 'block';
		obj.innerHTML ='<iframe width="100%" height="150" frameborder="0" src="top.html"></iframe>';
		folded = 0;
	}
}

function mouseOver() {
	document.getElementById('closeicon').src ="/bbsimg/close161.gif";
}

function mouseOut() {
	document.getElementById('closeicon').src ="/bbsimg/close16.gif";
}
function notifyNewPost(count) {
	if (count > 0) {
		top.document.title='('+count+') 婚姻类移民美国互助论坛之签证区';
		if (closed == 0) {
			document.getElementById('newcount').innerHTML=count+'个';
			document.getElementById('checknew').style.display='block';
		}
	} else {
		top.document.title='婚姻类移民美国互助论坛之签证区';
	}
}

function ajaxFunc() {
	getXMLHTTP("checknew.html?id=1024795");
	window.setTimeout(ajaxFunc, 15000);
}

function hideMenu() {
	document.getElementById('checknew').style.display = 'none';
	closed = 1;
}

function checkMenu() {
	var reTimer = 200;
	var obj = document.getElementById('checknew');
	if(document.body.offsetHeight > heightLimit) {
		var startPoint = parseInt(obj.style.top, 10);
		var endPoint;
		if (window.innerHeight) {
			endPoint = window.pageYOffset
		} else if (document.documentElement && document.documentElement.scrollTop) {
			endPoint = document.documentElement.scrollTop
		} else if (document.body) {
			endPoint = document.body.scrollTop;
		}
		endPoint = (menuTopMargin <= endPoint ) ? endPoint - menuTopMargin : 0;
		if(startPoint != endPoint) {
			moveAmount = Math.ceil(Math.abs(endPoint - startPoint) / 15);
			obj.style.top = parseInt(obj.style.top, 10) + ((endPoint<startPoint) ? -moveAmount : moveAmount)+"px";
			reTimer = menuSpeed;
		}
	} else {
		obj.style.top = 0;
	}
	setTimeout(checkMenu, reTimer);
}
if (document.documentElement.clientHeight > heightLimit) {
	document.getElementById('checknew').style.top = document.body.scrollTop;
}
//checkMenu();
//ajaxFunc();
</script> 
