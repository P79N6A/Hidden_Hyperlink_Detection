   Route.initDateItems();
   
});

function dispayPriceNote(o, t){
   var ps = $(o).offset();
   var d = $('.price_note_detail');
   d.css({left: ps.left - d.width() + $(o).width() + 8, top: ps.top + $(o).height() + 3});
   if (t == 0) {
      d.hide();
   } else {
      d.show();
   }
}
function printRoute(){
   var timetables = '';
   var i = 0;
   var maxLen = 8;
   $('#date_items').children().each(function(){
      if (i < maxLen) {
         var arr = $(this).attr('date').split('-');
         timetables += arr[1].replace(/^0/, '') + '/' + arr[2].replace(/^0/, '') + ', ';
         i++;
      } else {
         return false;
      }
   });
   timetables = timetables.replace(/\,\s*$/, '');
   $('#print_timetables').val(timetables);
   $('#print_form').submit();
}
 
 
 

 function sub(iptId){
    var num = $('#'+iptId).val();
    
    if(num>0){
      num--;
      if(num < 1){
        $('#'+iptId).siblings('#adC-le').removeClass('on');
        $('#'+iptId).siblings('#adA-le').removeClass('on');
        $('#'+iptId).val(0);
      }
    } 
    $('#'+iptId).val(num);
  }
  function add(iptId){
    var num = $('#'+iptId).val();
    var adultN = $('#adultN').val();
    var childN = $('#childN').val();
    var all_num = adultN*1+childN*1;
    var max_num  = parseInt($('#tuanti').html());
    
    if(num<0){
      num = 1;
    }else if (max_num<=all_num){
    	if(iptId=='adultN'){
    		num = max_num-childN;
    	}else if(iptId=='childN'){
    		num = max_num-adultN;
    	}
    }else{
      num++;
      $('#'+iptId).siblings('#adC-le').addClass('on');
      $('#'+iptId).siblings('#adA-le').addClass('on');
    }
    $('#'+iptId).val(num);
  }
        </script> 
  <script>
	$('.acute').keyup(function(){
		var values = parseInt($(this).val());
		
		if(values<=0 || !values)
		{
			$(this).val('');
		}
	});
	$(".cp-item").find('input').keyup(function(){
		if(parseInt($(this).val())<=0 || !parseInt($(this).val())){
			$(this).val('');
		}
	});


	//订单提交
$('.yuding_order').click(function(){
    var product_id = $('input[name=product_id]').val();
    var adultN     = parseInt($('input[name=adultN]').val());
    var childN     = $('input[name=childN]').val();
    var store_id   = $('#store_id').val();
    
	var route_type = $("#route_type11").val(); 
	//alert(route_type);return false;
    var tourtime = $('input[name=timetable_id]').val();
    if(tourtime == '' || tourtime ==0){
        layer.alert('请选择出游时间');
        $('input[name=timetable_id]').focus();
        return false;
    }
    
    if(adultN<=0){
        layer.alert('成人出游人数不能小于1');
        $('input[name=adultN]').focus();
        return false;
    } 
    var total = parseInt(adultN) + parseInt(childN);
    var members = 16; //alert(members);return false;
   
    var members = $('#tuanti').html();//alert(members)
    if(total > members)
    {
      var m = $('#member_notice');
	  if (!m.size()) 
	  { 
		 m = $('<span id="member_notice" style="color:red">超过可预订总人数,  请重新输入!</span>');
		 $('#member_notice_content').append(m);
		 $('#member_notice_content').parent().css('display','block');
	  }
      return false;
    }
	var condition='';
    if(store_id)
    {
         condition = "&store_id="+store_id;
    }
    window.location.href="/front/newtravel/orders?cat_id="+route_type+"&step=1&product_id="+product_id+"&tourtime="+tourtime+"&adultN="+adultN+"&childN="+childN+condition;
});


  $(function(){
      $(window).scroll(function(){
       var scrollTop = $(document).scrollTop();
       var contentItems = $("#departure").find(".tripdays");
       contentItems.each(function(){
         var contentItem = $(this);
         var offsetTop = contentItem.offset().top;
         if(scrollTop > offsetTop-200){
           currentItem = "#" + contentItem.attr("id");
           if(currentItem!=$("#menubar").find(".current").attr("href")){
             $("#menubar").find(".current").removeClass("current");
             $("#menubar").find("[href=" + currentItem + "]").addClass("current");
           }
         }                   
       });
      });
      $('#wrap ul').hide();
      $('#wrap span').mouseover(function(){
        $('#wrap ul').show();
      });
      $('#wrap ul').mouseover(function(){
        $('#wrap ul').show();
      });
      $('#wrap').mouseout(function(){
        $('#wrap ul').hide();
      });
      $("#wrap ul li").click(function(){ 
        var value = $(this).attr("rel");
        var txt = $(this).text();
        
        $('input[name=timetable_id]').val(value);
        
       
		var name = $(this).attr("myvalue");
		var private_price = $(this).attr("private_price");
		
		$("#tuanti").html(name);
		$(".private_price").find('b').html(private_price);
        $('#wrap span').text(txt);
        $('#wrap span').css({color:'#333'});
        $("#wrap ul").hide(); 
      });
  });
  
  
  
  
        </script> 
  <script type="text/javascript">
            //张鹏 2016.3.29 更改左上部图片效果!
            $(function () {
                var picBox = $(".slidePic");
                var bigPic = $(".g_showbox li", picBox);
                var picUl = $(".tempWrap ul");
                var minPic = $(".tempWrap ul li", picBox);
                picUl.width(minPic.length * 126)
                bigPic.eq(0).css("display", "block");
                minPic.mouseover(function () {
                    bigPic.css("display", "none")
                    bigPic.eq($(this).index()).css("display", "block");
                })
                $(".sPrev", picBox).click(function () {
                    if (parseInt(picUl.css("left")) !== 0) {
                        picUl.css("left", (parseInt(picUl.css("left")) + 126))
                    }
                })
                $(".sNext", picBox).click(function () {
                    var picUl = $(".tempWrap ul");
                    leftMax = 126 * ((minPic.length) - 4);
                    if (Math.abs(parseInt(picUl.css("left"))) != leftMax) {
                        picUl.css("left", (parseInt(picUl.css("left")) - 126))
                    } else {
                    }
                })
            })
        </script> 
