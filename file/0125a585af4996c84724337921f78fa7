  <script>

Timetable.custom = {};
Timetable.loop =  {"type":"day","value":1,"range":"2016-02-03_2016-02-28","data":{"adult":"2880","child":"2380","members":"16","b_day":"3","b_hour":"0","b_minute":"0","mission_time":"8:00","order_members":0,"js_datetime":"1970-01-01 08:00:00","start_time":-28800}};
Timetable.newtime =  '2016-02-18 15:46:17';

$(document).ready(function(){
	
   Timetable.init(
    'timetable', 
    {
   	  numberOfMonths: 1,
      firstDay: 0,
	  showOtherMonths:true,
	  monthNames:['1月','2月','3月','4月','5月','6月','7月','8月','9月','10月','11月','12月'],
	  hideIfNoPrevNext: true,
      minDate: 'today',
       //maxDate: '2015-10-02', 
