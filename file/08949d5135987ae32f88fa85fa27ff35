<script type="text/javascript">
var map = new BMap.Map("myMap_Contact");
map.addControl(new BMap.NavigationControl());
var myGeo = new BMap.Geocoder();
myGeo.getPoint("广东省佛山市禅城区汾江中路9号兴运大厦新鸿运电脑", function (point) {
    if (point) {
        map.centerAndZoom(point, 16);
        var maker = new BMap.Marker(point);   
        map.addOverlay(maker);
		  map.enableScrollWheelZoom();
        map.openInfoWindow(new BMap.InfoWindow("<h4 style='margin:0;font:bold 14px Tahoma;color:#222222;'><a href='http://sab8392059.atobo.com.cn' target='_blank'>佛山市蓝天盾电子工程设备有限公司太平洋门</a></h4><p style='color:#111111;font-size:12.5px;'>地址：广东省佛山市禅城区汾江中路9号兴运大厦新鸿运电脑</p><p style='color:#111111;font-size:12.5px;'>联系人：蓝小林</p><p style='color:#111111;font-size:12.5px;'>电话：<img src='http://img.atobo.com/ProductImg/Phone/Big.gif?7VI0UdGfBEBcP24zayUK6g%3d%3d'  border='0' align='absmiddle' alt=''/></p>",{offset:new BMap.Size(0,-17)}),point);
    }
}, "广东 佛山");
</script>
