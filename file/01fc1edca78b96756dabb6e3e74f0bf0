  <script>
$(window).scroll(function() {
	if($(window).scrollTop()==0){
		$(".backToTop").fadeOut(500);
	}else{
 		$(".backToTop").fadeIn(500);
	}
 });
$(".backToTop .top").click(function (){
	 $("html, body").animate({ scrollTop: 0 }, 200);

});
$(".backToTop .weixin").click(function (){
	art.dialog({
		title:'关注我们的微信',
		content:'<img src="'+$(this).attr('imgurl')+'" width="150" height="150" />'
	});
});
$(".backToTop .foot").click(function (){
	 $("html, body").animate({scrollTop:$(document).height()},200);
});
//搜索
$("#keyword").keydown(function(e){ 
								if(e.keyCode==13){ 
									$("#keyword").nextAll("a:first").trigger("click");
								} 
							}); 
$("#keyword").nextAll("a:first").attr("href",'javascript:;');
$("#keyword").nextAll("a:first").click(function (){
		location.href='/ZhuanLan/?keyword='+$("#keyword").val();
});
</script>
  <script>
//自动刷新
function ajax_push(){
	if($("#ch_push").attr("checked")){
		
	}else{
		$("#weibo li").css("background","none");
		var max_id=$("#weibo li:first").attr("data-id");
		$.get('/ajax/ajax_kb.php?max_id='+max_id,function(data){
			$("#weibo li:first").before(data);
		})
		$("#weibo li").gt(9).remove();
	}
}
setInterval('ajax_push()',10000);
//
$(".weibo_face").remove();
$(".weibo_info .m").each(function(){
	//$(this).children("a").eq(1).text('深蓝财经');
});
$(".weibo_detail").each(function (){
	var href=$(this).children(".weibo_all").attr('href');
	$(this).children(".weibo_all").remove();
	$(this).children(".weibo_info").children(".weibo_pz").html('<a href="'+href+'" target="_blank">查看全文</a> | ');
	
	$(this).children("p").children("a").eq(0).text('');
	$(this).children("p").children("span").eq(0).remove();
	$(this).children(".weibo_info").children(".m").children("a:eq(1)").remove();
});
$(".weibo_img_small").remove();//删除图片
//
							$('#wd').keydown(function(e){ 
								if(e.keyCode==13){ 
									$("#baidu_btn").trigger("click");
								} 
							}); 
							//
$("#baidu_content").slideDown('fast');
	$("#wb [value='']").attr('selected','selected');
	$("#baidu_btn").click(function (){
		var wd=$("#wd").val();
		var wb=$("#wb").val();
		if(!wd) {
			Alert(1,'error','请先填写关键字！');
			return false;
		}
		location.href='?do=search&wd='+wd+'&wb='+wb;
	});
	$("#wb").change(function (){
		var wd=$("#wd").val();
		var wb=$("#wb").val();
		location.href='?do=search&wd='+wd+'&wb='+$(this).val();
	});
	$("#wd").focus(function(){
		$('#keyword_list').slideDown("slow");
	}).blur(function(){
		$('#keyword_list').slideUp("slow");
	});
	$("#page a").live('click',function(){
		$("#baidu_content").text('加载中...');
		 //$("html, body").animate({ scrollTop: 0 }, 50);
		var wd="标题党";
		var wb=$("#wb").val();
		var url=$(this).attr('href');
		url=url.split('&');
		for(var i=0;i<url.length;i++){
			var name=url[i];
			name=name.split('=');
			if(name[0]=='pn') var pn=name[1];
		}
		$(this).attr('href','javascript:;');
		location.href='?do=search&wd='+wd+'&wb='+wb+'&pn='+pn;
		
	});
	$(".weibo_pz").append('<a href="javascript:;" class="zhuanfa">转发到社区</a> ');
	//判断是否登录
	$("a.zhuanfa").live('click',function (){
					alert('请先登录');
			location.href='/login/';
				var txt=$(this).parent().parent().parent().children("p:eq(0)").text();
		var html=$(this).parent().parent().parent().children("p:eq(0)").html();
		txt=txt+'【来自：深蓝云】';
		html=html+'【来自：深蓝云】';
		//
		html=delHtmlTag(html);
		var id='EF893L';
		$("#"+id).attr('id','');
		$(this).attr('id',id);
		var pic=$(this).parent().parent().parent().find("img").attr("data-bgimg");
		var checkbox='';
		if(pic){
			checkbox='<input type="checkbox" checked="checked" name="pic_url" id="pic_url" value="'+pic+'" />同时转发图片';
		}
		checkbox=checkbox;
		art.dialog({
			content: '<textarea id="sNeiRong" name="sNeiRong" style="width:380px;height:180px;">'+html+'</textarea><br /><br />'+checkbox,
			title:'转发到我的新鲜事',
			id: id,
			width:380,
			height:200,
			follow: document.getElementById(id),
			button:[
				{
					name:'转发',
					callback:function(){
						//入库
							$.ajax({
								   type: "POST",
								   url: "/u_xinxianshi_data.php",
								   data: "pic_url="+$('#pic_url').val()+"&sContent="+$("#sNeiRong").val()+"&act=add",
								   success: function(msg) {
									   var jsonData = JSON.parse(msg);
									   if (jsonData.msg[0] == "ERROR") {
										   Alert('1','error',jsonData.msg[1]);
									   } else if (jsonData.msg[0] == "S") {
									   			//关闭
										   		var list = art.dialog.list;
												for (var i in list) {
													list[i].close();
												};
												Alert_close(1,'ok','转发成功！');
									   } else Alert('1','error',"发布失败");
								   }
							});
						//入库
					},
					focus:true
				},
				{
					name:'取消',
					callback:function(){},
					focus:false
				}
			]
		});
	});
//分享
$("span.share_weibo").click(function(){
	var url=$(this).attr("href");
	window.open(url);
});
$(".weibo_detail").hover(function(){
	$("span.share_weibo").hide();
	$(this).children(".weibo_info").children(".m").children("span.share_weibo").show();
});
</script> 
