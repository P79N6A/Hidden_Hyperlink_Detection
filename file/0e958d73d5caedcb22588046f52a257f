   <script>
function choosedc(idnum){
$.ajax({
        cache: false,
        async: true,
        type: "GET",
		dataType: "json",
        url: "option.asp?action=dc&id=594&idnum="+idnum,
        success: function(data) {
			if(data.succ=="succ"){
			$("#dc1").html(data.dc1+"%");
			$("#dc2").html(data.dc2+"%");
			$("#dc3").html(data.dc3+"%");
			$("#dc4").html(data.dc4+"%");
			$("#dcn1").css("width",data.dc1+"%")
			$("#dcn2").css("width",data.dc2+"%")
			$("#dcn3").css("width",data.dc3+"%")
			$("#dcn4").css("width",data.dc4+"%")
			}
			else{
				alert("您已参加过此调查")
				}
            //clickalert(data);
        }
    })
}
function choose(t,num){
$.ajax({
        cache: false,
        async: true,
        type: "GET",
		dataType: "json",
        url: "option.asp?action=like&id=594&t="+t,
        success: function(data) {
			if(data.succ=="succ"){
			$("#" + t).html(parseInt($("#" + t).html())+1);
			 
			}
			else{
				alert("您已参加过此调查")
				}
            //clickalert(data);
        }
    })

}

function favous(id) {
	
    $.ajax({
        cache: false,
        async: true,
        type: "GET",
        url: "option.asp?action=favous&newstype=1&newsid="+id,
        success: function(data) {
			if(data=="succ"){
			$("#favouser").html("已收藏");
			
			$("#favous").html(parseInt($("#favous").html())+1);
			} else if(data=="login"){
				
				openpage('../pinglun/poplogin.asp','登陆',510,365); return false;
				}
			else{
				$("#favouser").html("收藏");
				$("#favous").html(parseInt($("#favous").html())-1);
				}
            //clickalert(data);
        }
    })
}
document.getElementById("header-channel13").className += " item-current"

</script> 
