<script>
function readCookie1(name) {
    var reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
    var r = document.cookie.match(reg);
    if (r != null) {
        return decodeURIComponent(r[2]);
    }
    return 0;
}

// function toIntVer(ver) {
//     var p1 = /^(\d+).(\d+)$/;
//     var p2 = /^(\d+).(\d+).(\d+)$/;
    
//     var v1 = 0;
//     var v2 = 0;
//     var v3 = 0;
//     if (ver != null) {
//         var rs = p1.exec(ver);
//         if (rs != null) {
//             v1 = rs[1];
//             v2 = rs[2];
//         } else {
//             rs = p2.exec(ver);
//             if (rs != null) {
//                 v1 = rs[1];
//                 v2 = rs[2];
//                 v3 = rs[3];
//             }
//         }
//     }
    
//     return v1 * 1000000 + v2 * 1000 + v3;
// }

function compareIntVer(ver1, ver2) {
    var p = /^(\d+)+(.\d+)*$/;
    
    if(ver1 == null || !p.test(ver1) || ver2 == null || !p.test(ver2)) {
        return null;
    }
    
    var list1 = ver1.split(".");
    var list2 = ver2.split(".");
    
    var n = list1.length > list2.length ? list1.length : list2.length;
    
    for(var i = 0; i < n; i++) {
        var v1 = 0;
        if(i < list1.length) {
            v1 = list1[i];
        }
        var v2 = 0;
        if(i < list2.length) {
            v2 = list2[i];
        }
        if(v1 > v2) {
            return 1;
        } else if(v1 < v2) {
            return -1;
        }
    }
    
    return 0;
}

function isAppFromCookie() {
    var appKey = "" + readCookie1("app_key");
    var appVer = "" + readCookie1("app_ver");
    var appSource = "" + readCookie1("appsource");

    if(appSource != null){
        if(appSource == "1"){
            return true;
        }
    }

    if(appKey != null && appVer != null) {
        appKey = appKey.toLowerCase();
        
        if(appKey == "price_android" || appKey == "price_ios"){
//             if(toIntVer(appVer) >= toIntVer("3.9.10")){
//                 return true;
//             }
            if(compareIntVer(appVer, "3.9.10") >= 0) {
                return true;
            }
        }

        if(appKey == "auto_android" || appKey == "auto_iphone"){
//             if(toIntVer(appVer) > toIntVer("5.0.0")){
//                 return true;
//             }
            if(compareIntVer(appVer, "5.0.0") > 0) {
                return true;
            }
        }
        //车商城APP
        if(appKey == "mall_android" || appKey == "mall_iphone"){
            return true;
        }
    }
}

if(typeof(__ISAPP) == "undefined") {
    var __ISAPP = null;
}

if (__ISAPP == null || __ISAPP != "1") {
    if(isAppFromCookie()){
        var url = location.href;
        var urlTag = null;
        var pos = url.indexOf("#");
        if(pos >= 0) {
            urlTag = url.substring(pos);
            url = url.substring(0, pos);
        }

        //兼容当链接中含有isapp但是值不是1的情况
        if(url.indexOf("isapp=") >= 0){
            var reg = eval('/(isapp=)([^&]*)/gi');
            url = url.replace(reg,'isapp=1');
        }else{
            if(url.indexOf("?") >= 0) {
                url += "&isapp=1"
            } else {
                url += "?isapp=1"
            }
        }

        if(urlTag != null && urlTag.length > 0) {
            url += urlTag;
        }
        // 为app跳转过来的链接加一个独特的记号"#QWERTYUIOP"
        url += "#QWERTYUIOP";
        
        location.href = url;
    }
}

</script>
    
