try {
	var currentLocation = location.href;
	var htmlIndex = currentLocation.indexOf(".html");
    var currentLocationSuffix = "";
    var mosaicCurrentLocationSuffix = "";
	if(htmlIndex != -1){
        currentLocationSuffix = currentLocation.substring(htmlIndex + 5);
        mosaicCurrentLocationSuffix = currentLocationSuffix.replace("?","&");
        mosaicCurrentLocationSuffix = mosaicCurrentLocationSuffix.replace("#","&");
	}
	} catch (e) {
}
</script>

