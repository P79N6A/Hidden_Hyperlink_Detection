	<script type="text/javascript">
	function getTypeByItemSource(){
	 	   var pvtype = detailPageData.frontData.basic.source;
	        if(detailPageData.frontData.basic.source==1){
	        	if(detailPageData.frontData.basic.productType==1){
	        		pvtype=11;
	        	}else{
	        		pvtype=12;
	        	}
	        }
	        if(detailPageData.frontData.basic.source==4 && !detailPageData.frontData.basic.collectSaleLeads){
	            pvtype=42;
	        }
	        if(detailPageData.frontData.basic.source==8){
		       	   pvtype=6;
		   	   }
		   	    if(detailPageData.frontData.basic.source==10){
		   	        pvtype=90;
		   	    }
		   	    if(detailPageData.frontData.basic.source==11){
		   	        pvtype=91;
		   	    }
				if(detailPageData.frontData.basic.source==12){
                    pvtype=13;
				}

				if(detailPageData.frontData.basic.source==17){
                    pvtype=15;
				}
		   	    return pvtype;
	}
	
        var eventTrack = function () { };
        eventTrack.a1 = detailPageData.frontData.basic.itemId;
        eventTrack.a2 = getTypeByItemSource();
        if(__CITY_ID&&__CITY_ID!=0){
        	eventTrack.a3 =__CITY_ID;
        }else{
        	eventTrack.a3=__DEFAULT_CITY_FROM_COOKIE;
        }
        var referrer = document.referrer;
        var mpvareaid="";
        try{
	        if (pvareacookie.getHost(referrer).toLowerCase() == "mall.autohome.com.cn" || pvareacookie.getHost(referrer).toLowerCase() == "m.mall.autohome.com.cn" || pvareacookie.getHost(referrer).toLowerCase() == "mall.m.autohome.com.cn" || pvareacookie.getHost(referrer).toLowerCase() == "1111.autohome.com.cn" || pvareacookie.getHost(referrer).toLowerCase() == "1111.m.autohome.com.cn") {
	        	mpvareaid="";
	        }
        } catch (e) {
    	}
        if(mpvareaid==""){
        	mpvareaid = window.readCookie("mpvareaid");
        }
        if(!mpvareaid){
        	mpvareaid="";
        }
        eventTrack.a4=mpvareaid;
    </script>
