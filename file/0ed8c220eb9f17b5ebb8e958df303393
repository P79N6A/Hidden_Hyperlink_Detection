<script>
	/* 统计点击事件 begain　  */
    var _ah_click = _ah_click || [];
    try{
        !function(doc){
        var _as = doc.createElement('script');
        _as.type = 'text/javascript';
        _as.async = true;
        _as.src = '//x.autoimg.cn/bi/common/ah_click_s.min.js?d=' + (new Date()).toLocaleDateString().replace(/\//g, "");
        var s = doc.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(_as, s);
        }(document);
    }catch(e){};

    function readCookie(name) {
        var reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
        var r = document.cookie.match(reg);
        if (r != null) {
            return decodeURIComponent(r[2]);
        }
        return 0;
    }

    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null)return unescape(r[2]);
        r = window.location.hash.substr(1).match(reg);
        if (r != null)return unescape(r[2]);
        return null;
    }
    /* 点击事件统计 end　  */

</script>
<script>
    /**
     * 百万代理人种cookie
     */
    function initMillionAgent(){
        var platformActivityID = getQueryString("PlatformActivityID");
        var platformAgentID = getQueryString("PlatformAgentID");
        var millionAgentJson = "";
        if(platformActivityID != null && platformActivityID != "" && platformAgentID != null && platformAgentID != ""){
            millionAgentJson = "{\"PlatformActivityID\":\""+platformActivityID+"\",\"PlatformAgentID\":\""+platformAgentID+"\"}";
            var date = new Date();
            date.setHours(date.getHours() + 1);
            document.cookie = "millionAgent=" + millionAgentJson + "; expires=" + date.toGMTString() + "; path=/; domain=mall.autohome.com.cn";
        }
    }

    initMillionAgent();
</script>
