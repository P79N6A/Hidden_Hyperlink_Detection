                });
            })();
        }


    });


    function pvevent(edi){
//        var curl = encodeURIComponent(window.location.href);
//        var rurl = encodeURIComponent(document.referrer);
//        var ctime = new Date().getTime();
        _trackEvent.push({'eid':edi,'val':1});
    }
