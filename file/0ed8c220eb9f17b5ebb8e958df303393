	var pvTrack = function () { };
	pvTrack.site = 1211058;
	pvTrack.category = 582;
	pvTrack.subcategory = 3338;
	pvTrack.objectid = detailPageData.frontData.basic.itemId;
	pvTrack.type = detailPageData.frontData.basic.source;
	
	if(detailPageData.frontData.basic.source==1){
    	if(detailPageData.frontData.basic.productType==1){
    		pvTrack.type=11;
    	}else{
    		pvTrack.type=12;
    	}
    }
    if(detailPageData.frontData.basic.source==4 && !detailPageData.frontData.basic.collectSaleLeads){
        pvTrack.type=42;
    }
    if(detailPageData.frontData.basic.source==8){
        pvTrack.type=6;
    }
    if(detailPageData.frontData.basic.source==10){
        pvTrack.type=90;
    }
    if(detailPageData.frontData.basic.source==11){
        pvTrack.type=91;
    }
  	if(detailPageData.frontData.basic.source==12){
        pvTrack.type=13;
    }

    if(detailPageData.frontData.basic.source==17){
        pvTrack.type=15;
    }
		//objInit.activityid;
	pvTrack.typeid=0;
	if(__CITY_ID&&__CITY_ID!=0){
		pvTrack.typeid =__CITY_ID+ '|' + readCookie('app_key') + '|' + readCookie('app_platform') + '|' + readCookie('app_deviceid');
	}else{
		pvTrack.typeid=__DEFAULT_CITY_FROM_COOKIE+ '|' + readCookie('app_key') + '|' + readCookie('app_platform') + '|' + readCookie('app_deviceid');
	}
	pvTrack.seriesid =detailPageData.frontData.basic.seriesId;
	pvTrack.specid = detailPageData.frontData.basic.specId;
	pvTrack.level = 0;
	pvTrack.dealer = __PROVIDER_ID;
