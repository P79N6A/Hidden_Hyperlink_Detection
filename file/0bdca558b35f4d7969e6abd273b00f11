<script type="text/javascript">
	var timer1;
	var timer2;
	function redirectDownPage() {
		var downpage_link = document.getElementById('downpage_link');
		var url = "/file/down/qq_3501BEFF98/4797fc6d.html";
			if (document.getElementById('vcode').value==""){
				alert("请输入正确的验证码!");		
				return false ;
			}
			url = url.replace(".html","/"+document.getElementById('vcode').value+".html");
		downpage_link.setAttribute("href",url);
		if(downpage_link.click){
			downpage_link.click();
			if(isFF5())
				window.location = url;
		}
		else{
			window.location = url;
		}
	}
	function updateTime(){
		if(document.getElementById('wait_span').innerHTML>0){
			document.getElementById('wait_span').innerHTML = document.getElementById('wait_span').innerHTML-1;
		}
		else{
			clearInterval(timer1);
			redirectDownPage();
		}
	}
	function show_vcode(){
		if(document.body.clientWidth>100){
			var cvimgvip2 = document.getElementById("cvimg2");
			cvimgvip2.setAttribute("src","/verifyimg/getPcv"+""+".html");
		}
		document.getElementById('slow_th').style.display = "none";	
		document.getElementById('vip_th').style.display = "none";
		document.getElementById('vcode_th').style.display = "";
			dialog_Open2();
	}
	function startWait(){
		document.getElementById('premium_div').style.display = "none";	
		if(document.getElementById('interval_div').style.display=="none") {
			document.getElementById('slow_button').disabled = true;
			timer1 = window.setInterval('updateTime()', 1000); 
			document.getElementById('wait_div').style.display = "";
			document.getElementById('inputDownWait').style.display = "none";
		} 
	}

</script>
 

<script type="text/javascript">
	var inEdit = document.getElementById("vcode");
	function validate(f){
		s = inEdit.value;
		if (s=="")
		{
			inEdit.focus();
			inEdit.select();
			alert("请输入正确的验证码!");		
			return false 
		}
			startWait();
	}
	function keyPress(e)   
	{   
		var keyCode=0;   
		if(window.event) // IE   
		{   
			keyCode = e.keyCode   
		}   
		else if(e.which) // Netscape/Firefox/Opera   
		{   
			keyCode = e.which   
		}
		if(keyCode==13)
			document.getElementById("slow_button").click();
	}
</script>
