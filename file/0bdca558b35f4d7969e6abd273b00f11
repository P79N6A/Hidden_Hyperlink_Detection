<script>
	function isFF5(){
		var ua = navigator.userAgent;
		if(ua)
			return ua.indexOf("Firefox/5")>0;
		return false;
	}
	function redirectPremium() {
		var premium_link = document.getElementById('premium_link');
		if(premium_link.click){
			premium_link.click();
			if(isFF5())
				window.location = premium_link.href;
		}
		else
			window.location = premium_link.href;
	}
	function redirectPremiumLink() {
		var premium_link = document.getElementById('premium_link');
		if(premium_link.click)
			premium_link.click();
		else
			window.location = premium_link.href;
	}
</script>

