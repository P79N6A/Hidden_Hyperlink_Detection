      <script>
                
                new JP.mouseover.auto({
                    css_mouseover_class: 'cancelFav',
                    element_types: ['img', 'input'],
                    root_node: 'moreInfo',
                    show_broken_img: true
                });
                var formHtml = "http://secure.verycd.com/signin?source=mini&continue=http://www.verycd.com/resetLoction.html";
                var formHtmlwithoutContinue = "http://secure.verycd.com/signin?source=mini&continue=http://www.verycd.com/resetLoctionNofun.html";
                if(old_get("addFav")!=null){
                    old_get("addFav").onclick=function(e){
                        hidden_favorite_tips();
                        if(!VeryCD.Member.Id) {
                            VC_popup(formHtml);
                        } else {
                           fromClick=true;
                           add2fav();
                        }
                        if (!e) var e = window.event;
                        e.cancelBubble = true;
                        if (e.stopPropagation) e.stopPropagation();
                    }
                 }
                 if(old_get("cancelFav")!=null){

                     old_get("cancelFav").onclick=function(){
                        if(!VeryCD.Member.Id) {
                            VC_popup(formHtml);
                        } else {
                           del4fav();
                        }
                    }
                 }
                try{
                    old_get("cancel_btn").onclick=function(){
                        favFwindow.style.display = "none";
                        favBwindow.style.display = "none";
                        return false;
                    }
                }catch(e){}
                
                </script> 
