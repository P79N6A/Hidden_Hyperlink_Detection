function dianpin(t, ids) {  //微信朋友圈窗口  
    JqueryDialog.Open1('微信朋友圈', 'http://news.efu.com.cn/news2/weixin.aspx?t='+t+'&url=http://zs.efu.com.cn/newsview-'+ids+'-1.html', 430, 480);
}

	function getbeibo(cid) { //备播广告,刷新随机显示，后台缓存10分钟
            $.getJSON("http://zs.efu.com.cn/ws/beibo.ashx?cid=" + cid + "&jsoncallback=?", 
function(data) {
                var len = 0;                            
                $.each(data.Table, function(i, item) {
                    len++;
                });               
                len = len - 1;
                var vi = Math.round(Math.random() * len);//随机数                
                $("#newsdiv1").html(data.Table[vi].adv_code);
				if(vi==len){
					$("#newsdiv2").html(data.Table[vi-1].adv_code);
				}
				else {
					$("#newsdiv2").html(data.Table[vi+1].adv_code);
				}
            });
                      
        }

        $(function() {
        //getbeibo(6);
        });

		function getbeibo2(cid1) { //备播广告,刷新随机显示，后台缓存10分钟
            $.getJSON("http://zs.efu.com.cn/ws/beibo.ashx?cid=" + cid1 + "&jsoncallback=?", 
			function(data) {
                var len = 0;                            
                $.each(data.Table, function(i, item) {
                    len++;
                });               
                len = len - 1;
                var vi = Math.round(Math.random() * len);//随机数                
                $("#newsdiv1a").html(data.Table[vi].adv_code);				
            });
                      
        }
		
        $(function() {
        //getbeibo2(7);
        });
		
		
