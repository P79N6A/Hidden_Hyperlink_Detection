function postpl(idval) { //提交评论
   
    var txt1 = document.getElementById("pltxt_" + idval);
    var uid = document.getElementById("uid_" + idval);
    if (txt1.value == "") {
        alert('请输入评论内容！');
        txt1.focus();
        return;
    }
    $.ajax({
        type: "POST",
        url: "/Ws/plOk.ashx",
        data: "db=efudb5&tab=newsdata&uid=" + uid.value + "&reid=0&keyid=" + idval + "&txt1=" + txt1.value,
        success: function(msg) {
            if (msg == "true") {
               
                $("#mypl").html("<div class=\"con-mPhoto\"><div class=\"sw-ui-photo60\"> <a class=\"sw-ui-photo60-box\" href=\"javascript:void(0);\" rel=\"nofollow\"> <img width=\"60\" onerror=\"this.src='http://cs1.chinaefu.net/images/notx.png'\" src=\"http://cs1.chinaefu.net/images/notx.png\"> </a> </div>"
           + " </div><div class=\"con-mBox\"> <span class=\"li-sp-01\"><i class=\"i-a\"><em>我</em></i> <i class=\"i-time\">日期：刚才</i> </span> <span class=\"li-sp-02\"> <p> " + txt1.value+" </p> </span> </div>")
                txt1.value = "";
                alert("提交评论完成！");
            } else {
                alert(msg);
            }
        }
    });

}

