function ajaxf(urlNum,view){
	$.ajax({
		type:'GET',
		url:urlNum,
        success: function(data){
				//$(divNum).html(data)
				var dataNum = data
				if(view == 1){
				View (dataNum)
				}
			}
		})
	}
function View(dataNum){
	if(dataNum!=""){
		var index
		var arl = dataNum.split("|");
		var m=arl.length-1;
	    index=Math.round(Math.random()*m);
		$("#ad4").html(arl[index])
		}

	}

