jQuery(document).ready(function(){
jQuery('#nav_21').addClass('nHm');

var mc = decodeURIComponent(get_cookie('_member_info'));
if (mc) {
	var arr = mc.split('::');
	var dt_member = ' 欢迎<span class="f_red f_b">' + arr[2] + '</span> 登录马洲贸易网 | <a href="/member/my/setup/index.jspx">会员管理</a>  | <a href="/member/indexLogout.htm">退出</a>';
	jQuery('#memberinfo').html(dt_member);
	
	var memberloginboard = document.getElementById("memberloginboard");
	var membermainboard = document.getElementById("membermainboard");
	var user_wel_truename = document.getElementById("user_wel_truename");
	var memberwebsite = document.getElementById("memberwebsite");
	if (memberloginboard != null && membermainboard != null && user_wel_truename !=null && memberwebsite != null) {
		memberloginboard.style.display = 'none';
		membermainboard.style.display = 'block';
		user_wel_truename.innerHTML = '<span class="f_red f_b">' + arr[2] + '</span> ';
		memberwebsite.href='http://' + arr[1] + '.21deal.com/index-' + arr[1] + '.htm';
	}
} else {
	var dt_member = '&nbsp; 欢迎加入马洲贸易网! | <a href="/member/login.htm">会员登录</a> | <a href="/member/signup.htm">注册</a> | <a href="/member/login.htm">忘记密码?</a>';
	jQuery('#memberinfo').html(dt_member);
	var memberloginboard = document.getElementById("memberloginboard");
	var membermainboard = document.getElementById("membermainboard");
	var user_wel_truename = document.getElementById("user_wel_truename");
	var memberwebsite = document.getElementById("memberwebsite");
	if (memberloginboard != null && membermainboard != null && user_wel_truename !=null && memberwebsite != null) {
		memberloginboard.style.display = 'block';
		membermainboard.style.display = 'none';
		user_wel_truename.innerHTML = '';
		memberwebsite.href='#';
	}
}
});
