  <script type="text/javascript">
        function showModalPic(picUrl) {
            if (picUrl.indexOf('.gif') >= 0) {
                $('#ModalPic .modal-body img').attr("src", picUrl).toggleClass("img-rounded").attr("width", "");
            } else {
                $('#ModalPic .modal-body img').attr("src", picUrl).toggleClass("img-rounded");
            }
            $('#ModalPic').modal({});
        }

        function showModalVideo(videoUrl) {
            if (videoUrl.indexOf(".mp4") == -1) {
                $('#video').html('<iframe class="embed-responsive-item" src="' + videoUrl + '"></iframe>');
                //$('#ModalVideo .modal-body iframe').attr("src", videoUrl);
            } else {
                $('#video').html('<video controls="controls"><source src="' + videoUrl + '" type="video/mp4" /></video>');
            }
            $('#ModalVideo').modal({ backdrop: 'static' });
        }
    </script> 
  <script type="text/javascript">
        function addFavorite(speakId) {
            $.getJSON("/SpeakFavorite/_Add?rnd" + Math.random(), {
                speakId: speakId
            }).done(function(data) {
                if (data.IsSuccess) {
                    $('#Favorite_' + speakId.toString()).text('已收藏').attr('disabled', 'disabled');
                } else {
                    showModalMessage(false, data.Message);
                }
            });
        }

        function addSpeakAttitude(speakId) {
            $.getJSON("/SpeakAttitude/_Add?rnd" + Math.random(), {
                speakId: speakId
            }).done(function(data) {
                if (data.IsSuccess) {
                    $('#Attitude_' + speakId.toString()).text('已赞').attr('disabled', 'disabled');
                } else {
                    showModalMessage(false, data.Message);
                }
            });
        }
    </script> 
  <script type="text/javascript">
        /*
        *打招呼
        */
        function sayHello(guId, name, email) {
            $('#ToGUId').val(guId);
            $('#TMCId').val(1);
            $('#NickName').val(name + '(' + email + ')');
            showAddTalkMessage();
        }

        /*
        *推销
        */

        function makePromote(guId, name, email) {
            $('#ToGUId').val(guId);
            $('#TMCId').val(2);
            $('#NickName').val(name + '(' + email + ')');
            showAddTalkMessage();
        }

        /*
        *请教
        */

        function makeAsk(guId, name, email) {
            $('#ToGUId').val(guId);
            $('#TMCId').val(3);
            $('#NickName').val(name + '(' + email + ')');
            showAddTalkMessage();
        }
    </script>   
