  <script src="/Scripts/uploadify/jquery.uploadify.js" type="text/javascript"></script> 
  <link href="/Scripts/uploadify/uploadify.css" rel="stylesheet" type="text/css" /> 
  <script type="text/javascript">
    var fromId = 'SpeakAdd';
    function onSuccess(jsonResult) {
        if (jsonResult.IsSuccess) {
            document.getElementById(fromId).reset();
            picIndex = 0;
            $('#pics').html('');
        } else {
            alert(jsonResult.Message);
        }
    }

    var picIndex = 0;
    $('#file_upload').uploadify({
        width: 60,
        buttonText: "图片",
        'swf': '/Scripts/uploadify/uploadify.swf',
        'uploader': '/UpFile/Up',
        'onUploadSuccess': function (file, data, response) {
            var jsonData = $.parseJSON(data);
            var fileData = jsonData[0];
            //alert(fileData.PicWebDomain + "/" + fileData.FilePathName + "/" + fileData.Specification);
            var fileSrc = fileData.PicWebDomain + fileData.FilePathName;
            $('#pics').append('<img width="80px" height="80px" src="' + fileSrc + '" alt="" class="img-rounded">');
            $('#pics').append('<input type="hidden" id="Pics[' + picIndex + '].PicWebDomain" name="Pics[' + picIndex + '].PicWebDomain" value="' + fileData.PicWebDomain + '" />');
            $('#pics').append('<input type="hidden" id="Pics[' + picIndex + '].FilePathName" name="Pics[' + picIndex + '].FilePathName" value="' + fileData.FilePathName + '" />');
            $('#pics').append('<input type="hidden" id="Pics[' + picIndex + '].Specification" name="Pics[' + picIndex + '].Specification" value="' + fileData.Specification + '" />');
            picIndex++;
        }
    });

    function setTag(tag) {
        $('#Tag').val(tag);
    }
</script> 
