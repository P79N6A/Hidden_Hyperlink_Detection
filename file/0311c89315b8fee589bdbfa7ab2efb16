  <script type="text/javascript">
function AdvClick(){
	  var a=500;  
	  $(".dt_toSmall").click(function(){
	    $(".dt_big").stop().slideUp(a);
	    $(".top_link").delay(a).animate({right: '0'},1000);
	  });
	  $(".dt_toBig").click(function(){
	    $(".dt_big").slideDown(1000);
	    $(".top_link").delay(a).animate({right: '-97'},400);
	  })  
}
function AdvAuto(){
	  $(".dt_big").delay(500).slideDown(1500);
	    var a=1500;
	    var b=12*1000;
	    $(".dt_big").delay(b).slideUp(a,function(){
	      $(".top_link").animate({right: '0'});
	    });
	    setTimeout("AdvClick()",2000); 
}
//顶部通览可展开收起效果
</script> 
