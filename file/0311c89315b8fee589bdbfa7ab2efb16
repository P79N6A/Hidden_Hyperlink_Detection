      function  addFav() {   //加入收藏夹   
		if  (document.all) {   
			window.external.addFavorite('http://www.ecduo.cn', '多商网-珠三角工厂货源批发市场');   
		}else if  (window.sidebar) {   
			window.sidebar.addPanel('多商网-珠三角工厂货源批发市场', 'http://www.ecduo.cn',  "" );   
		}
	  }
      
      function ReadMsg(read_id) {
    	  $.ajax({
    		  url:"/new_index.php?app=member&act=set_messages_read",
    		  data:{'mess_id' : read_id},
    		  type : 'POST',
    		  dataType :'json',
    		  success:function(mess){
    			  if(mess.status == '200') {
        			 location.replace(location.href);
    			  }
    				 
    		  }
    	  });
      }
      
      $(document).ready(function(){
    	    $('.ecd-index-nav-chn>li').hover(function(){
    	        $(this).addClass('navcurb').find('.hidden-box').slideDown(200);
    	    },function(){
    	        $(this).removeClass('navcurb').find('.hidden-box').hide();
    	    });
    	});	
