  <script>
        function search() {
       		var search = $.trim($('#q_keyword').val());
       		if (search) {
				$.ajax({
					url:'/new_index.php?app=search&act=tags&kw='+search,
					type:'get',
					async:true,
				});
       			var link = '/glist-k_'+search+'-d_0.html';
       			window.location.href = link;
       		}
        }
        function enterPress(e) {
        	if (e.keyCode == 13) {
        		search();
            }
        }

        $(function(){
           return; 
           var oLogin=document.getElementById('search');
           var oLogin_text=getByClass(oLogin,'search-keyword')[0];
           var oLogin_div=getByClass(oLogin,'search_suggest')[0];
           var oLogin_ul=oLogin_div.getElementsByTagName('ul')[0];
         
         //console.log(oLogin_div);
           var ie=!-[1,];
           var judge=true;
           var iNow=-1;

        
           oLogin_text.onfocus=function(){
              oLogin_ul.style.display='block';
           }
          
           oLogin_text.onblur=function(){
                         oLogin_ul.style.display='none';
          }

  oLogin_text.onkeydown=function(ev)
        { 

          var ev=ev||window.event;
         var oLogin_li=oLogin_ul.getElementsByTagName('li');
        if(ev.keyCode==38)
        {if(iNow==0){iNow=oLogin_li.length-1}
        else{iNow--;}
        for(var i=0;i<oLogin_li.length;i++)
        {oLogin_li[i].className='';}
        oLogin_li[iNow].className='active';
        }
        else if(ev.keyCode==40)
        {if(iNow==oLogin_li.length-1){iNow=0}
        else{iNow++;}
        for(var i=0;i<oLogin_li.length;i++)
        {oLogin_li[i].className='';}
        oLogin_li[iNow].className='active';}
        else if(ev.keyCode==13)
        {
          oLogin_text.value=oLogin_li[iNow].innerHTML;
         window.location.href = '/glist-k_'+oLogin_li[iNow].innerHTML+'-d_0.html';
         iNow=-1;
        }
        }

          function hasli(){
             var oLogin_li=oLogin_ul.getElementsByTagName('li');
                oLogin_ul.style.height=oLogin_li[0]*oLogin_li.length;
                for(var i=0;i<oLogin_li.length;i++)
                { 
                    oLogin_li[i].index=i;
                    oLogin_li[i].onmouseover=function(){
                    for(var j=0;j<oLogin_li.length;j++)
                      {oLogin_li[j].className='';}
                      if(oLogin_text.value!='')
                      {this.className='active';
                      iNow=this.index;}}
                      oLogin_li[i].onmousedown=function()
                      { 
                        oLogin_text.value=this.innerHTML;
                        window.location.href = '/glist-k_'+this.innerHTML+'-d_0.html';
                      }
                }
        }
 
        if(ie){oLogin_text.onpropertychange=tochange;}
        else{oLogin_text.oninput=tochange;}
        function tochange()
        {
              var search = $.trim($('#q_keyword').val());
              $.ajax({
                url:'/includes/openSearch/search_note.php?query='+search,
                type:'get',
                async:true,
                dataType: "json",
                success: function(data){
                  if(data.status=='OK'){
                      var result = data.result;
                      var length = result.length;
                      if(length>0){
                          var html='';
                          for(var i=0;i<length;i++){
                             html = html+ '<li data='+ result[i] +'>' + result[i] + '</li>';
                             //console.log(html);
                          }
                          $('.search_suggest ul').html(html);
                           hasli();
                          oLogin_ul.style.display='block';

                      }
                  }
                }
              });
        }

         function getByClass(oParent,sClass)
         {   
           var sResult=[];
           var re=new RegExp('\\b'+sClass+'\\b','i');
          var oEle=oParent.getElementsByTagName("*");
           for(var i=0;i<oEle.length;i++){
           if(re.test(oEle[i].className)){sResult.push(oEle[i]);}
           }
            return sResult;
           }
        })
	</script> 
