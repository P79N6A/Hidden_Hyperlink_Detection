  <script>
function ajax_login() {
	var uname=$('#uname').val();
	var pass=$('#pass').val();
	if(uname==''){
		$('#uname_span').attr({"class": "Validform_checktip Validform_wrong"});
		$('#uname_span').html('请填写用户名！');
	 	return false;
	}
 	if (pass=='') {
		$('#pass_span').attr({"class": "Validform_checktip Validform_wrong"});
		$('#pass_span').html('请填写密码！');
	 	return false;
 	}
	$('#uname_span').removeAttr('class')
	$('#uname_span').html('');
	$('#pass_span').removeAttr('class');
	$('#pass_span').html('');
	$.ajax({  
		url: '/new_index.php?app=member&act=ajax_login',
		data:{
			"username":uname,
			"password":pass
			},
		type:'POST',   
		dataType: 'json',
		success: function(json){
			if (json.status==200) {
			 	$('#uname_span').attr({"class": "Validform_checktip Validform_right"});
				$('#uname_span').html('通过信息验证！');
				$('#pass_span').attr({"class": "Validform_checktip Validform_right"});
				$('#pass_span').html('通过信息验证！');
				location.reload();
			} else {
				$('#uname_span').attr({"class": "Validform_checktip Validform_wrong"});
				$('#uname_span').html('用户名或密码错误！');
			 	return false;	
			}
		}
 	})
 	return false;
}
$(function(){
	// 避免用户产生幻觉
	$('#uname').blur(function(){
		$('#uname_span').removeAttr('class')
		$('#uname_span').html('');
	});
	// 避免用户产生幻觉
	$('#pass').blur(function(){
		$('#pass_span').removeAttr('class')
		$('#pass_span').html('');
	});

})
function show_login_box() { 
	document.getElementById("bg").style.display ="block";
	document.getElementById("loginshow").style.display ="block";
 }
function hide_login_box() { 
	document.getElementById("bg").style.display ="none";
	document.getElementById("loginshow").style.display ="none";
 }
 </script> 
