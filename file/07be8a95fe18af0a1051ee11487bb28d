   <script type="text/javascript">
    
    $(function(){
        $('#hm_backtop li').hover(
            function(){
                $(this).find('div.backbox').show();
                $(this).find('s').removeClass("alpha");
            },
            function(){
                $(this).find('div.backbox').hide();
                $(this).find('s').addClass("alpha");
            }
        );
        goto_top();
    });

	function createAjax() {			
		var _xmlhttp;
		try {	
			_xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");	
		}
		catch (e) {
		try {
			_xmlhttp=new XMLHttpRequest();	//
		}
		catch (e) {
			_xmlhttp=false;		//
		}
	}
	return _xmlhttp;	//
	}
	
	function gettptitle(val)
	{
		if (val==""){
			alert("请填写反馈内容，谢谢！");
			return false;
			}
		var url='/TP_Float_op.asp?tptitle='+val;
		var xmlhttp=createAjax();	
		if (xmlhttp) {		
	
			xmlhttp.open('get',url,true);	
			xmlhttp.onreadystatechange=function() {		
				if (xmlhttp.readyState==4 && xmlhttp.status==200) {	
					document.getElementById("td_tpbtn_check").innerHTML="";
					document.getElementById("tptitle").value="感谢您的参与，欢迎继续使用我们网站，谢谢！";
				}
				else {
					document.getElementById("td_tpbtn_check").innerHTML="";
					document.getElementById("tptitle").value="网络繁忙，请稍后再试，谢谢！";
				}
			}
			xmlhttp.send(null);
		}
	 
	}
</script> 
