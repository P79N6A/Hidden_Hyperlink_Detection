	function checklogin(){
		if(islogin==false){
			art.dialog.alert("您尚未登录");
			setTimeout("gotologin()",2000);
		}
	}
	function get_employinfo(to_userid,page){
		if(!page) page = 1;
		$.post('http://tu.vx.com/index.php?m=index&a=getemployinfo',{'to_userid':to_userid,'page':page},function(data){
			var _lists = data.lists;
			var html;
			var num = (page-1)*10+1;
			if(_lists){
				for(i in _lists){
					html += "<tr><td align='center'>"+num+"</td><td align='center'><a data-userid='"+_lists[i]['userid']+"' href='javascript:void(0);' class='ds-user-name ds-highlight showtuserinfo'>"+_lists[i]['nickname']+"</a></td><td align='center'>"+_lists[i]['money']+"</td><td align='center'>"+_lists[i]['addtime']+"</td></tr>";
					num++;
				}
				$('#employinfo tbody').html(html);
				$('.pages').html(data.pages);
			}
		});
	}
	$('.pages a').live('click',function(){
		var page = $(this).attr('href');
		get_employinfo(188545,page);
		return false;
	});
	$('#hiredBtn').click(function(){
		$('#defaultForm').bootstrapValidator('validate');
		if($('#defaultForm').data('bootstrapValidator').isValid()){
			$.post($('#defaultForm').attr('action'),$('#defaultForm').serialize(),function(data){
				if(data.status == 1){
					art.dialog.success(data.info);
					$('#defaultForm').data('bootstrapValidator').resetForm(true);
					get_employinfo(188545);
				}else
					art.dialog.error(data.info);
			});
		}
		return false;
	});
</script> 
  <script type="text/javascript">
var btn = document.getElementById("sendBtn");
btn.onclick = function(){
	var phone = $('#phone').val();
	if (phone == "") {
		art.dialog.error("请输入您的手机号码");
		return false;
	}
	$.getJSON(GV.SITE_ROOT+"api.php?a=sms&phone="+phone+"&action=employ&tempid=91&callback=?",function(data){
		if(data.status == 1){
			art.dialog.success(data.info);
			sms.init(btn);
		}else{
			art.dialog.error(data.info);
		}
	});
	return false;
};
</script> 13 
