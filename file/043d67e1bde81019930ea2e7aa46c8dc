        function callAnalytics(trackingCode, userSegment, trackPage, transactionAdd, transactionItem) {
            ga('create', trackingCode, 'auto');
            ga('set', 'dimension1', userSegment);
            ga('send', 'pageview', trackPage);
        
            if(transactionAdd != null){
