  <!-- jQuery (necessary for Bootstrap's JavaScript plugins) --> 
  <script src="http://cdn.bootcss.com/jquery/1.11.3/jquery.min.js"></script> 
  <!-- Include all compiled plugins (below), or include individual files as needed --> 
  <script src="js/bootstrap.min.js"></script> 
  <script>
      
      $(document).ready(function(){

    	   $('#wxad').hover(function(){
    	                     $("#wxadpic").show();
    	                      },function(){
    	                    	  $("#wxadpic").hide();
    	                         });
    	   
    	   $('#login').hover(function(){
               $("#login_k").show();
                },function(){
              	//  $("#login_k").hide();
                   });
    	  
    	   
     
    		   $("#sch").click(function(){
    			 
    			   $("#search-form").toggle();
    		   });
    			//搜索功能
    			$("#search").click(function(){
    				var url = $(this).attr('url');
    		       var query  = $('.search-form').find('input').serialize();
    		       query = query.replace(/(&|^)(\w*?\d*?\-*?_*?)*?=?((?=&)|(?=$))/g,'');
    		       query = query.replace(/^&/g,'');
    		       if (query == '' || query == null || query == undefined) {
    		    	   alert("请输入要搜索的关键词！");
    		    	   return true;
    		    	   } else {
    		    	 
    		    	
    		     
    		    	   
    		       if( url.indexOf('?')>0 ){
    		           url += '&' + query;
    		       }else{
    		           url += '?' + query;
    		       }
    				window.location.href = url;
    		    	   }
    			});
    			//回车搜索
    			$(".search-input").keyup(function(e){
    				if(e.keyCode === 13){
    					$("#search").click();
    					return false;
    				}
    			});


    	 
    	});
      </script> 
  <script type="text/javascript" src="http://cdn.staticfile.org/jquery.qrcode/1.0/jquery.qrcode.min.js"></script> 
  <script type="text/javascript">
var detail_url="http://www.chuangribao.com/";
 
 $(document).ready(function(){

	   $('.share_weixin').mouseover(function(){ 
		     var id= $(this).attr("data_id");
		     var data_url= $(this).attr("data_url");
		     var data_text= $(this).attr("data_text");
		   $("#weixin_qrcode_dialog_"+id).show();
		   $('#qrcode_id_'+id).empty();
		   jQuery('#qrcode_id_'+id).qrcode({width: 130,height: 130,text: data_url});

	   });
	   $('.weixin_popup_close').click(function(){ 
		   var id= $(this).attr("data_id");
		   $(this).parent().parent().hide();
		$('#qrcode_id_'+id).empty();
	   });
	   
	   $('.share_weixin').mouseleave(function(){ 
		   var id= $(this).attr("data_id");
		   $("#weixin_qrcode_dialog_"+id).hide();
		$('#qrcode_id_'+id).empty();
	   });
 });
 </script>  
