  <script type="text/javascript">
    function SearchOnFocus() {
        var searchinput = document.getElementById("textfield");
        if (searchinput.value == "此处按剧本名搜索") { searchinput.value = ""; }
    }
    function SearchOnBlur() {
        var searchinput = document.getElementById("textfield");
        if (searchinput.value.replace(/(^\s+)|(\s+$)/g, "").length <= 0) { searchinput.value = "此处按剧本名搜索"; }
    }
    function showClassList() {
        var dropdiv = $("#class_cnt1");
        if (dropdiv.is(":hidden")) {
            dropdiv.slideDown();
        } else {
            dropdiv.slideUp();
        }
    }
    function SearchCheck() {
        var searchinput = document.getElementById("textfield");
        if (searchinput.value == "此处按剧本名搜索" || searchinput.value.length == 0) { alert("请输入要搜索的内容"); return false; }
        return true;
    }

    $(function() {
        //顶部加载
        $.ajax({
            type: "post",
            url: "/ajax/OptionProcess.ashx?optiontype=GetLoginStatus&random=" + Math.random(),
            data: null,
            async: true,
            cache: false,
            success: function(msg) {
                if (msg != "nologin") {
                    $("#top_r").html("<a href=\"/myhome/homepage.aspx\">[" + msg + "&nbsp;个人中心]</a> &nbsp;<a href='?ext=1'>退出</a>");
                    //$("#top_r").html("<a href=\"/myhome/homepage.aspx\">个人中心</a> &nbsp;<a href=\"/login/register.aspx\">注册</a> | <a href=\"/login/login.aspx\">登录</a>");
                }
            },
            error: function(e) { window.location = "/login/login.aspx"; }
        });
    });
</script> 
