  <script>
    $(function() {
        var hit = 10; // 显示的下拉提示列表的长度，可修改
        setTimeout(function() {
            $( "#alySearch" ).autocomplete({
                delay: 0,
                source: function(request, response) {
                    $.ajax({
                        url: "sugggest.php"/*输入你的serverUrl*/ ,
						//跨域请求
                        xhrFields: {
                            withCredentials: true
                        },
						//数据类型
                        dataType: 'json',//如果需要为jsonp类型，则需要在下面的data属性中加上callback: ?
                        data: {query: $( "#alySearch" ).val()},
                        success: function(data) {							
                            if(data.status === 'OK' && data.result) {
                                if(data.result.length >= hit) {
                                    response(data.result.slice(0, hit));
                                } else {
                                    response(data.result);
                                }
                            } else if( data.status === 'FAIL' && data.errors ){
                                //alert(data.errors[0].message);
                            }
                        }

                    });
                }
            }).bind("input.autocomplete", function () {
                // 修复在Firefox中不支持中文的BUG
                $( "#alySearch" ).autocomplete("search", $( "#alySearch" ).val());
            }).focus();
        }, 0);
    });
</script> 
