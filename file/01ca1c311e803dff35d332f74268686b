$(function(){
	//澶撮ㄤ绾ц
	$('.topulnav li').hover(function(){
		$(this).find('.childnav').show();
		$(this).addClass('on');
	},function(){
		$(this).find('.childnav').hide();
		$(this).removeClass('on');
	})	
	
	//澶撮ㄦ绱㈡?
	$('#searchkeywords').on('focus',function(){
		if($(this).val()=='杈ョ濂宠杞...'){
			$(this).val('');
			$(this).css('color','#f1f1f1');
		}
	}).on('blur',function(){
		if($(this).val()==''){
			$(this).val('杈ョ濂宠杞...');
			$(this).css('color','#9f9f9f');
		}
	})
})
</script> 
  <!-- Simple Lazyload 2.7 - css and js --> 
  <style type="text/css">
.sl_lazyimg{
opacity:0.1;filter:alpha(opacity=10);
background:url(/skin/ecms065/images/loading2.gif) no-repeat center center;
}
</style> 
