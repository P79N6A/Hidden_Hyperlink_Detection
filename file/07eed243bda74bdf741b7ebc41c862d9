   <script type="text/javascript">
$(function(){
    $('.pg,.page').hide();
    $('.add-more').click(function(event) {
        /* Act on the event */
        var _maxPage = $('.max_page').val();
        var _currUrl = $('.curr_url').val();
        var _pageNum = $('.page_num').val();
        if(parseInt(_pageNum) > parseInt(_maxPage)){
            $(this).remove();
            //alert("已经是最后一页了");
            return false;
        }
        _href = _currUrl+"&page="+_pageNum;
        if(_href != null)
        auto_load_postlist(_href);
        $('.page_num').val(parseInt(_pageNum)+1);
    });
    

    var range = 50; //距下边界长度/单位px
    var totalheight = 0;
    jQuery(window).scroll(function(){
        var srollPos = jQuery(window).scrollTop();    //滚动条距顶部距离(页面超出窗口的高度)
        totalheight = parseFloat($(window).height()) + parseFloat(srollPos);
        if((jQuery(document).height()-range) <= totalheight) {
            //jQuery('.add-more').click();
        }
    });
});

$('.favbtn').on('click', function() {
var obj = $(this);
$.ajax({
type:'POST',
url:obj.attr('href') + '&handlekey=favbtn&inajax=1',
data:{'favoritesubmit':'true', 'formhash':'20c88091'},
dataType:'xml',
})
.success(function(s) {
popup.open(s.lastChild.firstChild.nodeValue);
evalscript(s.lastChild.firstChild.nodeValue);
})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
});
return false;
});
</script> 
   <script type="text/javascript">
    jQuery(function(){
        jQuery('.do_comment_add').click(function(event) {
            if(!0){
                window.location.href='member.php?mod=logging&action=login';
            }
            /* Act on the event */
            var _hash = '20c88091',
            _tid = $(this).attr('data-tid'),
            _url = 'forum.php?mod=misc&action=recommend&do=add&tid='+_tid+'&hash='+_hash+'&ajaxmenu=1&inajax=0&ajaxtarget=recommend_add_menu_content';
            $.ajax({
                url: _url,
                type: 'GET',
                dataType: 'html',
            })
            .always(function(data){
                /*optional stuff to do after success */
                var res = $(data).find('.jump_c').find('p').html();
                var new_rec = $('.num_'+_tid).html();
                if(jQuery(data).find('#recommentv').html() != undefined){
                    jQuery('.num_'+_tid).html(parseInt(new_rec)+1);
                    // 自动回复
                    $('input[name="message"]').val('感谢楼主分享');
                    $('#fastpostform').submit();
                }
                var _msg = setContent(res);
                popup.open(_msg,'alert');
            });
        });
        jQuery('.do_reply').click(function(){
            $('#fastpostmessage').focus();
        })
    })
</script> 
