     <script>
	function Show_Tab2s(id)
	{
		$("#"+id).addClass('hover');
		$("#m_c_"+id).show();
	}
	function hide_menu_level1(id)
	{
		$("#"+id).removeClass('hover');
		$("#m_c_"+id).hide();
	}
	function Showlayer(id)
	{
		$("#"+id).addClass('hover');
		$("#m_c_"+id).show();
	}
	function hide_menu_level2(id)
	{
		$("#"+id).removeClass('hover');
		$("#m_c_"+id).hide();
	}

	function HiddenDiv(id)
	{
		$("#"+id).removeClass('hover');
		$("#m_c_"+id).hide();
	}

</script> 
