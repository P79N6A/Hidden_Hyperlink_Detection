(function() {
$('.sub_forum').css('display', 'block');
$('.subforumshow').on('click', function() {
var obj = $(this);
var subobj = $(obj.attr('href'));
if(subobj.css('display') == 'none') {
subobj.css('display', 'block');
obj.find('img').attr('src', './template/zds_touch/touch/images/img/collapsed_yes.png');
} else {
subobj.css('display', 'none');
obj.find('img').attr('src', './template/zds_touch/touch/images/img/collapsed_no.png');
}
});
 })();
</script> 
