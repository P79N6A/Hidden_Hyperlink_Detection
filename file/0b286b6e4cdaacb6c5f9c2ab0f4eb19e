  <script>
$(".kai").click(function(){
    $(".simple-dec").css("height","auto");
})
$(".addfav").click(function(){
    var obj = $(this);
    $.ajax({
        url:'/Share/favorite',
        data:{aid:46441,title:"B人妻解放区",type:1,url:"/Dongman/arc/id/46441.html"},
        dataType:"text",
        type:"post",
        success:function(da){
            if (da == 'nologin') {
                show("请先登陆");
                return false;
            }else if (da == 'exists') {
                show("已经收藏过了，不能重复收藏！");
                return false;
            }else if (da == 'error') {
                show("收藏失败，请稍后重试！");
                return false;
            }else if (da == 'success') {
                $(obj).find("num").text(parseInt($(obj).find("num").text())+1);
                show("收藏成功！");
                return false;
            }else{
                show("操作失败!");
                return false;
            }
        }
    });
});
</script> 
