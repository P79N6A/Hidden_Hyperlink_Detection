<script>
$(function(){
        //底层页
        var qp_inde_show;
        if ($.cookie('qp_show') ==null ) 
        {
            $(".jclose_show").show();
            qp_inde_show=setTimeout(function(){$(".jclose_show").hide();},15000);
            $.cookie('qp_show','1',{expires:365,path:'/',domain:'rayli.com.cn'});
        }
	$(".jclose_show img").click(function(){
		clearTimeout(qp_inde_show);
		$(this).parent().hide();
                $.cookie('qp_show','1',{expires:365,path:'/',domain:'rayli.com.cn'});
	});
        //列表页
        var qp_inde_list;
        if ($.cookie('qp_list') ==null ) 
        {
            $(".jclose_list").show();
            qp_inde_list=setTimeout(function(){$(".jclose_list").hide();},15000);
            $.cookie('qp_list','1',{expires:365,path:'/',domain:'rayli.com.cn'});
        }
        
	$(".jclose_list img").click(function(){
		clearTimeout(qp_inde_list);
		$(this).parent().hide();
                $.cookie('qp_list','1',{expires:365,path:'/',domain:'rayli.com.cn'});
	});
        //首页
        var qp_inde;
        if ($.cookie('qp_index') ==null ) 
        {
            $(".jclose_index").show();
            qp_inde=setTimeout(function(){$(".jclose_index").hide();},15000);
            $.cookie('qp_index','1',{expires:365,path:'/',domain:'rayli.com.cn'});
        }
	$(".jclose_index img").click(function(){
		clearTimeout(qp_inde);
		$(this).parent().hide();
                $.cookie('qp_index','1',{expires:365,path:'/',domain:'rayli.com.cn'});
	});
})
</script>
