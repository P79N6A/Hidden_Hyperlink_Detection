     <!-- 文章标题 --> 
     <h1 itemprop="headline">水井坊遭证监会立案调查 或面临巨额赔偿</h1> 
     <!-- 正文标签 --> 
     <div class="label-ad clear" id="commentTab"> 
      <div class="label"> 
       <ul> 
