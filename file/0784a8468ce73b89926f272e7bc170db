   <script language="JavaScript" type="text/javascript">
    var vcode;
    var _interval;
    function  getVcode() { 
        var phoneFormat = /^1[3|4|5|7|8][0-9]\d{8}$/gi;
        if(!phoneFormat.test($("#mobile").val())){
            alert('请输入正确的手机号码！'); return;
        }
        if($(".a-verify").hasClass("bg-grey")) {
            return;
        }
        vcode = parseInt((Math.random() * 9 + 1) * 1000).toString();    
        $.ajax({
            type : "POST",
            url : sendRegSms_url1,
			data:{
				"mobile":$("#mobile").val(),
				"vcode":vcode
			},
            async: false,
            success : function(data) {
				//var datas = eval('('+data+')');
				console.log(data);
                //更改样式
                $(".a-verify").addClass("bg-grey");
                var i = 60;
                $(".a-verify").html(i + "秒后重试");
                _interval = setInterval(function () {
                    $(".a-verify").html((--i) + "秒后重试");
                    if (i == 0) {
                        clearInterval(_interval);
                        isCode = true;
                        $(".a-verify").html("获取验证码");
                        $(".a-verify").removeClass("bg-grey");
                    }
                }, 1000);
            }
        });
    }
lastScrollY=0;
function heartBeat(){
var diffY;
if (document.documentElement && document.documentElement.scrollTop)
diffY = document.documentElement.scrollTop;
else if (document.body)
diffY = document.body.scrollTop
else
{/*Netscape stuff*/}
//alert(diffY);
percent=.1*(diffY-lastScrollY);
if(percent>0)percent=Math.ceil(percent);
else percent=Math.floor(percent);
document.getElementById("ALayer1").style.top=parseInt(document.getElementById("ALayer1").style.top)+percent+"px";
document.getElementById("ALayer2").style.top=parseInt(document.getElementById("ALayer1").style.top)+percent+"px";
lastScrollY=lastScrollY+percent;
//alert(lastScrollY);
}
/*suspendcode12="<DIV id=\"ALayer1\" style='left:10px;PosITION:absolute;TOP:70px;FILTER: alpha(opacity=85);z-index: 99999;'><div align=left><img src='./tpl/Web/default/common/images/close.gif' border=0 onclick='closeBanner();' /></div><img src='./tpl/Web/default/common/images/01.jpg' width='100' /></div>"
suspendcode14="<div id=\"ALayer2\" style='right:10px;PosITION:absolute;TOP:70px;z-index: 99999;FILTER: alpha(opacity=85);'><div align=right><img src='./tpl/Web/default/common/images/close.gif' border=0 onclick='closeBanner();' /></div><img src='./tpl/Web/default/common/images/2.jpg' width='100' /></div>"
document.write(suspendcode12);
document.write(suspendcode14);
window.setInterval("heartBeat()",1);
function closeBanner()
{
document.getElementById("ALayer1").style.display= 'none';
document.getElementById("ALayer2").style.display= 'none';
}
