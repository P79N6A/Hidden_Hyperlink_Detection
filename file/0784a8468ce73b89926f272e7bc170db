   <script type="text/javascript">
function setTab(o, f) {
	$(o).parent().find("a").removeClass('on');
	$(o).addClass('on');
	if(f) f(o);
}

$(document).ready(function(){
	$(".news_tabs .society_right_title a").mouseover(function(){
		setTab(this, function(o){
			$(o).parents(".society_right_channel").find(".artical_channel").css("display","none");
			$(o).parents(".society_right_channel").find(".artical_channel").eq($(o).parent().find("a").index($(o))).css("display","");
		});
		return false;
	});
});
</script> 
   <style type="text/css">
.news_content table{ width:100%; box-sizing:border-box; border:1px solid #000; border-bottom:none; border-right:none}
.news_content table td{border-bottom: 1px solid;
    border-right: 1px solid;}
</style> 
