$("#formReg").validate({
	submitHandler:function(){
		if(!$('#regProtoctol').prev().is(':checked')){
			alert('你需要同意遵守烟台传媒网(官网)-烟台新闻门户网站，掌上烟台，手机烟台，数字烟台使用协议');return false;
		}
		$.ajax({
			cache: true,
			type: "POST",
			url: "/index.php?g=Web&m=Members&a=reg",
			data: $('#formReg').serialize(),
			async: false,
			error: function(request) {
				alert("注册失败");
			},
			success: function(data) {
				data = eval('('+data+')');
				if(data.status == 0) {
					alert(data.info);
				} else {
					alert(data.info);
					document.location.href = "/index.php?g=Web&m=Index&a=index";
				}
			}
		});
	}  
});

$("#formLogin").validate({
	submitHandler:function(){
		$.ajax({
			cache: true,
			type: "POST",
			url: "/index.php?g=Web&m=Members&a=login",
			data: $('#formLogin').serialize(),
			async: false,
			error: function(request) {
				alert("登录失败");
			},
			success: function(data) {
				data = eval('('+data+')');
				if(data.status == 0) {
					alert(data.info);
					$('#verifyLi').show();
					$('.gt_refresh_button')[0].click();
					//setTimeout('location.reload();', 0);
				} else {
					if($('#loginType').val() == 1){
						CloseWinBox('loginbox');
						$('#loginInfo').text(data.name);
						alert('登陆成功！');
					}else{
						//document.location.href = "/index.php?g=Web&m=Index&a=index";
						location.reload();
					}
				}
			}
		});
	}  
});

</script> 
