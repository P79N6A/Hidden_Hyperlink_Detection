  $('.arrow-right').on('click', function(e){
    e.preventDefault();
    mySwipe.swipePrev();
  });
  $('.arrow-left').on('click', function(e){
    e.preventDefault();
    mySwipe.swipeNext();
  });
</script> 
  <script>
  var mySwip = new Swiper('.swiper-contain',{
    pagination: '.pagination',
    loop:true,
    grabCursor: true,
    paginationClickable: true,
    autoplay:2000,
	speed :1000
  });
  $('.arrow-right-r').on('click', function(e){
    e.preventDefault();
    mySwip.swipePrev();
  });
  $('.arrow-left-l').on('click', function(e){
    e.preventDefault();
    mySwip.swipeNext();
  });
</script> 
