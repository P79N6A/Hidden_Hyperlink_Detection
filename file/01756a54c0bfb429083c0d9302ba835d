   <script type="text/javascript">
    var flag = true;
    function star_wink() {
        if (flag) {
            document.title = '您有 0 个新消息';
            flag = false;
        }
        else {
            document.title = '';
            flag = true;
        }
        setTimeout("star_wink()", 1000);
    }

	setTimeout(function(){
		$('div.userMenu').load('/checklogin.aspx?_r=' + Math.random());
