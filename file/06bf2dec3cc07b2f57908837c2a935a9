$(function(){
	jQuery('.lightbox').lightbox();
});
$(window).scroll(function(){ 
    var $scrollTops = $(this).scrollTop(),$goTopBtn=$('#goTop');
    $scrollTops>0?$goTopBtn.show():$goTopBtn.hide();//返回顶部
    $('#goTop').click(function(){$(document).scrollTop(0);});
});
</script> 
  <script>
$(function(){
  var _top = $('.content-tag').offset().top;                              //获取offsetTop
  $(window).scroll(function(){                                            //滚动
      var _scroll = $(window).scrollTop();                                //滚动条高度
      if(_scroll>=_top){                                                                            //判断当大于等于对象的offsetTop的时候
           if ($.browser.msie && ($.browser.version == "6.0") && !$.support.style) {                //针对IE6的判断
              $('.content-tag').css({'top':_scroll});
           }else{
              $('.content-tag').css({'position':'fixed','top':'0','margin-top':'0px'});
           }
      }else{
          if ($.browser.msie && ($.browser.version == "6.0") && !$.support.style) {                //针对IE6的判断
              $('.content-tag').css({'top':'550px'});
          }else{
              $('.content-tag').css({'margin-top':'0','position':'absolute','top':'auto'});
          }
      }
   });
})

$(window).scroll(function(){
	index1 = $("#tabs_content0").offset().top-40;
	index2 = $("#tabs_content1").offset().top-40;
	index3 = $("#tabs_content2").offset().top-40;
	index4 = $("#tabs_content3").offset().top-40;
	index5 = $("#tabs_content4").offset().top-40;
	index6 = $("#tabs_content5").offset().top-40;
	index7 = $("#tabs_content6").offset().top-40;
	index8 = $("#tabs_content7").offset().top-40;
	index9 = $("#tabs_content8").offset().top-40;
	var scrolltop = $(window).scrollTop()+40;
	//document.write(index1);
	if(scrolltop < index1){
		indexnum = 0;
	}else if(index1 < scrolltop && scrolltop < index2){
		indexnum = 0;
	}else if(index2 < scrolltop && scrolltop < index3){
		indexnum = 1;
	}else if(index3 < scrolltop && scrolltop < index4){
		indexnum = 2;
	}else if(index4 < scrolltop && scrolltop < index5){
		indexnum = 3;
	}else if(index5 < scrolltop && scrolltop < index6){
		indexnum = 4;
	}else if(index6 < scrolltop && scrolltop < index7){
		indexnum = 5;
	}else if(index7 < scrolltop && scrolltop < index8){
		indexnum = 6;
	}else if(index8 < scrolltop && scrolltop < index9){
		indexnum = 7;
	}else if(scrolltop > index9){
		indexnum = 8;
	}
	$("#tabs li a").removeClass("on-pico0").eq(indexnum).addClass("on-pico0");
})

function navmove(id,index){
	if(id == "#tabs_content0"){ var headheight = 0; }else{ var headheight = 0; }
	var offsettop = $(id).offset().top - headheight-40;
	$('html,body').stop().animate({scrollTop: offsettop},0, 'easeInOutQuint');
	return false;
}

function selfmove(){
	var navId = "";
	if(window.location.href.indexOf("#tabs_content0") > 0 ){navId = "#tabs_content0";}
	if(window.location.href.indexOf("#tabs_content1") > 0 ){navId = "#tabs_content4";}
	if(window.location.href.indexOf("#tabs_content2") > 0 ){navId = "#tabs_content2";}
	if(window.location.href.indexOf("#tabs_content3") > 0 ){navId = "#tabs_content3";}
	if(window.location.href.indexOf("#tabs_content4") > 0 ){navId = "#tabs_content4";}
	if(window.location.href.indexOf("#tabs_content5") > 0 ){navId = "#tabs_content5";}
	if(window.location.href.indexOf("#tabs_content6") > 0 ){navId = "#tabs_content6";}
	if(window.location.href.indexOf("#tabs_content7") > 0 ){navId = "#tabs_content7";}
	if(window.location.href.indexOf("#tabs_content8") > 0 ){navId = "#tabs_content8";}
	if (navId != ""){
		$(window).scrollTop(0);
		$('html,body').animate({scrollTop:$(navId).offset().top - 0},0, 'easeInOutQuint');
	}
}
