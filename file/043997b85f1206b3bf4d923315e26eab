    function get_vcode_img()
    {
        $.ajax({
            type:"get",
            url:"http://chongqing.liebiao.com/article/captcha/?refresh=1",
            dataType: "json",
            success:function (data) {

                var imgurl = data.url;
                imgurl1 = imgurl.replace("?v=", "/");
                imgurl1 = imgurl.replace("b2b", "chongqing");
                $(".vcode_img").attr("src", imgurl1);

            }
        });
    }
/*placeholder兼容ie8*/
    $(function(){
        <!--判断是否支持placeholder属性-->
        function isPlaceholderSupport() {
            return 'placeholder' in document.createElement('input');
        }
        var support=isPlaceholderSupport();
        var aInput = $('.yz-tishiyu .zxly-con input');
        function valChange(obj){
            var _this=$(obj);
            var label=_this.siblings('label');
            if(_this.val() == ""){
                label.css('display','block');
            }else{
                label.css('display','none');
            }
        }
        if(support == false){
            aInput.each(function(){
                var support=isPlaceholderSupport();
                this.onpropertychange=function(){
                    valChange(this);
                }
            })
        }
    });

</script> 
