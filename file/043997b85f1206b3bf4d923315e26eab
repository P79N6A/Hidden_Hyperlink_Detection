        function float_detail_menu($float_div_obj,$div_obj,className,$img_obj)
        {
            if($float_div_obj.length == 0 || $div_obj.length == 0) {
                return false;
            }
            $(window).on('scroll', function () {
                var doc_ScrollTop = $(this).scrollTop();
                var div_ScrollTop = $div_obj.offset().top-62;

                var dtop = [];
                dtop[0] = $("#xiangmu-js").offset().top-70;
                dtop[1] = $("#xiangmu-ys").offset().top-70;
                dtop[2] = $("#xiangmu-lc").offset().top-70;
                dtop[3] = $("#xiangmu").offset().top-70;
                dtop[4] = $("#ly-message").offset().top-70;
                dtop[5] = $("#remen").offset().top-70;

                if (doc_ScrollTop > div_ScrollTop) {
                    $float_div_obj.addClass(className);
                } else if (doc_ScrollTop <= div_ScrollTop) {
                    $float_div_obj.removeClass(className);
                }

                if(doc_ScrollTop > dtop[5]){
                    $float_div_obj.find('span').eq(5).addClass('current').siblings().removeClass('current');
                } else if(doc_ScrollTop > dtop[4]){
                    $float_div_obj.find('span').eq(4).addClass('current').siblings().removeClass('current');
                } else  if(doc_ScrollTop > dtop[3]){
                    $float_div_obj.find('span').eq(3).addClass('current').siblings().removeClass('current');
                } else if (doc_ScrollTop > dtop[2]) {
                    $float_div_obj.find('span').eq(2).addClass('current').siblings().removeClass('current');
                } else if (doc_ScrollTop > dtop[1]) {
                    $float_div_obj.find('span').eq(1).addClass('current').siblings().removeClass('current');
                } else if (doc_ScrollTop > dtop[0]) {
                    $float_div_obj.find('span').eq(0).addClass('current').siblings().removeClass('current');
                }

            });
        }

        float_detail_menu($(".zhaoshang-detail-div"),$("#xiangmu-js"),"zhaoshang-detail-div-cur",$('#xiangmu-ys'));
    });

