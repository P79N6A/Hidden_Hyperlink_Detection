  <script>
    $(".look-phone,#layer-phone").click(function(event){
        event.stopPropagation();
        var dianhuatanceng=layer.open({
            type: 1,
            closeBtn : false,
            shift: false,
            fix: false,
            skin: 'layer-ext-custoa',
            area: ['auto', 'auto'],
            shade: false,
            title: false,
            content: $("#layer-phone")
        });
        $("body,#btn-close").click(function(event){
            event.stopPropagation();
            layer.close(dianhuatanceng);
        });

        var saomasrc = $(".pic-saoma").attr("src");
        if(saomasrc){
            return false;
        }

        var postid = $(this).attr("data-postid");
        var phone = $(this).attr("data-phone");
        var url = "/ajax/weixinqrcode/";
        $.ajax({
            type: "post",
            url: url,
            data: {postid:postid, phone:phone},
            dataType: "json",
            success: function(data){
                $(".pic-saoma").attr("src", data.url);
            }
        });
    });
</script> 
  <script>
    var i=$('.message li').length;
    if(i==0){
        $('.messagebox').hide();
    }else if(i==1){
        $('.message').css('height',40+'px');
        $('.message-left').css('line-height',40+'px');
    }else if(i==2){
        $('.message').css('height',81+'px');
        $('.message-left').css('line-height',81+'px');
    }else if(i>=3){
        $('.message').css('height',122+'px');
        $('.message-left').css('line-height',122+'px');
    }
    if(i>3){
        var mes=$('.message ul');
        var orgHtml=mes.html();
        mes.html(orgHtml+orgHtml)
        var liLength=$('.message li').length;
        var timeId = null;
        var n=0;

        function meslook(){
            if(n==0){
                n=liLength-3;
            }else{
                n--;
            }
            mes.animate({'top':-41*n},100);
        }
        timeId=setInterval(meslook,4000);
        $('.message').hover(function(){
            clearInterval(timeId);
        },function(){
            timeId=setInterval(meslook,4000);
        });
    }
</script> 
