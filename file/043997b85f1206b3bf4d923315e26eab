    function settimer() {
        $('.errtip').hide();
        $('.zsjm-yfs').show();
        timer = setInterval(function(){

            var timeleft = $('.zsjm-yfs span').text();

            if(timeleft < 1){
                get_vcode_img();
                $('.zsjm-yfs').hide();
                if(ismyclick == 1){
                    $("#a-hqyzm").text("获取确认码");
                    $('.zsjm-yfs span').text('59')
                } else {
                    $('#a-hqyzm').text('重新获取');
                    $('.zsjm-yfs span').text('59')
                }
                clearInterval(timer);

            } else {
                timeleft = timeleft -1;
                $('.zsjm-yfs span').text(timeleft);
            }

        },1000);
    }


    //确认码文本显示参数
    var ismyclick = 0
    $(".btn-lytj").click(function(){

        var postid = "152280455";
        var subname = "新行业加盟";
        var msname = $("#zx_name input").val();
        var phone = $("#zx_tel input").val();
        var captcha = $("#zx_yzm input").val();
        var vcode = $(".vcode").val();
        var intentcity = $("#zx_city input").val();
        var message = $("#message").val();
        var err_num = 0;
        var phonepatt = new RegExp("1[34578]{1}[0-9]{9}$");

        if($.trim(message) == ''){
            $(".errtip").show();
            $(".errtip").html("请输入留言内容");
            err_num = 1;
        }

        if($.trim(captcha) == ''){
            $(".errtip").show();
            $(".errtip").html("请输入确认码");
            err_num = 1;
        }

        if($.trim(vcode) == ''){
            $(".errtip").show();
            $(".errtip").html("请输入验证码");
            err_num = 1;
        }

        if($.trim(phone) == ''){
            $(".errtip").show();
            $(".errtip").html("请输入正确的11位数手机号码");
            err_num = 1;
        } else if (!phonepatt.test(phone)){
            $(".errtip").show();
            $(".errtip").html("请输入正确的11位数手机号码");
            err_num = 1;
        }

        if(err_num == 1)
        {
            return false;
        }
        $(".errtip").hide();
        $(".errtip").html("");

        $.ajax({
            type:"post",
            url:"http://chongqing.liebiao.com/article/insertmessage/",
            data: {postid:postid,msname:msname,phone:phone,captcha:captcha,intentcity:intentcity,message:message,subname:subname},
            success:function (data) {

                layer.closeAll();

                if(data == 0){
                    layer.msg("留言成功", {icon: 1});
                    $('.zsjm-yfs').hide();
                    $('#a-hqyzm').text('获取确认码');
                    $("#zx_name input").val("");
                    $("#zx_tel input").val("");
                    $("#zx_yzm input").val("");
                    $("#zx_city input").val("");
                    $("#message").val("");
                    $(".vcode").val("");

                    $("#a-hqyzm").html("获取确认码");
                    ismyclick = 1;
                } else if (data == 2){
                    $(".errtip").show();
                    $(".errtip").html("确认码错误，请重新输入");
                }

            }
        });
    });

    $("#zx_tel input").keyup(function(){
        var phone = $(this).val();
        var phonepatt = new RegExp("1[34578]{1}[0-9]{9}$");

        $(".errtip").hide();
        $(".errtip").html("");

        if (phonepatt.test(phone)){
            $(".vcode_box").css("display", "block");
        } else {
            $(".vcode_box").css("display", "none");
        }
    });

    $("#a-hqyzm").click(function()
    {

        var phone = $("#zx_tel input").val();
        var vcode = $(".vcode").val();
        var err_num = 0;
        var phonepatt = new RegExp("1[34578]{1}[0-9]{9}$");
        var vcodepatt = new RegExp("[0-9a-zA-Z]{4}$");

        if($.trim(vcode) == ''){
            $(".errtip").show();
            $(".errtip").html("请输入验证码");
            err_num = 1;
        } else if (!vcodepatt.test(vcode)){
            $(".errtip").show();
            $(".errtip").html("验证码错误，请重新输入");
            err_num = 1;
        }

        if($.trim(phone) == ''){
            $(".errtip").show();
            $(".errtip").html("请输入正确的11位数手机号码");
            err_num = 1;
        } else if (!phonepatt.test(phone)){
            $(".errtip").show();
            $(".errtip").html("请输入正确的11位数手机号码");
            err_num = 1;
        }

        if(err_num == 1){
            return false;
        }

        $(".errtip").hide();
        $(".errtip").html("");
        $(".yzm_shadow").css("z-index", "999");

        //倒计时60秒
        settimer();

        $.ajax({
            type:"post",
            url:"http://chongqing.liebiao.com/article/sendsms/",
            data: {phone:phone,vcode:vcode},
            success:function (data) {

                layer.closeAll();

                if(data.error == 1){
                    clearInterval(timer);
                    $("#a-hqyzm").css("background", "#1783f0");
                    $(".yzm_shadow").css("z-index", "-1");
                    $(".errtip").show();
                    $(".errtip").html("您当天发送的确认码过多，请明天再试");
                    $('.zsjm-yfs').hide();
                    $('#a-hqyzm').text('获取确认码');
                    return false;
                } else if (data.error == 2){
                    clearInterval(timer);
                    $("#a-hqyzm").css("background", "#1783f0");
                    $(".yzm_shadow").css("z-index", "-1");
                    $(".errtip").show();
                    $(".errtip").html("验证码错误，请重新输入");
                    $('.zsjm-yfs').hide();
                    $('#a-hqyzm').text('获取确认码');
                    return false;
                } else {

                }

            }
        });
    });
