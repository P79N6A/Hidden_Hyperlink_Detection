    //快捷留言
    $('#msg p').each(function(){

        $(this).click(function(){
            var val = $('.textarea-txt').val();
            if(val.length<=200 && val.length!=0){
                $('.textarea-txt').val((val + "\n"+$(this).text()).substr(0,200));
            };
            if(val.length===0){
                $('.textarea-txt').val((val + $(this).text()).substr(0,200));
            }
        });
    });
    $('.textarea-txt').bind('input propertychange', function() {
        if($(this).val().length > 200){
            $(this).val($(this).val().substr(0,200));
        }
    });

    function validate(input,reg){
        if(reg.test(input.val())){
            return true;
        }
    }

    if($('#leavemsg').length>0 && $('#leavemsg tr').length>1){
        var max_h = (parseInt($('#leavemsg tbody').height())+1)*3;
        var scroll_h=parseInt($('#leavemsg tbody').outerHeight()+1);
        var count = $('#leavemsg tbody').length;
        if($('#leavemsg tbody').length>3){
            $('#leavemsg').css("height",max_h);
//留言切换方式为循环滚动
            function g_scroll (id,scrollheight,count,tag) {
                var container = $('#' + id),
                        scroll = container.find(tag);
                if (container.height() >= scroll.height()){
                    return false;
                }
                scroll.clone().addClass("clone").appendTo(container);
                var scrollFunc = function() {
                    var obj = container.find(tag+':first'),
                            h = null;
                    h = scrollheight * count;
                    if (obj.css('margin-top') === '-' + h + 'px') {
                        obj.appendTo(container);
                        container.find(tag).css('margin-top', 0);
                        container.find(tag+':first').animate({marginTop: '+=-' + scrollheight},300);
                        return false;
                    }
                    obj.animate({marginTop: '+=-' + scrollheight},300);
                };
                var t = setInterval(scrollFunc, 3000);
                container.mouseover(function() {
                    clearInterval(t);
                }).mouseout(function() {
                    t = setInterval(scrollFunc, 3000);
                })
            }
            g_scroll ('leavemsg',scroll_h,count,'table');
        }
    }

