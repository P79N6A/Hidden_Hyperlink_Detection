     <script type="text/javascript">
		function checkform(form) {
			var $form = $("#searchfrm");
			var q = $('input[name="word"]');
			var val = q.val();
//			if (val == '' || val == "请输入关键字") {
//			alert('请输入关键字！');
//			q.focus().select();
//			return false;
//			}
			if (val.length > 40) {
			alert('搜索关键词过长,不能超过40个字符');
			q.focus().select();
			return false;
			}
			q.val($.trim(q.val()));
			var index = $form.find('.lisearch li').index($form.find('.lisearch li.lisIn'));
			$('input[name="type"]').val(index);
			return true;
		}
        $(function() {
          $('.lisearch').each(function (i, item) {
          var $dd = $(item).find('li');
          $dd.hover(function () { $(this).addClass("current"); }, function () { $(this).removeClass("current"); });
          $dd.click(function () { $dd.removeClass("lisIn"); $(this).addClass("lisIn");
		   if($(this).attr("rel")=="prjtype") { $("#prjtype").css("display","block"); $("#ntype").css("display","none");$("input[name='types']").val("project"); }
		   if($(this).attr("rel")=="ntype") { $("#ntype").css("display","block"); $("#prjtype").css("display","none");$("input[name='types']").val("news"); }
		   if($(this).attr("rel")=="") { $("#ntype").css("display","none"); $("#prjtype").css("display","none"); }
		   $("#searchfrm").attr("action", $(this).attr("data-url")); 
		   });
          });
		  $(".select").each(function(){
				var s=$(this);
				var z=parseInt(s.css("z-index"));
				var dt=$(this).children("dt");
				var dd=$(this).children("dd");
				var _show=function(){dd.slideDown(200);dt.addClass("cur");s.css("z-index",z+1);};   //展开效果
				var _hide=function(){dd.slideUp(200);dt.removeClass("cur");s.css("z-index",z);};    //关闭效果
				//s.hover(function(){dd.is(":hidden")?_show():_hide();});
				s.hover(function(){
				   s.find("dd").show();
				},function(){
				   s.find("dd").hide();
				});
				dd.find("a").click(function(){
					dt.html($(this).html());
					_hide();
					$("input[name='types']").val($(this).attr("rel"));
				});
		  });
          var ipt=$(".ipt");
          ipt.focus(function(){
              var $th=$(this);
              $th.addClass("iptfocus");
              var val=$th.val();
           if(val=='请输入关键字'){
               $th.val('');
               };
          });
          ipt.blur(function(){
              var $th=$(this);
              $th.removeClass("iptfocus");
              var val=$th.val();
               if(val==''){
               $th.val('请输入关键字');
               };
          });
        });
        </script> 
