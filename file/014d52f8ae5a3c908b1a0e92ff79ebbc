    <script language="javascript" src="source/plugin/compress/static/js/webuploader.min.js"></script> 
    <script>

	var obj = $("#filedata").parent();
	if(obj){
	    $(obj).css({"display":"inline-block","position":"relative","width":obj.width});
		
	var uploader = WebUploader.create({
		swf: "http://b.3ajiepai.com/source/plugin/compress/static/images/Uploader.swf",
		server: "http://b.3ajiepai.com/misc.php?mod=swfupload&operation=upload&type=image&inajax=yes&infloat=yes&simple=2",
		formData: {uid:"0", hash:"6f54429f5ba6f36602a946e3efb64bc2"},
		fileVal: "Filedata",
		pick: obj,
		auto: true,
		prepareNextFile: true,
		compress: {
			width: 1600,
			height: 2400,
			quality: 90,
			preserveHeaders: true
		},
		accept: {
			title: "Images",
			extensions: "gif,jpg,jpeg,bmp,png",
			mimeTypes: "image/*"
		}
	});
	uploader.on('uploadBeforeSend', function( block, data, headers) {
		data.type = "image";
	});
	
	uploader.on('uploadProgress', function(file, percentage) {

	});
	uploader.on('uploadSuccess', function(file, response) {
		data = response._raw
		if(data == "") {
			popup.open('{lang uploadpicfailed}', 'alert');
		}
		var dataarr = data.split("|");
		if(dataarr[0] == "DISCUZUPLOAD" && dataarr[2] == 0) {
			popup.close();
			$("#imglist").append('<li><span aid="'+dataarr[3]+'" class="del"><a href="javascript:;"><img src="static/image/mobile/images/icon_del.png"></a></span><span class="p_img"><a href="javascript:;"><img style="height:54px;width:54px;" id="aimg_'+dataarr[3]+'" title="'+dataarr[6]+'" src="data/attachment/forum/'+dataarr[5]+'" /></a></span><input type="hidden" name="attachnew['+dataarr[3]+'][description]" /></li>');
		} else {
			var sizelimit = "";
			if(dataarr[7] == "ban") {
				sizelimit = "{lang uploadpicatttypeban}";
			} else if(dataarr[7] == "perday") {
				sizelimit = "{lang donotcross}"+Math.ceil(dataarr[8]/1024)+"K)";
			} else if(dataarr[7] > 0) {
				sizelimit = "{lang donotcross}"+Math.ceil(dataarr[7]/1024)+"K)";
			}
			popup.open(STATUSMSG[dataarr[2]] + sizelimit, "alert");
		}
	});
}
</script>
