  <script type="text/javascript">
    $(function(){
        $(".fix-wrapper").fixtotop();
    });
    (function($){
         $.fn.fixtotop = function(){
            var fix_wrapper=$(this);
            $(window).bind("scroll", function(){
                var isshow=$(document).scrollTop()>$(window).height()/2;
                fix_wrapper.find(".top").toggle(isshow);
                // IE6兼容fixed
                if($.browser.msie&&($.browser.version == "6.0")&&!$.support.style){
                     fix_wrapper.css("top",$(document).scrollTop()+$(window).height()-fix_wrapper.height()-50);
                }
            });
            // 点击隐藏
            fix_wrapper.find(".close").click(function(){
                fix_wrapper.find(".close").hide();
                fix_wrapper.find(".fix-parent").animate({"width":34,"height":34},400);
                fix_wrapper.find(".top").animate({"left":89},400,function(){
                     fix_wrapper.find(".slide").show();
                });
            });
            // 点击展开
            fix_wrapper.find(".slide").click(function(){
                $(this).hide();
                fix_wrapper.find(".fix-parent").animate({"width":125,"height":190},400,function(){
                    fix_wrapper.find(".close").show();
                });
                fix_wrapper.find(".top").animate({"left":9},400);
            });
            // 点击回到顶部
            fix_wrapper.find(".top").click(function(){
                var $body=(window.opera)?(document.compatMode=="CSS1Compat"?$('html'):$('body')):$('html,body');
                $body.animate({
                    scrollTop: 0
                },300);
                return false;
            });
            // 回到顶部按钮上移变色
            fix_wrapper.find(".top").hover(function(){
                $(this).addClass("top-hover");
            },function(){
                $(this).removeClass("top-hover");
            });
            // 展开隐藏按钮上移变色
            fix_wrapper.find(".slide").hover(function(){
                $(this).addClass("slide-hover");
            },function(){
                $(this).removeClass("slide-hover");
            });
        }
    })(jQuery);
</script> 
  <!-- 联系客服结束 --> 
  <script type="text/javascript">
	$("#feature-tab li").click(function(){
		$(this).addClass("current").siblings().removeClass("current");
		var item = $(this).attr("id");
		$("#" + item + "-content").show().siblings().hide();
	});
</script>  
