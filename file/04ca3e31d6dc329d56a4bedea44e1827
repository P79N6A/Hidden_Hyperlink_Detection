  <script>
if($('#main-height').size() > 0){
	$('#main-height').css('height', '');

	var t = $('#main-height').offset().top;
	var f = $('.footer').height();
	var h = $('#main-height').height();
	//var bh = $('body').height();
	//var wh = $(window).height();

	var height = document.body.offsetHeight - t - f;

	height = height > h ? height : h;

	if (!$.browser.msie || $.browser.version > '6.0') {
	    $('#main-height').css('height',height);
	}
}

</script> 
