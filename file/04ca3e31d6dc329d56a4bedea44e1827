},function(){
    var target=$(this).data("target");
    if(!!target){
        $("#"+target).hide();
    }
    $(".sub-nav").bind("mouseenter",showElement);
    $(".sub-nav").bind("mouseleave",hideElement);
    function showElement(){
        $(this).show();
    }
    function hideElement(){
        $(this).hide();
        $(".sub-nav").unbind("mouseenter",showElement);
        $(".sub-nav").unbind("mouseleave",hideElement);
    }
});
