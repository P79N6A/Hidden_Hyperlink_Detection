		$(".hover_img").mouseover(function(){
			$(this).find('.pic-hover').show();	});
		$(".hover_img").mouseout(function(){
			$(this).find('.pic-hover').hide();
			$(this).find('.pic-hover').removeClass("current");
		});
		$(".hover_img").click(function(){
			$(this).find('.pic-hover').addClass("current");
