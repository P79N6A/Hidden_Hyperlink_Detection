function htmlDecode(str){
     return str
           .replace(/&#39;/g, '\'')
           .replace(/<br\s*(\/)?\s*>/g, '\n')
           .replace(/&nbsp;/g, ' ')
           .replace(/&amp;nbsp;/g,' ')
           .replace(/&lt;/g, '<')
           .replace(/&gt;/g, '>')
           .replace(/&quot;/g, '"')
           .replace(/&amp;/g, '&');
 }

//服务号appId 分享图片 分享内容 分享标题
var headimg = "static2/user/1665749/article/8a299a295aa14942015aa20cb19f0271/1488777425320.jpg@!square-96";
var imgUrl = "";
var summary = htmlDecode("国家政策支持的项目，不要错过好好看看，好好领悟，再抓不住就可惜了云数贸的家人在马来西亚2015年11月17日");
var title = htmlDecode("云数贸是国家操盘的，话不多说自己看资料视频。");

if(headimg=="" || headimg.indexOf("add_box.jpg")!=-1 || headimg.indexOf(".gif")!=-1){
	imgUrl="http://oss.tieqq.com/resource/namecard/article/images/defaultPic.jpg";
}else if(summary==""){
	if(headimg=="" || headimg.indexOf("add_box.jpg")!=-1 || headimg.indexOf(".gif")!=-1){
		imgUrl ="http://oss.tieqq.com/resource/namecard/article/images/defaultPic.jpg";
	}else{
		imgUrl = "http://oss.tieqq.com/static2/user/1665749/article/8a299a295aa14942015aa20cb19f0271/1488777425320.jpg@!square-96";
	}
	summary = title;
}else{
	imgUrl = "http://oss.tieqq.com/static2/user/1665749/article/8a299a295aa14942015aa20cb19f0271/1488777425320.jpg@!square-96";
}

 
