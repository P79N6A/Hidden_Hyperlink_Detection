  <script type="text/javascript">
function gengduo(sid,num){
	$("#"+sid+" li.hide").each(function(ic){
		if(ic<num){
			$(this).removeClass("hide");
		}else{
			return false;
		}
	});
	if($("#"+sid+" li.hide").length==0){
		$("div.gengduo_"+sid).hide();
	}
}
gengduo('zixun',5);
function xian(sid){
	$("#"+sid).each(function(ic){
		var n =$("#"+sid).html();
		if(n==''){
			$("#"+sid+"text").remove();
		}	
	});
}
xian('tupian');
xian('bizhi');
</script> 
