          <p class="text-center text-s14 text-wr"> 已累计<i class="fa fa-spinner fa-spin"></i><font id="dingyue"></font>人订阅 <script>
	jQuery.post("http://nmg.zzzsxx.com/wp-admin/admin-ajax.php",{ action:'counter',id:'19749',cat:'jihua',sub_cat:'dingyue',step:'10',format:'1',i:'1000',x:'8000' },function( response ) 
	{
		jQuery("#dingyue").text( response ); 
		jQuery("#dingyue").prev().removeClass("fa-spinner").removeClass("fa-spin");
	});
	</script> <script>
			jQuery(document).ready(function(){
				
				function dy_have() {
					$("#dingyue_btn").text( "查看订阅" );
					$("#dingyue_btn").attr( "href" , "http://user.zzzsxx.com/user-dy");
				}
				
				//检查是否已经订阅了该学校
				jQuery.post(
					"http://nmg.zzzsxx.com/wp-admin/admin-ajax.php",
					{ action : 'dy_have', id : $("#dingyue_btn").attr("data") },
					function( response ) {
						if( response == "1" ) 
						{
							dy_have();
						}
						$("#dingyue_btn").removeAttr("disabled");
					}
				);
				
				//点击订阅
				$("#dingyue_btn").click(function(){
					
					if( $(this).text() ==  "查看订阅" ){ return; }
					
					$(this).button('loading');
					
					jQuery.post(
						"http://nmg.zzzsxx.com/wp-admin/admin-ajax.php",
						{ action : 'dy_can', id : $(this).attr("data") },
						function( response ) {
							
							$("#dingyue_btn").button('reset');
							
							var o = eval( "("+response+")" );
							
							//如果没有登录，就提醒登录
							if( o.state == "no_login" ){ alert("请先登录"); window.location.href="http://user.zzzsxx.com/login"; return; }
							
							//如果已经订阅了，就不重新订阅了
							if( o.state == "have" ){ alert("已经订阅"); dy_have(); return; }
							
							//如果没有额度了，则跳转到重置页面
							if( o.state == "ok" ) {
								
								if( o.message <= 0 ) {
									alert("订阅额度不足，请充值。");
									window.location.href="http://nmg.zzzsxx.com/g?id=3272";
								}else{
									//如果有剩余，则可以订阅
									if( confirm("你还能订阅" + o.message + "个学校，确定要订阅该学校吗？") == true ) {
										
										window.location.href="http://user.zzzsxx.com/user-dy?dingyue=" + $("#dingyue_btn").attr("data");
										return;
									}
								}
							}
						}
					);
				});
			});
		</script> </p> 
         </div> 
        </div> 
       </div> 
      </div> 
     </div> 
    </div> 
   </div> 
  </div> 
