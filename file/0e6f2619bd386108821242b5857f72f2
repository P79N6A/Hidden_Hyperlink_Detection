$.ajax({
    url: '//rank.vip.com/rankApi/findProductUrlList',
    type: 'GET',
    dataType: 'jsonp',
    jsonpCallback: 'getRankProduct',
    cache: true,
    data: {
        warehouse: $.Cookie.get('vip_wh') || "VIP_NH",
        rankCode: '1502571418'
    }
}).done(function (re) {
    var defaultData = [{"product_url": "//a.vpimg3.com/upload/upimg2/ued/product/home/rank/1.jpg"}, {"product_url": "//a.vpimg2.com/upload/upimg2/ued/product/home/rank/2.jpg"},{"product_url": "//a.vpimg3.com/upload/upimg2/ued/product/home/rank/3.jpg"}],
        data = [];
    if(re.code == 200 && re.data){
         data = re.data;
    }
    data = data.concat(defaultData);
    data.length = 3;
    var wrap =  $('#indexRankList');
    wrap.html($.Tpl('rankTpl', data));
    wrap.find('img').removeClass('lazy').attr('src', function () {
        return this.getAttribute('data-original');
    });

});
