function   getCookie(name) //cookies读取 
{ 
   var   search=name+"=";
  if(document.cookie.length>0)   
	{ 
	   offset=document.cookie.indexOf(search) 
		
		if(offset!=-1)   
		{ 
			offset+=search.length; 
			end=document.cookie.indexOf( "; ",offset);
			if(end ==-1){
				end =document.cookie.length
			}
                  var temp=decodeURIComponent(document.cookie.substring(offset,end));

			document.getElementById('showinfo').innerHTML="欢迎"+temp+"网友&nbsp;&nbsp;<a href=http://comment.zjol.com.cn:7001/m_comment/temp.jsp>安全退出</a>";
		}
		else{
			document.getElementById('showinfo').innerHTML=show1;
		}
	} 
}


