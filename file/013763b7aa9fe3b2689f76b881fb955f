var price = $('#price_type li').each(function(index, element) {
	var type_id=$(this).attr('id');
	type_id=type_id.split('_');
	//date_type(type_id[3]); //切换等级报价
	var price = $('#price_price_'+type_id[3]).val();
	price = price.split(',');
	price = price[0].split('|');
	price = price[1].split('-');
	var html = '<tr>\
              <td class="f15 yellow tdline pl5">' + $(this).find('a').html() + '</td>\
              <td  class="tdline"><div class="info">' + $(this).find('a').attr('data-ajax') + '</div></td>\
              <td colspan="2" class="tdline"><table width="100%" border="0" cellpadding="0" cellspacing="0">\
                  <tr>\
                    <td width="137" height="36" class="f15 line_s"><span class="pl20">会员价</span></td>\
                    <td width="110" class="f15 line_s">人数</td>\
                    <td width="104" class="f15 line_s">预定</td>\
                  </tr>\
                  <tr>\
                    <td height="60"><span class="f15 pl20">成人价：<em class="green">￥<span class="adult_price">' + price[0] + '</span></em></span></td>\
                    <td><span>\
                      <input type="button" class="j adult" value="-" />\
                      <input type="text" class="lxinfotext adultNum" value="1" />\
                      <input type="button" class="j adult" value="+" />\
                      </span></td>\
                    <td><input type="button" class="green_btn" value="立即预订"/></td>\
                  </tr>\
                  <tr>\
                    <td height="20"><span class="mt10 f15 pl20">儿童价：<em class="green">￥<span class="children_price">' + price[1] + '</span></em></span></td>\
                    <td><span>\
                      <input type="button" class="j children" value="-" />\
                      <input type="text" class="lxinfotext childrenNum" value="0" />\
                      <input type="button" class="j children" value="+" />\
                      </span></td>\
                    <td><input type="button" class="yellow_btn" value="快捷支付"/></td>\
                  </tr>\
                  <tr>\
                    <td height="50" colspan="3" align="center"><span class="f15 pl20">合计：￥<em class="yellow f25 total_price">' + price[0] + '</em></span></td>\
                  </tr>\
                </table></td>\
            </tr>';
    addTr('costs',0,html);
});
function addTr(tab, row, trHtml){
     //获取table最后一行 $("#tab tr:last")
     //获取table第一行 $("#tab tr").eq(0)
     //获取table倒数第二行 $("#tab tr").eq(-2)
     var $tr=$("#"+tab+" tr").eq(row);
     if($tr.size()==0){
        //alert("指定的table id或行数不存在！");
        return;
     }
     $tr.after(trHtml);
  }


