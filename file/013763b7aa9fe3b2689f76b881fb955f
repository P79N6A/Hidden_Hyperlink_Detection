	var _con = $('#myv_calendar');   // 设置目标区域
  	if(!_con.is(e.target) && _con.has(e.target).length === 0){ // Mark 1
    	$('#myv_calendar').hide()   // 功能代码
  	}
});

	//加载报价等级
            function myselectbaojia()
            {
            $("#price_type").html('<li><a href>拼命加载中，请稍后...</a></li>');
            var aid='6047';
            var yun_id='';
            $.ajax({
                async: false,
            type: "Get",
            url: "/add/ajax_select.php?post=myselectbaojia&aid="+aid+"&yun_id="+yun_id+"&t=" + Math.random(),
            data: "JSON",
              complete: function(data){
            //$(".loading").attr("style","display:block")
            },
            success: function(data) {
              if(data.code==1)
              {
				  var data=eval("("+data.msg+")");//转换为数组对象
				  
				  var t='';
				  for(var i=0;i<data.length;i++)
				  {
					  if(i==0){
						var sel='class="selected"';
            $('input[name="date_id"]').val(data[i].id);
			
			
			get_menshijia(data[i].id);//写入门市价
			get_youhuijia(data[i].id);//写入优惠价 /会员价
					  }else{
						var sel='';
					  }
					  if(yun_id>0){
						t=t+' <div class=lx_tc><li  '+sel+' id="li_date_type_'+data[i].id+'"><a href="javascript:void(0)" target="_self" title="'+decodeURIComponent(data[i].content)+'">'+decodeURIComponent(data[i].name)+'</a></li><input type="hidden" name="price_price" id="price_price_'+data[i].id+'" value="'+data[i].jiage+'" /></div>';
						if(!data[i].price_menshijia) var menshijia=parseInt(data[i].jiage)+300;
						else var menshijia=data[i].price_menshijia;
						//$(".O_price").html('06'+menshijia);
						
					  }else{
						t=t+' <div class=lx_tc><li '+sel+' id="li_date_type_'+data[i].id+'" class="huan">\
						<a href="javascript:void(0)" ' + ((i == 0) ? 'class="on"' : '') + 'title="'+decodeURIComponent(data[i].price_content)+'">'+data[i].price_name+'</a></li></div>\
						<input type="hidden" name="price_price" id="price_price_'+data[i].id+'" value="'+data[i].price_price+'" />'; 
						
					  }
				  }
				  if(t==''){
					t='<li><a href="javascript:">产品已过期或下架</a></li>';
				  }
				  $("#price_type").html(t);
				  }
				  
              } //本站的数据
              
              });
            
            }
			
            myselectbaojia(aid);
