function calc1(id,value,my){
	
	var num = $('.'+id+'Num').val();
	var type =my.attr('class');
	
	
	if(value == '+'){
		
		
		$('.'+id+'Num').val(parseInt(num)+1);
	if(type=='j adult1') $('input[name=adult]').val(parseInt(num)+1);
	else $('input[name=child]').val(parseInt(num)+1);
	
	
	
	}else if(value == '-'){
		
	if(type=='j adult1')
	{
	if(num<=1) return false;
	}
	
	
		if(num > 0)
		$('.'+id+'Num').val(parseInt(num)-1);
	if(type=='j adult1') $('input[name=adult]').val(parseInt(num)-1);
	else $('input[name=child]').val(parseInt(num)-1);	
		
		
	}

	//alert(id);
	//alert(value);
	
	}


/*团型费用计算*/
$('.adult').each(function(index, element) {
	   $(this).click(function(){
		calc('adult',$(this).val(),$(this));
	}); 
});
$('.children').each(function(index, element) {
	   $(this).click(function(){
		calc('children',$(this).val(),$(this));
	}); 
});
function calc(id,value,my){
	
	var mmy=my.parent().parent().parent().parent().parent('table');
	var num = mmy.find('.'+id+'Num').val();
	var type =my.attr('class');
	
	
	
	if(value == '+'){
		
		 mmy.find('.'+id+'Num').val(parseInt(num)+1);
	if(type=='j adult') $('input[name=adult]').val(parseInt(num)+1);
	else $('input[name=child]').val(parseInt(num)+1);
	
		var price = mmy.find('.adult_price').html();
		var ren = mmy.find('.adultNum').val();
		
		var price1 = mmy.find('.children_price').html();
		var ren1 = mmy.find('.childrenNum').val();
		
		mmy.find('.total_price').html((parseInt(price)*parseInt(ren)+parseInt(price1)*parseInt(ren1)));
	}else if(value == '-'){
		
	if(type=='j adult')
	{
		if(num<=1) return false;
	}
	
	
		if(num > 0)
		 mmy.find('.'+id+'Num').val(parseInt(num)-1);
	if(type=='j adult') $('input[name=adult]').val(parseInt(num)-1);
	else $('input[name=child]').val(parseInt(num)-1);	
		var price = mmy.find('.adult_price').html();
		var ren = mmy.find('.adultNum').val();
		
		var price1 = mmy.find('.children_price').html();
		var ren1 = mmy.find('.childrenNum').val();
		
		mmy.find('.total_price').html((parseInt(price)*parseInt(ren)+parseInt(price1)*parseInt(ren1)));
		//$('.'+id+'_price').html((parseInt(num)-1)*parseInt(price));
	}
	
	var m=my.parent().parent().parent('table');
	
	
	m.find('.total_price').html(parseInt(m.find('.adult_price').html()) + parseInt(m.find('.children_price').html()));
	//alert(id);
	//alert(value);
}
