            function date_type(type_id)
            {
				$('input[name=date_id]').val(type_id);
				$('#price_type').find("li").removeClass("selected");
				$('#price_type').find("#li_date_type_"+type_id).addClass("selected");
              
             	//loadVCalendar(aid, '', '');
				
				get_menshijia(type_id); //重置门市价
				get_youhuijia(type_id);//重置优惠价 /会员价
           		// $("#vdepartdate").val('请选择出发日期');
            }
			
