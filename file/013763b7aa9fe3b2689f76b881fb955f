loadVCalendar(); //加载日历
$("#myv_calendar").show();



/***********************************************
*************************************************/
	//滚动事件
	var conlist_top = $("#conlist").offset().top;
	var conlist_left = $("#conlist").offset().left;
	
	var daysNav_top = $("#daysNav").offset().top;
	var daysNav_left = $("#daysNav").offset().left;
	
	var aboutline_top=$(".aboutline").offset().top;
	var aboutline_left=$(".aboutline").offset().left;
	
	var xcheight=$("#xcheight").height()+$("#xcheight").offset().top-$(".daysNavBox").height();
	
	var xgheight=$(".subpage_w").height()+$(".subpage_w").offset().top-$(".aboutline").height()-45;
	
	var nums = 0,conts = new Array();
	
	var daysNav_nums = 0,daysNav_conts = new Array();
	
	$(document).ready(function(e) {
        //获取 nav_box 对应距离顶部的高度
		$(".nav_box").each(function(index, element) {
		nums = $(".nav_box").length;
		for(var i=0 ; i<nums; i++){
			conts[i] = $(".nav_box").eq(i).offset().top-20;
		}
		 })
		
			$(".myday").each(function(index, element) {
		daysNav_nums = $(".myday").length;
		for(var i=0 ; i<daysNav_nums; i++){
			daysNav_conts[i] = $(".myday").eq(i).offset().top-20;
		}
         })
	});
	


	//点击滚动到对应位置
	$("#conlist li").each(function(index, element) {
			$(this).click(function (){
				$(this).addClass("on").siblings().removeClass("on");
				$("html,body").stop().animate({scrollTop:conts[$(this).index()]-51},600);
			})
	});
	
	//监听滚动条
	$(window).scroll(function() { 
		var top = $(window).scrollTop(); 
		//alert(top);
		var left= $(window).scrollLeft();
		if(top>conlist_top){
			$("#conlist").css({ left:conlist_left + "px", top: 0 + "px" , position: "fixed"}); 
			//$("#conlist .reserve_btn").removeClass("none");
		}else{
			//$("#conlist").css({ left:conlist_left + "px", top: conlist_top + "px" , position: "absolute"}); 
			//$("#conlist .reserve_btn").addClass("none");
			$("#conlist").css({ left:"0px", top: "0px" , position: "relative"}); 
		}
		
		
		if(top>daysNav_top){
			$("#daysNav").css({ left:conlist_left + "px", top: 0 + "px" , position: "fixed"}); 
			if(top>xcheight-100){$("#daysNav").css({ left:"-52px", top: "20px" , position: "relative"}); }
			//$("#conlist .reserve_btn").removeClass("none");
		}else{
			//$("#conlist").css({ left:conlist_left + "px", top: conlist_top + "px" , position: "absolute"}); 
			//$("#conlist .reserve_btn").addClass("none");
			$("#daysNav").css({ left:"-52px", top: "20px" , position: "relative"}); 
			
		}
		
		if(top>aboutline_top){
			
			$(".aboutline").css({ left:aboutline_left+ "px",top:  "-20px" , position: "fixed"});
			if(top>xgheight) 	$(".aboutline").css({ left:"0px", top:"0px" , position: "relative"}); 
		}
		else
		{
			
			
			$(".aboutline").css({ left:"0px", top:"0px" , position: "relative"}); 
		}
		
		
		for(var j=0; j<conts.length; j++){
			
			
			if(top>conts[j]-52){
				$("#conlist li").eq(j).addClass("on").siblings().removeClass("on");	
			}	
		}
		
		
		for(var j=0; j<daysNav_conts.length; j++){
			
		//	alert(1);
			if(top>daysNav_conts[j]-90){
				$(".daysNavBox a").removeClass("cur");	
				$(".daysNavBox a").eq(j).addClass("cur");
			}	
			
			
		}
		
	}); 
	
 $('.daysNavBox li').click(function () { //点击天数定位到相应的位置
 
 
		            var tag = $(this).attr('tag');
					
		            var pos = $('#' + tag).offset().top - 56;
		            $('.daysNavBox li').find('a').removeClass('cur');
		            $(this).find('a').addClass('cur');
		            $('html,body').animate({ scrollTop: pos }, 0);
		        });
		        
				

