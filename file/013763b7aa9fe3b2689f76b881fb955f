  <script type="text/javascript">

/*游客说*/
function marks(mark){
  mark = mark * 1;
  switch(mark){
    case 5:
	return '非常满意';
    case 4:
      return '满意';
    case 3: return '一般';
    case 2: return '差';
    case 1: return '非常差';
    default: return '';
  }
}
var d,c;
function goReviews(){
	
  if(!''){
    location.href='/users/index.php';
    return;
  }
  d = dialog({
    title: '发表评论'
  }).show();
  $.get(
    '/add/cmt/getTpl.php',
    { id:'6047', mid: '', channel: '4'}, 
    function(response){
	
      d.content(response);
    }, 'text'
  );
}
thtml='';
function loadCmt(page){
  page = page > 0 ? page : 1;
  $.post(
    '/add/cmt/load.php',
    { goods_id: '6047', page: page}, 
    function(json){
      if(json.code == 0 && json.data.length > 0){
        var tpl='';
		
		$("#countyoukeshi").html('('+json.count+')');
		$("#countyoukeshi1").html('<font class="c_f90">'+json.count+'</font>');
		
        $.each(json.data, function(i, e){
			
			
			
			 /*点评字段*/
		 thtml='';
		  var jhtml=eval('('+e.html+')');
		  for(var i=0;i<jhtml.length;i++){  
		
		 
            thtml+=' <dd>'+decodeURIComponent(jhtml[i].title)+' :<b>'+marks(jhtml[i].v)+'</b> </dd>';
       
         }  
		 
		 
		 
			tpl +='<li>'+
           ' <div class="imginfo"> <img src="'+(e.faceurl?e.faceurl:'/uploads/userup/noface.jpg')+'" width="60" height="60" />'+
              '<p>'+e.name+'</p>'+
           ' </div> '+             
           ' <div class="li_r">'+
              '<p>'+e.content+'</p>'+
			  '<dl>'+thtml+'</dl>'+
              '<div class="clear"></div>'+
              '<p class="c_999">'+e.time+' 点评</p>'+
            '</div>'+
         ' </li>';
		
		
        });
		
		//alert( tpl);
		
        $('#youkeshuo').html(tpl);
	
          var count = Math.ceil(json.count / 10), ph = '';
          for(var i = 1; i <= count; i ++){
            ph += '<a '+ (page==i?'class="cur"':'rev="'+i+'"') + '>'+i+'</a>';
          }
		  var ptpl = ''+
              '<a href="javascript:;" rev=1>首页</a>'+
              '<a href="javascript:;" rev="'+(page > 1 ? page-1 : 'return false')+'" class="prev page">上一页</a>'+
              ph+
              '<a href="javascript:;" class="next page" rev="'+(page < count ? page + 1: 'return false')+'">下一页</a>'+
              '<a href="javascript:;" rev="'+count+'">尾页</a>'+
              ''+
              '';
          $('.fenye').html(ptpl);
          page ++;
        
      } else {
        if(page == 1){
          $('#youkeshuo').html('<p>还没有评论，沙发等你来抢</p>');
        }
       
	   
		
      }
    }, 
    'json'
  );
}
jQuery(function($){
  //load comments count
  
  $.post(
    '/add/cmt/loadCount.php',
    { id: '6047',channel:'4'}, 
    function(json){
      if(json.code == 0 ){
       $('#zongfen').html(json.zongfen+'分');
      
	  $(".dianpingziduan b").each(function(index, element) {
            var id=$(this).attr('id');
			
			
		   //    $(this).css('width', json.data[id] + '%');
        // $(this).parent().parent().find('dd[class=d4]').html(json.data[id]  + '%');
		
	 $(this).html(json.data[id]+'分');
	
        });
	
	
      }
    },'json'
  );
  //load comments
  
  loadCmt(1);
  /*分页*/




});
