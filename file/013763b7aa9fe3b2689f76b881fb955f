  <script type="text/javascript">
$(function () {
	$(".travel-tj-list img").lazyload({
		effect: "fadeIn"
	});
	$(".J-newChannel-list img,.hs_con img,.newAd img").lazyload({
		effect: "fadeIn"
	});
});

topMsg();       //登陆下方滚动效果
function topMsg(){       
	var scrolldown = new ScrollText("cust_news");        
	scrolldown.LineHeight = 26;        
	scrolldown.Amount = 1;        
	scrolldown.Direction = "up";        
	scrolldown.Start();    
}    //lazyload  		

$(".J-newChannel-right").each(function () {
	var that = $(this);
	that.find(".J-newChannel-tab li").mouseover(function () {
	var index = $(this).index();
	index=index-1;
	$(this).addClass("current").siblings().removeClass("current");
	that.find(".J-newChannel-list").hide().eq(index).show();
	});

	that.find(".J-newChannel-tab1 li").mouseover(function () {
		var index = $(this).index();
		$(this).addClass("current").siblings().removeClass("current");
		that.find(".J-newChannel-list").hide().eq(index).show();
	});

});

function setTab(name,cursel,n){

    for(i=1;i<=n;i++){
         var menu=document.getElementById(name+i);
         var con=document.getElementById("con_"+name+"_"+i);
         menu.className=i==cursel?"hover":"";
         con.style.display=i==cursel?"block":"none";
    }

} 

/*出游次数*/
$.ajax({
            async: true,
            type: "Get",
            url: "/add/add_comment.php?cpost=countchuyou&t=" + Math.random(),
            data: "text",
              complete: function(data){
            //$(".loading").attr("style","display:block")
            },
            success: function(data) {
				
				$("#countchuyou").html(data);
			}
});


