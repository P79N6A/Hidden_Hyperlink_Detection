  <script>
//侧边栏JS
$(function (){

$("#side_navs .ly_cs").hover(function (){
	$(this).find(".ly_phone_m").show();
},function (){
	$("#side_navs .ly_cs .ly_phone_m").hide();
})

$("#side_navs .ly_wtw_b").hover(function (){
	$(this).find(".ly_wtm_con").show();
},function (){
	$("#side_navs .ly_wtw_b .ly_wtm_con").hide();
})

$("#side_navs .side_link").hover(function (){
	$(this).find(".ly_kf_con").show();
},function (){
	$("#side_navs .side_link .ly_kf_con").hide();
})

$("#side_navs .back_top").click(function (){
	$("html,body").stop().animate({scrollTop:0},600);
})

//监听滚动条
	$(window).scroll(function() { 
		var top = $(window).scrollTop(); 
		var left= $(window).scrollLeft();
		if(top>100){
			$("#side_navs .back_top").show();
		}else{
			$("#side_navs .back_top").hide();	
		}
	}); 	
})
</script> 
