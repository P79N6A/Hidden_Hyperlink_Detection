(function(){
  'use strict';
  var views = $.cookie('line_views');
  views = views || '[]';
  views = $.parseJSON(views);
  // 已存在这个项目，跳过
  for(var view in views){
    if(views[view].id == '6047')
      return;
  }
 
  // 超过5个，删除第一个
  if(views.length >= 10)
  views.splice(0, 1);
  views.push({
    title: '澳洲名城8天（墨进悉出）',
       id: '6047',
	   jiage:$("#price").html(),
	   litpic:'/uploads/litimg/5716f8913a439.jpeg'
   //jiage: ''
  });
  $.cookie('line_views', JSON.stringify(views), {path: '/'});
})();


(function(){
  var views = $.cookie('line_views');
  views = views || '[]';
  views = $.parseJSON(views);
  var tpl = '<li> <a href=""><img src="" width="80" height="60"></a><p><a href="" class="ttttt"></a><b></b></p><i class="i1"></i></li>';
  for(var view in views){
	  var vieww=parseInt(view)+1;
	  //alert(views[view].jiage)
    var $tpl = $(tpl).clone();
    $tpl.find('a').attr('href', '/add/v.php?aid=' + views[view].id);
    $tpl.find('.ttttt').html(views[view].title);
	$tpl.find('i').html(vieww);
	$tpl.find('img').attr('src',views[view].litpic);
	$tpl.find('b').html('￥'+views[view].jiage);
    $('.historyList').append($tpl);
  }
})();



