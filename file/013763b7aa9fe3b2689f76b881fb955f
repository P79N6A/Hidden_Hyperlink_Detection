            var aid='';
			
			/* 预加载日历*/
            function loadVCalendar(aid, year, month) {
				
				var aid='6047';
				var c=$('#price_type').find("li[class=selected]").attr('id');
			//	alert(c);
				if(typeof(c) == "undefined") 
				{
					//alert(c);
				c=$('#price_type').find("li[class='huan selected']").attr('id');
				}
				c=c.split('_');
				var id=c[3];
				var p=$("#price_price_"+id).val();
			
				$.ajax({
				type: "POST",
				async:false,// 设置为同步请求
				data:{p:p},
				url: "/add/load_calendar_new.php?aid="+aid+"&y="+year+"&m="+month+"&time="+Math.random(),
            	complete: function(msg){
                
				//alert('complete');
				},
            	success: function(da){
                	$("#v_calendar").html(da);
              	}
              });
            }
			
         
		 /*重置优惠价*/
		function get_youhuijia(id)
		{
			var aid='6047';
            var yun_id='';
            $.ajax({
                async: false,
            type: "Get",
            url: "/add/ajax_select.php?post=myselectbaojia&aid="+aid+"&yun_id="+yun_id+"&t=" + Math.random(),
            data: "JSON",
              complete: function(data){
            //$(".loading").attr("style","display:block")
            },
            success: function(data) {
              if(data.code==1)
              {
				  var data=eval("("+data.msg+")");//转换为数组对象
				  
				  var t='';
				  for(var i=0;i<data.length;i++)
				  {
					  
					  if(data[i].id==id)
					  {
					  if(yun_id>0){
						
						
						$("#price").html(parseInt($("input[name=yun_price]").val())+parseInt($("input[name=dijie_down]").val()));
						$("#price_p").html(parseInt($("input[name=yun_price]").val())+parseInt($("input[name=dijie_down]").val()));
						
											
					  }else{
						
						var price=data[i].price_price;
						price=price.split(',');
						var jiage=new Array();
						for(var i=0;i<price.length;i++)
						{
							var j=price[i].split('|');
							j=j[1].split('-');
							
							//alert(j[0]);
							//return false;
							jiage[i]=j[0];		
						}
						
						var ojiage=jiage.sort(function(a,b){return a-b;});
						if(ojiage[0]==ojiage[(ojiage.length)-1])
						{
						$("#price").html(ojiage[0]);
						$("#price_p").html(ojiage[0]);
						}
						else
						{
						$("#price").html(ojiage[0]+'-'+ojiage[(ojiage.length)-1]);	
						$("#price_p").html(ojiage[0]+'-'+ojiage[(ojiage.length)-1]);
						}
						
					  }
					  
					  
					  
				  }
				  
				  }
			  }
				  
              } //本站的数据
			 });
		}
			
			/*重置门市价*/
		function get_menshijia(id)
		{
	
	  		var aid='6047';
            var yun_id='';
            $.ajax({
                async: false,
            type: "Get",
            url: "/add/ajax_select.php?post=myselectbaojia&aid="+aid+"&yun_id="+yun_id+"&t=" + Math.random(),
            data: "JSON",
              complete: function(data){
            //$(".loading").attr("style","display:block")
            },
            success: function(data) {
              if(data.code==1)
              {
				  var data=eval("("+data.msg+")");//转换为数组对象
				  
				  var t='';
				  for(var i=0;i<data.length;i++)
				  {
					  
					  if(data[i].id==id)
					  {
					  if(yun_id>0){
						
						if(!data[i].price_menshijia) var menshijia=parseInt(data[i].jiage)+300;
						else var menshijia=data[i].price_menshijia;
						$("#menshiprice").html('门市价：￥'+menshijia);	
						
											
					  }else{
						$("#menshiprice").html('门市价：￥'+data[i].price_menshijia);
					  }
				  }
				  
				  }
			  }
				  
              } //本站的数据
              
              });
	
}
