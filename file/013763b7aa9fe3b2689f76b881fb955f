$(function (){
	$(".side_menu").hover(function (){
		$("#tag_top_nav").show();
	},function (){
		$("#tag_top_nav").hide();
	})
	$("#tag_top_nav li").each(function(index, element) {
        $(this).hover(function (){
			$("#tag_top_nav li").removeClass("hover");
			$(this).addClass("hover");
			$("#tag_con_nav").show();
			$("#tag_con_nav .hide_box").eq($(this).index()).show().siblings("#tag_con_nav .hide_box").hide();	
		},function (){
			$("#tag_con_nav").hide();
		})
    });
	
	$("#tag_con_nav").hover(function (){
		$("#tag_con_nav").show();
	},function (){
		$("#tag_con_nav").hide();
	})
	
	
})
</script> 
