                            var numkey = Math.random();
                            numkey = Math.round(numkey * 10000);
                            var strShow = "<img src='http://www.dfhon.com/comm/Image2.aspx?k=" + numkey + "' onClick='this.src+=Math.random()' alt='图片看不清？点击重新得到验证码' style='cursor:pointer;' width='70' height='23'' hspace='4'//>";
                            document.getElementById("sCheckCode").innerHTML = strShow;
                        }                   
                    </script> 
