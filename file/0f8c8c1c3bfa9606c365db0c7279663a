  <script type="text/javascript">
    var interval;
    var iTime = 60;
    $(document).ready(function () {
 //提交问题
    $("#SubmitQuestion").click(function () {

        if ($("#TxtQuestionContent").val() == "") {
            alert('问题内容不能为空！');
            $("#TxtQuestionContent").focus();

        } else if ($("#TxtQuestionContent").val().length < 10) {
            alert('问题内容不能低于10字！');
            $("#TxtQuestionContent").focus();

        } else {
            var Question = $.trim($("#TxtQuestionContent").val());
            var time = new Date().toLocaleTimeString();
            var ProjectId = $("#projectid").val();
            $.ajax({
                type: "GET",
                url: "/GuestFeedBack/AddQuestion.ashx",
                cache: false,
                data: { Question: Question, time: time, ProjectId: ProjectId },
                success: function (data) {
                    if (data == "1") {
                        alert("提交成功");
                    } else if (data == "-1") {
                        alert("问题内容不能为空！");
                    } else if (data == "-2") {
                        alert("问题内容不能低于10字！");
                    } else if (data == "-3") {
                        alert("登录用户才可以提交问题！");
                    }
                    else {
                        alert("提交失败");
                    }

                },
                error: function () {
                    alert("提交失败！");

                }
            });
        }
    });
        $("#BtnSearch").click(function () {
            //获取URL
            var pathurl = window.location.pathname;
            var urlarray = pathurl.split('/');
            if (pathurl != "/" && pathurl != "/default.aspx" && pathurl != "/Default.aspx") {
                var searchword = $("#SearchWork").val().trim();
                if (searchword != null && searchword != "") {
                    if (pathurl.indexOf("/listening") > -1) {
                        window.location.href = "/" + urlarray[1] + "/listeningsearch/" + searchword + "/1/";
                    } else if (pathurl.indexOf("/down") > -1) {
                        window.location.href = "/" + urlarray[1] + "/downsearch/" + searchword + "/1/";
                    } else if (pathurl.indexOf("/news") > -1) {
                        window.location.href = "/" + urlarray[1] + "/newssearch/" + searchword + "/1/";
                    } else if (pathurl.indexOf("/live") > -1) {
                        window.location.href = "/" + urlarray[1] + "/livesearch/" + searchword + "/1/";
                    } else {
                        window.open("http://member.geedu.com/project/Projectlist.aspx");
                    }
                } else {
                    alert("搜索内容不能为空！");
                }
            } else {
                window.open("http://member.geedu.com/project/Projectlist.aspx");
            }




            
        });
        //考试倒计时
        var nowTime = new Date().getTime(),
        endTime = new Date("2017-6-17").getTime(),
        intDiff = endTime - nowTime;
        timer(intDiff);
    });
    function ClearCookie() {
        $.cookie('userName', null, { path: '/', domain: 'geedu.com' });
        $.cookie('Pwd', null, { path: '/', domain: 'geedu.com' });
        $.cookie('UserId', null, { path: '/', domain: 'geedu.com' });
        $.cookie('RealName', null, { path: '/', domain: 'geedu.com' });
        //清除模考
        $.cookie('User', null, { path: '/', domain: 'geedu.com' });
        window.location.href = 'http://www.geedu.com';
    }

    /*
        登录验证，没有登录的登录，登录之后跳转链接
        url:登录后跳转的url
        mod:模块
        isWindowsOpen: 1 当前页面跳转   2 打开新界面跳转
    */
    function aLoginVerify(url, mod, isWindowsOpen) {
        
        ShowLoginUrl_New('Login', '', url, isWindowsOpen);
        
    }

    //首页我想导航颜色切换
    function IndexSelectColor(selectid) {
        var list = $(selectid).siblings();
        $.each(list,function(index,data) {
            $(data).removeClass();
        });
        $(selectid).addClass("first");
    }
    function inLogin() {
        var userName = $("#inUser").val();
        var password = $("#inPass").val();
        if (userName == $("#inUser").attr("placeholder")) {
            alert("用户名/手机号不能为空");
            return;
        }
        if (password.length == $("#inPass").attr("placeholder")) {
            alert("密码不能为空");
            return;
        }

        jQuery.ajax({
            type: "POST",
            url: "/UserInfoAction.ashx",
            dataType: 'json',
            data: "Action=login&time=" + new Date().toLocaleTimeString() + "&U=" + userName + "&P=" + password,
            success: function (data) {
                if (data.Code == "-1") {
                    alert(data.Msg);
                }
                else {
                    window.location.reload();
                }
            }
        });
    }
    function inRegion() {
        var username = $("#inUser_reg").val();
        var phone = $("#inPhone_reg").val();
        var smsCode = $("#inSMSCode_reg").val();
        var pass = $("#inPass_reg").val();
        var rPass = $("#inVePass_reg").val();

        if (username == $("#inUser_reg").attr("placeholder")) {
            alert("用户名不能为空");
            return;
        }
        if (phone == $("#inPhone_reg").attr("placeholder")) {
            alert("手机号不能为空");
            return;
        }
        if (smsCode == $("#inSMSCode_reg").attr("placeholder")) {
            alert("手机验证码不能为空");
            return;
        }
        if (pass == $("#inPass_reg").attr("placeholder")) {
            alert("密码不能为空");
            return;
        }
        if (rPass == $("#inVePass_reg").attr("placeholder")) {
            alert("验证密码不能为空");
            return;
        }
        if (!isChinese(username)) {
            alert("不能含有汉字！");
            return false;
        }
        if (!$('#xieyi').attr('checked')) {
            alert("必须同意本网站用户协议才能成为我们会员！");
            return false;
        }
        jQuery.ajax({
            type: "POST",
            url: "/UserInfoAction.ashx",
            dataType: 'json',
            data: "Action=region&time=" + new Date().toLocaleTimeString() + "&U=" + username + "&M=" + phone + "&P=" + pass + "&VC=" + smsCode + "&RP=" + rPass,
            success: function (data) {
                if (data.Code == "-1") {
                    alert(data.Msg);
                }
                else {
                    window.location.reload();
                }
            }
        });
    }
    var interval1;
    var iTime1 = 30;
    function inSMSCode() {
        var phone = $("#inPhone_reg").val();
        if (phone.length < 1) {
            alert("手机号不能为空");
            return;
        }

        var reg = /^0?1[3|4|5|8|7][0-9]\d{8}$/;
        if (!reg.test(phone)) {
            alert("手机号错误");
            return;
        }
        $("#inMobileCode").css("background", "#9a0000 none repeat scroll 0 0");
        $("#inMobileCode").attr("disabled", true);
        interval1 = window.setInterval(ChangeText1, 1000);

        jQuery.ajax({
            type: "POST",
            url: "/UserInfoAction.ashx",
            dataType: 'json',
            data: "Action=sendSMS&time=" + new Date().toLocaleTimeString() + "&M=" + phone,
            success: function (data) {
                if (data.Code == "-1") {
                    alert(data.Msg);
                }
                else {
                }
            }
        });
    }
    function ChangeText1() {
        $("#inMobileCode").val("验证码(" + iTime1 + ")");
        iTime1 = iTime1 - 1;
        if (iTime1 == 0) {
            window.clearInterval(interval1);
            $("#inMobileCode").css("background", "#b60000 none repeat scroll 0 0");
            $("#inMobileCode").attr("disabled", false);
            $("#inMobileCode").val("重新发送");
            iTime = 30;
        }

    }
    function isChinese(temp) {
        if (/.*[\u4e00-\u9fa5]+.*$/.test(temp)) {
            return false;
        }
        return true;
    }

    function btnLogin() {
        var userName = $("#txtUser").val();
        var password = $("#txtPass").val();
        if (userName == $("#txtUser").attr("placeholder")) {
            alert("用户名/手机号不能为空");
            return;
        }
        if (password.length == $("#txtPass").attr("placeholder")) {
            alert("密码不能为空");
            return;
        }

        jQuery.ajax({
            type: "POST",
            url: "/UserInfoAction.ashx",
            dataType: 'json',
            data: "Action=login&time=" + new Date().toLocaleTimeString() + "&U=" + userName + "&P=" + password,
            success: function (data) {
                if (data.Code == "-1") {
                    alert(data.Msg);
                }
                else {
                    window.location.reload();
                }
            }
        });
    }
    function btnRegion() {
        var username = $("#txtUserName_reg").val();
        var phone = $("#txtPhone_reg").val();
        var smsCode = $("#txtSMSCode_reg").val();
        var pass = $("#txtPass_reg").val();
        var vPass = $("#txtVePass_reg").val();

        if (phone == $("#txtPhone_reg").attr("placeholder")) {
            alert("用户名/手机号不能为空");
            return;
        }
        if (smsCode == $("#txtSMSCode_reg").attr("placeholder")) {
            alert("手机验证码不能为空");
            return;
        }
        if (username == $("#txtUserName_reg").attr("placeholder")) {
            alert("用户名不能为空");
            return;
        }
        if (pass == $("#txtPass_reg").attr("placeholder")) {
            alert("密码不能为空");
            return;
        }
        if (vPass == $("#txtVePass_reg").attr("placeholder")) {
            alert("确认密码不能为空");
            return;
        }
        if (pass != vPass) {
            alert("两次密码不一致");
            return;
        }
        if (!isChinese(username)) {
            alert("不能含有汉字！");
            return false;
        }


        jQuery.ajax({
            type: "POST",
            url: "/UserInfoAction.ashx",
            dataType: 'json',
            data: "Action=region&time=" + new Date().toLocaleTimeString() + "&U=" + username + "&M=" + phone + "&P=" + pass + "&VC=" + smsCode + "&RP=" + vPass,
            success: function (data) {
                if (data.Code == "-1") {
                    alert(data.Msg);
                }
                else {
                    window.location.reload();
                }
            }
        });
    }
    function btnSMSCode() {
        var phone = $("#txtPhone_reg").val();
        if (phone.length < 1) {
            alert("手机号不能为空");
            return;
        }

        var reg = /^0?1[3|4|5|8|7][0-9]\d{8}$/;
        if (!reg.test(phone)) {
            alert("手机号错误");
            return;
        }
        $("#btnMobileCode").css("background", "#9a0000 none repeat scroll 0 0");
        $("#btnMobileCode").attr("disabled", true);
        interval = window.setInterval(ChangeText, 1000);

        jQuery.ajax({
            type: "POST",
            url: "/UserInfoAction.ashx",
            dataType: 'json',
            data: "Action=sendSMS&time=" + new Date().toLocaleTimeString() + "&M=" + phone,
            success: function (data) {
                if (data.Code == "-1") {
                    window.clearInterval(interval);
                    $("#btnMobileCode").css("background", "#b60000 none repeat scroll 0 0");
                    $("#btnMobileCode").attr("disabled", false);
                    alert(data.Msg);
                }
            }
        });
    }
    function ChangeText() {
        $("#btnMobileCode").val("验证码(" + iTime + ")");
        iTime = iTime - 1;
        if (iTime == 0) {
            window.clearInterval(interval);
            $("#btnMobileCode").css("background", "#b60000 none repeat scroll 0 0");
            $("#btnMobileCode").attr("disabled", false);
            $("#btnMobileCode").val("重新发送");
            iTime = 30;
        }

    }
</script> 
