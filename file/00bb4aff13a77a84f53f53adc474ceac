  <script type="text/javascript" charset="utf-8">
    ( function( $ ) {


 
      
   
          $(window).load(function() {
  
  
  
  		//datepicker		 - payment pages	
		 $('.datepickerTrigger').datepicker({
          duration		: 'fast',
          buttonImageOnly	: false,
		  changeMonth: true,
      	  changeYear: true,		
		  //yearRange: "-100:+0", // last hundred years  
          dateFormat		: 'dd-mm-yy',
		  minDate			: 'now'
        });	
		
		
  
  
  			  /*enewsletter signup - date of birth selector*/
        $('.newsletter-dob').datepicker({
          duration		: 'fast',
          buttonImageOnly	: false,
		  changeMonth: true,
      	  changeYear: true,		
		  yearRange: "-100:+0", // last hundred years  
          dateFormat		: 'dd-mm-yy',
        });	
		
		

/*add menu trigger*/
$(".nav2").addClass("js").before('<div id="mobile-menu-trigger">&#x2261;</div>');

/*hide mobile menu trigger */
$(window).resize(function(){
  if(window.innerWidth > 768) {
    $(".header").removeAttr("style");
    //$(".header").css("height","42px")
  }
});

                      

 $('#mobile-menu-trigger').on({
    click: function() {
    
    
    if ( $( this ).hasClass( "menuactive" ) ) {
      $(".nav-item").removeClass("active");
      $(".header").css("height","115px")
      $(".nav2").hide();
      $('#mobile-menu-trigger').removeClass("menuactive");
          
    }else{
      
      $(".nav-item").addClass("active");
      //$(".header").css("height","650px") //1050px
      $(".nav2").show();
      $('#mobile-menu-trigger').addClass("menuactive");
    }
    
    }
});
  




                          
                          
//	on submit - change the action url
   $('#bookform').submit(function(e) { // called by click, on auto submit or enter key


      submitURL = $('#reservation_rid').find(':selected').attr("data-action");
	  
	  
	    if(submitURL == ''){
      	  alert('Please Select Location');
       	 return false;
        }
	  
	  
      //alert(' '+submitURL);
	                        if (Mxm && Mxm.Tracker) {
                              submitURL += '?' + Mxm.Tracker.getQueryKey() + '=' + Mxm.Tracker.getRoiCode();
                            }
							
      $('#bookform').get(0).setAttribute('action', submitURL);
      


  });	// submit form




        $('#reservation_date_day, #reservation_date_month').change(function(e) {
          if ('' == $('#reservation_date_day').val() || '' == $('#reservation_date_month').val()) return;
          var dateText =  $('#reservation_date_month').val() + '-' + $('#reservation_date_day').val();
          $('#reservation_datetime_date').val(dateText);
        });
        
                

        $('#reservation_datetime_date').datepicker({
          showOn			: 'button',
          buttonImage		: '/wp-content/themes/brandstyle1/images/groupsite-brands/calendar-icon.png', //'/wp-content/themes/brandstyle1/images/groupsite-brands/calendar-icon.png',
          duration		: 'fast',
          buttonImageOnly	: false,
          minDate			: 'now',
          maxDate			: 'now + 12 months',
          dateFormat		: 'yy-mm-dd',
          onSelect		: function(dateText, widget) {
            var date = $.datepicker.parseDate('yy-mm-dd', dateText);
            $('#reservation_date_day').val($.datepicker.formatDate('dd', date));
            $('#reservation_date_month').val($.datepicker.formatDate('yy-mm', date));
            // $('#reservation_date_year').val($.datepicker.formatDate('yy', date));
            //var datedisplay = $.datepicker.parseDate('dd-mm-yy', dateText);
            $('#reservation_display').val($.datepicker.formatDate('dd-mm-yy', date));
          }
        }).datepicker( "setDate" ,new Date() );
        
        
          
        
        
          //show picker when display field is clicked
          $('#reservation_display').click(function(e) {
            e.preventDefault();
            $('#reservation_datetime_date').datepicker('show');
          });
                          
                          
                          
          }); // doc ready
    })(jQuery);
  </script>
	 
<script type="text/javascript" src="http://www.coqdargent.co.uk/wp-content/themes/brandstyle1/js/webflow.js"></script>

