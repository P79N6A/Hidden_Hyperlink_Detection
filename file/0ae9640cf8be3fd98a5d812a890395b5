  <script type="text/javascript">
jQuery(document).ready(function() {
function megaHoverOver(){
jQuery(this).find(".sub").stop().fadeTo(200, 1).show();
}
function megaHoverOut(){ 
  jQuery(this).find(".sub").stop().fadeTo(800, 0, function() {
  jQuery(this).hide(); 
  });
}
var config = {    
 sensitivity: 3, // number = sensitivity threshold (must be 1 or higher)    
 interval: 50, // number = milliseconds for onMouseOver polling interval    
 over: megaHoverOver, // function = onMouseOver callback (REQUIRED)    
 timeout: 500, // number = milliseconds delay before onMouseOut    
 out: megaHoverOut // function = onMouseOut callback (REQUIRED)    
};
jQuery("ul#fwmenu li .sub").css({'opacity':'0'});
jQuery("ul#fwmenu li").hoverIntent(config);
});

jQuery(document).ready(function(){
  jQuery('#tuijianyewu').mousemove(function(){
  jQuery('#tuijianyewu_pop').slideDown();//you can give it a speed
  });
  jQuery('#tuijianyewu').mouseleave(function(){
  jQuery('#tuijianyewu_pop').slideUp("fast");
  });
  
});
function checkurl()
{
var url = location.pathname;
var url_dz = location.href;
var menuli=document.getElementById("hjwmn_sns");
var count=0;
//xiewf BBS二级域名时的选中状态
if(url_dz.indexOf('bbs') > 0)
{
menuli=document.getElementById("hjwmn_sns");count++;
jQuery("#menuspite5").hide();
jQuery(menuli).addClass("on");
document.getElementById("portal_url").target="_blank";
return;
}
//xiewf
if(url.indexOf('/hj') != -1 || url.indexOf('/portal') != -1 || url.indexOf('/index') != -1 || url=="" || url=="/")
{
menuli=document.getElementById("hjwmn_index");count++;
jQuery("#menuspite1").hide();
document.getElementById("sns_href").target="_blank";
}
else if(url.indexOf('/back') != -1 ||　url.indexOf('/car')!= -1 || url.indexOf('/returnifo')!= -1 || url.indexOf('/certificate')!= -1)
{
menuli=document.getElementById("hjwmn_returninfo");count++;
jQuery("#menuspite3").hide();
jQuery("#menuspite4").hide();
}
else if(url.indexOf('/incubater') != -1 || url.indexOf('/job') != -1  )
{
menuli=document.getElementById("hjwmn_incubater");count++;
jQuery("#menuspite2").hide();
jQuery("#menuspite3").hide();
}
else if(url_dz.indexOf('bbs') < 0 || url.indexOf('/group') != -1)
{
menuli=document.getElementById("hjwmn_index");count++;
jQuery("#menuspite1").hide();
document.getElementById("sns_href").target="_blank";
}
jQuery(menuli).addClass("on");
}

</script> 
