   <script>
//右下角弹窗JS代码
function closead11adbox() {
jQuery('#boxad_kong').slideDown(1000,function(){
jQuery('#boxad11').height(33); 
jQuery('#boxad_top').show(); 
jQuery('#boxad_inner').hide(); 
jQuery('#boxad_kong').hide(); 
});
          
}
function zoomad11OutBox(time) {
jQuery('#boxad11').height(184);
jQuery('#boxad_kong').show(); 
jQuery('#boxad_top').hide();
jQuery('#boxad_inner').show(); 
jQuery('#boxad_kong').slideUp(1000,function(){  
if(time && time !="undefined") {
boxadtimer=setTimeout(function(){closead11adbox()},time);
}
}) ;        
}

jQuery(document).ready(function() 
{	
jQuery('#boxad_top .gototop').click(function(){jQuery('html,body').animate({scrollTop: '0px'}, 800);});
setTimeout(function(){
var boxadtimer;
        if(jQuery.cookie('hasshow') == "0") {
jQuery('#boxad11').show();
jQuery('#boxad_kong').hide();
jQuery('#boxad_inner').hide();
jQuery('#boxad_top').show();
jQuery('#boxad11').height(33);      
        }
else
{   
jQuery('#boxad11').show();
zoomad11OutBox(6000);
}
jQuery('#zoomadtop').click(function(){
clearTimeout(boxadtimer);
zoomad11OutBox();
var date = new Date();   
date.setTime(date.getTime() + (1 * 2 * 60 * 60 * 1000));   
jQuery.cookie('hasshow', "1" , {expires: date});
        });
        jQuery('#zoomad11').click(function(){
clearTimeout(boxadtimer);
closead11adbox()
var date = new Date();   
date.setTime(date.getTime() + (1 * 2 * 60 * 60 * 1000));   
jQuery.cookie('hasshow', "0",{expires: date});
        });

},1500);

});

</script>
