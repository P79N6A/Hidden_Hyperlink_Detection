   <script>
$(function(){
	$('.video_iframe').each(function(){
		$(this).attr('width',600);
		$(this).attr('height',400);
		var src = "";
		if($(this).data('src')){
			src = $(this).data('src');
		}
		else{
			src = $(this).attr('src');
		}
		$(this).attr('src',src.replace(/&width=300&height=200/g,"&width=600&height=400"));
	});	
})
</script>
