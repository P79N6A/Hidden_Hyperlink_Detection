} else if(values['type'] == 'del') {
fObj.innerHTML = '收听TA';
fObj.href = 'home.php?mod=spacecp&ac=follow&op=add&hash=78816886&fuid='+values['fuid'];
}
}
fixed_avatar([40902], 1);
</script> 
