  <script>
        $(function () {
            $(".hot-article li span:lt(3)").css("background-color", "#f90");
        })
        $(function () {
            $(".font-control span").click(function () {
                var thisEle = $(".news-content p").css("font-size"); //获取当前字体大小
                var textFontSize = parseInt(thisEle);//把当前字体大小转换为整数
                var className = $(this).attr("class");
                if (className == "size-add") {
                    textFontSize += 1;
                } else if (className == "size-minus") {
                    textFontSize -= 1;
                }
                if (textFontSize > 20 || textFontSize < 14) { return false; }//超出指定字体大小，停止运行
                $(".news-content p").css("font-size", textFontSize + "px");//赋值
            });
        });

    </script>   
