    function starOver(obj) {
        var rating_num = jQuery(obj).attr("id").substr(5, 1);
        for ( var i = 1; i <= 5; i++) {
            if (i <= rating_num) {
                jQuery("#star_" + i).addClass("star5").removeClass("star0");
            } else {
                jQuery("#star_" + i).addClass("star0").removeClass("star5");
            }
        }
        jQuery("#stars").val(rating_num);
    }
