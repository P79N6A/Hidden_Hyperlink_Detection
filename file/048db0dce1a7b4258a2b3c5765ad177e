       <script>
       function hdmd(id)
{
	if(id=="0")
	{   
	    document.getElementById("hdmd_3").style.display="none";
		document.getElementById("hdmd_2").style.display="none";
		document.getElementById("hdmd_1").style.display="block";
		document.getElementById("hdmd").className="hdm1";
    }
	else if(id=="1")
	{
		document.getElementById("hdmd_3").style.display="none";
		document.getElementById("hdmd_2").style.display="block";
		document.getElementById("hdmd_1").style.display="none";
		document.getElementById("hdmd").className="hdm2";
	}
	else
	{
		document.getElementById("hdmd_3").style.display="block";
		document.getElementById("hdmd_2").style.display="none";
		document.getElementById("hdmd_1").style.display="none";
		document.getElementById("hdmd").className="hdm3";
	}	
}

</script> 
