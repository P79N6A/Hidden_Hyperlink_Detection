  <script>
//acVids playPause
function func_animate(idx) {
    for (var i = 1; i < window.acVids.slides.length; i++) {
        var vidBtn = document.getElementById("vidBtn" + i);
        var myVideo = document.getElementById("vid" + i);
        if (idx != i - 1) {
            myVideo.pause();
            myVideo.width = 1;
            myVideo.height = 1;
        }
    }
}

function playPause(name, curr, n) {
    for (i = 1; i <= n; i++) {
        var vidBtn = document.getElementById(name + "Btn" + i);
        var myVideo = document.getElementById(name + i);
        if (i == curr) {
            if (myVideo.paused) {
                myVideo.play();
                myVideo.className="vidSize";
            } else {
                myVideo.pause();
                myVideo.className="";
            }
        } else {
            vidBtn.className = "vidBtn";
            myVideo.pause();
        }
    }
}

window.onload = function() {
    //Menu
    var acMenu = document.getElementById("acMenu"),
        acMenuSpan = acMenu.getElementsByTagName("span"),
        main = document.getElementById("main"),
        acNavBox = document.getElementById("acNavBox"),
        acNavBoxBg = document.getElementById("acNavBoxBg");

    acMenu.addEventListener('touchend', function() {
        if (this.clientHeight == 17) {
            this.style.height = 16 + "px";
            acMenuSpan[0].style.webkitTransform = "rotate(45deg) translate(5px,5px)";
            acMenuSpan[1].style.opacity = 0;
            acMenuSpan[2].style.webkitTransform = "rotate(-45deg) translate(5px,-5px)";
            acNavBox.style.height = "auto";
            acNavBoxBg.style.height = main.clientHeight - 50 + "px";
        } else {
            this.style.height = 17 + "px";
            acMenuSpan[0].style.webkitTransform = "rotate(0deg) translate(0px,0px)";
            acMenuSpan[1].style.opacity = 1;
            acMenuSpan[2].style.webkitTransform = "rotate(0deg) translate(0px,0px)";
            acNavBox.style.height = 0;
            acNavBoxBg.style.height = 0;
        }
    }, false);

    //acVids
    window.acVids = new Swiper(".acVids", {
        pagination: '.acVids-pagination',
        onSlideChangeEnd: function() {
            func_animate(window.acVids.activeIndex);
        }
    });

    //totop
    var scrolltop = document.body.scrollTop || document.documentElement.scrollTop;
    if (navigator.userAgent.indexOf("Android 2.2") == -1) {
        window.onscroll = function() {
            var st = document.body.scrollTop || document.documentElement.scrollTop;
            if (st < scrolltop && st != 0) {
                document.getElementById("acTopBox").style.display = "block";
            } else {
                document.getElementById("acTopBox").style.display = "none";
            }
            scrolltop = st
        }
    }
}
</script> 
