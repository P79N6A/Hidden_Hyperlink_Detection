   <script>
function hide_yzm(){ 
	$(".modal-backdrop").remove();
	$("#yzm_div").hide(); 
}
function show_yzm(){
	$("#send_info_btn").attr("disabled", true); 
	if($("#tg_name").val()==""){
		showerr("tg_name","姓名");
		return;
	}
	if($("#tg_tel").val()==""){
		showerr("tg_tel","电话");
		return;
	}
	if($("#tg_mj").val()==""){
		showerr("tg_mj","面积");
		return;
	}
	if($("#tg_lp").val()==""){
		showerr("tg_lp","楼盘");
		return;
	}	
	if(!checkStr($("#tg_name").val())){
		Dialog.alert("请输入中文姓名！");
		$("#send_info_btn").attr("disabled", false);
		return;
	}
	if(!checkTel($("#tg_tel").val())){
		Dialog.alert("请输入正确的手机号！");
		$("#send_info_btn").attr("disabled", false);
		return;
	}
	   var windowWidth = document.documentElement.clientWidth;   
       var windowHeight = document.documentElement.clientHeight;   
       var popupHeight = $("#yzm_div").height();   
       var popupWidth = $("#yzm_div").width();    
       $("#yzm_div").css({   
           "position": "fixed",   
			"z-index":"9999",
           "bottom": "80px",   
           "left": (windowWidth-popupWidth)/2
       }); 
	$("#yzm_div").show();
	var ts_bg=document.createElement("div");
	ts_bg.className="modal-backdrop fade in";
	$("body").append(ts_bg);
}
function virify(){
	var dc = {ckyzm : $("#ckyzm").val()};
	if(dc.ckyzm==""){
		alert("请输入验证码!");
		return false;
	}
	$.post("/zcms/e96617/ckYzmCode",dc,function(response){
		if(response!="") {
			alert(response);
		}else{
			$(".modal-backdrop").remove();
			$("#yzm_div").hide(); 
			send_tginfo();
		}
	},"text");
}
function send_tginfo(){
	var dc={};
	dc.name=$("#tg_name").val();
	dc.telphone=$("#tg_tel").val();
	dc.mj=$("#tg_mj").val();
	dc.IntentionHouse=$("#tg_lp").val();
	dc.siteid="3";
	dc.Type = "tg";
	$.post("/zcms/baoming", dc,
			function(data){
			   	if("报名成功!"==data){
			   		Dialog.alert("报名成功！感谢关注购房网！");
					 location.reload();
			   		
				}else if("用户名或电话已经存在！"==data){
					Dialog.alert(data);
			   		$("#send_info_btn").attr("disabled", false);
				}else{
					Dialog.alert('报名失败！');
			   		$("#send_info_btn").attr("disabled", false);
				}
			   	
			});	
}
//电话校验
function checkTel(tel){
	var rel =new RegExp("^(1[3|4|5|7|8][0-9]{9})$");
	if(!rel.test(tel)){
		return false;
	}
	return true;
}
// 中文名字校验
function checkStr(str){
	// [\u4E00-\uFA29]|[\uE7C7-\uE7F3]汉字编码范围
	var re1 = new RegExp("^([\u4E00-\uFA29]|[\uE7C7-\uE7F3])*$");
	if (!re1.test(str)){
	return false;
	}
	return true;
}
//Err
function showerr(t,t1){
	Dialog.alert("团购"+t1+"不能为空！");
	$("#send_info_btn").attr("disabled", false);
	$("#"+t).focus();
}
//close
function clsthis(){
	$("#BottomNav").hide();
}
</script> 
