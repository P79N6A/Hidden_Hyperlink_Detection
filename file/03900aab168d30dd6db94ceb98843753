   <script type="text/javascript">
var sloginname="";
Page.onReady(function() {
	
	var userLogoutFlagInCookie = Cookie.get("BBSLOGOUT");
	var userInfoInCookie = Cookie.get("BBS2ZCMS");
	var userName = null,password=null;
	if(userLogoutFlagInCookie!=null&&userLogoutFlagInCookie!=""){
		var url = 'http://www.ccgoufang.com/zcms//zcms/member/ajaxlogout?'+new Date().getTime();
		loadJs(url, function(){
			$("#span_login").css("display", "");
			$("#span_logout").css("display", "none");			
		});
		this.Cookie.remove("BBSLOGOUT","/",".ccgoufang.com");
		if(userInfoInCookie!=null){
			this.Cookie.remove("BBS2ZCMS","/",".ccgoufang.com");
		}
		return;
	}
	
    if(userInfoInCookie!=null&&userInfoInCookie!=""){
		var tmpA = userInfoInCookie.split(',');
		if(tmpA.length>=2){
			userName = tmpA[0];password = tmpA[1];
		}
		this.Cookie.remove("BBS2ZCMS","/",".ccgoufang.com");
    }
try {
	var url = 'http://www.ccgoufang.com/zcms/member/logined?timestamp=' + new Date().getTime();
	if(userName!=null){
		url = 'http://www.ccgoufang.com/zcms/memberEx/logined?timestamp=' + new Date().getTime()+'&UserName='+userName+'&Password='+password
			 +'&SiteID=3';
}
loadJs(url, function(){
		if(typeof result != 'undefined'){
			$("#span_login").css("display", result.IsLogin ? "none" : "");
			$("#a_commonlogin").css("display", result.IsLogin ? "none" : "");
			$("#span_logout").css("display", result.IsLogin ? "" : "none");
			if(result.IsLogin){
			  $("a.login_username").html("会员："+result.UserName);
			  this.Cookie.remove("ZCMS2BBSLOGOUT","/",".ccgoufang.com");
			  sloginname=result.UserName;
			}
		}else{
			$("#span_login").css("display", "");
			$("#span_logout").css("display", "none");			
		}
      });
} catch(e) {
		$("#span_login").css("display", "");
		$("#span_logout").css("display", "none");
	}
});
function logout() {
	//扩展定制了退出响应程序
	var url = 'http://www.ccgoufang.com/zcms/memberEx/ajaxlogout?'+new Date().getTime();
	loadJs(url, function(){
		this.Cookie.remove("ZCMS2BBS","/",".ccgoufang.com");
		window.location.href = "http://www.ccgoufang.com/";
	});	
}
$(document).ready(function(){
	$('#mintclose').click(function(){
		$("#mintbar").slideUp("slow");
	});
    $('#mint').click(function() {
        $('#mintbar').animate({height:"toggle"},{duration:1000});
    });
});
</script> 
