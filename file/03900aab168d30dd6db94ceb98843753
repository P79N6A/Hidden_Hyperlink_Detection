function showTab(name,num,n){
	 for(i=1;i<=n;i++){
	  var menu=document.getElementById(name+i);
	  var con=document.getElementById("show_"+name+"_"+i);
	  menu.className=i==num?name+"_btn_show search_color":name+"_btn_normal";
	  con.style.display=i==num?"block":"none";
	  con.className=i==num?name+"_text_show":name+"_text_normal";
	 }
	 sq0.className=name+"_btn_normal";
}
function px2(t){
	window.open("http://house.ccgoufang.com/default/------"+t+"-/","_blank");
}
