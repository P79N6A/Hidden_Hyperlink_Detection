   <script>
var CurrentObj;
var currentDefault;
$(function(){
	$("#city_qh").click(function(){
			$(".city_jilin").toggle();
			return false;
		})
});
function search_key(txtid){
	var seckey=$("#key"+txtid).val().replace(/(\s*$)/g, "").trim().replace("?","");
	if(seckey!=""&&seckey!="输入关键字..."){
		$("#h_list").hide();
		 var url ="http://www.ccgoufang.com/search/3-"+encodeURI(seckey)+"-"+txtid+"-0-/";
		//window.open(url,'_blank');
		location.href=url;
	}
}
var that,currentstr='';
$("input[name='keyword']").keyup(function(){
if($(this).val()!=""){
	if($(this).val().replace(/(\s*$)/g, "").trim()&&currentstr!=$(this).val()){
	    if (event.keyCode == 40) {//down
	        MoveFocus(1);
	    }
	    else if (event.keyCode == 38) {//up
	        MoveFocus(-1);
	    }
	    else if (event.keyCode == 13) {//回车
			search_key($(this).attr("id").replace('key','').trim());clear();
		}
		else{
			currentstr=$(this).val();
			that = this;
			nowsel=$(this).attr("id").replace('key','');
			var itype = $(this).attr("id").replace("key","");
			var dc = {SiteID:'3',key:$(this).val().trim(),itype:itype};
			Server.sendRequest("AllSitePullDown/getResultJsonNew", dc, function(response){
				if(response.Status == 1) {
					if(response.Message!="0"){
						var json = eval("("+response.Message+")");
						var t ="";
						for(var m=0;m<json.result.length;m++){
							var item = json.result[m];
							//t+="<li style='cursor:pointer;' onclick='selectval(this);' onmouseout='moveoutcss(this);' onmouseover='moveincss(this);'>"+item.title+"</li>";
							t+="<li style='cursor:pointer;' onclick='$(that).val($(this).html());search_key(\""+nowsel+"\");clear();' onmouseout='moveoutcss(this);' onmouseover='moveincss(this);'>"+item.title+"</li>";
						}
						showpulldown(t);
					}
					else
						$("#h_list").hide();
				} else {
					//Dialog.warn("数据获取失败"+response.Status);
				}
			},null,null,'json');
		}
	}
	else{
		if($(this).val().replace(/(\s*$)/g, "")){
            if (event.keyCode == 40) {//down
                MoveFocus(1);
            }
            else if (event.keyCode == 38) {//up
                MoveFocus(-1);
            }
            else if (event.keyCode == 13) {//回车
                search_key($(this).attr("id").replace('key','').trim());clear();
			}
			else{
				clear();
			}
		}
	}
}else{
	$("#h_list").hide();
}
});
$("input[name='keyword']").focus(function(){
	if($(this).val()==""){
		nowsel=$(this).attr("id").replace('key','');
		that = this;
		Server.sendRequest("AllSitePullDown/getDefaultJson", "", function(response){
			if(response.Status == 1) {
			if(response.Message!="0"){
				var json = eval("("+response.Message+")");
				var t ="";
				for(var m=0;m<json.result.length;m++){
					var item = json.result[m];

					t+="<li style='cursor:pointer;' onclick='urlup(this,\""+nowsel+"\")' onmouseout='moveoutcss(this);' onmouseover='moveincss(this);'>"
					s_t1=m+1;
					if(m<3){
						t+="<span class='searchtop1_3'>"+s_t1+"</span>"+item.title+"</li>";
					}else{
						t+="<span class='searchtop4_10'>"+s_t1+"</span>"+item.title+"</li>";
					}
				}
				showpulldown(t);
			}
			else
				$("#h_list").hide();
		} else {
			//Dialog.warn("数据获取失败"+response.Status);
		}
	},null,null,'json');
	}
});
//$("input[name='keyword']").blur(function(){	clear(); });
function clear(){
	$("#h_list").hide();
	currentstr='';
}
function urlup(ks,nowsel){
	var skey=$(ks).html().replace(/<span[\s]+[^>]+>([^<>]+)<\/span>/gi,"");
	var url ="http://www.ccgoufang.com/search/3-"+encodeURI(seckey)+"-"+txtid+"-0-/";
	location.href=url;
	clear();
}
function showpulldown(html){
	$("#h_list").width($(that).width()+18);
	$("#h_list").css({ left: $(that).css("left"), top: $(that).css("top")+$(that).height() });
	//alert($(that).parent("div").attr("class"));
	$("#h_list").show();
	$("#h_list").html(html);
}
function selectval(obj){
	$(that).val($(obj).html());
	$("#h_list").hide();
}
function moveoutcss(obj){
	$(obj).removeClass("pullbg");
}
function moveincss(obj){
	CurrentObj = obj;
	$(obj).addClass("pullbg");
}
function MoveFocus(index){
  console.log(index);
    if(CurrentObj){
        $(CurrentObj).mouseout();
        var pre_index = $("#h_list").find("li").index(CurrentObj)+index;
        if(pre_index>=$("#h_list").find("li").size()){
            pre_index = 0;
        }
        else if(pre_index < 0){
            pre_index = $("#h_list").find("li").size()-1;
        }
        $("#h_list").find("li").eq(pre_index).mouseover();
    }
    else
        $("#h_list").find("li").eq(0).mouseover();
    $("input[name='keyword']").val($(CurrentObj).html());
}
</script> 
