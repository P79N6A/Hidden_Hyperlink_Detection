  <script type="text/javascript">
$(document).ready(function() { 
    $("img.lazy_load").lazyload({
		effect: "fadeIn"
	});
	if ($('.recommend li') != null || $('.recommend li').length > 0) {
		$('.recommend li').mouseover(function() {
			$(this).css('backgroundColor', '#dcdcdc');
			$(this).children('.stars').css('display', 'none');
			$(this).children('.down').css('display', 'block');
		}).mouseout(function() {
			$(this).css('backgroundColor', '');
			$(this).children('.stars').css('display', 'block');
			$(this).children('.down').css('display', 'none');
		});
	}
    if ($('.pj_tabs') == null || $('.pj_tabs').length <= 0) {
        return true;
    }else{
        $('.pj_tabs').each(function() {
            var ids = [];
            var titles = $(this).find("a");
            titles.each(function() {
                var id = $(this).attr("href");
                if (id.indexOf("#") >= 0) ids.push(id);
            });
            titles.mouseover(function() {
                var id = $(this).attr("href");
                if (id.indexOf("#") >= 0) {
                    var _top = $(document).scrollTop();
                    $(document).scrollTop(_top + 2);
                    $.each(ids, function(k, v) {
                        $(v).hide();
                        titles.not(this).removeClass("tabactive");
                    });
                    $(this).addClass("tabactive");
                    var showTab = $(this).attr("href");
                    $(showTab).show();
                    $(document).scrollTop(_top);
                }
            });
            titles.first().mouseover();
        });
    }
});
function setTab(name,cursel,n){
    if($('#'+name+'_'+cursel).css("display")=="none"){
        for(i=1;i<=n;i++){
            if(typeof($("#"+name+i)[0])=='object'){
                document.getElementById(name+i).className=i==cursel?"on":"";
            }
            if(typeof($("#"+name+"_"+i)[0])=='object'){
                document.getElementById(name+"_"+i).style.display=i==cursel?'block':'none';
            }
        }
    }
}
</script> 
