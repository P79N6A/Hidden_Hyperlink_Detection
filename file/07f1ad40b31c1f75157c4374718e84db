<script type="text/javascript">
  if( !window._bd_share_config ){
    window._bd_share_config = { "share": [] };
  }
  var oGuideShare = {
    "tag": 'room_guide_share',
    "bdSnsKey":{},
        "bdText": "玩最火的游戏看最爽的直播，精彩就在#战旗直播#（分享自${from}）",
        "bdMini":"2",
    "bdMiniList":false,
    "bdPic":"https://img1.zhanqi.tv/live/20170401/28002_2017-04-01-18-56-54_big.jpg",
    "bdStyle":"0",
    "bdSize":"32",
    "bdUrl":'${room_url}',
    "bdDesc":"窗外的阳光丶正在直播，快来围观",
    onBeforeClick: function(cmd,config){
      var sFrom = '';
      switch( cmd ){
        case 'tsina':
          sFrom = '@战旗直播';
          break;
        case 'tqq':
          sFrom = '@战旗';
          break;
        default:
          sFrom = '战旗直播';
          break;
      }
      config.bdText = yp.format(config.bdText, { from: sFrom });
      config.bdUrl = yp.format(config.bdUrl, { room_url: $('#copy-live-url').val() });
      config.bdDesc = yp.format(config.bdDesc, { from: sFrom });
      yp.ajax(oPageConfig.oUrl.sShareReturnUrl);
      return config;
    }
  };
  window._bd_share_config.share.push(oGuideShare);
</script>


    

