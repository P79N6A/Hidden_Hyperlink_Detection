        function checkMobileUA(){
          try{
            var nav = navigator.userAgent.toLocaleLowerCase()
              , ua = '';

            for( var i = 0, len = aMobileType.length; i < len; i++ ){
              if( !ua && -1 < nav.indexOf( aMobileType[i] ) ){
                ua = aMobileType[i];
              }
            }
            if( !ua && -1 < nav.indexOf('mac') ){
              ua = 'mac';
            }
          } catch(e){}

          return ua;
        }

