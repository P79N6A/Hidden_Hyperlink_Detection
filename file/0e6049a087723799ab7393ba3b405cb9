  <script src="/res/lol/js/jquery-1.11.3.min.js"></script> 
  <script src="/res/common/js/jquery.lazyload.min.js"></script> 
  <script src="http://static.zhangyoubao.com/public/js/emoj.js"></script> 
  <script>

    $(function() {
        // 返回顶部
        $(window).scroll(function(){
            if($(window).scrollTop() > 600){
                $('#side-return').show();
            }else{
                $('#side-return').hide();
            }
        });
        $("#side-return").click(function(){
            $("html,body").animate({scrollTop:0},350);
        });

        //图片懒加载
        $("img.lazy").lazyload();

        if ($(".comment-list").length != 0) {
            emojiBaseUrl="http://static.zhangyoubao.com/public/emoji/";
            $(".comment-list").html(doEmojiReplace($(".comment-list").html()));
        };

        //评论切换
        $('#commentBtn .hot,#commentBtn .new').on('click', function(){
            $(this).parent('div').addClass('active').siblings('.tab').removeClass('active');
            if ($(this).hasClass('hot')) {
                $('#commentList').children('.hot').removeClass('none').siblings('.new').addClass('none');
            } else{
                $('#commentList').children('.new').removeClass('none').siblings('.hot').addClass('none');
            };
        });

        //热门视频切换
        $('#hotVideosBtn .day,#hotVideosBtn .week').on('click', function(){
            $(this).parent('div').addClass('active').siblings('.tab').removeClass('active');
            if ($(this).hasClass('day')) {
                $('#hotVideoList').children('.day').removeClass('none').siblings('.week').addClass('none');
            } else{
                $('#hotVideoList').children('.week').removeClass('none').siblings('.day').addClass('none');
            };
        });

        //热门咨询切换
        $('#hotNewsBtn .day,#hotNewsBtn .week').on('click', function(){
            $(this).parent('div').addClass('active').siblings('.tab').removeClass('active');
            if ($(this).hasClass('day')) {
                $('#hotNewsList').children('.day').removeClass('none').siblings('.week').addClass('none');
            } else{
                $('#hotNewsList').children('.week').removeClass('none').siblings('.day').addClass('none');
            };
        });

        //修改时间
        var nowYear = (new Date()).getFullYear();
        var right = 'Copyright 08 2014 - '+nowYear+' Anzogame. All Rights Reserved';
        $('#right').text(right);

        if($('#fixedAd').length > 0) {
            function getScrollTop(e){
                var scrollTop=0;
                if(document.documentElement&&document.documentElement.scrollTop){
                    return document.documentElement.scrollTop;
                }
                else if(document.body.scrollTop){
                    return document.body.scrollTop;
                }
                else{return scrollTop}//滚到最上方的是就是自己设置的scrollTop变量
            }
            function throttle(method,context){//函数节流，method是要执行的函数，context是作用域函数
                clearTimeout(method.tId);//先清除之前设置的定时器，定时器ID存储在函数的tId属性中
                method.tId=setTimeout(function(){method.call(context)},50)//使用call（）来确保方法在适当的环境运行，如果没第2个参数就在全局执行
            }
            var offsetFix=$("#fixedAd").offset();
            function gun(e){
                e=e||window.event;
                var top=getScrollTop();
                console.log(top);
                console.log(offsetFix);
                console.log(top > offsetFix.top + 487);
                if(top > offsetFix.top + 487) {
                    $("#fixedAd").css({
                        'left': offsetFix.left,
                        'position':'fixed',
                        'top': '0'
                    });
                    return;
                }
                $("#fixedAd").attr('style','');
            }
            window.onscroll=function(e){
                gun();
            };
        }
    });
    </script> 
