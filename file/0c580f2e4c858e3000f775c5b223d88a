    //从底部上升的遮罩效果开始
    $('.sjvideo li a').hover(function(){
        $(this).find('.sjdesc').stop().animate({height:'118px'},200);
        $(this).find('.sjdesc'+" img").stop().animate({opacity:"1"},200);
    },function(){
