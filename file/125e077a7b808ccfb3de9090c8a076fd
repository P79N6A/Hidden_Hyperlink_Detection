function hidediv() {
   document.getElementById("xjhb_zj").style.display ='none';
}
function xianshidiv() {
   document.getElementById("xjhb_zj").style.display ='block';
}
function Postyzm(openid) {
    var msg = document.getElementById('tjname').value;  
	$.post("index.php?s=/index/yanzhengma",{"msg":msg,"openid":openid},function(data){
		if(data == 1){
			window.location.href='index.php?s=/index/tuijiantu/openid/AB95D1D4534C7054CF1D6EAE9E249305/openkey/305B4CB7EDC2ACE1EB124FEFB43AC5E9/type/7';
		}else{
		  $("#yanzhengNew").html(data);
		}
	});    
}
function jcxianshidiv(openid){
	$.post("index.php?s=/index/jcyanzheng",{"openid":openid},function(data){
		if(data == 1){
			window.location.href='index.php?s=/index/tuijiantu/openid/AB95D1D4534C7054CF1D6EAE9E249305/openkey/305B4CB7EDC2ACE1EB124FEFB43AC5E9/type/7';
		}else{
		  xianshidiv();
		}
	}); 	
}
function daodingbu(){
fusion2.canvas.setScroll
({
top : -150
})
}
var AdBanner = {
position: function(b) {
var a = document.getElementById("guanggao_left");
a && 0 === b ? (a.className = a.className.replace(" guanggao_left", ""), a.style.top = "100px") : (0 > a.className.indexOf("guanggao_left") && (a.className += " guanggao_left"), a.style.top = b + 200 + "px");
}
};
fusion2.iface.updateClientRect(
function(rect)
{
AdBanner.position(rect.top);
if(document.getElementById("guanggao_left")){
document.getElementById("guanggao_left").style.top=rect.top+300+"px";
}
});
function showmsg(type,msg){
	fusion2.ui.showMsgbox
	({
		type : type,
		msg : msg,
		timeout :1500		
	})
}

function drw(type,id,openidend){ 
	var saveid = GetCookie('badgoodid'); 
	if(saveid != null)
	{ 
		var saveids = saveid.split(',');
		var hasid = false;
		saveid = '';
		j = 1;
		for(i=saveids.length-1;i>=0;i--)
		{
			if(saveids[i]==id && hasid) continue;
			else {
				if(saveids[i]==id && !hasid) hasid = true;
				saveid += (saveid=='' ? saveids[i] : ','+saveids[i]);
				j++;
				if(j==10 && hasid) break;
				if(j==9 && !hasid) break;
			}
		}
		if(hasid) { fusion2.ui.showMsgbox({type: 3,msg: "蛋友！您已表决过了喔",timeout: 1500}); return false;}
		else saveid += ','+id;
		SetCookie('badgoodid',saveid,1);
	}
	else
	{  
		SetCookie('badgoodid',id,1);
	}
	$.post("index.php?s=/index/drw",{"type":type,"id":id,"openidend":openidend},function(data){
		//改变内容
		if(type == 3){
			$("#wuliao"+id).html(data);
		}else if (type == 2){
			$("#rusuan"+id).html(data);
		}else{
			$("#danteng"+id).html(data);
		}
	});
}

function postBadGood(ftype,fid)
{     
	var taget_obj = document.getElementById(ftype+fid);
	var saveid = GetCookie('badgoodid');
	if(fid == -1){
	   showmsg(3,"自己不能评论");
	   return		
	}
	if(saveid != null)
	{
		var saveids = saveid.split(',');
		var hasid = false;
		saveid = '';
		j = 1;
		for(i=saveids.length-1;i>=0;i--)
		{
			if(saveids[i]==fid && hasid) continue;
			else {
				if(saveids[i]==fid && !hasid) hasid = true;
				saveid += (saveid=='' ? saveids[i] : ','+saveids[i]);
				j++;
				if(j==10 && hasid) break;
				if(j==9 && !hasid) break;
			}
		}
		if(hasid) { fusion2.ui.showMsgbox({type: 3,msg: "这条评论您已表决过了喔",timeout: 1500}); return false;}
		else saveid += ','+fid;
		SetCookie('badgoodid',saveid,1);
	}
	else
	{
		SetCookie('badgoodid',fid,1);
	}
	$.post("index.php?s=/index/dingtie",{"type":ftype,"tid":fid},function(data){
		$("#"+ftype+fid).html(data);
	});
}

//读写cookie函数
function GetCookie(c_name)
{
    if (document.cookie.length > 0)
    {
        c_start = document.cookie.indexOf(c_name + "=")
        if (c_start != -1)
        {
            c_start = c_start + c_name.length + 1;
            c_end   = document.cookie.indexOf(";",c_start);
            if (c_end == -1)
            {
                c_end = document.cookie.length;
            }
            return unescape(document.cookie.substring(c_start,c_end));
        }
    }
    return null
}

function SetCookie(c_name,value,expiredays)
{
    var exdate = new Date();
    exdate.setDate(exdate.getDate() + expiredays);
    document.cookie = c_name + "=" +escape(value) + ((expiredays == null) ? "" : ";expires=" + exdate.toGMTString())+";path=/"; //使设置的有效时间正确。增加toGMTString()
}
function share(title2,link,img,nid){
	var title3 = '[em]e502[/em][em]e502[/em] 快来看这个: ';
	var link = '';
	fusion2.dialog.sendStory({
		url:link,
		msg:title3+title2,
		summary:"ps:点击查看大图",
		title:'十张蛋疼图',
		source:nid,
		img:img,
		onSuccess: function(opt){
		}
	});
}
function PostComment(mgid,openid,openkey) { //发送评论
	var msg = document.getElementById('commentc' + mgid).value;
	var nface = '6';
	var nfeedbacktype = 'feedback';
	var nvalidate = '';
	var nnotuser = '';
	var nusername = '';
	var npwd = '';
	//var taget_obj = $DE('commetcontentNew' + mgid);
	var waithtml = "<div style='line-height:30px'><img src='/images/loadinglit.gif' />正在发送中...</div>";
	var patrn=/[\u4E00-\u9FA5]|[\uFE30-\uFFA0]/gi;
	if (msg == '' || msg == null || msg.length == 0) {
		showmsg(3,"评论内容不能为空");
		return
	};
	if (msg.length > 300) {
		showmsg(3,"请填写300字以内的评论");
		return
	};
	if(!patrn.exec(msg)){
		showmsg(3,"请填写评论文字");
		return
	}
	//alert(msg);
	$.post("index.php?s=/index/commentNew",{"cid":mgid,"msg":msg,"openid":openid,"openkey":openkey},function(data){
		$("#commetcontentNew").html(data);
	});
	showmsg(2,"评论成功");
}

