  <script>
loginurl="<p><a href=\"/reg/login.asp\" class=\"log\" target=\"_self\">登录</a><a href=\"/reg/reg.asp\" target=\"_self\" class=\"zc\">注册</a><a href=\"#\" class=\"pho\" target=\"_self\">移动版</a></p></div>"
function getlogin() {
	$.ajax({
		url:"/reg/option.asp?action=islogin",  
		 cache: false,
        async: true,
        type: "GET",
		success:function(data){  	
			if(data=="err"){
			 $("#loginState").html(loginurl);
			}
			else{
			 $("#loginState").html(data);
			}
			
		}  
    });
}
function outlogin()
 {
	$.ajax({
		url:"/reg/option.asp?action=isout",  
		 cache: false,
        async: true,
        type: "GET",
		success:function(data){ 
		if(data=="succ"){ 	
			 $("#loginState").html(loginurl);}
		}  
    });
}

getlogin()


