  <script>	
	var mousesite=0;
	window.onscroll = function(){
		if($(".menu_n").is(":visible") && $(document).scrollTop()>0 && mousesite==0){
			
	    		//$(".menu_n").hide();
				$(".menu").slideToggle(300);
				mousesite=1;
			//$(".nav_aboutbg").css('display', 'none');
/*			$(".nav_aboutbg").hide();
			$(".nav_pro1bg").hide();
			$(".nav_fanganbg").hide();
			$(".nav_servicebg").hide();
			$(".nav_contactbg").hide();	*/	
			//$("#mheight").slideToggle(300);
	
			$("#mheight").css("height","84");
							
		}
		if(!$(".menu_n").is(":visible") && $(document).scrollTop()==0 && mousesite==1)
		{
			$(".menu").slideToggle(100);			
			//$(".menu_n").show(100);
			//$(".menu").slideToggle(300);			
			mousesite=0;			
				$(".nav_aboutbg").show();
				$(".nav_pro1bg").show();
				$(".nav_fanganbg").show();
				$(".nav_servicebg").show();
				$(".nav_contactbg").show();
	
			$("#mheight").css("height","138");
		}
		else if(!$(".menu_n").is(":visible") && $(document).scrollTop()==0)
		{
			$(".menu").slideToggle(100);			
			//$(".menu_n").show(100);
			//$(".menu").slideToggle(300);			
			mousesite=0;
				$(".nav_aboutbg").show();
				$(".nav_pro1bg").show();
				$(".nav_fanganbg").show();
				$(".nav_servicebg").show();
				$(".nav_contactbg").show();	
	
			$("#mheight").css("height","138");		
		}	
	}
$(function(){
    $(".head").bind("mouseover",function()
	{
		if(!$(".menu_n").is(":visible") && $(document).scrollTop()>0)
		{			$(".menu").slideToggle(300);			
			//$(".menu_n").slideToggle(300);
			//mousesite=0;
				$(".nav_aboutbg").show();
				$(".nav_pro1bg").show();
				$(".nav_fanganbg").show();
				$(".nav_servicebg").show();
				$(".nav_contactbg").show();			
			}
		
	});
    $(".head").bind("mouseleave",function()
	{
		if($(".menu_n").is(":visible") && $(document).scrollTop()>0)
		{//$(".menu_n").hide();
				$(".menu").slideToggle(300);
				//mousesite=1;
			$(".nav_aboutbg").hide();
			$(".nav_pro1bg").hide();
			$(".nav_fanganbg").hide();
			$(".nav_servicebg").hide();
			$(".nav_contactbg").hide();						
				}
		
	});	
	
});
</script>
  <script>
  function jq(id) {
        return document.getElementById(id);
        }

function showMenu (baseID, divID) {
    baseID = jq(baseID);
    divID  = jq(divID);
    if (showMenu.timer) clearTimeout(showMenu.timer);
  hideCur();hideCura();
    //divID.style.display = 'block';   
	showMenu.timer = setTimeout(function () {divID.style.display = 'block';baseID.className = "menu_na";$('.nav_pro1bg').pngFix( );mousesite=1;}, 600);
			
  showMenu.cur = divID;
  showMenu.cura = baseID;

    if (! divID.isCreate) {
        divID.isCreate = true;
        //divID.timer = 0;
        divID.onmouseover = function () {
            if (showMenu.timer) clearTimeout(showMenu.timer);
      hideCur();
            divID.style.display = 'block';
        };

        function hide () {
		if (showMenu.timer) clearTimeout(showMenu.timer); // 
            showMenu.timer = setTimeout(function () {divID.style.display = 'none';baseID.className = "";mousesite=0;}, 300);
      
        }

        divID.onmouseout = hide;
        baseID.onmouseout = hide;
		mousesite=1;		
    }
  function hideCur () {
    showMenu.cur && (showMenu.cur.style.display = 'none');

  }
  function hideCura () {
    showMenu.cura && (showMenu.cura.className = '');
  }  
}
</script>
