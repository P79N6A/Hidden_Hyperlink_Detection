        <form id="form" onsubmit="return false;">
            <script>
            //表单提交方法
            var showMsg = function(msg , callback){
                 var $sub = $("#form").find("input[type='submit']");
                 $sub.prop("disabled" , "disabled");
                 $("#showInfor").html(msg);
                 !!callback && setTimeout(function(){
                        callback($sub);
                 } , 1000);
            };
            $("#form").submit(function  () {
                var data = {
                    username: $("#username").val(),
                    email: $("#email").val(),
                    qq: $("#qq").val(),
                    mobile: $("#mobile").val()
                }, msg;

                if (data.username == '') {msg = "姓名不能为空";  $("#showInfor").html(msg); return false;};
                if (data.email == '') {msg = "邮箱不能为空";  $("#showInfor").html(msg); return false;};
                if (data.qq == '' && data.mobile == '') {msg = "QQ和手机至少填写一个";  $("#showInfor").html(msg); return false;};

                var emailReg = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
                if (!emailReg.test(data.email)) {msg = "邮箱格式不对";  $("#showInfor").html(msg); return false;};

                if (data.qq != '') {
                    var qqReg = /^[1-9]\d{4,13}$/;
                    if (!qqReg.test(data.qq)) {msg = "QQ号码格式不对";  $("#showInfor").html(msg); return false;};
                };

                if (data.mobile != '') {
                    var mobileReg = /^1[3|4|5|6|7|8|9][0-9]{1}[0-9]{8}$/;
                    if (!mobileReg.test(data.mobile)) {msg = "手机号码格式不对";  $("#showInfor").html(msg); return false;};
                };

                $.post("/addprofile", data, function(ret) {
                    if ("object" === typeof ret) {
                        if ("0" == ret.errno) {
                            msg = "提交成功！";
                        } else {
                            msg = ret.errmsg;
                        }
                    }
                    $("#showInfor").html("");
                    showMsg(msg , function($sub){
                          $sub.removeProp("disabled");
                    });
                    return false;
                }, 'json');
                return false;
            });
            </script>
            <div class="tc-center">
                <div class="tc-td">
                    姓名
                    <input type="text" placeholder="" name="username" id="username">
