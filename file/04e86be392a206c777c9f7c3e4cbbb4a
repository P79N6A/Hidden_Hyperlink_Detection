    if($('#J_watch_more')[0]){
        var Box = $('#J_watch_more'),
            oBtnMore = $('.btn_wathc_more', Box),
            oArtcle = $('.first_floor');

        oBtnMore.on('click',function(){
            oArtcle.css('height','auto');
            oBtnMore.remove();
            Box.css('marginTop',0);
            $('.watch_more_cont',Box).css('border',0);
        });
    }

</script> 
  <script>
    $(function(){
        //百度分享
        if($('#J_share_widget')[0]){
            window._bd_share_config={
                "common":{
                    "bdSnsKey":{ },
                    "bdText":"",
                    "bdMini":"2",
                    "bdMiniList":false,
                    "bdPic":"",
                    "bdStyle":"0",
                    "bdSize":"32"
                },
                "share":{ }
            };
            with(document)0[(getElementsByTagName('head')[0]||body).appendChild(createElement('script')).src='http://bdimg.share.baidu.com/static/api/js/share.js?v=89860593.js?cdnversion='+~(-new Date()/36e5)];
        }

        $('.btn_more').bind('click',function(e){
            e.preventDefault();
            hideLayer();
            var oParent = $(this).parent();
            $('.more_list',oParent).fadeIn();
            e.stopPropagation();
        });

        $('.btn_share').bind('click',function(e){
            e.preventDefault();
            hideLayer();
            $('#J_share_widget').addClass('show');
            e.stopPropagation();
        })

        $('body').on('click',function(){
            hideLayer();
        });

        $('#J_share_widget .btn_cancel_share').on('click',function(){
            $('#J_share_widget').removeClass('show');
        });

        $(window).on('scroll',function(){
            var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
            if(parseInt(scrollTop) > 0){

                if($('#J_down_app')[0]){
                    $('#J_down_app .btn_close').click();
                }
                if($('#J_down_guide')[0]){
                    $('#J_down_guide .btn_close').click();
                }
                $('.sub_header').addClass('posi_fixed');
            }else{
                $('.sub_header').removeClass('posi_fixed');
            }
        });

        function hideLayer(){
            $('.more_list, .manage_more').hide();
            $('#J_share_widget').removeClass('show');
        }

    })

</script> 
