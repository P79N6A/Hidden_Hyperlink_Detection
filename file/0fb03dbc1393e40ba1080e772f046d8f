	function dPlayNext()
	{
        var shortname = ".html";
        var npage = 4;
        var totalpage = 6;
		var namehand = '79751';
		var displaytype = 'st';
		var gtimer = null;
        if(npage==totalpage)
		{
			if(namehand!='') location.href = namehand+shortname;
				else location.href = "view.php?aid=79751";
		}
		else
		{
			if(displaytype=='st' && namehand!='') location.href = namehand+"_"+(npage+1)+shortname;
			else location.href = "view.php?aid=79751&pageno="+(npage-1);
		}

	}
