var isqddslp = "1"; 
var currNewcode = '2814263674';
var newcode = '2814263674';
var city = '广州';	
var NewhouseIm = (function($,window,currNewcode,isqddslp,city) {
	var config = {
		'userinfo':{},
		'msgout':{}
	};
	// check login
	var loginInfo = function() {
		$.ajax({
			type:'post',
			url:'http://newhouse.fang.com/house/ajaxrequest/user_login_info.php',
			success:function(d) {
				var json_d = eval('('+d+')');
				if(json_d.code == '100') {
					config.userinfo = json_d.msg;
				}
				window.fangWebIM.init({
					type:'webxf',
					userid: config.userinfo.uid,//登录用户请传通行证用户ID，非登录不传
					username: config.userinfo.uname,//登录用户请传通行证用户名，非登录不传
					showname: config.userinfo.uname,//显示用
					city: city//城市必填
				});
			}
		});
	};
	// login init
	var imInit = function() {
		loginInfo();
	};
	// consultant init
	var advisterInit = function(list) {
		window.fangWebIM.addConsultant(list);
	};
	// borker init
	var borkerInit = function(list) {
		window.fangWebIM.addBroker(list);
	};
	// get  list
	var createList = (function() {
		imInit();
		$.ajax({
			type:'get',
			dataType:'jsonp',
			data:{'newcode':currNewcode ,'isqddslp':isqddslp},
			url:'http://newhouse.fang.com/house/ajaxrequest/userList.php',
			success:function(d) {
				if(d.conultant_list) {
					advisterInit(d.conultant_list);
				}
				if(d.agent_list) {
					borkerInit(d.agent_list);
				}
				if(d.msgout) {
					config.msgout = d.msgout;
				}
			}
		});
	}());
	/*-- talk --*/
	//startChatWith(\''+currNewcode+'\', \''+msgRes[key].user_id+'\', \''+msgRes[key].username+'\', \''+msgRes[key].realname+'\', \''+projname+'\', \''+city+'\', 0)
	var talkTo = function (data) {
		window.fangWebIM.openChat({
			"type": 'webxf',
			"contactId": 'x:'+data[0],
			"showname": data[1],
			"city": data[2],
			"pname": data[3],
			"user_id":data[4],
			"shop": data[5],
			"avatar": data[6],
			"tel": data[7],
			"tip": config.msgout
		}); 
		//埋码
		_ub.collect(24,{"vwg.page":maima_page,"vwn.projectid":currNewcode,"vwn.consultantid":data[4]});
	};
	/*open list*/ 
	var openIm = function() {
		window.fangWebIM.open();
	};
	/*-- click event--*/
	$("#contactsList1").click (function(e) {
		var tg=e.target;
		if (tg.nodeName.toLowerCase() == "span") {
			data=tg.id.split("|");
			window.fangWebIM.openChat({
				"type": 'webxf',
				"contactId": 'x:'+data[0],
				"showname": data[1],
				"city": data[2],
				"pname": data[3],
				"tip":config.msgout
			});
		}
	});
	
	$("#contactsList2").click(function(e) {
		var tg=e.target;
		if (tg.nodeName.toLowerCase() == "span") {
			data=tg.id.split("|");
			window.fangWebIM.openChat({
				"type": 'webxf',
				"contactId": 'x:'+data[0],
				"showname": data[1],
				"city": data[2],
				"pname": data[3],
				"tip":config.msgout
			});
		}
	});
	$("#contactsList3").click(function(e) {
		var tg=e.target;
		if (tg.nodeName.toLowerCase() == "span") {
			data=tg.id.split("|");
			window.fangWebIM.openChat({
				"type": 'webxf',
				"contactId": 'x:'+data[0],
				"showname": data[1],
				"city": data[2],
				"pname": data[3],
				"tip":config.msgout
			});
		}
	});
	return {
		talkTo:talkTo,
		config:config,
		openIm:openIm
	};
}($,window,currNewcode,isqddslp,city));
