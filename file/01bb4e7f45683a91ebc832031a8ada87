				var chatDomain = '';
				var isqddslp = '1';
				var ishuodongqddslp = '1';					
				var newsid = '';
				var userid = useiidd;
				var title = '';
				var imgpath = 'http://img.fang.com/newhousedata/images/default_img.png';
 
				
				
				var signup_phone_default = '请输入您的手机号';
				var signup_name_default = '';
				var signup_captcha_default = '';
				var signup_count_default = '';
				var bind_phone = '';

				$(function(){
					
					//判断用户是否是通行证+绑定手机号+绑定搜房卡
				    var ajaxUrl = newhouseDomain+"/house/ajaxrequest/singleIsCorrectMobileUser.php?rand="+Math.random();
				    $.ajax({
				        type:'get',
				        dataType:'jsonp',
				        url:ajaxUrl,
						scriptCharset: 'utf-8',
				        data:'',
				        complete:function(){},
				        error:function(){},
				        success:function(msg){
				            //如果用户已经登录
				            if(msg.uid) 
							{
				                bind_phone = msg.bindMobile;
				                useiidd = msg.uid;
				                $("#signup_phone").val(bind_phone); 
				            }
							else 
							{
				                bind_phone = '';
								$("#signup_phone").val(signup_phone_default);  
				            }
				        }
				    });
					
					$(".ljbm").click(function(){
						
					  
						
						var idAttr = $(this).attr("id");
						var tmp = idAttr.split("_");
						var signup_id = tmp[1];  
						var line_id = tmp[2];
						$("#signup_id").val(signup_id);
				        $("#line_id").val(line_id); 
						if($("#line_id").val() == line_id)
						{
							$("#signup_box").toggle();              
				        }
						else
						{
							$("#signup_box").show();
						}
						
						var housedata = house_jsondata;    //楼盘意向
						var yxloupan = '';
						var yxloupanInfo = housedata[line_id];
						for(var lpid  in yxloupanInfo)
						{
							yxloupan += '<div class="check-line" style="width:130px;float:left;"><input type="checkbox" class="lpyx-chk" value="'+lpid+'"/><label for="" class="lpyx-lab">'+yxloupanInfo[lpid].housetitle+'</label></div>';
						}
						$("#signup_yxloupan").html(yxloupan);  
					});
					
					$(".sub-btn").click(function(){
						var baoMingid = $(".ljbm").attr("id");
				        submitKFTbaoMing(baoMingid);
				    });
					
					$(".pop-close").click(function(){
						$("#signup_box").hide();
					});
					
					$(".xy").click(function(){
						$(".xy-detail").toggle();
					});
					
					//关闭报名成功提示
					$("#exit_bm_suc").click(function(){
						$("#baoming_suc").hide();
					});	
				});

				// 看房团报名
				function submitKFTbaoMing(baoMingid)
				{
					var linename = kft_linename;	
					var sign_id = $("#signup_id").val();
					var line_id = $("#line_id").val();
					var phone = $("#signup_phone").val();
					var name = $("#signup_name").val();
					var count = $("#signup_count").val();
					var phonecode = $("#signup_captcha").val();//手机号验证码
					var BehaviorID = "155";
					phonecode = (phonecode == '' || phonecode == signup_captcha_default) ? '' : $("#signup_captcha").val();
					if(phonecode == '' || phonecode == signup_captcha_default)
					{
						if($("#tg_getValCode").css("display") == 'none')
						{
						
						}
						else
						{		
							alert("请输入手机验证码");
							return false;
						}
					}
					var newcodedom = $('#signup_yxloupan :checked');
					var newcodelist = new Array();
					for(var i=0; i < newcodedom.length; i++) 
					{
						newcodelist[i] = $(newcodedom[i]).val();
					}
					newcodelist = newcodelist.join(',');
					var strP = /^\d+$/; 
					if( count > 10 || !strP.test(count) || count <= 0)
					{
						alert("你好，报名人数最多10人!");
						return false;
					}
					else if(phone == '' || phone == signup_phone_default) 
					{
						alert("请填写手机号！");
						return false;
					}
					else if(name == "" || name == signup_name_default) 
					{
						alert("请填写用户名！");
						return false;
					}
					else if(count <= 0 || count == signup_count_default) 
					{
						alert("报名人数不能为空！");
						return false;
					}
					 $.ajax({
						type:'get',
						dataType:'jsonp',
						data:{mobilephone:phone,service:"newhouse-open-web",mobilecode:phonecode},
						url:"https://passport.fang.com/loginverifysms.api",
						error:function(){
							alert("验证码错误，请重试");},
						success:function(msg){
							if(msg.Message == "Success" || bind_phone)
							{
								$.ajax({
									type:'get',
									dataType:'jsonp',
									url:newhouseDomain+"/house/ajaxrequest/singleKft.php",
									scriptCharset: 'utf-8',
									data:'BehaviorID='+BehaviorID+'&signid='+sign_id+'&lineid='+line_id+'&city='+urlencode(city)+'&phone='+phone+'&name='+urlencode(name)+'&count='+count+'&phonecode='+phonecode+'&yxloupan='+newcodelist,
									success:function(msg){  
										if(msg.result == 1) 
										{
											$("#signup_box").hide();   
											$("#baoming_suc").show();								
											signUpFormInit();
											
											_ub.collect(33,{'vwg.page':'nhkftpage','vwn.seehouseline':encodeURIComponent(linename),'vwn.name':encodeURIComponent(name),'vwn.number':encodeURIComponent(count),'vwn.phone':phone}); 
											
										}
										else
										{
											alert(msg.msg);
											signUpFormInit();
										}
									}
								});
							}
							else
							{
								alert(msg.Tip);
								return false;
							}
						}
					});   
				}

				//初始化表框
				function signUpFormInit() 
				{
				    $("#signup_phone").val((bind_phone != '') ? bind_phone : signup_phone_default);
				    if (bind_phone)
					{
				        //隐藏获取手机号验证码的对话框
				        $("#telephonemodule").css("display","none");

				    }
					else 
					{
				        //显示获取手机号验证码的对话框
				        $("#telephonemodule").css("display","block");
				    }
				}

				$(function(){
					//给手机号输入框添加事件
					$("#signup_phone").keyup(function(){
						//是否绑定手机号
						if($("#signup_phone").val() == bind_phone && bind_phone != signup_phone_default && bind_phone != '') 
						{

						}
						else 
						{
							//显示获取手机号验证码的对话框
							$("#telephonemodule").css("display","block");
							$("#tg_getValCode").css("display","block");
						}
					})

					//添加表框的动作事件
					$("#signup_phone").focus(function(){
						//检查是否和默认值相同，如果相同，那么直接清空
						if($("#signup_phone").val() == signup_phone_default) 
						{
							$("#signup_phone").val('');
						}
					})

					$("#signup_name").focus(function(){
						//检查是否和默认值相同，如果相同，那么直接清空
						if($("#signup_name").val() == signup_name_default) 
						{
							$("#signup_name").prev().text('');
						}
					})

					$("#signup_captcha").focus(function(){
						//检查是否和默认值相同，如果相同，那么直接清空
						if($("#signup_captcha").val() == signup_captcha_default)
						{
							$("#signup_captcha").prev().text('');
						}
					})
					
					$("#signup_count").focus(function(){
						//检查是否和默认值相同，如果相同，那么直接清空
						if($("#signup_count").val() == signup_count_default) 
						{
							$("#signup_count").prev().text('');
						}
					})
					
					$("#signup_count").blur(function(){
						//检查是否和默认值相同，如果相同，那么直接清空
						if($("#signup_count").val() == signup_count_default) 
						{
							$("#signup_count").prev().text('请输入报名人数');
						}
					})

					//失去焦点动作
					$("#signup_phone").blur(function(){
						//检查是否为空，如果为空那么直接等于默认值
						if($("#signup_phone").val() == '') 
						{
							$("#signup_phone").val(signup_phone_default);
						}
					})

					$("#signup_name").blur(function(){
						//检查是否为空，如果为空那么直接等于默认值
						if($("#signup_name").val() == '') 
						{
							$("#signup_name").prev().text('请输入用户名');
						}
					})

					$("#signup_captcha").blur(function(){
						//检查是否为空，如果为空那么直接等于默认值
						if($("#signup_captcha").val( )== '')  
						{
							$("#signup_captcha").prev().text('请输入手机验证码');
						}
					})
						
				});	


				
 </script>		 
