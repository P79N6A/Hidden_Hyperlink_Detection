<script type="text/javascript">
_ub.city = '广州';//必写，中文
_ub.request('vwg.business,vwn.position,vwn.subway,vwn.belongschool,vwn.avgprice,vwn.fixstatus,vwn.feature', function(){ //（直接把需要的往后添就行）
        _ub.load(2); //选取时间最近的值    
        //以下添加业务逻辑
        var urllocation ='/dongtai/allSee.do';
        
        if(location.hostname.indexOf('www.fang.com')==0){
        	urllocation = 'http://www.fang.com/news/ajax/allSee.do?flag=1&city='+encodeURIComponent(_ub.city);
        }
        var business=_ub['vwg.business'];
        	var position =_ub['vwn.position'];//区域
        	var subway =_ub['vwn.subway'];//地铁
        	var school =_ub['vwn.belongschool'];//学校
        	var prelprice =_ub['vwn.avgprice'];//平均价格
        	var fixstatus =_ub['vwn.fixstatus'];//装修
        	var feature =_ub['vwn.feature'];//热门
        	$.ajax({
    			url:urllocation,
    			type:'post',
    	        dataType:'html',
    	        data:{
    	        	"city":_ub.city,
    				"subway":subway,
    				"position":position,
    	        	'school':school,
    	        	'totalprice':prelprice,
    	        	'fixstatus':fixstatus,
    	        	'feature':feature,
    	        },
    	        success:function(data){
    	        	if(data==null || data.trim()==""){
    	        		$("#transactionId").empty();
    	        	}else{
    	        		$("#allSee").append(data);
    	        	}
    	        }
    		}); 
}); 
</script>
