	function check_pinglun()
	{   
	    /*var username = document.getElementById('username');
		var tel = document.getElementById('tel');
		var qq = document.getElementById('qq');
		var email = document.getElementById('email');
		var title = document.getElementById('title');
		var leavecontents = document.getElementById('leavecontents');*/
		
		var nicheng = document.frm_reg.nicheng;
		var contents = document.frm_reg.contents;
		
		if(nicheng.value=="")
		{
			alert("昵称不能为空！");
			nicheng.focus();
			return false;
		}
		if(nicheng.value.length<2 || nicheng.value.length>32)
		{
			alert("姓名必须为2-32个字符，请认真填写");
			nicheng.focus();
			return false;
		}

		if(contents.value=="")
		{
			alert("内容不能为空！");
			contents.focus();
			return false;	
		}
		
		return true;
	}
</script> 
