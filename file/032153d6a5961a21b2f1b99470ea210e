//右侧浮动
$(document).ready(function(e) {         
    t = $('.fixed_adv_right').offset().top;
    fh = $('.fixed_adv_right').height();
    $(window).scroll(function(e){
        s = $(document).scrollTop();    
        if(s > t){
            $('.fixed_adv_right').css('position','fixed');
            $('.fixed_adv_right').css('top','0px');
            mh = $('.main_news_l').height();
            if(s + fh > mh+163){
                $('.fixed_adv_right').css('top',mh+241-s-fh+'px');    
            }               
        }else{
            $('.fixed_adv_right').css('position','');
        }
    })
});
