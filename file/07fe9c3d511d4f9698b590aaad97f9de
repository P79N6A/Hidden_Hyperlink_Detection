  <script>

$(document).ready(function(){

		$.get("http://vgou1.com/member/cart/getnumber/"+new Date().getTime(),function(data){

			$("#sCartTotal").text(data);

		});

	});



document.onkeydown=function(event)

{ 

	e = event ? event :(window.event ? window.event : null);

	ss= document.getElementById('txtSearch').value;

	if(e.keyCode==13 && ss!=""){

	 window.location.href="http://vgou1.com/s_tag/"+$("#txtSearch").val();

	}

}



$(function(){

	$("#txtSearch").focus(function(){

		$("#txtSearch").css({background:"#fff"});

		$(this).attr("placeholder","");

	});

	$("#txtSearch").blur(function(){

		$("#txtSearch").css({background:"#FFF"});

		$(this).attr("placeholder","请输入要搜索的商品");	

	});

	$("#butSearch").click(function(){

		var val1="手机"

	    if($("#txtSearch").val()==""){

			window.location.href="http://vgou1.com/s_tag/"+val1;

		}else

		if($("#txtSearch").val()==$("#txtSearch").val()){

			window.location.href="http://vgou1.com/s_tag/"+$("#txtSearch").val();

		}

	});

});



	$(".yu_ff").mouseover(function(){

	  $(".h_1yyg_eject").show();

	})

	$(".yu_ff").mouseout(function(){

	  $(".h_1yyg_eject").hide();

	})



		

	//收藏

	function AddFavorite(sURL, sTitle){

		try

		{

			window.external.addFavorite(sURL, sTitle);

		}

		catch (e)

		{

			try

			{

				window.sidebar.addPanel(sTitle, sURL, "");

			}

			catch (e)

			{

				alert("您可以通过快捷键Ctrl+D进行添加");

			}

		}

	}

	

	$(".mobile").mouseover(function(){

		$(".h_mobile").show();

		$(".h_mobile  s").css("background","../images/headbg11.png").css("background-position","5px -64px");

	})

	$(".h_mobile").mouseout(function(){

		$(".h_mobile").hide();

	})

	$(".mobile1").mouseover(function(){
		$(".h_mobile1").show();
		$(".h_mobile1  s").css("background","../images/headbg11.png").css("background-position","5px -64px");
	})
	$(".h_mobile1").mouseout(function(){
		$(".h_mobile1").hide();
	})

	

</script>
