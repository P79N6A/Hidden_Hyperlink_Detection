    function smenuTab(index) {
        $("#showList .index-tags").stop(true, false).hide().eq(index).stop(true, false).show();
    }
    $showid = '';
    if ($showid == 1 || ($showid > 10 && $showid < 20)) $showid = 2;
    else if ($showid == 2 || ($showid > 20 && $showid < 30)) $showid = 1;
    else if ($showid == 3 || ($showid > 30 && $showid < 40)) $showid = 3;
    else if ($showid == 4 || ($showid > 40 && $showid < 50)) $showid = 4;
    else $showid = '0'
    $showclassid = '#show' + $showid;
    $($showclassid).addClass('current');
    smenuTab($showid);
