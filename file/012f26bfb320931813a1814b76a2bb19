   <script language="javascript">
$(function(){
	if($("#login").length==0) return false;
	$("#login").submit(function(){
		var f=$(this).get(0);
		var user=f.user.value;
		var pass=f.pass.value;
		if(user=="") {
			alert("用户名不能为空");
			f.user.focus();
		} else if(pass=="") {
			alert("密码不能为空");
			f.pass.focus();
		} else {
			$(".login_left").block({message:"<img src='images/loading.gif' /> 请稍后...",css:{padding:"5px"}});
			$.ajax({
				type: "POST",
				url: "login.asp?action=login",
				data: {user: escape(user), pass: escape(pass)},
				cache: false,    // 禁止缓存
				success: function(content){ // 成功时的回调函数
					if(content.length>0) alert(content);
					else window.location.href="loginafter.asp";
					$(".login_left").unblock();
				},
				error: function(XMLHttpRequest, textStatus) {
					alert("服务器端错误，请稍后重试！");
					$(".login_left").unblock();
				}
			});
		}
		return false;
	});
});
</script> 
