   <script type="text/javascript">

$("#closepic").live("click",function(){
	setTimeout("$('#thisdiv').css('right','95%')",500);
		$("#thisdiv").stop().animate({
			right:2000
		});
		$("#closepic").css("display","none");
		$("#openpic").css("display","block");
		$("#openit").css("display","block");
	$("#thisdiv").css("display","none");
});
$("#openit").live("click",function(){
	$("#thisdiv").stop().animate({
		right:0
	});
	$("#thisdiv").css("display","block");
	$("#openit").css("display","none");
	$("#openpic").css("display","none");
	$("#closepic").css("display","block");
});
