            if(jdata['HuoDongNum']>0)
                document.getElementById('HuoDongNum').innerHTML=jdata['HuoDongNum'];
            if(jdata['PhotoNum']>0)
                document.getElementById('PhotoNum').innerHTML=jdata['PhotoNum'];
            if(jdata['ZixunNum']>0)
                document.getElementById('ZixunNum').innerHTML=jdata['ZixunNum'];
            if(jdata['ChushouNum']>0)
                document.getElementById('ChushouNum').innerHTML=jdata['ChushouNum'];
            if(jdata['ChuzuNum']>0)
                document.getElementById('ChuzuNum').innerHTML=jdata['ChuzuNum'];
            if(jdata['DuanzuNum']>0)
                document.getElementById('DuanzuNum').innerHTML=jdata['DuanzuNum'];
            /*
            if(jdata['QiuzuNum']>0)
                document.getElementById('QinzuNum').innerHTML=jdata['QiuzuNum'];
            */
            if(jdata['ShopNum']>0)
                document.getElementById('ShopNum').innerHTML=jdata['ShopNum'];
            if(jdata['FoodNum']>0)
                document.getElementById('FoodNum').innerHTML=jdata['FoodNum'];
            if(jdata['TiaozaoNum']>0)
                document.getElementById('TiaozaoNum').innerHTML=jdata['TiaozaoNum'];
            if(jdata['CouponNum']>0)
                document.getElementById('CouponNum').innerHTML=jdata['CouponNum'];
            if(jdata['RepairNum']>0)
                document.getElementById('RepairNum').innerHTML=jdata['RepairNum'];
            if(jdata['CaseNum']>0)
                document.getElementById('CaseNum').innerHTML=jdata['CaseNum'];
            if(jdata['NetShopNum']==1){
                var content = '<a id="wangdian" href="'+shequdomain+'/wangdian/" class="linkblue1208">搜房网店</a><div class="span2018" id="NetShopNum"></div>';
                document.getElementById('innerwd').innerHTML=content;
                //var wdobj = document.getElementById('wangdian');
                //var pnode = wdobj.parentNode;
                //pnode.innerHTML="";
            }
            if(jdata['CuxiaoNum']>0)
            {
                document.getElementById('CuxiaoNum').innerHTML=jdata['CuxiaoNum'];
            }   
        }    
    }
    function $_(id)
	{
	    return document.getElementById(id);
	}
	function jsonToarray(dat){
        return eval('(' + dat + ')')
    }
    var tmp = new Array();
    //Xml.Request("/house/esf/getuser.php?city="+cityencode+"&newcode="+newcode+"&citycode=", null, dologinuser,tmp);
    function dologinuser(userpic)
    {
        var user_data = this.responseText;//获取cookie
        if(user_data){
            var obj = jsonToarray( user_data );
            if(obj.uname && obj.uid)
            {
                $_('toplogin1').style.display="none";
                $_('toplogin2').style.display="none";
                $_('topusername').innerHTML=obj.uname;
                $_('loginBar').style.display="block";
            }
        }
    }
</script>
	
