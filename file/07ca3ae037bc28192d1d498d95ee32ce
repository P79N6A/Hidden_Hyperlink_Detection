function addColorClass(id){
    $("#leftAlbum li img").remove();
	$("a[href^='#part_']").each(function(){
		$(this).removeClass("up");
	});
	$("#"+id).before("<img src='http://img1.soufun.com/house/detail_php/images/sanjiao.png'/>");
	$("#"+id).addClass("up");
}
</script>
