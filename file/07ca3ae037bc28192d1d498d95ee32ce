<script type="text/javascript">
function adjustFloatNavPos(){
    if(parseInt($(document.body)[0].clientWidth)<1200){
	    $("#floatingBox").hide();
	}else{
	    $("#floatingBox").show();
	}
	var x=$("#toDingBu").offset();
	if(x.top>500){
	    $("#toDingBu").show();
	}else{
	    $("#toDingBu").hide();
	}
	
}
$(window).resize(adjustFloatNavPos);
$(window).scroll(adjustFloatNavPos);
$(function() {
    $("#toDingBu").hide();
   adjustFloatNavPos();
   setInterval(adjustFloatNavPos, 100); 
});
function showDbox(index)
{
	var obj = document.getElementById("dbox_"+index);
	obj.style.display = '';
}
function hiddenDbox(index)
{
	var obj = document.getElementById("dbox_"+index);
	obj.style.display = 'none';
}
</script>
	 
	 	


