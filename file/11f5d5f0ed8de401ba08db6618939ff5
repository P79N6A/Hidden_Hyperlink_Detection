  <script type="text/javascript">
     function change(type)
     {
         var searchBox = document.getElementById("searchBox");
         searchBox.name = type;
     }
     function submitForm()
     {
         if ($("#searchBox").attr("name") == "q")
         {
             window.open("http://www.baidu.com/s?wd=" + $("#searchBox").val() + "&si=www.vccoo.com&ct=2097152");
         }
         else
         {
             document.getElementById('formSearch').submit();
         }
     }
     function check()
     {
         if ($("#searchBox").attr("name") == "q")
         {
             window.open("http://www.baidu.com/s?wd=" + $("#searchBox").val() + "&si=www.vccoo.com&ct=2097152");
             // location.href = "http://www.baidu.com/s?wd=" + $("#searchBox").val() + "&si=www.vccoo.com&ct=2097152";
             return false;
         }
         return true;
     }
    </script> 
