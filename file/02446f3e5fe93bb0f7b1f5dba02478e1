function resizeImg(obj) { 
var obj = document.getElementById(obj); 
var objContent = obj.innerHTML;
var imgs = obj.getElementsByTagName('img'); 
if(imgs==null) return; 
	for(var i=0; i<imgs.length; i++) { 
		var imgw=imgs[i].width;
		var imgh=imgs[i].height;
		if(imgs[i].width>600) { 
		imgs[i].width = 300;
		imgs[i].height = 300 * imgh / imgw;
		} 
		if(imgs[i].width>300) { 
		imgs[i].width = 200;
		imgs[i].height = 200 * imgh / imgw;
		} 
	} 
} 
window.onload = function() {resizeImg('testDiv');} 
</script> 
