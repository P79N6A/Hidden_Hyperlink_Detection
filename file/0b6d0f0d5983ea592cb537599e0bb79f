    $.ajax({
        type:'get',
        url:"http://hangzhou.liebiao.com/ajax/postclicks/?postid=21234931",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
