<script type="text/javascript" language="javascript">

    function checkWindow() {
        if (window.location != window.top.location) {
            window.top.location = '/home/';
        }
    }

    function bodyOnLoad() {
        var loginTable = document.getElementById("insideLoginTable");

        if (loginTable.style.visibility != "hidden") {
            document.frmLogin.txtUser.focus();
            checkWindow();
        }
    }

    function activateBox() {
        if ($('#divNewDevUser')) {
            $('#divNewDevUser').dialog({
                closeOnEscape: true,
                minWidth: 500,
                minHeight: 520,
                modal: true,
                resizable: false,
                autoOpen: true,
                title: '<img src="img/RLHeaderLogo-233x46.png" alt="Retail Link" />'
            });
        }
        return false;
    }

</script>


