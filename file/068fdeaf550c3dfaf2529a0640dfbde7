<script type="text/javascript">
    function addRandom(){
        var href = location.href;
        
        var head=document.getElementsByTagName("head");
        var gc = head[0].getAttribute('data-gc');

        if(gc) {
            gc = gc.toString();
        }

        var list=document.getElementsByTagName("a");
        for(var i=0,len=list.length;i<len;i++){
            list[i].onclick=function(){
                var href = this.getAttribute('href');
                if((gc && (gc.indexOf("/all_cate/user") != -1 || gc.indexOf("/user/login/") != -1)) || (href && href.indexOf("user/center/") != -1 && href.indexOf("javascript:") == -1 && href.substring(0,1) != "#")) {
                    var rand = Math.random();
                    if( href.match(/_rid=[0-9]+\.\d*/) ) {
                       this.setAttribute("href",href.replace(/_rid=[0-9]+\.\d*/,"_rid=" + rand));
                    } else {
                        this.setAttribute("href",href.trim()+(href.indexOf("?") == -1 ? "?" : "&") + "_rid=" + rand);
                    }
                }
            }
        }
    }
    addRandom();
</script>


