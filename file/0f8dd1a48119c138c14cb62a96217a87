          <!-- 推荐 楼数 --> 
          <div class="o_info" id="first-ad"> 
           <div login_user="" user_id="546750794" rid="24374971" tid="3639918" floor="11" have_set="" style="height:30px;position:absolute;bottom:0;right:78px;z-index:10"> 
