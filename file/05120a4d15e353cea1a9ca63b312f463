<script language="javascript" type="text/javascript">

function menuload()
{
  dmenu = document.cookie.match(new RegExp("(^| )ajleftmenu=([^;]*)(;|$)"));
  dmenu = (dmenu==null)?11111:(parseInt(unescape((dmenu)[2])));
}

function ocmenu(menuid,isopen)
{
  if ((document.getElementById("lm" + menuid + "_h_5") != null) && (document.getElementById("lm" + menuid + "_k_5") != null))
  {
   if (isopen==1)
   {
    document.getElementById("lm" + menuid + "_h_5").style.display='none';
    document.getElementById("lm" + menuid + "_k_5").style.display='';
   }
   else
   {
    document.getElementById("lm" + menuid + "_h_5").style.display='';
    document.getElementById("lm" + menuid + "_k_5").style.display='none';
   }
  }
}

function menurefresh()
{
  for (i=0;i<5;i++)
  {
    if ((dmenu % Math.pow(10,(i+1)))/Math.pow(10,i) >= 1) {ocmenu(i,1)}else{ocmenu(i,0)}
  }
}

function onleft(tospan)
{
  document.getElementById(tospan).style.display = '';
}

function outleft(tospan)
{
  document.getElementById(tospan).style.display = 'none';
}
</script>

