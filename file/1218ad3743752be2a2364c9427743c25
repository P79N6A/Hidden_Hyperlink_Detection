     <!--中间详细介绍--> 
     <div class="kc-mid-left">
      <span class="zise14-b">详细介绍</span>
     </div> 
     <div class="box2">
      <a href="/kaiban.asp?KechengID=W450U158941PHD0" target="_balnk"><span class="zise12">最新开班</span></a>
