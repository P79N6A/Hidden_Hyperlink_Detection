$("#loading").ajaxStart(function(){         
　　    $(this).html("<img src='static/image/mobile/images/icon_load.gif'>");   
        $(this).css("display", "block");
    });
    $("#loading").ajaxSuccess(function(){
         $(this).html("加载完成!");
         $(this).css("display", "none");
    });
$('.pi').click(function(){
var pid = $(this).attr('name')
$('#rate_'+pid).show()
})
    $('.score_l_c').click(function() {   	
    	var pid = $(this).attr('name')
    	$('.layer_'+pid).toggle()
    	$(this).children('.post_score_arrowr').toggleClass('post_score_arrowb')
});
$('.right_m').click(function(){
$(this).toggleClass('left_m');
var pid = $(this).attr('name');
var obj = $("#manage_"+pid+" ul li")
var width = $("#manage_"+pid+" ul").width();
var len = obj.length;
(len==1)?obj.css('width',(width/len)+'%'):obj.css('width',((width/len-2)+'%'))
$("#manage_"+pid).toggle();
});
</script> 
