<script type="text/javascript">
    var getInfo = function (data) {
        var trainArr = $('.traininfo  .c-dGray');
        var yupiao = "";
        for (var i = 0, len = trainArr.length; i < len; i++) {
            var key = $(trainArr[i]).text();
            // 火车票 硬座100 软座200 硬卧300 软卧400 一等座500 二等座600 高级软卧700 无座800
            if (!data[0][key]) {
                $('[name="' + key + '"]').find('.spanCount').html('暂无数据');
                continue;
            }
            if (data[0][key][0]['100'] != '--') {
                yupiao += "&nbsp;&nbsp;硬座:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['100'] + "</span>&nbsp;张<br>";
            }

            if (data[0][key][0]['200'] != '--') {
                yupiao += "&nbsp;&nbsp;软座:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['200'] + "</span>&nbsp;张<br>";
            }

            if (data[0][key][0]['300'] != '--') {
                yupiao += "&nbsp;&nbsp;硬卧:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['300'] + "</span>&nbsp;张<br>";
            }

            if (data[0][key][0]['400'] != '--') {
                yupiao += "&nbsp;&nbsp;软卧:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['400'] + "</span>&nbsp;张<br>";
            }

            if (data[0][key][0]['500'] != '--') {
                yupiao += "&nbsp;&nbsp;一等座:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['500'] + "</span>&nbsp;张<br>";
            }

            if (data[0][key][0]['600'] != '--') {
                yupiao += "&nbsp;&nbsp;二等座:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['600'] + "</span>&nbsp;张<br>";
            }

            if (data[0][key][0]['700'] != '--') {
                yupiao += "&nbsp;&nbsp;高级软卧:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['700'] + "</span>&nbsp;张<br>";
            }

            if (data[0][key][0]['800'] != '--') {
                yupiao += "&nbsp;&nbsp;无座:&nbsp;<span class='c_f50 f14'>" + data[0][key][0]['800'] + "</span>&nbsp;张<br>";
            }

            var total = 0;
            for (var k = 1; k <= 8; k++) {
                var kk = data[0][key][0][k + '00'];
                if (kk != '--') {
                    total += parseInt(kk);
                }
            }

            if (total == 0) {
                yupiao = "<span class=\"finished\">售完</span>"
            } else {
                yupiao = total + "张";
            }


            $('[name="' + key + '"]').find('.spanCount').html(yupiao);
            if (data[0][key] && data[0][key][0]['state'] == '0') {
                $('[name="' + key + '"]').find('.spanCount').html(yupiao + "<br/>不可预订");

            }
        }
    }

    var getYupiao = function () {
        var start = "邯郸";
        var end = "德州"; ;
        var trainDate = "".split("/");
        //===================================================================
        var trainArr = $('.traininfo  .c-dGray');

        // 获取余票信息
        $.ajax({
            url: 'http://open.hezuo.58.com/lieche/yupiao?action=yupiao&month=' + trainDate[1] + '&day=' + trainDate[2] + '&from=' + encodeURIComponent(start) + '&to=' + encodeURIComponent(end) + "&callback=getInfo",
            dataType: "jsonp",
            success: function (data) {
            }
        });
        //===================================================================s
    }


    if (typeof (Mclient) == "undefined") {
         var Mclient = {};
    }


    Mclient.touchStyle = function(obj, hover) {
        $(obj).bind("touchstart", function() {
            $(this).addClass(hover);
        });
        $(obj).bind("touchmove", function() {
            $(this).removeClass(hover);
        });
        $(obj).bind("touchend", function() {
            $(this).removeClass(hover);
        });
        $(obj).bind("touchcancel", function() {
            $(this).removeClass(hover);
        });
    };

    var touchStyle = function () {
        $(".traininfo >a ").each(function (iii, obj) {
            Mclient.touchStyle(obj, "active");
        });
    }
    var backtonormal1 = function () {
        $(".ico-prev").attr("href", "javascript:history.go(-1)");
        $(".tab-date").show();
        $(".traininfo").show();
        $(".filter").hide();
        $(".ico-prev").unbind("click");
        return false;
    }


    var traintype = $("#traintype");
    var bindTrainType = function () {
        traintype.bind("click", function () {
            $(".tab-date").hide();
            $(".traininfo").hide();
            $(".filter").show();
            $(".ico-prev").bind("click", function () { return backtonormal1() });
        });
    }

    $(function () {
        getYupiao();
        touchStyle();
        bindTrainType();
    });



    var pageindex = 1;
    $(".tomore").bind("click", function () {
        $(".tomore").attr("disabled", true);

        pageindex++;
        var listsize = 0;
        $.ajax({
            url: '/majax.aspx',
            data: { action: "zds2slist", "start": "22", sort: "1", t: "", "end": "76", "pageindex": pageindex },
            dataType: "json",
            success: function (data) {
                var ulstr = "";
                $(data.stationlist).each(function (i, obj) {
                    listsize = data.stationlist.length;
                    ulstr += " <a href=\"/" + obj.startcode + "-" + obj.endcode + "/" + obj.traincode.replace("/", "-") + "/?2017/04/03\">";
                    ulstr += "<ul name = \"" + obj.traincode + "\" class=\"traininfo-list\">";
                    ulstr += "	<li><span class=\"c-dGray\">" + obj.traincode + "</span><br /><span class=\"f18 c-orange\">" + obj.starttime + "</span><br />" + obj.arrivetime + "</li>";
                    ulstr += "	<li><span class=\"ico-time\"></span>" + obj.ls + "<br><span class=\"" + obj.startclass + "\"></span>" + obj.start + "<br /><span class=\""+obj.endclass+"\"></span>" + obj.end + "</li>";
                    ulstr += "	<li><span class=\"spanCount\"></span><br />" + obj.minprice + "</li>";
                    ulstr += "</ul>";
                    ulstr += "</a>";
                    if (i == 1) {
                        ulstr += createRecommendHtml()[0];
                    }
                    
                });

                $(ulstr).insertBefore(".tomore");

                 touchStyle();

                $(".tomore").attr("disabled", false);
                if (listsize < 10)
                    $(".tomore").hide();

                getYupiao();
                showlog();
            },
            error: function () {
                $(".tomore").attr("disabled", false);
            }
        });
    });

</script>


        

