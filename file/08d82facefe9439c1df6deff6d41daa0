  <script type="text/javascript" src="/js/jquery-1.9.1.min.js"></script> 
  <script type="text/javascript">
$(document).ready(function(){
	$(document).click(function(){
		$(".weixin_icon").find("div").hide();
		$(".select_language ul").slideUp("fast");
		$(".search").hide();
		<!---->
		var width = "440px";
		<!---->
		$(".index_top").animate({"width":width});
	});
	
	$(".select_language").click(function(e){
		e.stopPropagation();
		$(this).find("ul").slideToggle("fast");
	});
	
	$(".search_icon").click(function(e){
		e.stopPropagation();
		var status = $(".search").css("display");
		<!---->
		var width1 = "440px";
		var width2 = "690px";
		<!---->
		if(status == 'none'){
			$(".index_top").animate({"width":width2},function(){
				$(".search").show();
			});
		}else{
			$(".search").hide();
			$(".index_top").animate({"width":width1});
		}
	});
	$(".search").click(function(e){
		e.stopPropagation();	
	});
	
	$(".weixin_icon").click(function(e){
		e.stopPropagation();
		$(this).find("div").show();
	});
	
	$(".menu ul li").mouseover(function() {
		$(this).find(".pop_menu").show(); 
	});
	$(".menu ul li").mouseleave(function() {
		$(this).find(".pop_menu").hide(); 
	});
	
});
</script> 
