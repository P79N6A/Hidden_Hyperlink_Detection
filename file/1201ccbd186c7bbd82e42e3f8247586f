     <script>
    $("pre img").each(function(){
        $(this).parent("a").attr("rel","lightbox");
        if($(this).parent("a").html()==undefined)
        {
            $(this).wrap("<a></a>");
            var href=$(this).attr("src");
            if(href.match(/-\d+x\d+/g)!=null)
                href = href.replace(/-\d+x\d+/g, "");
            $(this).parent("a").attr({"rel":"lightbox","href":href});
        }
    })
</script> 
