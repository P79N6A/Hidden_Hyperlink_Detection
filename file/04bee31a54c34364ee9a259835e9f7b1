<script type="text/javascript">
	function searchFooterFunc(){
		$.each($('.ftChoosePop ul li a'),function(i,n){
			if($(this).text() == $('.ftChooseList').text()){
				$('#hide_page_id').val($(this).attr('value'));
				return;
			}
		});
		var world = $.trim($(".ftSearchTxt").val());
		if(world == '' || world == null){
			alert("关键词不能为空！");
			return false;
		}
		return true;
	}
</script>
