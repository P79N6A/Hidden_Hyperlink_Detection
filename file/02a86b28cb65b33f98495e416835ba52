<script type="text/javascript">

function getCookie(c_name)//获得指定名称的cookie值
		{
					if (document.cookie.length>0)
					  {
					  c_start=document.cookie.indexOf(c_name + "=")
					  if (c_start!=-1)
						{ 
						c_start=c_start + c_name.length+1 
						c_end=document.cookie.indexOf(";",c_start)
						if (c_end==-1) c_end=document.cookie.length
						return unescape(document.cookie.substring(c_start,c_end))
						} 
					  }
					return ""
		}
		
var cookie_name=decodeURIComponent(getCookie('Example_auth_username'));
var cookie_name_epaper=decodeURIComponent(getCookie('Example_auth_username_epaper'));

if(document.getElementById('loginbar_new')!=null)
				{
					var if_loginbar_new = 1;
				}
				
				if(if_loginbar_new == 1)
				{
					if(cookie_name.length>0&&cookie_name_epaper.length>0)
					{
						document.getElementById('loginbar_new').innerHTML="<div id='login'>"+cookie_name_epaper+"&nbsp;&nbsp;您好!&nbsp;&nbsp;<a style='color:#FF6600;' href='http://home.gmw.cn/' target='_blank'>光明网用户统一登录平台</a>&nbsp;&nbsp;<a href=\"http://www.gmw.cn/gm\" target=\"_blank\" style='color:#FF6600;'>光明日报社员工门户</a>&nbsp;&nbsp;<a href='http://epaper.gmw.cn/app/web/logout.html' target='_self'>安全退出</a></div>";
					}
					else if(cookie_name.length>0&&cookie_name_epaper.length<=0)
					{
					document.getElementById('loginbar_new').innerHTML="<div id='login'>"+cookie_name+"&nbsp;&nbsp;您好!&nbsp;&nbsp;<a style='color:#FF6600;' href='http://home.gmw.cn/' target='_blank'>光明网用户统一登录平台</a>&nbsp;&nbsp;<a href='http://epaper.gmw.cn/app/web/logout.html' target='_self'>安全退出</a></div>";
					
					}
					else if(cookie_name.length<=0&&cookie_name_epaper.length>0)
					{
					document.getElementById('loginbar_new').innerHTML="<div id='login'>"+cookie_name_epaper+"&nbsp;&nbsp;您好!&nbsp;&nbsp;<a href=\"http://www.gmw.cn/gm\" target=\"_blank\" style='color:#FF6600;'>光明日报社员工门户</a>&nbsp;&nbsp;<a href='http://epaper.gmw.cn/app/web/logout.html' target='_self'>安全退出</a></div>";
					}
					else
					{
						document.getElementById('loginbar_new').style.display="none";
					}

				}

</script>

<script type="text/javascript" src="http://img.gmw.cn/js/jquery.js"></script>
<script>
$(document).ready(function(){
	$("#articleContent p[mce_keep=true]").each(function(){
		if($(this).html() == "&nbsp;" ){ //如果没有文字，仅为一个空格
			$(this).remove();//删除空行	
		}
	});

	
	for(var i=0; i<$("#articleContent p").length; i++){
		var str = $("#articleContent p").eq(i).html(); 
		var strNew = str.replace('&nbsp;&nbsp;&nbsp;&nbsp;','　　'); //替换为全角空格
		$("#articleContent p").eq(i).html(strNew);
	}
});
</script>

