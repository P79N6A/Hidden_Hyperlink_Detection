  <script language="javascript">
		var adobj_hot = $(".hotlist");
		adheight_hot = 70;
		rollheight_hot = $(document).scrollTop();
		adroll_hot(rollheight_hot,adheight_hot);
		$(window).scroll(function()
		{
			rollheight_hot = $(document).scrollTop();
			adroll_hot(rollheight_hot,adheight_hot);
		});
		function adroll_hot(rollheight,adheight){
			if(rollheight>=adheight){
				$(".hotlist").css('top', '0');
			}
			if(rollheight<=adheight){
				$(".hotlist").css('top', '70px');
			}

		}
	</script> 
  <script type="text/javascript">
		$(document).ready(function(){
			var myDate = new Date();
			var curday = myDate.getDate();
			var curmonth = myDate.getMonth() + 1;
			var daystr = '';
			var siteurl = 'http://www.todayonhistory.com';
			var days = new Array(31,29,31,30,31,30,31,31,30,31,30,31);
			for(var i = 1; i <= days[curmonth-1]; i++){
				if(i == curday){
					daystr += "<a class=\"cur\" href='"+siteurl+"/"+ curmonth +"/"+i+"/'>"+i+"</a>";
				}else{
					daystr += "<a href='"+siteurl+"/"+ curmonth +"/"+i+"/'>"+i+"</a>";
				}
			} 
			$("#rlday").html(daystr);
			$('#month').change(function(){
				var month = $(this).children('option:selected').val();
				var curday = myDate.getDate();
				if(month == 0)
					return;
				var daystr = '';
				for(var i = 1; i <= days[month-1]; i++){
					if(i == curday){
						daystr += "<a class=\"cur\" href='"+siteurl+"/"+ month +"/"+i+"/'>"+i+"</a>";
					}else{
						daystr += "<a href='"+siteurl+"/"+ month +"/"+i+"/'>"+i+"</a>";
					}
				} 
				$("#rlday").html(daystr);
			})
		})
	</script> 
