var url = window.location.href;
$.ajax({
	type: "get",
	async: false,
	url: "http://weixin.rednet.cn/redpack/jssdk.php?url=" + encodeURIComponent(url),
	dataType: "jsonp",
	jsonp: "jsoncallback",
	jsonpCallback: "success_jsonpCallback",
	success: function(json) {
		wx.config({
			appId: json.appId,
			timestamp: json.timestamp,
			nonceStr: json.nonceStr,
			signature: json.signature,
			jsApiList: [
				"onMenuShareTimeline",
				"onMenuShareAppMessage",
				"onMenuShareQQ",
				"onMenuShareWeibo",
				"onMenuShareQZone"
			]
		});
	}
});

