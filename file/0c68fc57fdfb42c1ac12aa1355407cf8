	//滚动加载列表
	var scrollloading = false;
	$(document).ready(function(){
		//向上滚动加载
		$(window).scroll(function(){
			var documentHeight = $(document).height();
			var windowHeight = $(window).height();
			var scrollHeight = $(window).scrollTop();
			if (windowHeight + scrollHeight >= documentHeight-300 && scrollloading == false) {
				if(scrollloading==true) return false;  
				scrollloading = true;
				loadingContent();//页面加载函数由各自页面定义，加载完，isloading置为false
			}
		});
		
	});

	//分页
	var search_condition = {page:1,pagesize:10,notop:1};
			$.extend(search_condition,{q:'rdzx'});
		
	
	function getAjaxList(){
			if(search_condition.page>1) $("#tuijian_box").append("<div class='page_loading'></div>");
			var url = "/index.php?m=home&c=news&a=index&city=sy";
			$.get(url,search_condition,function(res){
				res = eval("(" + res + ")");
				if(res.status == 1){
					var item = res.msg;
					var _html = res.tpl;
					if(item.length > 0){
						search_condition.page++;
						$("#tuijian_box").append(_html);
						if(item.length>=10) scrollloading = false;
						if(item.length<10) $("#tuijian_box").append('<p align="center" class="p12 topl bg_white drag"><a class="pre"><i></i>对不起，没有更多数据了</a></p>');
					}
				}else{
					alert(res.msg);
				}
				$(".page_loading").remove();
				return false;
			});
		}
		
	function loadingContent(){
		getAjaxList();
	}
	getAjaxList();
	$('.detail-footer').find('.close').on('click' , function(){
					$('.detail-footer').fadeOut(250);
				})
</script> 
