              <script type="text/javascript">
function _showOneKeymenu() {
	if(!$('onekeymenu_menu')) {
		menu = document.createElement('div');
		menu.id = 'onekeymenu_menu';
		menu.style.display = 'none';
		menu.className = 'p_pop';
