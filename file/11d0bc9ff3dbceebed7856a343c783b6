  <!-- 右侧快速订购悬浮窗 --> 
  <script type="text/javascript">
$(function() {
	$('#easy-button').live('click', function(e) {
		var str1 = $('#easy_code').val().replace(/(^\s*)|(\s*$)/g, "");
		var str2 = $('#easy_num').val().replace(/(^\s*)|(\s*$)/g, "");
		if (str1 == ""){
			alert('必需填写货号。');
			$('#easy_code').focus();
			return false;
		}
		if(str2 == ''){
			str2 = '1';
		}
		var type="^[0-9]*[1-9][0-9]*$"; 
        var re = new RegExp(type);
		if (str2.match(re)==null) {
			alert('数量必须是数字。');
			form.$('#easy_num').focus();
			return false;
		}
		
		var cartJson = {
			product_code:str1,
			qty:str2,
		}
		
		$.ajax({
			   type: "post",
			   url: base_url + "/ajax/easyAddCart",
			   data: cartJson,
			   dataType : 'json',
			   success: function(responseJSON) {
			       if(responseJSON.result) {
			       	var qty = parseInt($('.header.cart_number').html()) + responseJSON.chg_qty;
			       	$('.header.cart_number').html(qty);
			       	if(responseJSON.chg_qty>0) {
						addCart(e);

			       	} else {
			       		jError($('#cart_qty_msg').html(),{
							  HorizontalPosition : 'center',
							  VerticalPosition : 'center',
							  ShowOverlay : true
							});
			       	}
			       } else {
					$.ajax({
						   type: "post",
						   url: base_url + "/ajax/default_url",
						   dataType : 'json',
						   success: function(responseJSON) {
						   }
					});
			       	$('#loginModalLabel').html(responseJSON.title);
			       	$('#loginModal p.help-block').html(responseJSON.error);
			       	$('#loginModal').show();
					$('div.modal-backdrop').show();
					$('#loginModal button.close,#loginModal button.btn').click(function() {
						$('#loginModal').hide();
						$('div.modal-backdrop').hide();
					});
			       }
			   }
			});
	});
});

</script> 
