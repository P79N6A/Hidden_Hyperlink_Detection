  <script type="text/javascript">$('#noscript').hide();if(!(document.cookie||navigator.cookieEnabled)){$('#noscript').html('您的浏览器禁止了Cookie,将无法正常使用本站功能!').show();}
$(".gotopa").click(function(){$("html,body").animate({scrollTop: 0}, 300); return false;});
$(".sl").scrollLoading();
$(function(){
$(window).scroll(function(){gotoparrow();}).resize(function(){gotoparrow();});
function gotoparrow(){
var $scrollTop = $(window).scrollTop();
if($(window).scrollTop() > $('#fixed_s').offset().top){
$("#gotop").fadeIn(500);
$('.nav').addClass('nav_fixed');
}else if($(window).scrollTop() <= $(window).height()/2){
$("#gotop").fadeOut();
$('.nav').removeClass('nav_fixed').show();
}
}
});
</script> 
