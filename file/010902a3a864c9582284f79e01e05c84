<script language="javascript">
var navHeight = 36; // 导航高度，计算不准，写死
function adjustFloatNavPos()
{
    var oriNavOffset = $("#orginalNaviBox").offset();
    var oriNavPosition = $("#orginalNaviBox").position();
    var detailheadPostion = $(".sftitle").position();
    var scrTop = $(window).scrollTop();
    var docuTop = scrTop-oriNavOffset.top;  // 当docuTop>=0时，表示导航已不在视野
    var top1 = oriNavOffset.top-oriNavPosition.top;
    if(docuTop >= 0)
    {
		$("#floatNaviBox").removeClass("newnavwrap").addClass("newnavwrapFloat");
		$("#floatNaviBox").css("position","fixed");
		$("#floatNaviBox").css("top","0");
		$("#floatNaviBox").css("left", oriNavPosition.left);
		$("#floatNaviBox").show();
		$("#navi_tg").removeClass("nnrwdown").addClass("nnrwdownFloat");
		$("#navi_tg").css("top", navHeight).css("left", detailheadPostion.left);
    }
    else
    {
		$("#floatNaviBox").hide();
		$("#floatNaviBox").removeClass("newnavwrapFloat").addClass("newnavwrap");
		$("#navi_tg").removeClass("nnrwdownFloat").addClass("nnrwdown");
		$("#navi_tg").css("left", detailheadPostion.left).css("top", oriNavPosition.top+navHeight);
    }        
}
function showDetailBox(){
	if($("#navi_tg").is(":visible")){
		$(".nnlxqcomw").hide();
	}else {
        $(".nnlxqcomw").show();
	}
}
function showTgBox()
{
    if($(".nnrwytg").hasClass('wytgon')){
	    $(".nnrwytg").removeClass('wytgon');
	}else{
	    $(".nnrwytg").addClass('wytgon');
	}
    $("#navi_tg").toggle();
	showDetailBox();
}
$(window).resize(adjustFloatNavPos);
$(window).scroll(adjustFloatNavPos);
$(function() {
   adjustFloatNavPos();
});
</script>


<script language="javascript">
  $(document).ready(function(){
	$('.nnlxqcom a').click(function(event){
		event.preventDefault();
		var full_url = this.href;
		var parts = full_url.split("#");
        var trgt = parts[1];
		var anchorTarget = $(this).attr('href') + "_anchor";
		var scrollTopNum = $( anchorTarget ).offset().top - 75;
		$('html, body').animate({
			scrollTop: scrollTopNum
		}, 500);
		window.location.hash ="#"+trgt;
	});
	var hashAnchor = window.location.hash;
	if(hashAnchor.indexOf("xq_") != -1){
		var trgt = hashAnchor.replace("#", "");
		var anchorTarget = "#" + trgt + "_anchor";
		var scrollTopNum = $( anchorTarget ).offset().top - 75;
		$('html, body').animate({
			scrollTop: scrollTopNum
		}, 500);
	}
	showDetailBox();
});
</script>
