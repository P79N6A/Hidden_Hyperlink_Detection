<script src="http://static.aizhan.com/js/history-1.3.js"></script>
<script type="text/javascript">	var quequeKey = ['ce9aad8ad8a3f65b5b422bb9d10013cd','f56d408e0d1bf5b6e55796d6e5ca3310','24ce3890c0760e547f5237467d43fb04','c503b98faaba4ed85cbd1f98a98caec8','bb33302a858404cf31b50f64ae5bb540','5ddb0ecc336a91c06e5f673ef0afcc45','0b4264bc3eb986648c7414bd1e27d239','8e08c0f9ed3cc353b0fcfc608898ce9a','7577c4a9b2aff0e84ebedc166980ac8b','2ba689c71b3c9f1f582276a8b75f5161','63991210c0fa7dbb2ebafa669b1dfe12','c0437aa7f1476e81f7e76f00e13ea396'];

	window.onload = function(){
		var links_obj = $('.dns-links'),
			links_len = links_obj.length;
			links = [];

		for(var i=0; i < links_len; i++){
			links.push(links_obj.eq(i).children('a').text());
		}

		getTitle(0,links);
		getbr(0,links);
		getpr(0,links);
	};

	function getpr(id,links,re) {
		var elem = $('.queque_pr').eq(id);

		if(!re){
			var url = links[id];
			if(id>=links.length){
				return false;
			}
		}else{
			var url = links;
			elem.html('<img src="http://static.aizhan.com/images/loading.gif"/>');
		}

		var rfunc = "getpr("+id+",'"+url+"',1);",
				rhtml = '<a href="javascript:void(0);" onclick="'+rfunc+'">重查</a>';

		$.ajax({
			url:"http://www.aizhan.com/ajaxAction/pr.php",
			data:{'domain':url,'cc':quequeKey[id],'rn':1491052535},
			dataType:"jsonp",
			timeout:3000,
			cache:false,
			error:function(){
				if(!re){
					getpr((id+1),links);
				}
				elem.html(rhtml);
			},
			success:function(json){
				if(!re){
					getpr((id+1),links);
				}
				if(Object.prototype.toString.call(json) !== '[object Object]' || json==""){
					elem.html(rhtml);
					return;
				}
				elem.html((json['pr'] < 0)?0:json['pr']);
			}
		});
	}

	function getbr(id,links,re) {
		var elem = $('.queque_br').eq(id);

		if(!re){
			var url = links[id];
			if(id>=links.length){
				return false;
			}
		}else{
			var url = links;
			elem.html('<img src="http://static.aizhan.com/images/loading.gif" />');
		}

		var rfunc = "getbr("+id+",'"+url+"',1);",
			rhtml = '<a href="javascript:void(0);" onclick="'+rfunc+'">重查</a>';

		$.ajax({
			url:"http://www.aizhan.com/getbrinfo.php",
			data:{'url':url,'cc':quequeKey[id],'rn':1491052535},
			dataType:"jsonp",
			timeout:3000,
			cache:false,
			error:function(){
				if(!re){
					getbr((id+1),links);
				}
				elem.html(rhtml);
			},
			success:function(json){
				if(!re){
					getbr((id+1),links);
				}
				// 错误信息
				if(Object.prototype.toString.call(json) !== '[object Object]' || json==""){
					elem.html(rhtml);
					return;
				}
				elem.html('<a href="http://baidurank.aizhan.com/baidu/'+url+'/" target="_blank">'+'<img src="http://static.aizhan.com/images/br/'+json["baidurank"]+'.gif" align="absmiddle"/></a>');
			}
		});
	}

	function getTitle(id,links,re){
		var elem = $('.td-spill').eq(id);

		if(!re){
			var url = links[id];
			if(id>=links.length){
				return false;
			}
		}else{
			var url = links;
			elem.html('正在获取中<img src="http://static.aizhan.com/images/loading.gif" />');
		}

		if((elem.html()).indexOf('loading')==-1){
			getTitle((id+1),links);
			return;

		}
		var rfunc = "getTitle("+id+",'"+url+"',1);",
			rhtml = '页面获取失败&nbsp;&nbsp;『<font color="red" style="cursor:pointer" onclick="'+rfunc+'">重试</font>』',
			rh = '『<font color="red" style="cursor:pointer" onclick="'+rfunc+'">重试</font>』',
			title = '';

		$.ajax({
			url:'http://dns.aizhan.com/api/title',
			data:{'domain':url,'cc':quequeKey[id],'rn':1491052535},
			type:"GET",
			dataType:"json",
			timeout:8000,
			cache:false,
			error:function(){
				if(!re){
					getTitle((id+1),links);
				}
				elem.html(rhtml);
			},
			success:function(json){
				if(!re){
					getTitle((id+1),links);
				}

				if(Object.prototype.toString.call(json) !== '[object Object]' || json==""){
					elem.html(rhtml);
					return;
				}
				if(!!json['error']){
					elem.html('<font color="red">'+json['error']['message']+'</font>'+rh);
					return;
				}

				if(!!json["statusCode"]){
					if(json["statusCode"]==200){
						title = !!json['title']?json['title']:"无标题";
					}else if(json["statusCode"]==301 || json["statusCode"]==302){
						title = "<font color=red>页面"+json["statusCode"]+"跳转</font>: "+"<a target='_blank' rel='nofollow' href='"+json['redirectUrl']+"'>"+json['redirectUrl']+"</a>";
					}else{
						title = "<font color=red>页面"+json['statusCode']+"状态</font>";
					}
					elem.html(title);
				}else{
					elem.html(rhtml);
				}
			}
		});
	}

	function check_form(){
		var obj = document.getElementById('site'),
			query_domain = url2domain(obj.value);

		if(query_domain){
			location.href = '/'+query_domain+'/';
		}else{
			alert('请输入要查询的域名！');
			obj.focus();
		}
		return false;
	}

	var obj = document.getElementById('site'),
		query_domain = url2domain(obj.value);

	if(query_domain){
		if(isDomain(query_domain)){
			addAllSites(query_domain);
		}
	}
	//nick
	$('#zoned_bot').html(con_bot_content);
</script>

