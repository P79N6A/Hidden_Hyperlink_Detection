  <script>
	  $(function(){
		  $(".subsiteNavbar").hover(function(){
			  $("dl",$(".subsiteNavbar")).height(195);
			  },function(){
			  $("dl",$(".subsiteNavbar")).height(142);
			  });
					$(".searchMod").mouseover(function(){
				$("#search_type_content").addClass("cur");
				$("#search_info_list").show();	
			}).mouseleave(function(){
				$("#search_type_content").removeClass("cur");
				$("#search_info_list").hide();	
			});	
			$("#search_info_list >li").mouseover(function(){
			  $(this).addClass("hov");		
			}).mouseleave(function(){
			$(this).removeClass("hov");		
			});		
			$("#search_info_list > li"). mouseover(function(){
						var thisName = $(this).children().text()+"<i></i>";
						var thisTip= $(this).attr("alt");
						var thisParam = $(this).attr("param");
					    $("#Search_keyword").val(thisTip);
						$('#search_type_content').attr("param",thisParam).html(thisName);
						$("#search_info_list").hide();
				});	  
		  })	
</script> 
