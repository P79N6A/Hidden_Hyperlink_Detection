	subMenu1.addItem('- 阳光少年宫制度汇编','X_Class.asp?classid=29&itcn_B_ClassID=186&XClassID=49');
	subMenu1.addItem('- 阳光少年宫工作职责','X_Class.asp?classid=29&itcn_B_ClassID=186&XClassID=52');
	subMenu1.addItem('- 阳光少年宫报名表','X_Class.asp?classid=29&itcn_B_ClassID=186&XClassID=50');
	subMenu1.addItem('- 各班学生分流情况','X_Class.asp?classid=29&itcn_B_ClassID=186&XClassID=51');
	subMenu1.addItem('- 阳光少年宫“家校”责任书','X_Class.asp?classid=29&itcn_B_ClassID=186&XClassID=55');
	subMenu1.addItem('- 阳光少年宫“家校”责任书','X_Class.asp?classid=29&itcn_B_ClassID=186&XClassID=54');
