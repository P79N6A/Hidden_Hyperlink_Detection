  <script>
    jQuery(document).ready(function ($) {
       //keyup 
      $('#schform').on('keypress', function(e) {
            var keyCode = e.keyCode || e.which;
            if (keyCode === 13) {
                e.preventDefault();
                checkSearch();
                return false;
            }
        });
      //统计点击广告
      $('.click-ad').click(function(){
        var adid = $(this).attr('data-id');
        $.ajax({
            type : "GET",  //提交方式
            url : "/csgo/e/extend/api/ad.php?id="+adid,//路径
            dataType:'json',
            success : function(result) {//返回数据根据结果进行相应的处理
                e.preventDefault();
                return false;
            }
        });
      });
    });
    function checkSearch(){
        //全局搜索，先异步请求
        var keyboard = $('#keyboard').val();
        $.ajax({
            type : "GET",  //提交方式
            url : "/csgo/e/sch/index.php?keyboard="+keyboard+"&Submit=Search&check=1",//路径
            dataType:'json',
            success : function(result) {//返回数据根据结果进行相应的处理
                if(result.status == 0){
                    $('#keyboard').val('');
                    $('#keyboard').attr('placeholder',result.msg);
                    //error(result.msg);
                    e.preventDefault();
                    return false;
                }else{
                    window.location.href="/csgo/e/sch/index.php?keyboard="+keyboard+"&Submit=Search";
                }
            }
        });
    }
</script> 
