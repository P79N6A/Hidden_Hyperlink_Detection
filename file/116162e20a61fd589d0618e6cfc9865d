                         $('siderIMchat_main').setStyle('display','none');
                         $('siderIMchat_hiddenbar').setStyle('display','block')
                         })    ;    
    siderIMchatsetGoTop();    
    
    
});

function siderIMchatsetGoTop(){
    $('siderIMchat').tween('top',$E('body').getScroll().y+100)
}          
