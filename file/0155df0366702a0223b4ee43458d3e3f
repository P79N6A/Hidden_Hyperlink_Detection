$(function() {//首页幻灯片
    $("#owl-carousel").owlCarousel({autoPlay: true,slideSpeed: 300,paginationSpeed: 400,singleItem: true,goToFirst: true});
function selectSlideBtn(objID,SelectNum){
for(var i=0;i<=2;i++){
if(SelectNum==i){
$('#'+objID+i).attr('class','t1');
}else{
$('#'+objID+i).attr('class','');
}}}
