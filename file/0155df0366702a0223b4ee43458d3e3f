$('.products ul').width(2160);
var _speed=20;
var pw = $(".products").width();
var _slideli1 = $('.products ul');
$("#pro2").html($("#pro1").html());
function Marquee(){
$('#tt').val($(".products").scrollLeft());
var pl = $(".products").scrollLeft();
var pu = _slideli1.width();
if((pl)>= pu){
$(".products").scrollLeft(0);
}else{
$(".products").scrollLeft(pl+1);
}
}
$(function(){
var sliding=setInterval(Marquee,_speed);
var _slide = $(".products");
_slide.hover(function() {
//鼠标移动DIV上停止
clearInterval(sliding);
},function(){
//离开继续调用
sliding=setInterval(Marquee,_speed);
});
});
</script>
