<script language="javascript">
    $(function() {
        $(".btn-remove").click(function() {
            var arrIdx = "";
            $("input[name='ck']:checked").each(function() {
                arrIdx = arrIdx + ',' + $(this).attr("param");
            });
            arrIdx = arrIdx.substr(1, arrIdx.length - 1);
            $.ajax({
                type: "GET",
                url: "/ajax.php",
                data: {
                    action: 'boarddel',
                    arrIdx: arrIdx,
                    t: Math.random()
                },
                dataType: "json",
                success: function(data) {
                    alert('留言删除成功...');
                    window.location.href = '/';
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {}
            });
        });
        $(".btn-selectall").click(function() {
            if ($(this).attr("checked")) {
                $("input[name='ck']").each(function() {
                    $(this).attr("checked", true);
                });
            } else {
                $("input[name='ck']").each(function() {
                    $(this).attr("checked", false);
                });
            }
        });
        $('.message-list').find('.btn-reply').bind('click',
        function() {
            var itemid = $(this).attr("param");
            $('#itemid').val(itemid);
            $('.message-list').css('display', 'none');
            $('#replay-board').css('display', 'block');
            $('#rboard').html($('#board_' + itemid).html());
        });
        $('#replay-board').find('.btn-cancel').bind('click',
        function() {
            $('.message-list').css('display', 'block');
            $('#replay-board').css('display', 'none');
        });
        $('#replay-board').find('.btn-submit').bind('click',
        function() {
            var idx = $('#itemid').val();
            var reply = $('#txt_reply').val();
            $.post('/ajax.php', {
                action: 'reply',
                idx: idx,
                reply: reply
            },
            function(data) {
                alert('留言回复成功.');
                window.location.href = '/';
            });
        });
    });
</script>
<style>
    .txt-reply { height: 30px; width: 360px; line-height: 20px; padding: 3px
    8px 5px; background: none repeat scroll 0% 0% transparent; font-size: 14px;
    box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.04) inset;}
</style></div>
</div>
</div>
