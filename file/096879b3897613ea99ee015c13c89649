        var last = 0;
        for (var i = 0; i < links.length; i++)
        {
            var linkurl = lilen[i].getAttribute("href");
            if (currenturl.indexOf(linkurl) != -1)
            {
                last = i;
            }
        }
        links[last].className = "a";  //高亮代码样式
    </script> 
