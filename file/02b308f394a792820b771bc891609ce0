    var pvareacookie = {init: function () {
        var pvareaid = this.getQueryString("pvareaid");
        var referrer = document.referrer;
        try {
            if (this.getHost(referrer).toLowerCase() == "mall.autohome.com.cn" || this.getHost(referrer).toLowerCase() == "m.mall.autohome.com.cn" || this.getHost(referrer).toLowerCase() == "mall.m.autohome.com.cn") {
                return
            }
