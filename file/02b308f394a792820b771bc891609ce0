<script>

    $(".banner").each(function(){
        var banner = $(this);
        var bannerli = banner.find(".focusimg-pic li[id]");
        var focusInitTimer = setInterval(function () {
            var focusliInitLength = 0;
            bannerli.each(function () {
                if ($(this).find("img").length > 0) {
                    focusliInitLength++;
                }
            });
            if (bannerli.length == focusliInitLength) {
                banner.focusImg({ interval: 3700 });
                clearInterval(focusInitTimer);
            }
        }, 100);
    });

    var __PROVIDERID = 83118421;
    var __SHOPID=83118421;
    var __MMALLHOST ="//m.mall.autohome.com.cn";
    var __ISAPP =$("#isapp").val();



    function loadCustomGoodsList(){
        $(".customGoodsList").each(function () {
            if( $(this).attr("data-json")){
                var itemList = eval("("+$(this).attr("data-json")+")");
                var id = itemList.siteUserCustomGoodsId;
                if(itemList && itemList.goodsIds) {
                    $.ajax({
                        type: "POST",
                        url: "/shop/ajaxSiteModuleContext.html",
                        data: {"shopId": __SHOPID, "goodsIds": itemList.goodsIds, "isapp": __ISAPP},
                        dataType: "html",
                        success: function (result) {
                            $("#customGood" + id).html(result);
                        }
                    });
                }
            }
        });
    }
    loadCustomGoodsList();
    function loadOther(){
        if($.trim($("#loadDefaultList").find("#loadDefaultListspan").text()) == "加载更多"){
            $("#loaderrorDefaultList").hide();
            $("#loadDefaultList").hide();
            $("#loading").show();
            var param = {};
            param.page = $("#loadDefaultList").attr("data-value-page");
            param.shopId = $("#loadDefaultList").attr("data-value-shopId");
            param.cityID = $("#filterCity a[class='current']").attr("data-value");
            param.j = $("#loadDefaultList").attr("data-json");
            param.isapp = $("#loadDefaultList").attr("access-type");
            param.pageSize = $("#loadDefaultList").attr("data-value-pageSize");
            $.ajax({
                type:"POST",
                url: "/activity/store/loadactivity.jtml",
                data: param,
                dataType: "html",
                timeout:3000,
                success: function (result) {
                    $("#defaultList").append(result);
                    $("#loadDefaultList").attr("data-value-page",parseInt(param.page) + 1);
                    if(parseInt($("#loadDefaultList").attr("data-value-page")) > parseInt($("#loadDefaultList").attr("data-value-pages"))){
                        $("#loadDefaultList").find("#loadDefaultListspan").text("加载完毕");
                    }
                    $("#loading").hide();
                    $("#loadDefaultList").show();
                },
                error:function(XmlHttpRequest,textStatus, errorThrown){
                    $("#loading").hide();
                    $("#loaderror").show();
                }
            });
        }
    }

    function loadDefaultItemList(){
        $(".defaultItemList").each(function () {
            if( $(this).attr("data-json")){
                var itemList = eval("("+$(this).attr("data-json")+")");
                if(itemList && itemList.row){
                    $.ajax({
                        type: "POST",
                        url: "/shop/ajaxSiteModuleContext.html",
                        data: {"shopId": __SHOPID,"page":1,"pageSize":itemList.row,"isapp":__ISAPP},
                        dataType: "html",
                        success: function (result) {
                            $("#defaultList").html(result);
                            $("#loadDefaultList").attr("data-value-pages",$("#defaultListPages").val());
                            $("#loadDefaultList").attr("data-value-pageSize",$("#defaultListPageSize").val());
                            var pageSize =$("#defaultListPageSize").val();
                            var count =$("#defaultListCount").val();
                            if(count >= pageSize){
                                $("#loadDefaultList").find("#loadDefaultListspan").text("加载更多");
                            }else{
                                if(count>0){
                                    $("#loadDefaultList").find("#loadDefaultListspan").text("加载完毕");
                                }
                            }
                            $("#loadDefaultList,#loaderrorDefaultList").on("click",function(){
                                loadOther();
                            });
                        }
                    });
                }
            }
        });
    }
    loadDefaultItemList();






