    var redPacketInit = false;
    var redPacketScroll = null;
    $('#layerIscrollRedpacket').on('touchmove', function (e) { e.preventDefault(); });
    $("#js-getredpacket").click(function () {
        //iscroll滚动部分
        setTimeout(function(){
            if(!redPacketInit){
                redPacketScroll = new IScroll('#layerIscrollRedpacket',{click:true});
                redPacketInit = true;
            }
            redPacketScroll.refresh();
        },30);

        $("#js-layer-redpacket").layer('show');
        return false;
    });


    /**
     红包逻辑start
     */
    $(function(){
        getCouponInfo();
    });

    function getCouponInfo(){
        var coupondata ={
            providerId:'83118421'
        }
        $.ajax({
            type: "GET",
            url: "/shop/getCouponInfo.jtml",
            data: coupondata,
            dataType:"json",
            success: function(resultData){
                if(resultData&&resultData.returncode==0){
                    var result = resultData.result;
                    if(result&&result.returncode&&result.returncode==1000){
                        if(result.resultList && result.resultList.length > 0){
                            buildCouponHTML(result.resultList);
                        }
                    }
                }

            }
        });
    }

    /**
     * 目前店铺只允许发一张券（取返回list中第一个）
     * @param resultList
     */
    function buildCouponHTML(resultList){
        var couponHTML = '';
        var maxCoupon = 0;
        if(resultList&&resultList.length > 0){
            var coupon = resultList[0];
            maxCoupon = coupon.couponSum;

            var useEnd = coupon.useEnd;
            var endDateTime = Date.parse(useEnd.replace(/-/g, '/'));
            var endDate = new Date(endDateTime);

            couponHTML += '<li> ' +
            '<div class="hb-info"> ' +
            '<p class="hb-money"><em>¥</em>'+coupon.couponSum+'</p> ' +
            '<p class="hb-type">店铺红包</p> ' +
            '<p class="hb-time">有效期截止'+endDate.format("yyyy-MM-dd")+'</p> ' +
            '</div> ' +
            '<div class="hb-btnarea"> ' +
            '<a href="javascript:;" class="btn btn-hb js-redpacket-ts" data-couponMode="'+coupon.couponMode+'" data-batchid="'+coupon.batchId+'" data-money="'+coupon.couponSum+'" data-useEnd="'+endDateTime+'">领取</a> ' +
            '</div> ' +
            '</li>';
        }

        if(couponHTML.length > 0){
            $(".layer-content-hb").html(couponHTML);
            $("#shop-hongbao-money").html(maxCoupon+'元');
            $(".popindex-form").show();
        }

        // 红包浮层内点击领取的提示
        $(".js-redpacket-ts").on("click",function(){
            var couponMode = $(this).attr("data-couponMode");
            var batchId = $(this).attr("data-batchid");
            var money = $(this).attr("data-money");
            var useEnd = $(this).attr("data-useEnd");

            if(mallUser&&mallUser.isLogin){  //判断是否登录
                grantCouponCode(mallUser.userId,batchId);
            }else{
                gologinUrl();
            }
        });
    }

    function grantCouponCode(userId,batchId,mobile){
        if(typeof(userId) == 'undefined'){
            userId = '';
        }
        if(typeof(mobile) == 'undefined'){
            mobile = '';
        }
        if(typeof(batchId) == 'undefined'){
            batchId = '';
        }
        var grantCoupondata ={
            _appid:"mall",
            providerId:'83118421',
            batchId:batchId,
            userId:userId,
            mobile:mobile
        }
        $.ajax({
            type: "GET",
            url: "/shop/grantCouponCode.jtml",
            data: grantCoupondata,
            dataType:"json",
            success: function(resultData){
                if(resultData&&resultData.returncode==0){
                    if(resultData.result.returncode == 1000){
                        $.toast({
                            'text' : '领取成功！您可以在我的订单查看已领取的红包',
                            'duration' : 2000
                        });
                    }else{
                        $.toast({
                            'text' : resultData.result.message,
                            'duration' : 2000
                        });
                    }
                }else{
                    $.toast({
                        'text' : '领取失败',
                        'duration' : 2000
                    });
                }

            }
        });
    }



// 对Date的扩展，将 Date 转化为指定格式的String
// 月(M)、日(d)、小时(h)、分(m)、秒(s)、季度(q) 可以用 1-2 个占位符，
// 年(y)可以用 1-4 个占位符，毫秒(S)只能用 1 个占位符(是 1-3 位的数字)
// 例子：
// (new Date()).Format("yyyy-MM-dd hh:mm:ss.S") ==> 2006-07-02 08:09:04.423
// (new Date()).Format("yyyy-M-d h:m:s.S")      ==> 2006-7-2 8:9:4.18
    Date.prototype.format = function(fmt)
    { //author: meizz
        var o = {
            "M+" : this.getMonth()+1,                 //月份
            "d+" : this.getDate(),                    //日
            "h+" : this.getHours(),                   //小时
            "m+" : this.getMinutes(),                 //分
            "s+" : this.getSeconds(),                 //秒
            "q+" : Math.floor((this.getMonth()+3)/3), //季度
            "S"  : this.getMilliseconds()             //毫秒
        };
        if(/(y+)/.test(fmt))
            fmt=fmt.replace(RegExp.$1, (this.getFullYear()+"").substr(4 - RegExp.$1.length));
        for(var k in o)
            if(new RegExp("("+ k +")").test(fmt))
                fmt = fmt.replace(RegExp.$1, (RegExp.$1.length==1) ? (o[k]) : (("00"+ o[k]).substr((""+ o[k]).length)));
        return fmt;
    }
</script>


