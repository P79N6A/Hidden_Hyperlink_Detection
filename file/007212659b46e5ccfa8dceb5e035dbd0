  <script type="text/javascript">var STYLEID = '15', STATICURL = 'http://static.sgnet.cc/bbs/', IMGDIR = 'http://static.sgnet.cc/bbs/image/common', VERHASH = 'u3z', charset = 'utf-8', discuz_uid = '0', cookiepre = 'r7Vs_9571_', cookiedomain = '.sgnet.cc', cookiepath = '/', showusercard = '0', attackevasive = '0', disallowfloat = 'newthread|viewvote|tradeorder|activity|debate', creditnotice = '1|威望|,2|金钱|,3|博客积分|,4|博客经验|,5|银子|', defaultstyle = '', REPORTURL = 'aHR0cDovL2Jicy5zZ25ldC5jYy90aHJlYWQtMjU0NzY4Ny0xLTEuaHRtbA==', SITEURL = 'http://bbs.sgnet.cc/', JSPATH = 'http://static.sgnet.cc/bbs/js/';</script> 
  <!--2014.6.4 zm add share weixin --> 
  <script type="text/javascript">
    var imgUrl = 'http://bbs.sgnet.cc/bbslogo.jpg';
    var lineLink = 'http://bbs.sgnet.cc/forum.php?mod=viewthread&tid=';
    var descContent = "中国寿光网";
    var shareTitle = "中国寿光网";
    var appid = '';

    function shareFriend() {
        WeixinJSBridge.invoke('sendAppMessage',{
                            "appid": appid,
                            "img_url": imgUrl,
                            "img_width": "640",
                            "img_height": "640",
                            "link": lineLink,
                            "desc": descContent,
                            "title": shareTitle
                            }, function(res) {
                            _report('send_msg', res.err_msg);
                            })
    }
    function shareTimeline() {
        WeixinJSBridge.invoke('shareTimeline',{
                            "img_url": imgUrl,
                            "img_width": "640",
                            "img_height": "640",
                            "link": lineLink,
                            "desc": descContent,
                            "title": shareTitle
                            }, function(res) {
                            _report('timeline', res.err_msg);
                            });
    }
    function shareWeibo() {
        WeixinJSBridge.invoke('shareWeibo',{
                            "content": descContent,
                            "url": lineLink,
                            }, function(res) {
                            _report('weibo', res.err_msg);
                            });
    }
    // 当微信内置浏览器完成内部初始化后会触发WeixinJSBridgeReady事件。
    document.addEventListener('WeixinJSBridgeReady', function onBridgeReady() {

            // 发送给好友
            WeixinJSBridge.on('menu:share:appmessage', function(argv){
                shareFriend();
                });

            // 分享到朋友圈
            WeixinJSBridge.on('menu:share:timeline', function(argv){
                shareTimeline();
                });

            // 分享到微博
            WeixinJSBridge.on('menu:share:weibo', function(argv){
                shareWeibo();
                });
            }, false);
</script> 
