       <script type="text/javascript">
$('.report-handler').bind('click',function(){
	location.hash='report:'+$(this).attr('data-id');
	report_box($(this).attr('data-id'),'zixun');
})

function report_box(id,type){
	$.getJSON('/utils/report/'+id+'/'+type, function(m){
		if(m.code == -1){
			per_login_div();
		}else{
			$('#report_box>div').html(m.cont)
			easyDialog.open({
	          container : 'report_box'
	        });
		}
	})
}

$('.report-close').bind('click',function(){
	location.hash='abc';
	easyDialog.close()
})

function del_my_confirm(id){
	EConfirm({msg:'Are you sure you want to delete this?',okFun:function(){
		cmt_del(id)
	}})
}

$('.show-report-num').hover(function(e){
	$('.admin-show-report-num .repnum').html("<b style='color:red;'>"+$(this).attr('num')+"</b>"+' user'+(parseInt($(this).attr('num'))>1?'s':''));
	$('.admin-show-report-num a').attr('href',$(this).attr('_href'))
	w = $('.admin-show-report-num').width()-130;
	pos = $(this).offset();
	$('.admin-show-report-num').css({left:pos.left-w,top:pos.top-70,'position':'absolute','display':'block'});
},function(){
	$('.admin-show-report-num').hide();
})
$('.admin-show-report-num').hover(function(){
	$(this).show();
},function(){
	$(this).hide();
});
(function(){
	$('.comment-div-form .scode-handler:first').click();
	
})()

setTimeout(function(){
	if(/report:(\d+)/.exec(location.hash.substr(1)) != null){
		var id = /report:(\d+)/.exec(location.hash.substr(1))[1];
		if(id){
			report_box(id,'zixun');
		}
	}
},100)
</script> 
