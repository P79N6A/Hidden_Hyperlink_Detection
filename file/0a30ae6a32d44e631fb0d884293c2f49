$(function(){$('.feeling').calc_vote_percent();});
function _vote(t){
    $.post("/game/vote", { id:25764,t: t, 'CSRF_TOKEN': 'd6a14739b30c8d19f7509dfe6ff62b33ff15fbcb' }, function(data){
            if(!data.errno){
                $('#v_'+t).html(parseInt($('#v_'+t).html())+1);
                $('.feeling').calc_vote_percent();
            }else{
                alert(data.errmsg);
            }
    }, 'json');
    return false;
}
</script>

