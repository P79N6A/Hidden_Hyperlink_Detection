          function getDetailTime() {
                    var s = "";
                    todayDate = new Date();
                    date = todayDate.getDate();
                    month = todayDate.getMonth() + 1;
                    year = todayDate.getYear();
                    s += "今天是：";
                    if (navigator.appName == "Netscape") {
                        s += (1900 + year);
                        s += "年";
                        s += month;
                        s += "月";
                        s += date;
                        s += "日";
                        s += " "
                    }
                    if (navigator.appVersion.indexOf("MSIE") != -1) {
                        s += year;
                        s += "年";
                        s += month;
                        s += "月";
                        s += date;
                        s += "日";
                        s += " ";
                    }
                    if (todayDate.getDay() == 5) s += "星期五"
                    if (todayDate.getDay() == 6) s += "星期六"
                    if (todayDate.getDay() == 0) s += "星期日"
                    if (todayDate.getDay() == 1) s += "星期一"
                    if (todayDate.getDay() == 2) s += "星期二"
                    if (todayDate.getDay() == 3) s += "星期三"
                    if (todayDate.getDay() == 4) s += "星期四"
                    return s;
                }
                document.write(getDetailTime());
