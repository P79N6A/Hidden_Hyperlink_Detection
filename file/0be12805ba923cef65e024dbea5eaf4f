    var house_browseList_array = new Array();
    house_browseList_array = house_browseList.split("|");
    var flag = 0;
    for(var tem in house_browseList_array){
        //console.log(house_browseNow+"%%%%%%"+house_browseList_array[tem]);
        if(house_browseNow == house_browseList_array[tem]){
            flag = 1;
            break;
        }
    }
    if(flag == 0){
        if(house_browseList_array.length >= 20){
            house_browseList_array.splice(0,1);
            house_browseList = house_browseList_array.join("|");
            house_browseList = house_browseList == ""?house_browseNow:house_browseList+"|"+house_browseNow;
            store.set("house_browseList",house_browseList);
        }else{
            house_browseList = house_browseList==""?house_browseNow:house_browseList+"|"+house_browseNow;
            store.set("house_browseList",house_browseList);
        }

    }
	
