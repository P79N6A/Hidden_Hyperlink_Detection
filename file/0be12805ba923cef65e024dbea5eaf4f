$(document).ready(function(){
    $(".nDover").height("205px");
    var id = '657';
    var city='bb';
    var url = "/index.php?m=home&c=newhouse&a=getnewhouseprice";
    $.ajax({
        url: url,
        type: "post",
        dataType: "text",
        data: {
            id:id,
            city:city
        },
        success: function (data) {
			//console.log(data);
            var louPanDatas = data;
            if(data != ""){
                var projName = '百乐门文化经贸广场';
                var louPanPrices = new Array();
                if(louPanDatas!=""){
                    louPanPrices = louPanDatas.split(";")[1].split(",");
					//console.log(louPanPrices);
                    var loupaMonth = louPanDatas.split(";")[0].split(",");
					//console.log(loupaMonth);

                }
                $(".lpzs").attr("style","display:block;");
                LineChart(
                    "chartCon",
                    1000,
                    0,
                    {
                        "0":{
                            "color":"#f90","name":projName,"arr":louPanPrices,"showPoint":true
                            }
                    },
                    loupaMonth
                    );
                }else{
					  $(".lpzs").hide();
				}
            }
        });
});
