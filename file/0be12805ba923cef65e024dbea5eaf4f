				//$("#imgback").load(function(){})
					var w = $(window).width();
					var h = $(window).height();
					var screenImage = $("#imgback");
					var theImage = new Image();
					theImage.src = screenImage.attr("src");
					var imageWidth = theImage.width;
					var imageHeight = theImage.height;
					var rh = w*imageHeight/imageWidth;
					screenImage.css('height',rh+'px'); 
					screenImage.css('width',w+'px');
					$(".birdView__target").each(function(){
						var l = $(this).attr("l");
						var b = $(this).attr("b");
						b = rh*b;
						l = w*l;
						$(this).css("bottom",b+"px");
						$(this).css("left",l+"px");
						$(this).find(".ou_title").css("width",w+"px");
						var title_w = $(this).find(".ou_title").width();
