//导航收藏请求函数
function h_collect(is_collect){
	var url=$("#collect").val();
	var param={
		listid:"657",
		uid:getCookie("passport_uid"),
		channel:'5',
		action : is_collect==1 ? 'away' : 'add',
		city:$("#city").val()
	}
	$.get(url,param,function(res){
		res = eval("("+res+")");
		if(res.result == 1){
			h_collect_call_back();//请求成功回调函数
		}
		show("#one");
		$("#msgcontentone").html(res.msg);
		setTimeout('dialoghide()',3000);
	});
}

