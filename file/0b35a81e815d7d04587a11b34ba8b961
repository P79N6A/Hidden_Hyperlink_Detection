   <script>
  var pageMax = '20';
  var nextPage = '2';
  var word = '凌辱人妻';
  var pageNextStatus = 1;
   function load_fine_book(page){
    if( !is_numeric(page) || empty(page)){
      return false;
    }
	if(page > pageMax){
		return false;
	}
	str ='<div id="bar" style=" position:fixed; left:5%; right:5%; bottom:120px; text-align:center;"><span style="color:#fff; font-size:12px; border-radius:5px;-webkit-border-radius:5px;-moz-border-radius:5px; background:#000; opacity:0.7; display:inline-block; padding:10px 12px ;">';
	str += '<img src="http://tcdn.2cloo.com/images/speed.gif" /></span></div>';
	$('#tab_box').after(str);
	$.ajax({
		 type : "get",
			url:"/Search-index?is_ajax=1&word="+word+"&page="+page,
			timeout:10000,
			success:function(data,statu){
					$('#bar').remove();
					$('#tab_box').append(data); 
					
					nextPage = Number(page)+1;
					pageNextStatus = 1;
					if( nextPage > pageMax){
						pageNextStatus = 0;
					 
					}
			},
			error:function(e,status){
					$('#bar').remove();
					str ='<div id="bar" style=" position:fixed; left:5%; right:5%; bottom:50px; text-align:center;"><span style="color:#fff; font-size:12px; border-radius:5px;-webkit-border-radius:5px;-moz-border-radius:5px; background:#000; opacity:0.7; display:inline-block; padding:10px 12px ;">';
					str += '网络异常,请刷新重试!</span></div>';
					$('#tab_box').after(str);
					setTimeout(function(){
						$('#bar').remove();
					},3000);
					
			}


	})

  }
  $(window).scroll(function () {
		var bot = 200; //bot是底部距离的高度
		if ((bot + $(window).scrollTop()) >= ($(document).height() - $(window).height())) {
		   //当底部基本距离+滚动的高度〉=文档的高度-窗体的高度时；
			//我们需要去异步加载数据了
			if( pageNextStatus == 1 ){
				pageNextStatus = 0;
				load_fine_book(nextPage);
			}
		}
	});
</script> 
