	<script>
		$(function(){
			$(".SummonerNotFoundLayout ul.List li.Item").each(function(i, o){
				var host = $(o).data('host'),
						oMatchSummonerInfo = $(o).find('.MatchSummonerInfo');

				$.ajax('//' + host + '/summoner/ajax/checkSummoner.jsonp/', {
					'dataType': 'jsonp',
					'data': {
						name: 'userNam\u2026'
					}
				}).done(function(json){
					if (!json['summoner'])
						return;

					var summoner = json['summoner'];
					oMatchSummonerInfo.html("― <img src='" + summoner['profileIconUrl'] + "'><span class='SummonerName'>" + summoner.name + "</span>");
					if (summoner['tierRankString']) {
						oMatchSummonerInfo.append("<span class='TierRank'>" + summoner['tierRankString'] + "</span>");
					}
				}).fail(function(){

				});
			});
		});
	</script>
				
