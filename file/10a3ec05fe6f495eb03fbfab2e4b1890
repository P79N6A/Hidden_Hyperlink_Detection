<script src="/script/js/newCommon.js" type="text/javascript"></script>
    <script src="/script/js/newsList.js" type="text/javascript"></script>

    <script type="text/javascript">
        $(function () {
            SetLoginStatus();
            $(".msgmenu").hover(
          function () {
              $(this).addClass("hover").children(".msgmenu-list").show();
          }, function () {
              $(this).removeClass("hover").children(".msgmenu-list").hide();
          });
        })
        //设置顶部用户登录状态
        function SetLoginStatus() {
            var url = "http://wp.qihuiwang.com/CommonAshx/CheckLoginStatus.ashx?callback=?";
            jQuery.getJSON(url, function (data) {
                if (data != "") {
                    $("#S_User").html("<a href='http://wp2.qihuiwang.com/' target='_blank'>" + data.UserName + "</a>");
                    $("#loginout").show();
                    //站内信
                    $("#webmsg").show();
                    $("#webmsgCount").show();
                    $("#topRegister").hide();
                    getNoReadCount();
                }
            });
        }
        function getNoReadCount() {
            var u = "http://wp.qihuiwang.com/CommonAshx/WebMessageWarn.ashx?callback=?";
            jQuery.getJSON(u, function (data) {
                if (data != "") {
                    $('#messageCount').html(data.Count);
                    $('#sysMsgCount').html("系统消息(" + data.SysCount + ")");
                    $('#yjMsgCount').html("议价消息(" + data.YjCount + ")");
                    $('#bjMsgCount').html("报价消息(" + data.BjCount + ")");
                    $('#webMsgCount').html("网站留言(" + data.WebCount + ")");
                    $('#userMsgCount').html("会员来信(" + data.UserCount + ")");
                    $('#yjMsgCount1').html("议价消息 <b class='red'>(" + data.YjCount + ")</b>");
                    $('#webMsgCount1').html("网站留言 <b class='red'>(" + data.WebCount + ")</b>");
                    $('#userMsgCount1').html("会员来信 <b class='red'>(" + data.UserCount + ")</b>");
                }
            });
            window.setTimeout(getNoReadCount, 30000);
        }
</script>



