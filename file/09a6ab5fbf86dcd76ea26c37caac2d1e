function znForDz(siteConfig){
    bdjq(function(){
        var zn = 'http://zhannei.baidu.com/cse/search?s=1478697672367854295';
        if (siteConfig && siteConfig.resultUrl){
            zn = 'http://' + siteConfig.resultUrl + '/cse/search?s=1478697672367854295'
        }
        var btn = bdjq("a[href^='search.php?mod']");         
