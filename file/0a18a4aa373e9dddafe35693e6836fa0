<script>
function GetRequest() {
   var url = location.search; 
   var theRequest = new Object();
   if (url.indexOf("?") != -1) {
      var str = url.substr(1);
      strs = str.split("&");
      for(var i = 0; i < strs.length; i ++) {
         theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);
      }
   }
   return theRequest;
}

var Request = new Object();
Request = GetRequest();   
var pathname = location.pathname;
var host = location.host;
var topicid;
if (host == 't.m.youth.cn' ){
	if(pathname.replace (/\//g,'').substr(0,8)=='transfer'){
		var str1=pathname.split('/').pop().split('.')[0];
		var str2=str1.split('_')[1];
		topicid='t'+str2;
	}
	if(Request['url']){
		var search = Request['url']; 
		var str1 = search.split('/').pop().split("_")[1]
		var str2 = str1.substr(0,str1.indexOf(".htm"));
		topicid = 't' + str2; 
	}
}else if(pathname.replace (/\//g,'').substr(0,2) == 'pc'){
	topicid = 'c' + pathname.substr(11);
}else if(pathname.replace (/\//g,'') == 'comment.shtml'){
      var search = Request['id'];
      topicid = 'c' + search;
  }else{
	topicid = 'cm' + pathname.split('/').pop();
}
$('#SOHUCS').attr('sid', topicid);

</script>
