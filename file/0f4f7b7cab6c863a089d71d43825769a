            zn = 'http://' + siteConfig.resultUrl + '/cse/search?s=9273071013546871837'
        }
        var btn = bdjq("a[href^='search.php?mod']");         
        if (btn.length > 0) {
            btn.click(function(event){
                window.location.href = zn+ '&q=&partner=discuz';
                event.preventDefault();
            });
        }
    });
}               
</script> 
