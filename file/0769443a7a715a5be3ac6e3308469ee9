	var re_tid = "198860";
	//实例化编辑器 参数设置
	var ue_option = {
		"serverUrl":"/index.php?c=attachment/attachment&m=upload_attachment&fid="+re_fid,
        "postMethod":"send_reply",
		"imageFieldName":"attachment",
		"imageUrlPrefix": "",
		"imageAllowFiles": [".png", ".jpg", ".jpeg", ".gif"],
		"elementPathEnabled":false,
                "textarea":"message"
	};
	reply_ue = UE.getEditor("reply_message",ue_option);
</script> 
