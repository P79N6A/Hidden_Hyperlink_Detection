                var div = $("#AboveFocusFullColumn ul");
                $.each(result, function (i, field) {
                    div.append(field.Title2 != "" ? FullTwoColumn(field) : FullOneColumn(field));
                });
            });
            //焦点图下通栏
            $.getJSON("/House/Dynamic/AD?location=BelowFocusFullColumn", function (result) {
                var div = $("#BelowFocusFullColumn ul");
                $.each(result, function (i, field) {
                    div.append(field.Title2 != "" ? FullTwoColumn(field) : FullOneColumn(field));
                });
            });
            //对联
            $.getJSON("/House/Dynamic/AD?location=Couplet&top=1&orderby=Sort DESC", function (result) {
                $.each(result, function (i, field) {
                    if (field.Title1 != "") {
                        $("#LeftCouplet").append(LeftCouplet(field));
                    }
                    if (field.Title2 != "") {
                        $("#RightCouplet").append(RightCouplet(field));
                    }
                });
            });
            //直接弹窗
            $.getJSON("/House/Dynamic/AD?location=DirectCPM&top=1&orderby=Sort DESC", function (result) {
                $.each(result, function (i, field) {
                    if (field.Width1 >= 5000 || field.Height1 >= 5000) {
                        window.open("/ADCounter/Index?id=" + field.ID + "&url=" + field.Links1 + "&location=left");
                    }
                    else {
                        window.open("/ADCounter/Index?id=" + field.ID + "&url=" + field.Links1 + "&location=left", 'newwindow' + field.ID, 'height=' + field.Height1 + ',width=' + field.Width1 + ',top=0,left=0,toolbar=no,menubar=no,scrollbars=no,resizable=no,location=no, status=no');
                    }
                });
            });
            //底部浮动通栏
            $.getJSON("/House/Dynamic/AD?location=BottomFloatFullColumn&top=1&orderby=Sort DESC", function (result) {
                var div = $("#BottomFloatFullColumn ul");
                var flag = false;
                $.each(result, function (i, field) {
                    flag = true;
                    div.append(FullOneColumn(field, true));
                    var widths = parseInt(div.find('li').data('width'));
                    $('.all_bmgd').css({ marginLeft: -(widths / 2) });
                });
                if (!flag) {
                    div.parents(".all_bmgd").html("");
                }
            });
            //全屏浮动
            $.getJSON("/House/Dynamic/AD?location=FullScreenFloating&top=1&orderby=Sort DESC", function (result) {
                var div = $("#FullScreenFloating ul");
                $.each(result, function (i, field) {
                    div.append(FullOneColumn(field, true));
                    var widths = parseInt(div.find('li').data('width'));
                    var height = parseInt(div.find('li').data('height'));
                    $('.all_fullscreen').css({ marginLeft: -(widths / 2) }).css({ marginTop: -(height / 2) });
                    div.parents(".all_fullscreen").show();
                });
            });
            //左下角视窗
            $.getJSON("/House/Dynamic/AD?location=LeftCorner&top=1&orderby=Sort DESC", function (result) {
                var div = $("#LeftCorner ul");
                $.each(result, function (i, field) {
                    flag = true;
                    div.append(FullOneColumn(field, true));
                    div.parents(".left_fdck").show();
                });
            });
            //右下角视窗
            $.getJSON("/House/Dynamic/AD?location=RightCorner&top=1&orderby=Sort DESC", function (result) {
                var div = $("#RightCorner ul");
                $.each(result, function (i, field) {
                    flag = true;
                    div.append(FullOneColumn(field, true));
                    div.parents(".right_fdck").show();
                });
            });
            //任意点击弹窗
            $.getJSON("/House/Dynamic/AD?location=ClickCPM&top=1&orderby=Sort DESC", function (result) {
                $.each(result, function (i, field) {
                    $(document.body).append('<a onclick="float_Full_body();" class="a_float" href="/ADCounter/Index?id=' + field.ID + '&url=' + field.Links1 + '&location=left" target="' + field.Target1 + '"></a>');
                });
            });
        });
    </script>   
 </body>
</html>