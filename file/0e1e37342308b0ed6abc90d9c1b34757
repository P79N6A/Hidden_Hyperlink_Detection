  <script type="text/javascript">
          $(document).ready(function(){
            $('.top-login').mouseover(function(){
              $(this).children().children('ul').slideDown(100);
            });
            $('.top-login').mouseleave(function(){
              $(this).children().children('ul').slideUp(100);
            });

            $('.hot-field-sidebar .row').mouseover(function(){
                $('.hot-field-sidebar .row').each(function(){
                    $(this).find('a').removeClass('a-select');
                    $(this).removeClass('row-select');
                    $(this).find('.field-label').removeClass('field-select')
                }); 
                $(this).find('a').addClass('a-select');
                $(this).addClass('row-select');
                $(this).find('.field-label').addClass('field-select')  
            });

            $('.post-hot-sidebar .row').mouseover(function(){
                $('.post-hot-sidebar .row').each(function(){
                    $(this).find('.col-xs-12').hide();
                    $(this).find('.col-xs-4').hide();
                    $(this).find('.col-xs-8').hide();
                }); 
                $(this).find('.col-xs-12').show();
                $(this).find('.col-xs-4').show();
                $(this).find('.col-xs-8').show();
            });
          });
      </script> 
