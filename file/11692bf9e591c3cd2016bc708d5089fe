    $.ajax({
        type:'get',
        url:"http://guangzhou.liebiao.com/ajax/postclicks/?postid=195832522",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
