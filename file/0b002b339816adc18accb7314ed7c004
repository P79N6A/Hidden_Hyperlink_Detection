     <script type="text/javascript">
                (function(){
				    var inputrel;
					var userinput
				    $(".search_fk").focus(function(){
					      inputrel = $(this).attr("rel");
						  currentvalue = $(this).val();
						  if(inputrel == currentvalue) {
						      $(this).val('');
						  }
					});

					$(".search_fk").blur(function(){
						  currentvalue = $(this).val();
						  if(currentvalue == '') {
						      $(this).val(inputrel);
						  }
					});

					$("#headersou1").click(function(){
					      inputrel = $("#search_fk1").attr("rel");
					      currentvalue = $("#search_fk1").val();
						  if(inputrel == currentvalue) {
						      $("#search_fk1").val('');
						  }
					});

					$("#headersou2").click(function(){
					      inputrel = $("#search_fk2").attr("rel");
					      currentvalue = $("#search_fk2").val();
						  if(inputrel == currentvalue) {
						      $("#search_fk2").val('');
						  }
					});

					$("#search_fk1").keydown(function(event){
					   if(event == 13) {
					      inputrel = $("#search_fk1").attr("rel");
					      currentvalue = $("#search_fk1").val();
						  if(inputrel == currentvalue) {
						      $("#search_fk1").val('');
						  }
					   }
					});

					$("#search_fk2").keydown(function(event){
					   if(event == 13) {
					      inputrel = $("#search_fk2").attr("rel");
					      currentvalue = $("#search_fk2").val();
						  if(inputrel == currentvalue) {
						      $("#search_fk2").val('');
						  }
					   }
					});

				})();
			</script> 
