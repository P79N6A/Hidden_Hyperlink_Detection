												},
												function(){
													$("#college_filter").css({"height":"72px","overflow":"hidden"});
													$("#extendnavi").empty();
													$("#extendnavi").append("展开 <img src='/Public/Images/list-down.gif' style='vertical-align:middle'/>");
												}					
											);		
					  
										
									})();
								</script> 
