//弹出窗口
$(document).ready(function(){
	//header登录弹窗
	$(".open_now1").click(function() {
		$("#facebox").overlay().load();
		var sd1 = document.getElementById('login_box');
		var sd2 = document.getElementById('reg_box');
		var tag = document.getElementById('box_tag');
		sd1.style.display='block';
		sd2.style.display='none';
		tag.innerHTML = '登录';
	});
	// 登录弹窗遮蔽
	$("#facebox").overlay({
		// custom top position
		top: 50,
		// some mask tweaks suitable for facebox-looking dialogs
		mask: {
			// you might also consider a "transparent" color for the mask
			color: '#fff',
			// load mask a little faster
			loadSpeed: 200,
			// very transparent
			opacity: 0.5
		},
		// disable this for modal dialog-type of overlays
		closeOnClick: false,
		// load it immediately after the construction
		load: false
	});
	//是否判断窗口遮蔽
	var triggers = $(".modalInput").overlay({
	    // some mask tweaks suitable for modal dialogs
	    mask: {
	        color: '#ebecff',
	        loadSpeed: 200,
	        opacity: 0.9
	    },
	    closeOnClick: false,
		load: false
	});
});
</script> 
   <script language="JavaScript">

	function loginHandle(data,status){
		if (status == 1) {
			setTimeout("window.location.reload()",1000);
		}
	}

	//登录
	function sendtologin() {
		var current_url = window.location.href;
		if(current_url.indexOf('i.edpass.org') != -1) {
			ThinkAjax.sendForm('loglinform','/Public/checkLogin/',loginHandle,'result');
		}else{
			var account = $("#login_email").val();
			var pass = $("#login_pass").val();
			var chk_remember_pwd = $("#chk_remember_pwd").val();
			var LXT_URL = "http://i.edpass.org";
			var LXT_URL_RE = "http://i.edpass.org/index.php/Public/checkLogin?account="+account+"&password="+pass+"&chk_remember_pwd="+chk_remember_pwd+"&callback=?";
			$.getJSON(LXT_URL_RE,function(data){
				if (data.status==1) {
					$('#result').html('<img src="'+LXT_URL+'/Public/Images/ok.gif" border="0" width="20px" height="20px" alt="success..." align="absmiddle"> <span style="color:blue;">'+data.info+'</span>');
					setTimeout("window.location.reload()",1000);
				}else{
					$('#result').html('<img src="'+LXT_URL+'/Public/Images/update.gif" border="0" alt="error..." align="absmiddle"> <span style="color:red;">'+data.info+'</span>');
					window.setTimeout(function (){ $('#result').empty();}, 3000);
				}
			});
		}
	}

	function keydownlogin(e){
		var e = e || event;
		if (e.keyCode==13) {
			sendtologin();
		}
	}

	function SwitchBox(){
		var sd1 = document.getElementById('login_box');
		var sd2 = document.getElementById('reg_box');
		var tag = document.getElementById('box_tag');
		if(sd1.style.display=='block'){
			sd1.style.display='none';
			sd2.style.display='block';
			tag.innerHTML = '注册';
		}else{
			sd1.style.display='block';
			sd2.style.display='none';
			tag.innerHTML = '登录';
		}
	}

</script> 
