   <script type="text/javascript">

	var InterValObj; //timer变量，控制时间
	var count = 60; //间隔函数，1秒执行
	var curCount;//当前剩余秒数

	function sendMessage() {
	    var reg_code = $("#reg_code").val();
		var reg_phone = $("#reg_phone").val();
		//var code_session = '';
		//alert(reg_phone+','+reg_code);
		//alert(code_session);
		ThinkAjax.send('/Public/sendmessage/','ajax=1&reg_code='+reg_code+'&reg_phone='+reg_phone,aftersendmsg,'result_r');
	}

	//timer处理函数
	function SetRemainTime() {
    	if (curCount == 0) {                
        	window.clearInterval(InterValObj);//停止计时器
        	$("#btnSendCode").removeAttr("disabled");//启用按钮
        	btnSendCode.style.color='#fff';
        	btnSendCode.style.background='#33bfeb';
        	$("#btnSendCode").val("重新发送");
    	}else {
        	btnSendCode.style.color='#898989';
            btnSendCode.style.background='#78d3f1';
            curCount--;
            if(curCount < 10){
            	$("#btnSendCode").val("0" + curCount + "秒剩余");
            }else {
            	$("#btnSendCode").val(curCount + "秒剩余");
            }
        }
    }
    function aftersendmsg(data,status){
    	if (status==1) {
			curCount = count;
		　　//设置button效果，开始计时
		    $("#btnSendCode").attr("disabled", "true");
		    btnSendCode.style.color='#898989';
		    btnSendCode.style.background='#78d3f1';
		    $("#btnSendCode").val(curCount + "秒剩余");
		    InterValObj = window.setInterval(SetRemainTime, 1000); //启动计时器，1秒执行一次
		　　//向后台发送处理数据
		    $.ajax({
		    	type: "POST", //用POST方式传输
		     　　dataType: "text", //数据格式:JSON
		     　　url: 'Login.ashx', //目标地址
		    　　 data: "dealType=" + dealType +"&uid=" + uid + "&code=" + code,
		    　　 error: function (XMLHttpRequest, textStatus, errorThrown) { },
		     　　success: function (msg){ }
		    });
		}
    }
	function afterphonereg(data,status){
		if(status==1) {
			var account = $("#reg_phone").val();
			var pass = $("#reg_pwd").val();
			var LXT_URL = "http://i.edpass.org";
			var LXT_URL_RE = "http://i.edpass.org/index.php/Public/checkLogin?account="+account+"&password="+pass+"&callback=?";
			$.getJSON(LXT_URL_RE,function(data){
				$('#result_r').html('<img src="'+LXT_URL+'/Public/Images/ok.gif" border="0" width="20px" height="20px" alt="success..." align="absmiddle"> <span style="color:blue;">注册成功啦，并已为您登录！</span>');
				setTimeout("window.location.reload()",1000);
			});
		}
	}
	//手机注册
	function CheckMsgCode() {
		var reg_phone = $("#reg_phone").val();
		var reg_msg = $("#reg_msg").val();
		var reg_pwd = $("#reg_pwd").val();
		var user_agreement = $("#user_agreement").is(':checked');
		//var code_session = '';
		//alert(reg_msg+','+user_agreement+','+reg_pwd);
		//alert(code_session);
		ThinkAjax.send('/Public/checkmsgcode/','ajax=1&reg_msg='+reg_msg+'&reg_phone='+reg_phone+'&reg_pwd='+reg_pwd+'&user_agreement='+user_agreement,afterphonereg,'result_r');
	}
	$(function(){
		$("#getcode").click(function(){
			$(this).attr("src",'/Public/Code/code_char.php?' + Math.random());
		});
		$("#msgcode").click(function(){
			$("#getcode").attr("src",'/Public/Code/code_char.php?' + Math.random());
		});
	});
	function keydownreg(e){
		var e = e || event;
		if (e.keyCode==13) {
			CheckMsgCode();
		}
	}

</script> 
