<script type="text/javascript" src="http://img1.tuniucdn.com/j/20130605/3rd/common/jquery.Map.3.0.js,nrzx/TN_date.js,classpage/query_teamdate.js,yii_calendar/recall_tuandate.js,yii_calendar/jquery-ui-1.8.17.custom.min.js,yii_calendar/fullcalendar-min.js,common/lazyloadnew.min.js"></script>

<script type="text/javascript">
    In('TN_common_classification','siteJs','TN_common_init',function(){
        $('#begin_date,#end_date').TN_date();
    });
    //图片异步加载。
    $("img").lazyload({
        effect: "fadeIn",
        failurelimit: 25,
        threshold: 300
    });
    var cruiseSearchCond = new Array();
            cruiseSearchCond['cruiseLineId'] = 0;
                cruiseSearchCond['cruiseCompanyAb'] = 'all';
                cruiseSearchCond['harbourAb'] = 'cnchq';
                cruiseSearchCond['page'] = 1;
        
    function resetPrice(){
        cruiseSearchCond['lowPrice'] = document.getElementById("first_price").value;
        if(!cruiseSearchCond['lowPrice']){
            cruiseSearchCond['lowPrice'] = 0;
        }else{
            cruiseSearchCond['lowPrice'] = parseInt(cruiseSearchCond['lowPrice']);
        }
        cruiseSearchCond['highPrice'] = document.getElementById("second_price").value;
        if(!cruiseSearchCond['highPrice']){
            cruiseSearchCond['highPrice'] = 0;
        }else{
            cruiseSearchCond['highPrice'] = parseInt(cruiseSearchCond['highPrice']);
        }
        redirectUrl(formatCruiseSCPUrl());
    }

    function setPromoico(){
        var promos = document.getElementsByName("promoIco");
        var promoIco = 0;
        for(i = 0; i<promos.length; i++){
            if(promos[i].type == 'checkbox' && promos[i].checked){
                promoIco += parseInt(promos[i].value);
            }
        }
        cruiseSearchCond['promoIco'] = promoIco;
        //{CRUPC-2 edit fanchonghe@tuniu.com 增加船票类型的筛选
        cruiseSearchCond['ticketType'] = $('.specil_filter [name="ticketType"]').is(':checked')
            ? 0            : 1;
        //CRUPC-2}

        //add fanchonghe@tuniu.com 增加铁定成团筛选
        cruiseSearchCond['formed'] = $('.specil_filter [name="formed"]').is(':checked') ? 1 : 0;
        redirectUrl(formatCruiseSCPUrl());
    }


    function formatCruiseSCPUrl(){
        var url = "http://youlun.tuniu.com";
        //航线
        url += "/d" + parseInt(cruiseSearchCond['cruiseLineId']);
        //邮轮品牌
        url += "-" + selfTrim(cruiseSearchCond['cruiseCompanyAb']);
        //出发港口
        url += "-" + selfTrim(cruiseSearchCond['harbourAb']) + "/";
        //其他条件
        var urlCond = new Array();
        //出游时间
        if(cruiseSearchCond['beginDate'] && cruiseSearchCond['endDate']){
            urlCond.push("a" + cruiseSearchCond['beginDate']+'_'+cruiseSearchCond['endDate']);
        }
        //天数
        if(parseInt(cruiseSearchCond['dayNum'] > 0)){
            urlCond.push("d" + parseInt(cruiseSearchCond['dayNum']));
        }
        //排序方式
        if(parseInt(cruiseSearchCond['sort'])>0){
            urlCond.push("s" + parseInt(cruiseSearchCond['sort']));
        }
        //价格
        if(cruiseSearchCond['lowPrice'] || cruiseSearchCond['highPrice']){
            urlCond.push("p" + parseInt(cruiseSearchCond['lowPrice']) + "_" + parseInt(cruiseSearchCond['highPrice']));
        }
        //推荐、特价、热卖三个checkbox
        if(cruiseSearchCond['promoIco']){
            urlCond.push("i" + parseInt(cruiseSearchCond['promoIco']));
        }
        //CRUPC-2 edit fanchonghe@tuniu.com 增加船票类型的筛选
        if (cruiseSearchCond['ticketType'] == 0) {
            urlCond.push("t" + cruiseSearchCond['ticketType']);
        }
        //add fanchonghe@tuniu.com 增加出发城市的筛选
        if (cruiseSearchCond['departCityCode']) {
            urlCond.push("c" + cruiseSearchCond['departCityCode']);
        }
        if (cruiseSearchCond['formed']) {
            urlCond.push("f" + cruiseSearchCond['formed']);
        }
        if(urlCond.length > 0){
            url += "list-" + urlCond.join('-') + "/";
        }
        //页数
        if(parseInt(cruiseSearchCond['page']) > 1){
            url += parseInt(cruiseSearchCond['page'])+"/";
        }

        return url;
    }
    function selfTrim(str){
        return str.replace(/(^\s*)|(\s*$)/g, "");  
    }

    function redirectUrl(url){
        window.location.href = url;
        return false;
    }
    
    
    var niuren = {
        changeDate : function(){
            var begin_date = this.dateTransform($("#begin_date").val());
            var end_date =this.dateTransform($("#end_date").val());
            cruiseSearchCond['beginDate'] = begin_date;
            cruiseSearchCond['endDate'] = end_date;
            if(begin_date && end_date){
                var url = formatCruiseSCPUrl();
                redirectUrl(url); 
                return;
            }
        },

        dateTransform : function(data){
            if(data=='yyyy-mm-dd'){
                return '';
            }
            var data_arr = data.split('-');
            var year = parseInt(data_arr[0]);
            var month = parseInt(data_arr[1]);
            var day = parseInt(data_arr[2]);
            month = month<10 ? '0'+month : month;
            day = day<10 ? '0'+day : day;
            var retStr = year.toString()+month.toString()+day.toString();
            return retStr;
        }
    }
    
</script>

