    $(document).ready(function () {
        var catLoaded = false;
        $(document).mousemove(function () {
            if (!catLoaded) {
                catLoaded = true;
                $(".categorys").load('/header/bj');
                                //底部图片异步加载。
                $("#TN-footer img").lazyload({
                    effect: "fadeIn",
                    failurelimit: 25,
                    threshold: 300
                });
                            }
        });
    });
</script>

