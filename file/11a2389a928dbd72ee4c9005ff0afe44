      <script type="text/javascript">
    function onButtonClick(){
        var node = document.getElementById("qs");
        var searchUrl = "";
        // 分类
        if(document.getElementById("sort")) {
            var sort = document.getElementById("sort").value;
            if(sort != "" && sort != "undefined" && typeof sort != "undefined" && sort != "rupdate") {
                if(searchUrl != "") {
                    searchUrl += "_";
                }
                searchUrl += sort;
            }
        }
        for(var i=0; i<node.childNodes.length;i++){
            if(node.childNodes[i].nodeType == 1){
                var field=node.childNodes[i].id;
                var value=node.childNodes[i].value;
                //alert(field+":"+value+":"+node.toString() );
                if(value != "" && value != "undefined" && typeof value != "undefined"){
                    if(searchUrl != "") {
                        searchUrl += "_";
                    }
                    searchUrl += value;
                }
            }
        }
        if(searchUrl != "" && searchUrl != "undefined" && typeof searchUrl != "undefined") {
            searchUrl = "~" + searchUrl + "/";;
        }
        var url = "http://www.verycd.com/sto/" + searchUrl;
        window.location.href = url;
    }
    function doReset() {
        var node = document.getElementById("qs");
        var sort = document.getElementById("sort");
        if(sort) {
            if(sort.length > 7) {
                sort.options.selectedIndex = 5;
            } else if(sort.length == 7) {
                sort.options.selectedIndex = 4;
            }
        }
        for(var i=0; i<node.childNodes.length;i++){
            if(node.childNodes[i].nodeType == 1){
                var f=node.childNodes[i].id;
                old_get(f).options.selectedIndex = 0;
            }
        }
    }
    function setType(t,field){
        document.getElementById(t).value = field;
    }
</script> 
