    function get_ip (str) {
        var reg = /(.*?)(\d+\.\d+\.\d+\.\d+)(.*?)/;
        var ip = str.match(reg);
        return ip[2];
    }
    function post_aj (data_type) {
        var url = window.location.href;
        var ip = get_ip($('#ipinfo').text());
