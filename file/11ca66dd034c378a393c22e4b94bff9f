$(document).ready(function(){
$(".tabequal li.topen").click(function(){
$(".tabequal li").eq($(this).index()).toggleClass("openyes").siblings().removeClass('openyes');
$(".showbox").eq($(".tabequal li.topen").index(this)).toggleClass("boxopen").siblings().removeClass('boxopen'); 
});	
$("body").prepend($(".mm-menu"));
$(".mm-menu, .mm-blocker").height($(window).height() + 100);
$(".mm-openmenu").click(function(){$("body").addClass("mm-opening");$(".mm-blocker").delay(200).fadeIn(100);});	
$(".mm-blocker").click(function(){$("body").removeClass("mm-opening");$(this).hide();});
$(window).scroll(function(){if($(window).scrollTop()>300){$(".scroll_top").show()}else{$(".scroll_top").hide()}});	
})
</script>  
