	var jwVideo	= $(".jw-video.jw-reset")[0];
	if (jwVideo != null) {
			jwVideo.onplay = function() {
				if (onceTag) {
					addPlayTimes(1642754);
				}
				onceTag = false;
		}
	}
},1800); // add delay due to jwplayer 
