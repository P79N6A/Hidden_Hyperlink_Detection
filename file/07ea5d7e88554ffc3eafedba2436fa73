  <script type="text/javascript">


$("#shoucang").click(function(){
	//判断是否已经登录
	if($("#username").val()=="")
	{
		$(window).scrollTop(0);
		$(".box-mask").show();
		$(".loginbox").show();
		return false;
	}
	
	var nid=$("#nid").val();
	var hosturl = $("#hosturl").val();
	$.get(hosturl+"index.php?m=Message&a=addshoucang&nid="+nid,function(data,status){
		if(data=="1")
		{
			alert("收藏成功!");
		}
		else if(data=="0")
		{
			alert("收藏失败!");
		}
		else
		{
			alert("您已经收藏过该文章!");
		}
	});
});
</script> 
