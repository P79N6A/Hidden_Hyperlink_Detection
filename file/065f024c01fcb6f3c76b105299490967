                return false;
            })

        }
        if (EplatformID == 20004) {
            function class_dlbox() {
                $(".dlbox").click(function () {
                    if ($(this).attr("class") == "dlbox") {
                        $(".dlboxon").attr("class", "dlbox").find(".dlxq").hide();
                        $(".dlbox").find("dl").css("border-bottom", "#ccc 1px dotted");
                        $(this).attr("class", "dlboxon").find(".dlxq").show();
                        $(this).find("dl").css("border-bottom", "none");
                    } else {
                        $(this).attr("class", "dlbox").find(".dlxq").hide();
                        $(this).find("dl").css("border-bottom", "#ccc 1px dotted");
                    }
                });
            }

