<script type="text/javascript">

$(function(){
	    $.getJSON("http://www.adidas.com.cn/api/ajaxheader.php", function(data) {
	        if(data.flag !=0){
	            $('.loginGroup .login_redesign').text(data.name);
	        }else{
	        	$('.loginGroup .login_redesign').text('登录');
	        }
	        if(data.qty == 0){
                $(".manicarBox").hide();
                $(".emptyCart").show();
            }else{
                $(".emptyCart").hide();
				$(".minicart .minicartNum .miniNum").html(data.qty);
				$(".minicart").addClass("bgMiniCart");
            }
	    });
})

function getPopUrl(key){
	if(key == 'addCart'){
		return 'http://www.adidas.com.cn/specific/Ajaxshowuserlogin/successAddCartPc/' ;
	}
	if(key == 'addCartFail'){
		return 'http://www.adidas.com.cn/specific/Ajaxshowuserlogin/mixedAddCart/' ;
	}
	if(key == 'miniShoppingcar'){
		return 'http://www.adidas.com.cn/skin/frontend/arvato/default/' + 'component/popup_minishoppingcar.php' ;
	}
	if(key=="headAccount"){
            
	} 
        if(key=="registerSuccess"){
            return 'http://www.adidas.com.cn/skin/frontend/arvato/default/' + 'component/popup_register_success.php' ;
        }
    if(key=="subscribeSuccess"){
        return 'http://www.adidas.com.cn/skin/frontend/arvato/default/' + 'component/popup_subscribe_success.php' ;
    }
     if(key=="r3scEditKey"){
        return 'http://www.adidas.com.cn/specific/Ajaxshowuserlogin/r3shoppingCart/';
       // return '/skin/frontend/arvato/default/release_three/component/popup_r3shoppingCart.html' ;
        
    }
}
function getPopUrlUser(callback){
   return false;
}
function getAjaxUrl(key){
	if(key =='miniShoppingcar'){
		return  {
			url:"http://www.adidas.com.cn/specific/Ajaxshoppingcart/arvatoAjaxCart/",type:"GET",data:{}
		};
	}
	if(key =='search'){
		var q =  jQuery('#arvato_q').val() ; 
		return {
			url:"http://www.adidas.com.cn/catalogsearch/ajax/suggest/",type:"GET",data:{"q":q}
		};
	}
}


