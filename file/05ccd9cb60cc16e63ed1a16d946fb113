function utag_addto_search_term(itemUrl,search_product,search_term){
  console.log(itemUrl+"-----"+search_product+"----"+search_term);
  window.utag.link({  
      event_name : 'SEARCH-PRODUCTS', 
      search_term : search_term, 
      search_product : search_product
  });
  location.href=itemUrl;
}
