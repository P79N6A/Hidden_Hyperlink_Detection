<script>
    jQuery(function($){
        $('#copment .pdpCommentPage a').on('click',function() {
            var url = $(this).attr('data');
            var strPatt = /p=(\d*)/i;
            var pageArray = url.match(strPatt);
            page = pageArray[1];
            if(page<1){return false;}
            var strs='';
            if($("#commentsort").val()!=''){strs='&psort='+$("#commentsort").val()}
            var qData = "id=329626&p=" + page+strs;
            $.ajax({
                url:"http://www.adidas.com.cn/catalog/product/ajaxreview/",
                data:qData,
                dataType:'html',
                success:function(html){
                    $(".pdpCommentsList").html(html);
                }
            });
            return false;
        });

    })
</script>
                </div>
                <!--end pdp commonts list-->
            </div>
