                        $(".layer .box").eq(1).find(".item").eq(1).find(".kecheng li").removeClass("select");
                        $(".layer .box").eq(1).find(".item").eq(1).find(".kecheng li").eq(1).addClass("select");
                        $(".layer .box").eq(1).find(".item").eq(1).find(".courseList").hide();
                        $(".layer .box").eq(1).find(".item").eq(1).find(".courseList").eq(1).show();
                    }
                    break;
                case "23":
                case "232":
                    $(".tabSelect .tags>li.select").removeClass("select");
                    $(".tabSelect .tags>li").eq(0).addClass("select");
                    $(".layer").eq(0).find(".box").hide();
                    $(".layer").eq(0).find(".box").eq(0).show();

                    $(".tabSelect .banji_2>li.select").removeClass("select");
                    $(".tabSelect .banji_2>li").eq(1).addClass("select");
                    $(".layer").eq(0).find(".box").hide();
                    $(".layer").eq(0).find(".box").eq(1).show();
                    $(".layer .box").eq(1).find(".menu>li.select").removeClass("select");
                    $(".layer .box").eq(1).find(".menu>li").eq(2).addClass("select");
                    $(".layer .box").eq(1).find(".item").hide();
                    $(".layer .box").eq(1).find(".item").eq(2).show("select");
                    if (qh == "232") {
                        $(".layer .box").eq(1).find(".item").eq(2).find(".kecheng li").removeClass("select");
                        $(".layer .box").eq(1).find(".item").eq(2).find(".kecheng li").eq(1).addClass("select");
                        $(".layer .box").eq(1).find(".item").eq(2).find(".courseList").hide();
                        $(".layer .box").eq(1).find(".item").eq(2).find(".courseList").eq(1).show();
                    }
                    break;
                case "31":
                case "312":
                    $(".tabSelect .tags>li.select").removeClass("select");
                    $(".tabSelect .tags>li").eq(0).addClass("select");
                    $(".layer").eq(0).find(".box").hide();
                    $(".layer").eq(0).find(".box").eq(0).show();

                    $(".tabSelect .banji_2>li.select").removeClass("select");
                    $(".tabSelect .banji_2>li").eq(2).addClass("select");
                    $(".layer").eq(0).find(".box").hide();
                    $(".layer").eq(0).find(".box").eq(2).show();
                    if (qh == "312") {
                        $(".layer .box").eq(2).find(".item").eq(0).find(".kecheng li").removeClass("select");
                        $(".layer .box").eq(2).find(".item").eq(0).find(".kecheng li").eq(1).addClass("select");
                        $(".layer .box").eq(2).find(".item").eq(0).find(".courseList").hide();
                        $(".layer .box").eq(2).find(".item").eq(0).find(".courseList").eq(1).show();
                    }
                    break;
                case "32":
                case "322":
