                    $(".layer .box").eq(2).find(".item").hide();
                    $(".layer .box").eq(2).find(".item").eq(1).show("select");
                    if (qh == "322") {
                        $(".layer .box").eq(2).find(".item").eq(1).find(".kecheng li").removeClass("select");
                        $(".layer .box").eq(2).find(".item").eq(1).find(".kecheng li").eq(1).addClass("select");
                        $(".layer .box").eq(2).find(".item").eq(1).find(".courseList").hide();
                        $(".layer .box").eq(2).find(".item").eq(1).find(".courseList").eq(1).show();
                    }
                    break;
                case "33":
                case "332":
                    $(".tabSelect .tags>li.select").removeClass("select");
                    $(".tabSelect .tags>li").eq(0).addClass("select");
                    $(".layer").eq(0).find(".box").hide();
                    $(".layer").eq(0).find(".box").eq(0).show();

                    $(".tabSelect .banji_2>li.select").removeClass("select");
                    $(".tabSelect .banji_2>li").eq(2).addClass("select");
                    $(".layer").eq(0).find(".box").hide();
                    $(".layer").eq(0).find(".box").eq(2).show();
                    $(".layer .box").eq(2).find(".menu>li.select").removeClass("select");
                    $(".layer .box").eq(2).find(".menu>li").eq(2).addClass("select");
                    $(".layer .box").eq(2).find(".item").hide();
                    $(".layer .box").eq(2).find(".item").eq(2).show("select");
                    if (qh == "332") {
                        $(".layer .box").eq(2).find(".item").eq(2).find(".kecheng li").removeClass("select");
                        $(".layer .box").eq(2).find(".item").eq(2).find(".kecheng li").eq(1).addClass("select");
                        $(".layer .box").eq(2).find(".item").eq(2).find(".courseList").hide();
                        $(".layer .box").eq(2).find(".item").eq(2).find(".courseList").eq(1).show();
                    }
                    break;
                case "41":
                case "412":
                    $(".tabSelect .tags>li.select").removeClass("select");
                    $(".tabSelect .tags>li").eq(1).addClass("select");
                    $(".layer").hide();
                    $(".layer").eq(1).show();
                    if (qh == "412") {
                        $(".layer").eq(1).find(".item").eq(0).find(".kecheng li").removeClass("select");
