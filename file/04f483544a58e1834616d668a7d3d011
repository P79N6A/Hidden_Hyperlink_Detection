                    $(".tabSelect .tags>li").eq(1).addClass("select");
                    $(".layer").hide();
                    $(".layer").eq(1).show();
                    $(".layer").eq(1).find(".menu>li.select").removeClass("select");
                    $(".layer").eq(1).find(".menu>li").eq(1).addClass("select");
                    $(".layer").eq(1).find(".item").hide();
                    $(".layer").eq(1).find(".item").eq(1).show("select");
                    if (qh == "422") {
                        $(".layer").eq(1).find(".item").eq(1).find(".kecheng li").removeClass("select");
                        $(".layer").eq(1).find(".item").eq(1).find(".kecheng li").eq(0).addClass("select");
