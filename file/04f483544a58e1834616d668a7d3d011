                case "53":
                case "532":
                    $(".tabSelect .tags>li.select").removeClass("select");
                    $(".tabSelect .tags>li").eq(3).addClass("select");
                    $(".layer").hide();
                    $(".layer").eq(2).show();
                    $(".layer").eq(2).find(".menu>li.select").removeClass("select");
                    $(".layer").eq(2).find(".menu>li").eq(2).addClass("select");
                    $(".layer").eq(2).find(".item").hide();
                    $(".layer").eq(2).find(".item").eq(2).show("select");
                    if (qh == "532") {
                        $(".layer").eq(2).find(".item").eq(2).find(".kecheng li").removeClass("select");
                        $(".layer").eq(2).find(".item").eq(2).find(".kecheng li").eq(0).addClass("select");
                        $(".layer").eq(2).find(".item").eq(2).find(".courseList").hide();
                        $(".layer").eq(2).find(".item").eq(2).find(".courseList").eq(0).show();
                    }
                    break;
            }
        }
        $(".tabSelect .tags>li").click(function () { if ($(this).attr("tabindex") == null) return; $(this).siblings(".select").removeClass("select"); $(this).addClass("select"); $("blockquote .layer").hide(); $("blockquote .layer").eq($(this).attr("tabindex")).show(); });
        $(".tabSelect .banji_2>li").click(function () { $(this).siblings(".select").removeClass("select"); $(this).addClass("select"); $(this).parents(".layer").find(".box").hide(); $(this).parents(".layer").find(".box").eq($(this).attr("tabindex")).show(); });
        $(".box .menu>li").click(function () {
            if ($(this).attr("tabindex") == null) return;
            $(this).siblings(".select").removeClass("select");
            $(this).addClass("select");
            $(this).parents(".box").find(".item").hide();
            $(this).parents(".box").find(".item").eq($(this).attr("tabindex")).show();
        });
        $(".kecheng li").click(function () {
            $(this).siblings(".select").removeClass("select");
            $(this).addClass("select");
            $(this).parents(".item").find(".courseList").hide();
            $(this).parents(".item").find(".courseList").eq($(this).attr("tabindex")).show();
        });
        $(".allCourse>.xlDiv>ul>li").mouseenter(function () { $(this).find(".info").show(); });
        $(".allCourse>.xlDiv>ul>li").mouseleave(function () { $(this).find(".info").hide(); });
        var aisplay = $(" a.isplay");
        for (i = 0; i < aisplay.length; i++) {
            if ($(aisplay[i]).attr("data-title") != null) {
                $(aisplay[i]).append("<div class=\"playtips\">本节课未到直播或重播时间，请学习其它课程</div>");
            }
            else {
                $(aisplay[i]).append("<div class=\"playtips\">" + ($(aisplay[i]).attr("data-play") == "" ? "请至上一页播放" : ("本章节的授课内容包含在 " + $(aisplay[i]).attr("data-play") + " 课程目录中")) + "</div>");
            }
        }
        $("a.isplay").mouseenter(function () { $("a.isplay").css({ "z-index": "1" }); $(this).css({ "z-index": "2" }) });

    </script>   
 </body>
</html>