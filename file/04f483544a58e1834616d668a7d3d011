                    $(".tabSelect .banji_2>li").eq(2).addClass("select");
                    $(".layer").eq(0).find(".box").hide();
                    $(".layer").eq(0).find(".box").eq(2).show();
                    $(".layer .box").eq(2).find(".menu>li.select").removeClass("select");
