        seajs.use(["otheruserinfo", "tcar", "tkbei", "TMyTopic", "otherhelpinfo", "jquery", "sorttable", "tab", "focus", "select", "layer", "tip", "operaterelations", "garagebox", "gotop"], function (otheruserinfo, tcar, tkbei, TMyTopic, otherhelpinfo, $) {
