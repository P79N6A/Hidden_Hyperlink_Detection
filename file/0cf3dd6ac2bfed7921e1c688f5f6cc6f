            //焦点图
            $('#focusimg02').focus({
                anim: true,
                interval: 3000000,
                pause: 'hover'
            });


            $.fn.scrollFixed = function () {

                var opt = {
                    top: 0,
                    distance: 0
                },
                elem = this, t;

                elem.css({ position: 'static', top: 0 });

                t = elem.offset().top;

                $.extend(opt, arguments[0] || {});

                function set() {
                    if ($(window).scrollTop() + opt.distance > t) {
                        elem.css({
                            top: opt.top + 'px',
                            position: 'fixed'
                        });
                    }
                    else {
                        elem.css({
                            top: 0,
                            position: 'static'
                        });
                    }
                }

                $(window).on('scroll', set);

                set();
            };
            var userTimer = new atga({
                timerParent: "#dynamic",
                timerName: "showTime1"
            });
            $(function () {
                if ($('.attention-people') != undefined && $('.attention-people').length > 0) {
                    $('.attention-people').scrollFixed({
                        top: 55,
                        distance: 55
                    });
                }
                $("#gotop").show();
                userTimer.begin_timer();
            });

        });
        //window.onscroll = ReturnToTop;
</script>
