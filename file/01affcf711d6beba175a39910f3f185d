  <script type="text/x-handlebars-template" id="toolbar-history">
    <div class="toolbar-panels-header">
        <h2>
            <span class="toolbar-panels-close" onclick="toolbar_history_close()">&times;</span>
            <img src="http://img.easeeyes.com/common/toolBar/9.png" alt=""> 我的足迹
        </h2>
    </div>
    <div class="toolbar-panels-main">
        <div class="toolbar-history-wrapper">
            <div class="toolbar-history-items clearfix">
                {{#each items}}
                <div class="toolbar-history-item">
                    <div class="toolbar-history-item-thumb">
                        <a href="{{link}}" title="{{title}}" target="_blank">
                            <img src="{{thumb}}" width="60" height="60" alt="{{title}}"/>
                        </a>        
                    </div>
                    <div class="toolbar-history-item-title">
                        <a href="{{link}}" title="{{title}}" target="_blank">{{title}}</a>
                    </div>
                    <div class="toolbar-history-item-price">{{price}}</div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    <div class="toolbar-panels-footer">
        <button class="toolbar-history-clear" onclick="clear_history_menu()">清空记录</button>
    </div>
</script> 
  <script type="text/tpl" id="toolbar-cart">
    <div class="toolbar-panels-header"></div>
    <div class="toolbar-panels-main"></div>
    <div class="toolbar-panels-footer"></div>
</script> 
  <script src="http://file.easeeyes.com/js/handlebars.min.js"></script> 
  <script src="http://file.easeeyes.com/js/blur.js"></script> 
  <script>
// panels渲染
function renderPanels(itemId,data){
    var source=$("#"+itemId).html()    
    var template=Handlebars.compile(source);
    var result=template(data)
    $('#toolbar-panels').html(result)
}
// 签到
function qiandao(){
    $.ajax({
      type: 'POST',
      url: 'exchange.php?act=sign' ,
      data:'user_id=&m='+Math.random(),
      success:function(data){
        var $btn=$('.toolbar-member-btn');
        if($btn.attr('disabled')==true) return false;
        $btn.addClass('disabled').attr('disabled',true).text('今日已签到');
        $('#qdjf').show().animate({'top':'-20px','opacity':'0'},500,function(){
            $(this).hide();
        })
      }
    });
}
// 清空记录
function clear_history_menu(){
    if(confirm('确认删除？')){
        $.ajax({
          type: 'GET',
          url: 'user.php?act=clear_history',
          success:function(data){
            $(".toolbar-history-item-thumb").empty();
            $(".toolbar-history-wrapper").text('还没有浏览记录');
          }
        });
    }    
}
// 返回顶部
function toTop(){
    $('body,html').animate({'scrollTop':'0'},50)
}
// 开关panels
var toolbarHistoryBtn=document.getElementById('toolbarHistory')
var panelState=0
function panelSwitch(direct,cb){
    var $panels=$('.toolbar')
    switch(direct){
        case 'on':
            $panels.addClass('toolbar-open');
            panelState=1
            break;
        case 'off':
            $panels.removeClass('toolbar-open');
            panelState=0
            break;
        case 'toggle':
            if(!panelState){
                $panels.addClass('toolbar-open');
                panelState=1
            }else{
                $panels.removeClass('toolbar-open')
                panelState=0
            }
    }
    cb && cb(panelState)
}
// 点击历史记录处理函数
function toolbar_history_handle(){
    $.ajax({
        url:'/ajax_history.php?r=Math.random()',
        type:'get',
        dataType:'json',
        beforeSend:function(){
            $('.toolbar-history-wrapper').html('加载中...')
        },
        success:function(res){
            renderPanels('toolbar-history',res)
        }
    })   
    panelSwitch('toggle',function(state){
        state ? toolbarHistoryBtn.style.backgroundColor='#2bbdd5' : toolbarHistoryBtn.style.backgroundColor=''
    }) // 开关
}
// 点击购物车处理函数
function toolbar_cart_handle(){
    renderPanels('toolbar-cart') // 渲染
    panelSwitch('toggle') // 开关
}
// 关闭按钮
function toolbar_history_close(){
    panelSwitch('off',function(state){
        toolbarHistoryBtn.style.backgroundColor=''
    })
}
$(function(){
    // 点击document关闭panels
    blur($('.toolbar').get(0),function(){
        toolbar_history_close()
    },[$('.toolbar-wrapper').get(0)])
    // 导航hover状态
    $('.toolbar-tab').hover(function(){
        if(!$(this).hasClass('unhover')){
            $(this).toggleClass('hover')
        }
    })
})
</script> 
