            a.removeClass("on");
            $(this).addClass("on");

            $("#lineInfo,#feeInfo,#visaInfo,#noticeInfo,#travelTips,#routeInfo").css("display","none");
            $("#CommentContent").css("display","block");
        });

    })
</script>  
 </body>
</html>