         <script type="application/javascript">
							$(function(){
								$.getJSON("/navmenu/find_all_by_page",{
									pageSize:100,
									pageIndex:1,
									order:'asc',
			        				sort:'order'
								},function(data){
							    		var html = template('index_menu_list', data);
							   			document.getElementById("menuNav").innerHTML=html;
							   			var li=$("#menuNav li");							   			
							   			$("#menuNav li a i").eq(0).remove();
							   			$("#menuNav li div").eq(0).remove();
							   			for(var i = 0;i<li.length;i++){
							   				$("#menuNav li div").eq(i).find("a:last").addClass("menuorder");
							   				$("#menuNav li div").eq(i).find("a:last").append("<i>&gt;&gt;</i>");
							   			}
							   			li.mouseover(function(){
							   				$(this).addClass("hover");
							   				li.eq(0).removeClass("hover");							   									   				
							   			});							   		
							   			li.mouseout(function(){
							   				$(this).removeClass("hover");							   				
							   			});	
							   			var typeId=$("#hideTravelTypeId").val();
							   			if(typeId==1){
							   				li.eq(2).addClass("current");
							   			}
							   			if(typeId==2){
							   				li.eq(3).addClass("current");
							   			}
							   			if(typeId==3){
							   				li.eq(4).addClass("current");
							   			}
							   			if(typeId==4){
							   				li.eq(5).addClass("current");
							   			}
							   			if(typeId==5){
							   				li.eq(1).addClass("current");
							   			}
							    	});
							});							 			
						</script> 
