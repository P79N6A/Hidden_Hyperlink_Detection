      <script type="text/javascript">
                    function orderProduct(){
                    	var date2 = new Date().getTime()+3*24*60*60*1000;
                    	var date1 = new Date($("#buyday").val()).getTime();
                    	if(date1<date2){
                    		alert("该产品需要提前"+3+"天预订！请重新选择出发时间！");
                    	}else{
                    		$("#orderSubmitForm").submit();
                    	}
                    	
              //  		window.location.href="/productorderpage?productId="+id;
                    }
