		var service = "https://aq.suning.com/asc/auth?targetUrl=http%3A%2F%2Ftry.m.suning.com%2Fpending%2F1000103015_110013864464%2Flist.htm";
		    service = service.replaceAll('&amp;','&');
		    
		var getRequestParam = function(){
    	   var url = location.search; //获取url中"?"符后的字串   
    	   var theRequest = new Object();   
    	   if (url.indexOf("?") != -1) {   
    	      var str = url.substr(1);   
    	      strs = str.split("&");   
    	      for(var i = 0; i < strs.length; i ++) {   
    	         theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);   
    	      }   
    	   }   
    	   return theRequest;   
    	};
		
		// 去注册
		var goToReg = function() {
			var Request = new Object();
		    Request = getRequestParam();
		    var userTargetUrl = "?loginTheme=wap_new&service=https%3A%2F%2Faq.suning.com%2Fasc%2Fauth%3FtargetUrl%3Dhttp%253A%252F%252Ftry.m.suning.com%252Fpending%252F1000103015_110013864464%252Flist.htm".split("targetUrl%3D")[1];
		    var custSource = Request['custSource'];
		    var freeToReg_URL = srs_prefix_domain + "wap/person.do";
		    if(userTargetUrl){
		    	freeToReg_URL  = freeToReg_URL + "?myTargetUrl="+decodeURIComponent(userTargetUrl);
		    	if(custSource){
		    		freeToReg_URL = freeToReg_URL + "&custSource=" + custSource;
		    	}
		    }else{
		    	if(custSource){
		    		freeToReg_URL = freeToReg_URL + "?custSource=" + custSource;
		    	}
		    }
		    window.location.href = freeToReg_URL;
		}
	    
		// 找回密码
		var goForgetPwd = function() {
			var _targetUrl_ = "?loginTheme=wap_new&service=https%3A%2F%2Faq.suning.com%2Fasc%2Fauth%3FtargetUrl%3Dhttp%253A%252F%252Ftry.m.suning.com%252Fpending%252F1000103015_110013864464%252Flist.htm".split("targetUrl%3D");
			if(_targetUrl_[1]){
				window.location.href = asc_prefix_domain + "wap/forgetpsw/show_0.do?nextTargetUrl="+_targetUrl_[1];
			}else{
				window.location.href = asc_prefix_domain + "wap/forgetpsw/show_0.do";
			}	
		}
		// qq登陆
		var qqLogin = function(){
			var qq="https://graph.qq.com/oauth2.0/authorize?response_type=code&client_id=101224012&redirect_uri=https%3A%2F%2Fpassport.suning.com%2Fids%2Flogin%3Foauth_provider%3DWapQQProvider%26loginTheme%3Dwap_new&state=https%3A%2F%2Faq.suning.com%2Fasc%2Fauth%3FtargetUrl%3Dhttp%253A%252F%252Ftry.m.suning.com%252Fpending%252F1000103015_110013864464%252Flist.htm";
			
			//var remVal = $(".auto-login .sn-checkbox").prop("checked");
			//if(remVal) {
				qq = qq.substring(0, qq.indexOf("&state=")) + encodeURIComponent("&rememberMe=true") + "&state=state";
