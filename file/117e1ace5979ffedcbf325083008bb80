           $('a[data-src]').css('position','relative').on('mouseenter',function(e){
			var t = $(this);
			if(t.find('img').length==0){
				t.append('<img src="'+t.data('src')+'" style="display:block;width:auto;position:absolute;top:20px;left:-286px;" id="img-hover"></img>')
			}
		}).on('mouseleave',function(){
			$('#img-hover').remove()
		})
	</script>
    

