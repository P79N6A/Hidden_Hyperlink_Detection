   <script type="text/javascript">
$(function(){
        $(window).on("scroll",function(){
        $(".hj-col-lg-4").height($("#area2_body").height());
        var scrollHgt = $(this).scrollTop();
        var headerOft =document.getElementById("nav").offsetTop;
       var footerOft = $(document).height() - $(".h-footer").height()+50;
        var menuOft = $(".mleft-menu").height();
        //alert($(".mleft-menu").height());
        var offsetLeft = parseFloat(($(document).width() - $("#container").width())/2); 
        if(scrollHgt <=  $(".main-left").offset().top) {
           $(".mleft-menu").removeClass("menu-relative").removeClass("menu-fixed");
        }
        if(scrollHgt >= $(".main-left").offset().top && scrollHgt < footerOft){
            $(".mleft-menu").removeClass("menu-relative").addClass("menu-fixed").css({"left":offsetLeft+10,"top":56});
        }
        if(scrollHgt >= $(".h-footer").offset().top-850){
             $(".mleft-menu").removeClass("menu-fixed").addClass('menu-relative').css({"left":0,"top":($(".hj-col-lg-4").height() - menuOft)});
            }
    });
    })
</script> 
