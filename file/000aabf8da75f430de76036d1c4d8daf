    <script type="text/javascript">
        /////模似下拉列表
    $('#sea-con')
    .on('mouseover','.select',function(){$(this).addClass('hover');})
    .on('mouseout','.select',function() {$(this).removeClass('hover');})
    .on('click','em',function(){
                   var $select=$(this.parentNode.parentNode);
                   $select.find('.txt1')[0].innerHTML=this.innerHTML;
                   $select.find('input')[0].value=$(this).attr('rel');
                   $(this).addClass('act').siblings().removeClass('act');
    })
    var $txts=$('#sea-con').find('.txt'),$tabtit = $('#tabbar1').find('span');
    $('#tabbar1').on('click','span',function(e) {
        $tabtit.removeClass('act');
        $(this).addClass('act');
        $('#sea-con').find('.sea-form').css('display', 'none').eq($tabtit.index(e.target)).css('display', 'block');
        $txts.blur();
        return false;
    })
</script> 
