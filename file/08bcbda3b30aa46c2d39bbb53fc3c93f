  <script>
function topsearchsubmit()
	{
		var keyword=document.getElementById("key_words").value;
	
		if(keyword=="" || keyword=="请填写查找内容")
		{
		}
		else
		{
			var ckey=gb2312(keyword)
			window.open("http://search.sanhaostreet.com/search_price.php?slt=price&keywords="+ckey+"");
		}
	}
	function gb2312(key)
	{
	
	var r = "";
	for(var i=0;i<key.length;i++)
	{
			var t = key.charCodeAt(i);
			if(t>=0x4e00 || t==0x300A || t==0x300B)
			{
					try
					{
					execScript("ascCode=hex(asc(\""+key.charAt(i)+"\"))", "vbscript"); r += ascCode.replace(/(.{2})/g, "%$1"); }
						catch(e)
						 {}
				 }
				else{r += escape(key.charAt(i))}
		   }
	return r;
	}
</script>
