  <script type="text/javascript">
function succeedhandle_followmod(url, msg, values) {
var fObj = jq('followmod_'+values['fuid']);
if(values['type'] == 'add') {
fObj.innerHTML = '不收听';
fObj.href = 'home.php?mod=spacecp&ac=follow&op=del&fuid='+values['fuid'];
} else if(values['type'] == 'del') {
fObj.innerHTML = '收听TA';
fObj.href = 'home.php?mod=spacecp&ac=follow&op=add&hash=865b12ee&fuid='+values['fuid'];
}
}
</script> 
  <script src="http://icon.virtualevent.net/js/jquery.min.js" type="text/javascript" language="JavaScript"></script> 
  <script src="http://icon.virtualevent.net/js/score.js" type="text/javascript"></script> 
  <script>
var jq=$.noConflict();
jq(function(){
jq(".submitcase").click(function(){
jq(".viewcase").css("display","");
});

var score = new Score({
callback: function(cfg) {
cfg.obj.parent().removeClass('star');
jq.ajax({
type: 'post',
url : '/files/score_ajax.php',
dataType : 'text',
data : {score:cfg.starAmount,pid:cfg.pid},
success: function(results){
var checkobj = jQuery.parseJSON(results);
if(checkobj.code=='10000'){
jq('.avgrat_'+cfg.pid).find('li').removeClass('on');
jq('.avgrat_'+cfg.pid).find('li').each(function(i,avgrating){
if(parseInt(jq(avgrating).text())<=parseInt(checkobj.message)){
jq(avgrating).addClass('on');
}
});
jq('.csc-avg-rating-'+cfg.pid).text(parseInt(checkobj.message));
jq('.csc-num-vote-'+cfg.pid).text(parseInt(jq('.csc-num-vote-'+cfg.pid).text())+1);
if(parseInt(checkobj.daynum)>=10){
jq('.star').removeClass('star').addClass('numstar');
}
}
},
error: function(code, message){
return false;
}
});
return false;
}
});
jq(document).on('mouseover','.numstar', function() {
if(jq(this).find(".qu_hiy").hasClass('hidden')){
jq(this).find(".qu_hiy").find("p").text("本日评价已完成").parent().removeClass('hidden').delay(3000).hide(0);
}else{
jq(this).find(".qu_hiy").find("p").text("本日评价已完成").parent().show().delay(3000).hide(0);
}
});


jq(document).on('mouseover','.ownstar ul li',function(){
if(jq(this).parent().siblings(".qu_hiy").find("p").parent().hasClass('hidden')){
jq(this).parent().siblings(".qu_hiy").find("p").text("不能自我评价").parent().removeClass('hidden').delay(3000).hide(0);
}else{
jq(this).parent().siblings(".qu_hiy").find("p").text("不能自我评价").parent().show().delay(3000).hide(0);
}
});

jq(document).on('mouseover','.nostar ul li',function(){
if(jq(this).parent().siblings(".qu_hiy").find("p").parent().hasClass('hidden')){
jq(this).parent().siblings(".qu_hiy").find("p").text("登录后才能评分").parent().removeClass('hidden').delay(3000).hide(0);
}else{
jq(this).parent().siblings(".qu_hiy").find("p").text("登录后才能评分").parent().show().delay(3000).hide(0);
}
});
})
</script> 
