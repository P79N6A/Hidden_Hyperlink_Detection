		function buyDown(root,pid){
			//加入跳转到新服务器
			var iframe = document.createElement("iframe");
			iframe.name="iframebydown";
			iframe.style.display="none";
		    var login_url="/buy/buyDown.do?pid="+pid;
			iframe.src = login_url;
			if (iframe.attachEvent)
				{
					 iframe.attachEvent("onload", function(){
						document.body.removeChild(iframe);
					});
				}else{
						iframe.onload = function(){
						document.body.removeChild(iframe);
					};
				}
			document.body.appendChild(iframe);
		}
		
		function showPic(path){
			window.showModalDialog("/newpage/jsp/showBigPic.jsp?path="+path,"", "dialogWidth:800px; dialogHeight:700px;state:no;help:no;location:no;scroll:true;");
		}
				
