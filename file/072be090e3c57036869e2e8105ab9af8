		function getpos(element) {
		    if (arguments.length != 1 || element == null) {
		      return null;
		    }
		    var elmt = element;
		    var offsetTop = elmt.offsetTop;
		    var offsetLeft = elmt.offsetLeft;
		    var offsetWidth = elmt.offsetWidth;
		    var offsetHeight = elmt.offsetHeight;
		    while (elmt = elmt.offsetParent) {
		      if (elmt.style.position == 'absolute' || (elmt.style.overflow != 'visible' && elmt.style.overflow != '')) {
		        break;
		      }
		      offsetTop += elmt.offsetTop;
		      offsetLeft += elmt.offsetLeft;
		    }
		    return {
		      top: offsetTop,
		      left: offsetLeft,
		      right: offsetWidth + offsetLeft,
		      bottom: offsetHeight + offsetTop
		    };
		  }
		$(document).ready(function () {
			$('#bigImage') .mousemove(function (event) {
                KeleyiImgNext(document.getElementById('bigImage'),event);
            });
			$('#bigImage') .mouseover(function (event) {
                KeleyiImgNext(document.getElementById('bigImage'),event);
            });
        });
		 function KeleyiImgNext(bigimg,event) {
			 var width = bigimg.width;
	            var height = bigimg.height;
	            var photopos = getpos(document.getElementById("bigImage"));
	                if((parseInt(event.clientX) - photopos.left) <width/2){
	                	document.getElementById('bigImage').style.cursor="url(/include/img/btn_left.cur),auto";
	                	document.getElementById('img_a').onclick=function(){
	                		goPrePic();
				    	}
	                }else{
	                	document.getElementById('bigImage').style.cursor="url(/include/img/btn_right.cur),auto";
	                	document.getElementById('img_a').onclick=function(){
	                		goNextPic();
				    	}
	                }
		 }
		
		//上一张图片
		function goPrePic(){
			document.getElementById('bigImage').style.cursor="url(/include/img/btn_left.cur),auto";
			var isPre=false;
			var preid=0;
			for(var i=0;i<pic.length;i++){
				if(i==0&&pic[i]==cpic){
					isPre=true;
					break;
				}else if(pic[i]==cpic){
					preid=pic[i-1];
					break;
				};
			}
			if(isPre){
				goPrePage();
			}else{
				goPic(preid);
			};
		}
		
		//下一张图片
		function goNextPic(){
			document.getElementById('bigImage').style.cursor="url(/include/img/btn_right.cur),auto";
			var isNext=false;
			var nextid=0;
			for(var i=0,len=pic.length;i<len;i++){
				if(i==(len-1)&&pic[i]==cpic){
					isNext=true;
					break;
				}else if(pic[i]==cpic){
					nextid=pic[i+1];
					break;
				};
			}
			
			if(isNext){
				goNextPage();
			}else{
				goPic(nextid);
			};
		}
		
		function goPic(pid){
			document.location.href="/channelplay/6200/5522741/"+pid+"/0.html";
		}
		
		var startIndex='0';
		
		
		function viewOp(pid,dctype){
			$.ajax({ 
				url:"/photo/viewPic.do",
				data:{
					"id":pid,
					"dctype":dctype
				},
				dataType:"json",
				type:"post",
				success:function(data){
					if(data.success){
						if(dctype==1){
							$("#stepup").html(data.message);
						}else{
							$("#stepdown").html(data.message);
						}
						//_this.value=data.message;
					}else{
						alert(data.message);
					}		
				},
				error:function(xmlReq, textStatus, errorThrown){
					alert("服务器繁忙！请稍后尝试..");
				}
		 	});
		}
		
		function goDiv(div) {
		 var move = $("#"+div).offset().top;
		 $("html,body").animate('{scrollTop:move}', 1); 
		}
		
		
		
