		<script type="text/javascript">
		function login(){
			//跳转到登陆页面
			window.location.href="/newpage/jsp/login.jsp";
		}		
			
			/**http://gm.gmw.cn/home/pic/gmw_right_3.gif
			document.onkeypress=function(){
				var e = event.srcElement;
				var r =e.createTextRange();
				r.moveStart("character",e.value.length);
				r.collapse(true);
				if(r.select() && getEvent().keyCode==13){
					login();
				}
			};
			***/
			
			function getEvent(){
				if (document.all) {
					return window.event;
				}
				func = getEvent.caller;
				while (func != null) {
					var arg0 = func.arguments[0];
					if (arg0) {
						if ((arg0.constructor == Event || arg0.constructor == MouseEvent) || (typeof (arg0) == "object" && arg0.preventDefault && arg0.stopPropagation)) {
							return arg0;
						}
					}
					func = func.caller;
				}
				return null;
			}
			
			
			//$(document).ready(function(){
			//	$("#account").val(getCookie('Example_auth_username'));
			//});
			
			function getpassword(){
			
						//window, "dialogWidth:1200px;dialogHeight:700px;status:no;help:no;location:no;scroll:true;resizable:no }");
				window.location.href="/newpage/jsp/picgetpassword.jsp";	
			}
			
		</script>
