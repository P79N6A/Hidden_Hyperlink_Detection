    <script>
var comiis_page = 1;
var comiis_ispage = 0;
function comiis_list_page(){
comiis_ispage = 1;
comiis_page++;
if(comiis_page <= 2){
$('.comiis_multi_box').html('<div class="comiis_loadbtn f_d">正在加载...</div>');
$.ajax({
type:'GET',
url: 'forum.php?mod=viewthread&tid=7185943&page=' + comiis_page + '&inajax=1' ,
dataType:'xml',
}).success(function(s) {		
if(typeof(s.lastChild.firstChild.nodeValue) != "undefined"){
$('.comiis_postlist').append(s.lastChild.firstChild.nodeValue);
if(comiis_page >= 2){
$('.comiis_multi_box').html('<div class="comiis_loadbtn f_d">亲，已经到底了！</div>');
}else{
$('.comiis_multi_box').html('<a href="javascript:;" onclick="comiis_list_page()" class="comiis_loadbtn bg_e f_d">点击加载更多</a>');
}
popup.init();
}else{
comiis_page--;
$('.comiis_multi_box').html('<a href="javascript:;" onclick="comiis_list_page()" class="comiis_loadbtn bg_e f_d">重新加载</a>');
}
comiis_ispage = 0;
}).error(function() {
comiis_page--;
$('.comiis_multi_box').html('<a href="javascript:;" onclick="comiis_list_page()" class="comiis_loadbtn bg_e f_d">重新加载</a>');
comiis_ispage = 0;
});
}
}
var comiis_page_timer;
$(window).scroll(function(){
clearTimeout(comiis_page_timer);
comiis_page_timer = setTimeout(function() {
var comiis_scroll_bottom = $(window).scrollTop() + $(window).height();
var comiis_list_bottom = $('.comiis_postlist').height() + $('.comiis_postlist').offset().top - 200;
if(comiis_scroll_bottom >= comiis_list_bottom && comiis_ispage == 0){
comiis_list_page();
}	
}, 200);
});
</script> 
