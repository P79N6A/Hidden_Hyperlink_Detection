  <script>

// 导航下拉菜单
(function(){
	var nav_ul = $('.main-nav-list'),
		oLi = nav_ul.find('li'),
		select_list = $('.select_list'),
		timer = null;

		oLi.hover(
			function(){
				$(this).addClass('active');
			},
			function(){
				$(this).removeClass('active');

			}
		)
		
})()

//设置PD位置校正函数
function pb_top(){
	$("#pb").css("width",$(window).width()+"px");
	$("#pb").css("height",$(window).height()+"px");
}

//设置#fd_big位置校正函数
function fd_big_top(){
	var fd_big_left=($(window).width()-$("#fd_big").css("width").substr(0,3))*0.5;
	var fd_big_top=($(window).height()-$("#fd_big").css("height").substr(0,3))*0.382;
	$("#fd_big").css("left",fd_big_left+"px");
	$("#fd_big").css("top",fd_big_top+"px");
}

$(document).ready(function(){
	pb_top();
	fd_big_top();
	//显示留学意向框
	if(intent=='yes'){
		// $("#pb").show();
		// $("#fd_big").show();	
	}
})

$(window).resize(function(){
	pb_top();
	fd_big_top();
})

$(".intent_pre_step").click(function(){
	$(this).hide();
	$(".intent_next_step").show();
	$(".intent_table_2").hide();
	$(".intent_stop_over").hide();
	$(".intent_table_1").show();
	
})

$(".intent_next_step").click(function(){

	var mobilePhone = /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$/;

		itt_phone = $('.itt_phone');

		if(!itt_phone.val()){

			alert('请填写手机号！');

			return false;

		}else {

			if(!mobilePhone.test(itt_phone.val())) {

				alert('请填写正确的手机号码');

				return false;

			}

		}

	$(this).hide();
	$(".intent_pre_step").show();
	$(".intent_table_1").hide();
	$(".intent_table_2").show();
	$(".intent_stop_over").show();
	
})

//获取majors
$.ajax({
	type:'get',
	url:'/uk/schoolsearch/get_intent_majors',
	cache:false,
	data:{uid:$(".uid").val(),password:$(".password").val()},
	datatype:'json',
	timeout:'3000',
	success:function(data){
		var cbk=$.parseJSON(data);
		
		for(i=0;i<cbk.length;i++){
			$(".itt_want_major").append('<option value="'+cbk[i]['id']+'">'+cbk[i]['major']+'</option>');
		}
	},
	error:function(XMLHttpRequest,textStatus,errorThrown){
		//
	}
});

$(".itt_want_major").change(function(){
	//获取majors方向
	$.ajax({
		type:'get',
		url:'/uk/schoolsearch/get_intent_majors_sons',
		cache:false,
		data:{id:$(".itt_want_major").val()},
		datatype:'json',
		timeout:'3000',
		success:function(data){
			var cbk=eval('('+data+')');
			$(".itt_want_major_sons").find("option").remove();
			for(i=0;i<cbk.length;i++){
				$(".itt_want_major_sons").append('<option value="'+cbk[i]['id']+'">'+cbk[i]['major']+'</option>');
			}
		},
		error:function(XMLHttpRequest,textStatus,errorThrown){
			//
		}
	});
})

//提交留学意向intent
$(".intent_stop_over").click(function(){
	var itt_now_school = $('.itt_now_school'),
		itt_now_major = $('.itt_now_major'),
		itt_mark = $('.itt_mark'),
		itt_want_major_sons = $('.itt_want_major_sons');

		if(!(itt_now_school.val()&&itt_now_major.val()&&itt_mark.val()&&itt_want_major_sons.val())) {

			alert('请填写完整意向');

			return false;

		}

	//获取majors方向
	$.ajax({
		type:'post',
		url:'/students/schoolsearch/save_intent',
		cache:false,
		data:{
			itt_go_country:$(".itt_go_country").val(),
			itt_year:$(".itt_year").val(),
			itt_month:$(".itt_month").val(),
			itt_own_country:$(".itt_own_country").val(),
			itt_age:$(".itt_age").val(),
			itt_phone:$(".itt_phone").val(),
			itt_now_school:$(".itt_now_school").val(),
			itt_now_major:$(".itt_now_major").val(),
			itt_language:$(".itt_language").val(),
			itt_mark:$(".itt_mark").val(),
			itt_want_major:$(".itt_want_major option:selected").text(),
			itt_want_major_sons:$(".itt_want_major_sons option:selected").text(),
			
		},
		datatype:'text',
		timeout:'3000',
		success:function(data){
			alert(data);
			//隐藏浮动层
			$("#pb").hide();
			$("#fd_big").hide();
		},
		error:function(XMLHttpRequest,textStatus,errorThrown){
			//
		}
	});
})
</script> 
