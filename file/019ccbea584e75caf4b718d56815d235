  <script>
var intent=null;//留学意向弹窗标识符
var uname=decodeURI(getCookie('user_name'));//获取USERNAME
var groupid=decodeURI(getCookie('groupid'));//获取GROUPID
var user_id=decodeURI(getCookie('uid'));//获取GROUPID

if(groupid==5){//如果不是普通学员则不弹窗
	//alert(groupid);
	$.ajax({
		type:'get',
		url:"/user/check_user/"+user_id,
		datatype:'json',
		success:function(data){
			var data =$.parseJSON(data);
			//alert(data.intention);
			if(data.intention=='false'){
				$("#pb").show();
				$("#fd_big").show();
			}
		},
		error:function(a,b,c){
			intent='yes';
		}
	})
}
</script> 
