                    type: "GET",
                    url: "/Login/SendAuthMobileSMS",
                    data: { "mobile": $("#mobileAuthNum").val() },
                    dataType: "json",
                    success: function (data) {
                        if (data.StatusCode == "true") {
                            alert(data.StatusMessage);
                            //获得手机验证码
                            var oMobile = document.getElementById("mobileAuthNum");
                            var oCode = document.getElementById("mobileAuthCode");
                            oCode.removeAttribute("disabled");
                            oCode.value = "";
                            oCode.focus();
                            time(me, oMobile, oCode);
                        } else {
                            alert(data.StatusMessage);
                        }
                    },
                    error: function () {
                        alert("系统出错了");
                    }
                });
            });
            //7天未登录认证处理
            $("#mobileAuthLogin").click(function () {
                if ($("#mobileAuthCode").val() == "") {
                    alert("请输入手机收到的验证码");
                }
                var sitename, username, password, loginVcode;
                if ($("#logintype").val() == "user") {
                    username = document.getElementById("username").value;
                    password = document.getElementById("userpass").value;
                } else {
                    sitename = document.getElementById("sitename").value;
                    username = document.getElementById("siteusername").value;
                    password = document.getElementById("siteuserpass").value;
                }
                password = getPassword(username + password);
                username = getPassword(username);
                loginVcode = document.getElementById("loginVcode").value;
                $.ajax({
                    type: "POST",
                    url: "/Login/MobileAuth",
                    data: { "SiteName": sitename, "UserName": username, "Password": password, "LoginVcode": loginVcode, "SystemCode": 'YDGL', "returnUrl": '', "mobile": $("#mobileAuthNum").val(), "vcode": $("#mobileAuthCode").val() },
                    dataType: "json",
                    success: function (result) {
                        if (result.StatusCode == "true") {
                            //认证信息成功，跳转页面
                            //window.location.href = "/System";
                            window.location.href = result.Data.LoginUrl;
                        } else if (result.StatusCode == "needAuth") {
                            alert("您的账号未经过认证，请联系网点管理员完善你的个人资料，如姓名、唯一用户名、手机号码，QQ号码等联系信息。");
                        }
                        else {
                            alert("验证出错:" + result.StatusMessage);
                        }
                    },
                    error: function () {
                        alert("系统出错了");
                    }
                });
            });

