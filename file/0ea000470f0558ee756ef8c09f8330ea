        var wait = 180;
        //定时器
        function time(o, oMobile, oCode) {
            if (wait == 0) {
                o.removeAttribute("disabled");
                o.value = "免费获取验证码";
                wait = 180;
                oMobile.removeAttribute("disabled");
                //oMobile.focus();
                //oCode.setAttribute("disabled", true);
            } else {
                o.setAttribute("disabled", true);
                oMobile.setAttribute("disabled", true);
                o.value = "重新发送(" + wait + ")";
                wait--;
                setTimeout(function () {
                    time(o, oMobile, oCode);
                },
                    1000);
            }
        };
        //获取唯一用户名的验证码
        function timeNick(o) {
            if (wait == 0) {
                o.removeAttribute("disabled");
                o.value = "获取唯一用户名";
                wait = 180;
            } else {
                o.setAttribute("disabled", true);
                o.value = "重新获取发送(" + wait + ")";
                wait--;
                setTimeout(function () {
                    timeNick(o);
                },
                    1000);
            }
        };
