            $("#mobileNick").click(function () {
                var me = this;
                if ($("#mobileNum").val() == "") {
                    alert("请输入手机号码");
                    $("#mobileNum").focus();
                    return;
                }
                if ($("#vcode").val() == "") {
                    alert("请输入验证码");
                    return;
                }
                $.ajax({
                    type: "POST",
                    url: "/Login/GetNickName?radom=" + Math.floor(Math.random() * 100),
                    data: { "mobile": $("#mobileNum").val(), "vcode": $("#vcode").val() },
                    dataType: "json",
                    success: function (data) {
                        if (data.StatusCode == "true") {
                            //发送成功
                            timeNick(me);
                        } else {
                            alert(data.StatusMessage);
                        }
                    },
                    error: function () {
                        alert("系统出错了");
                    }
                });
            });

            //发送
            function sendMobileSMS(mobile, me) {
                //提交短信发送请求
                $.ajax({
                    type: "GET",
                    url: "/Login/SendMobileSMS",
                    data: { "mobile": mobile },
                    dataType: "json",
                    success: function (data) {
                        if (data.StatusCode == "true") {
                            //获得手机验证码
                            alert(data.StatusMessage);
                            var oMobile = document.getElementById("authMobile");
                            var oCode = document.getElementById("authVerificationCode");
                            oCode.removeAttribute("disabled");
                            oCode.value = "";
                            oCode.focus();
                            time(me, oMobile, oCode);
                        } else {
                            alert(data.StatusMessage);
                        }
                    },
                    error: function () {
                        alert("系统出错了,请联系客服。");
                    }
                });
            };

            //修改密码发送验证码
            $("#getMobileVcode").click(function () {
                var me = this;
                var oldPassword = $("#oldPassword").val();
                if (!oldPassword) {
                    alert("请输入原密码");
                    return;
                }
                var newPassword = $("#newPassword").val();
                if (!newPassword) {
                    alert("请输入新密码");
                    return;
                }
                var mobile = $("#mobile").val();
                if (!mobile || !/1[3-8]+\d{9}/.test(mobile)) {
                    alert("请输入正确的手机号码");
                    return;
                }
                sendMobileSMS(mobile, me);
            });

            //发送验证码
            $("#sendAuthCodde").click(function () {
                var me = this;
                var authMobile = document.getElementById("authMobile").value;
                if (!authMobile || !/1[3-8]+\d{9}/.test(authMobile)) {
                    alert("请输入正确的手机号码");
                    return;
                }
                sendMobileSMS(authMobile, me);
            });

            //确认修改密码
            $(".sureUpPwd").click(function () {
                var oldPassword = document.getElementById("oldPassword").value;
                var newPassword = document.getElementById("newPassword").value;
                var reNewPassword = document.getElementById("reNewPassword").value;
                var mobile = document.getElementById("mobile").value;
                var mobileVcode = document.getElementById("mobileVcode").value;
                var errorMsg;
                var focusId;
                if (!oldPassword || oldPassword == '请输入原密码') {
                    errorMsg = "请输入原密码";
                    focusId = "oldPassword";
                }
                if (!newPassword || newPassword == '请输入新密码') {
                    errorMsg = "请输入新密码";
                    focusId = "newPassword";
                }
                if (!reNewPassword || reNewPassword == '请输入新密码') {
                    errorMsg = "请输入新密码";
                    focusId = "reNewPassword";
                }
                if (reNewPassword != newPassword) {
                    errorMsg = "确认密码与新密码应一致";
                    focusId = "reNewPassword";
                }
                if (!mobile || mobile == '请输入手机号码') {
                    errorMsg = "请输入手机号码";
                    focusId = "mobile";
                }
                if (!mobileVcode || mobileVcode == '请输入手机获取的验证码') {
                    errorMsg = "请输入手机获取的验证码";
                    focusId = "mobileVcode";
                }
                if (errorMsg) {
                    alert(errorMsg);
                    document.getElementById(focusId).focus();
                } else {
                    $.ajax({
                        type: "POST",
                        url: "/Login/ChangePassword",
                        data: { "oldPassword": oldPassword, "newPassword": newPassword, 'mobile': mobile, 'mobileVcode': mobileVcode },
                        dataType: "json",
                        success: function (data) {
                            if (data.StatusCode == "ChangePasswordOK") {
                                //密码修改成功，跳转页面
                                window.location.href = "/System";
                            } else {
                                alert(data.StatusMessage);
                            }
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            //alert("系统出错了");
                            alert("系统出错了:"+XMLHttpRequest.status + "," + XMLHttpRequest.readyState + "," + textStatus);
                        }
                    });
                }
            });
            //认证操作
            $("#sureAuth").click(function () {
                //认证处理 输入验证
                var authRealName = document.getElementById("authRealName").value;
                var authUserName = document.getElementById("authUserName").value;
                var authNickName = document.getElementById("authNickName").value;
                var authIdCard = document.getElementById("authIdCard").value;
                var authQQ = document.getElementById("authQQ").value;
                var authTel = document.getElementById("authTel").value;
                var authMobile = document.getElementById("authMobile").value;
                var authVerificationCode = document.getElementById("authVerificationCode").value;
                var errorMsg;
                var focusId;
                if (!authRealName || authRealName.length < 2) {
                    errorMsg = "真实姓名不能为空，至少2个汉字吧!";
                    focusId = "authRealName";
                } else if (!authUserName || authUserName.length < 3) {
                    errorMsg = "用户名不能为空!长度不能小于3";
                    focusId = "authUserName";
                } else if (!authNickName || authUserName.length < 3) {
                    errorMsg = "唯一用户名不能为空!长度不能小于3";
                    focusId = "authNickName";
                } else if (!authIdCard || !checkIdCard(authIdCard)) {
                    errorMsg = "请填写真实身份证号码!";
                    focusId = "authIdCard";
                } else if (!authQQ || !(/^[0-9]{5,10}$/.test(authQQ))) {
                    errorMsg = "QQ号码不能为空，请填入正确的QQ号码!";
                    focusId = "authQQ";
                } else if (!authMobile || !/1[3-8]+\d{9}/.test(authMobile)) {
                    errorMsg = "手机号码不能为空!";
                    focusId = "authMobile";
                } else if (!authVerificationCode) {
                    errorMsg = "手机验证码不能为空!";
                    focusId = "authVerificationCode";
                }
                if (errorMsg) {
                    alert(errorMsg);
                    document.getElementById(focusId).focus();
                } else {
                    $.ajax({
                        type: "POST",
                        url: "/Login/Auth",
                        data: { "authModel.RealName": $("#authRealName").val(), "authModel.UserName": $("#authUserName").val(), "authModel.NickName": $("#authNickName").val(), "authModel.IDCard": $("#authIdCard").val(), "authModel.QQ": $("#authQQ").val(), "authModel.Tel": $("#authTel").val(), "authModel.mobile": $("#authMobile").val(), "authModel.VerificationCode": $("#authVerificationCode").val() },
                        dataType: "json",
                        success: function (data) {
                            if (data.StatusCode == "true") {
                                //认证信息成功，跳转页面
                                alert(data.StatusMessage);
                                window.location.href = "/System";
                            } else {
                                alert(data.StatusMessage);
                            }
                        },
                        error: function () {
                            alert("系统出错了");
                        }
                    });
                }
            });
            //7天未登录验证
            $("#authMobileCode").click(function () {
                var me = this;
                var mobileAuthNum = document.getElementById("mobileAuthNum").value;
                if (!mobileAuthNum || !/1[3-8]+\d{9}/.test(mobileAuthNum)) {
                    alert("请输入正确的手机号码");
