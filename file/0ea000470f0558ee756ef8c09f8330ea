        //忘记密码
        function ForgetPassword() {
            if ($("#Email").val() == "") {
                $(".phoneway").css("display", "block");
                $(".mailway").css("display", "none");
                $("#duanxin").html("<span>验证中，请稍等......</span>");
            } else {
                $(".mailway").css("display", "block");
                $(".phoneway").css("display", "none");
                $("#youjian").html("<span>验证中，请稍等......</span>");
            }
            $(".findway").css("display", "none");
            $.post("/ForgetPassword/ForgetPassword",
                { "SiteName": $("#ForgetSiteName").val(), "UserName": $("#ForgetUserName").val(), "Phone": $("#Phone").val(), "Email": $("#Email").val() },
                function (result) {
                    if (result == null) {
                        $(".mailway").css("display", "none");
                        $(".phoneway").css("display", "none");
                        $(".errorway").css("display", "block");
                        $("#error").html("出错啦！");
                        return false;
                    }
                    if (result.StatusCode == "true" || result.StatusCode == true) {
                        if ($("#Email").val() == "") {
                            $(".phoneway").css("display", "block");
                            $(".mailway").css("display", "none");
                            $("#duanxin").html(result.StatusMessage);
                        } else {
                            $(".mailway").css("display", "block");
                            $(".phoneway").css("display", "none");
                            $("#youjian").html(result.StatusMessage);
                        }
                    }
                    else {
                        $(".mailway").css("display", "none");
                        $(".phoneway").css("display", "none");
                        $(".errorway").css("display", "block");
                        $("#error").html(result.StatusMessage);
                    }
                }, "json");
        }
        var bNeedloginVcode = true;
        function getPassword(password) {
            setMaxDigits(129);
            var key = new RSAKeyPair("010001", "", "A890DFA9EA78DE929353E886E9546B1C6106DF91E2B338FAF9C91D32196CE4A89D7B9EDEB883938D184F56BE4815CD63A1602C13486239AF2582EB6343834473334204F43F418511C50CA385DFC31C42E6ACFBA588897257CC02ABAA2EA27BC39A65AF325C699CD5B0A8A91E55C702382BAFB09BD09560C0DC8AC8A44ADC9F2F");
            return encryptedString(key, password);
        }
        //登录
        function login() {
            var errorMsg = "";
            var sitename, username, password, returnUrl, loginVcode;
            //登录成功跳转到的页面 暂时不考虑这么做
            returnUrl = document.getElementById("ReturnUrl").value;
            if ($("#logintype").val() == "user") {
                username = document.getElementById("username").value;
                password = document.getElementById("userpass").value;
                loginVcode = document.getElementById("loginVcode").value;
                if (username == '请输入唯一用户名') {
                    username = "";
                }
                if (password == '请输入密码') {
                    password = "";
                }
                if (loginVcode == '请输入验证码') {
                    loginVcode = "";
                }

                if (!username) {
                    errorMsg += "用户名不能为空!";
                }
                if (!password) {
                    errorMsg += "密码不能为空!";
                }
                if (bNeedloginVcode && !loginVcode) {
                    errorMsg += "验证码不能为空!";
                }

                if (errorMsg != "") {
                    $("#user_info").html(errorMsg);
                    $("#user_info").show();
                    return false;
                } else {
                    $("#user_info").show();
                    $("#user_info").html("正在登录中...");
                }
            } else {
                sitename = document.getElementById("sitename").value;
                username = document.getElementById("siteusername").value;
                password = document.getElementById("siteuserpass").value;
                if (sitename == "请输入站点名称") {
                    sitename = "";
                }
                if (username == "请输入账号") {
                    username = "";
                }
                if (password == "请输入密码") {
                    password = "";
                }
                if (!sitename) {
                    errorMsg += "站点不能为空!";
                }
                if (!username) {
                    errorMsg += "账号不能为空!";
                }
                if (!password) {
                    errorMsg += "密码不能为空!";
                }
                if (errorMsg != "") {
                    $("#site_info").html(errorMsg);
                    $("#site_info").show();
                    return false;
                } else {
                    $("#site_info").show();
                    $("#site_info").html("正在登录中...");
                }
            }
            //登录处理
            //setMaxDigits(129);
            //var key = new RSAKeyPair("010001", "", "A890DFA9EA78DE929353E886E9546B1C6106DF91E2B338FAF9C91D32196CE4A89D7B9EDEB883938D184F56BE4815CD63A1602C13486239AF2582EB6343834473334204F43F418511C50CA385DFC31C42E6ACFBA588897257CC02ABAA2EA27BC39A65AF325C699CD5B0A8A91E55C702382BAFB09BD09560C0DC8AC8A44ADC9F2F");
            //password = encryptedString(key, password);
            password = getPassword(username + password);
            username = getPassword(username);
            //var xt = new Xxtea(key);
            //password = xt.xxtea_encrypt(password);
            $.post("/Login/Login",
                { "SiteName": sitename, "UserName": username, "Password": password, "LoginVcode": loginVcode, "SystemCode": 'YDGL', "returnUrl": '' },
                function (result) {
                    if (result == null) {
                        if ($("#logintype").val() == "user") {
                            $("#user_info").html("登陆失败!");
                        } else {
                            $("#site_info").html("登陆失败!");
                        }
                        return false;
                    }
                    if (result.StatusCode == "true" || result.StatusCode == true) {
                        if ($("#logintype").val() == "user") {
                            $("#user_info").html("登录成功，正在转到主页...");
                        } else {
                            $("#site_info").html("登录成功，正在转到主页...");
                        }
                        window.location.href = result.Data.LoginUrl;
                        //window.location = result.LoginUrl ;
                    } else if (result.StatusCode == "needUpdatePassWord") {
                        //弹出修改密码窗口
                        var jsonResult = eval("(" + result.StatusMessage + ")");
                        alert(jsonResult.Msg);
                        $("#xgmmsjts").html("修改密码：手机提示:" + jsonResult.Mobile + "");
                        $(".bgbox").css("display", "block");
                        $(".bg").show();
                        $(".upPwdShow").show();
                    }
                    else if (result.StatusCode == "needNickLogin") {
                        //需要使用唯一用户名登录
                        var jsonResult = eval("(" + result.StatusMessage + ")");
                        alert("您已认证，请使用唯一用户名登录。如忘记唯一用户名，请进入中天或K8系统查看或通过手机" + jsonResult.Mobile + "找回唯一用户名。");
                        $("#nickLogin").click();
                    }
                    else if (result.StatusCode == "needAuth") {
                        alert("您的账号未经过认证，请联系网点管理员完善你的个人资料，如姓名、唯一用户名、手机号码，QQ号码等联系信息。");
                        //弹出认证窗口
                        //$("#user_info").html("账号未认证，请认证");
                        //$(".bgbox").css("display", "block");
                        //$(".bg").show();
                        //$(".authShow").show();
                        ////设置部分输入框的值
                        //var jsonResult = eval("(" + result.StatusMessage + ")");
                        //$("#authRealName").val(jsonResult.RealName);
                        ////if (jsonResult.RealName) {
                        ////    $("#authRealName").attr("readonly", "readonly").attr("title", "不允许修改").css("background", "wheat");
                        ////}
                        //$("#authUserName").val(jsonResult.UserName);
                        //if (jsonResult.UserName && jsonResult.UserName.length > 2) {
                        //    $("#authUserName").attr("readonly", "readonly").attr("title", "不允许修改").css("background", "wheat");
                        //}
                        //if (jsonResult.NickName && jsonResult.NickName.length > 2) {
                        //    $("#authNickName").val(jsonResult.NickName).attr("readonly", "readonly").attr("title", "不允许修改").css("background", "wheat");
                        //}
                        //$("#authIdCard").val(jsonResult.IDCard);
                        //$("#authQQ").val(jsonResult.QQ);
                        //$("#authTel").val(jsonResult.Tel);
                        //$("#authMobile").val(jsonResult.Mobile);
                    } else if (result.StatusCode == "needMobileAuth") {
                        //需要获取手机验证码进行登录
                        var jsonResult = eval("(" + result.StatusMessage + ")");
                        $("#tishiMobile").html(jsonResult.Mobile);
                        alert("超七天未登录，需要通过手机" + jsonResult.Mobile + "验证。");
                        $("#mobileAuthValidate").html("超七天未登录手机验证");
                        $(".bgbox").css("display", "block");
                        $(".bg").show();
                        $(".mobileAuthShow").show();
                        $("#checkcodeA").click();
                    } else if (result.StatusCode == "needSolveException") {
                        //登录异常需要手机验证
                        var jsonResult = eval("(" + result.StatusMessage + ")");
                        alert("系统判断您的账号有异常登录，请通过手机" + jsonResult.Mobile + "认证解除异常信息。");
                        $("#tishiMobile").html(jsonResult.Mobile);
                        $("#mobileAuthValidate").html("异常登录手机验证");
                        $(".bgbox").css("display", "block");
                        $(".bg").show();
                        $(".mobileAuthShow").show();
                        $("#checkcodeA").click();
                    } else if (result.StatusCode == "passwordError") {
                        //密码错误显示验证码
                        $("#vcodeLogin").show();
                        bNeedloginVcode = true;
                        $("#checkcodeA").click();
                        $("#user_info").html(result.StatusMessage);
                    } else if (result.StatusCode == "passwordErrorLeaveNum") {
                        //弹出提示尝试密码剩下多少次
                        alert(result.StatusMessage);
                        $("#user_info").html(result.StatusMessage);
                        $("#vcodeLogin").show();
                        bNeedloginVcode = true;
                        $("#checkcodeA").click();
                    } else if (result.StatusCode === "vcodeError") {
                        //验证码不正确
                        $("#vcodeLogin").show();
                        $("#checkcodeA").click();
                        $("#user_info").html(result.StatusMessage);
                        alert(result.StatusMessage);
                    } else if (result.StatusCode == "pageInvalid") {
                        alert(result.StatusMessage);
                        window.location.href = "/";
                    } else if (result.StatusCode == "loginException") {
                        alert("登录异常：" + result.StatusMessage + "请点击确定重新登录。");
                        window.location.href = "/";
                    }
                    else {
                        if ($("#logintype").val() == "user") {
                            $("#user_info").html(result.StatusMessage);
                        } else {
                            $("#site_info").html(result.StatusMessage);
                        }
                        $("#vcodeLogin").show();
                        bNeedloginVcode = true;
                        $("#checkcodeA").click();
                    }
                }, "json");
        };
    </script>
