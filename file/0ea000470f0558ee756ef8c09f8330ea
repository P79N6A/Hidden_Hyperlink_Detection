        // 2016-05-13 增加了默认值 return null 改为 return "PORTAL"
        function GetQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return "PORTAL";
        }

        var systemcode = GetQueryString("SystemCode");
        if (systemcode.toLowerCase() != "gk") {
            var sUserAgent = navigator.userAgent,
            mobileAgents = ['Windows CE', 'iPod', 'Symbian', 'iPhone', 'BlackBerry', 'Android', 'Windows Phone'];
            if ((sUserAgent.match(/iPad/i))) {
                location.href = '/Login/MobileIndex?systemcode=' + systemcode;
            }
            for (var i = 0, len = mobileAgents.length; i < len; i++) {
                if (sUserAgent.indexOf(mobileAgents[i]) !== -1) {
                    location.href = '/Login/MobileIndex?systemcode=' + systemcode;
                    break;
                }
            }
        }
