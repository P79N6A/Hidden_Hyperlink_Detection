function chk_form(){
	if ($F('name').replace(/ /g, "").length==0){
		alert("请输入用户名！"); return false;
	}
	if ($F('password').replace(/ /g, "").length==0){
		alert("请输入密码！"); return false;
	}
    var key_reg = /^\d{6}$/;
    if(!$F('key')==""&&!key_reg.test($F('key'))){
        alert("动态令牌输入错误，应为6位数字");
        return false;
    }
    if(!$F('leader_key')==""&&!key_reg.test($F('leader_key'))){
        alert("主管动态令牌密码输入错误，应为6位数字");
        return false;
    }
}
$('name').focus();

var show=false
function showleader(){
  if (show){
    document.getElementById('userkey').style.display="block"
    document.getElementById('leaderdiv').style.display="none";
    document.getElementById('showkey').innerHTML ="忘带令牌?";
    document.getElementById('notice').style.display="none";

    show=false;
  } else{
    document.getElementById('userkey').style.display="none"
    document.getElementById('leaderdiv').style.display="block";
    document.getElementById('showkey').innerHTML ="用自己令牌?";
    document.getElementById('notice').style.display="block";
    show=true;
  }
}
