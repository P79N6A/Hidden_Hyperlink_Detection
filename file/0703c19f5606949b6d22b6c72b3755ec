<script type="text/javascript">
  (function(){
    var d = document.body||document.documentElement
    var top_box = document.getElementById('rcc_top_box')
    var is_ie = navigator.userAgent.toLowerCase().indexOf('msie')!=-1

    if(document.addEventListener){
      window.addEventListener('scroll',toggle_rcc_top_box,false)
    }else{
      window.attachEvent('onscroll',toggle_rcc_top_box)
    }

    function toggle_rcc_top_box(){
      if(d.scrollTop > 100){
        top_box.style.visibility = 'visible'
        if(is_ie){
          top_box.style.top = (d.scrollTop + d.clientHeight/2)+'px'
        }
      }else{
        top_box.style.visibility = 'hidden'
      }
    }

  })()
</script>

