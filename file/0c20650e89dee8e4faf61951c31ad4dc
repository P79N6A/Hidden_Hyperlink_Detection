  <script>
    $('.bd_new .list').eq(0).show().siblings().hide();
    $('.bd_switch').each(function(){
        $('.bd_switch > ul >li').hover(function(){
            $(this).addClass('active').siblings().removeClass('active');
            $('.bd_new .list').eq($(this).index()).show().siblings().hide();
        }).eq(0).hover();
    });

</script> 
