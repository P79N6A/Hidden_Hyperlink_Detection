  <script>
        $(document).ready(function(){
           $(document).scroll(function(){
               var hei=$(".index_right").height();
//               var hei2=$(".bd_last").height();
//               var hei3=$(".bd").height();
//               var hei4=hei*1-hei2*1-hei3*1;
               if($(document).scrollTop()>=hei&&$(document).scrollTop()<hei*1+200){
                $("#right_rd").removeClass("rr2");
                $("#right_rd").addClass("rr");
               }else if($(document).scrollTop()>=hei*1+200){
                $("#right_rd").removeClass("rr");
                     $("#right_rd").addClass("rr2");
               }else{
                   $("#right_rd").removeClass("rr");
               }
               
           })
        })
    </script>
