  <script>
    $(document).bind('scroll',function(){
        var len=$(this).scrollTop()
        if(len>=500){
            //显示回到顶部按钮
            $("#gotop").fadeIn();
        }else{
            //隐藏回到顶部按钮
            $("#gotop").fadeOut();

        }
    })
    $('#gotop a').click(function(){
        //动画效果，平滑滚动回到顶部
        alert('test');
        $(document).animate({ scrollTop: 0 });
        //不需要动画则直接回到顶部
        //$(document).scrollTop(0)
    })
</script> 
