  jq(".text-num-last").hide(300);

});

});

function AnnounceScroll(obj){

jq(obj).find("ul:first").animate({

marginTop:"-30px"

},500,function(){

jq(this).css({marginTop:"0px"}).find("li:first").appendTo(this);

});

}



</script> 
