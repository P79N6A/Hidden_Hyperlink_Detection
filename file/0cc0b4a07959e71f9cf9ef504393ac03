function iframeCallback(){
	updateHits();
	//加载百度VIP验证时的跨域回调函数
	try {
		if(typeof(eval(window.iframeCallback2))=="function")  {
			window.iframeCallback2();
		}
	}catch(e){
		//alert("not function"); 
	}
}
function updateHits(){
	
	var ajaxUrl= _proUrl + "/index/vod/updateViewHit/id/"+ _playId+"/pid/"+700+"/dramaId/"+7462778+ '?' + new Date().getTime();
	$.ajax({
			type:'GET',
			async:false,
			url:ajaxUrl,
			dataType:"jsonp",
			jsonp:"jsoncallback",
			success:function(data){
				$('#play_vod_hits i').html(data);
				if($('#album_chits').length > 0){
					$('#album_chits').html(data);
				}
			}
		});
}
</script> 
