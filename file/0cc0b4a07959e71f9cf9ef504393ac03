		});
		$(".bg1 .share_btn").click(function(){
			if($("#p1").css("display")=="block"){
				$(".share_xl").css({"display":"block"});
				$("#p1").css({"display":"none"});
			}
		});
	   $("#p_close").click(function(){
		    $("#p_shadow").css('display','none');
		    $("#p_popbox").fadeOut('fast');
	   });
	});
	
	function ShwoWx(){
		$("#p_shadow").css('display','block');
		$("#p_popbox").fadeIn('slow');
	}
</script> 
