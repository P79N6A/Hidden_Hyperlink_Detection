//===============================点击收藏部分开始，分享===================================//
function getCookierl(name){
	var cookieName  = encodeURIComponent(name) + "=",
		cookieStart = document.cookie.indexOf(cookieName),
		cookieValue = null;
	if(cookieStart>-1){
		var cookieEnd = document.cookie.indexOf(";",cookieStart);
		if(cookieEnd == -1){
			cookieEnd = document.cookie.length;
		}
		cookieValue = decodeURIComponent(document.cookie.substring(cookieStart + cookieName.length,cookieEnd));
	}
	return cookieValue;
}

function setCookierl(name,value,expires,path,domain,secure) {
	var exdate=new Date()
	exdate.setDate(exdate.getDate()+expires)
	  var cookieText = encodeURIComponent(name) + "=" + encodeURIComponent(value);
	    cookieText += ";expires=" + exdate.toGMTString();
	  if(path){
	    cookieText += ";path=" + path;
	  }
	  if(domain){
	    cookieText += ";domain=" + domain;
	  }
	  if(secure){
	    cookieText += ";secure";
	  }
	  document.cookie = cookieText;
	}

var uid=getCookierl('_discuz_uid');

var bool='';
var al_col_btn=true;
$(document).keydown(function(event){
	//当按下esc的时候将遮罩和弹出框都进行隐藏'
	// if(event.keyCode ==27&&$('#weixin_bg').display != 'none'){
	if(event.keyCode ==27){
		if($('#weixin_bg').css('display')!=='none') {
			win_close();
		}
		img_comment.hiddenbox();
	}
});

function win_close(){
	$('#news_pop').hide("");
	$('#weixin_bg').hide("");
}
//查找用户是否收藏

var col_btn=true;

function col_check_ajax(uid){
	$.ajax({
			url:'http://my.xcar.com.cn/interface/fav_article/fav_article_cms_jsonp.php',
			dataType:"jsonp",
			jsonp:"jsonpcallback",
			data:{
				status:1,
				uid:uid,
				article_id:1935380			},
			success:function(data){
				if(data.num){

                    $("#collection_bottom").html("已收藏");
                    $("#collection_bottom").addClass("collect_allready_txt");
                    $("#collection_bottom").unbind('click').css('cursor','default');//是否取消绑定
					bool='had';
				}else{

					$("#collection_bottom").html("收藏本文");
					bool='no';
				};

				if(!col_btn){
					col_ajax(bool);
				};

			},
			error:function(data){
				return true;
			}
		});
}
//改变鼠标移入小方块的提示语言
if(uid==undefined || uid=='' || uid==null){
    $("#collection_bottom").html('收藏本文');
}else{
	col_check_ajax(uid);
}

//进行用户收藏
function col_ajax(bol){

	if(bol=='had'){
		$("#collection_bottom").unbind('click').css('cursor','default');//是否取消绑定
		$("#attentionHover").css('display','none');
		$('#news_pop').css('display','inline');
		$('#weixin_bg').css('display','inline');
		$('.repeat').css('display','inline');
		$('.once').css('display','none');
       		hot_art = new btnRoll({
			root : 'hot_art_root',
			easing : "easeOutCirc",
			preCallBefore : function(c){
				change_art(c);
			},
			nextCallBefore : function(c){
				change_art(c);
			},
			goToCallBefore : function(c){
				change_art(c);
			}
		});
	}else if(bol=='no'){
        $("#collection_bottom").html("已收藏").addClass("collect_allready_txt");
        $("#collection_bottom").css('cursor','default');

		bool='had';
        $("#attentionHover").css('display','none');
		$('#news_pop').css('display','inline');
		$('#weixin_bg').css('display','inline');
		$('.repeat').css('display','none');
		$('.once').css('display','inline');
		hot_art = new btnRoll({
			root : 'hot_art_root',
			easing : "easeOutCirc",
			preCallBefore : function(c){
				change_art(c);
			},
			nextCallBefore : function(c){
				change_art(c);
			},
			goToCallBefore : function(c){
				change_art(c);
			}
		});

		var links=$("#newsbody p a");
		var img=null;
		for(var i=0;i<links.length;i++){
			if(links.eq(i).attr("href").indexOf("&aid=")>0){
				img=links.eq(i).find("img").attr("src");
				break;
			}
		}

	 	if(img){
			var res=img.split("/");
			res.push('s_'+res.pop().split("_").pop());
			var pic=res.join("/");
		}else{
			pic='http://icon.xcar.com.cn/news/images/xcarimg.jpg';
		}
	 	var jsonp_data={
			uid:uid,
			username:encodeURI(' '),
			channel_id:1,
			stype:31,
			pstype:3,
			coulumn_id:'',
			article_id:1935380,
			channel_name: encodeURI('yp'),
			coulumn_name: encodeURI(''),							//栏目名
			title:encodeURI('起亚斗气别公交 驾驶员反被公交压断腿'),
			pic_url:pic,
			arcurl:'http://yp.xcar.com.cn/201605/news_1935380_1.html',
			description:encodeURI('近日，青州市，一辆公交车与一辆白色起亚发生碰撞。本以为是一场在普通不过的追尾事故，但后方的公交车似乎并未停下，依然连续多次撞击白色起亚。'),
			author:encodeURI('李.响'),
			pub_time:'1463673600',
			collection_time:1491074253 ,
			source:encodeURI('文章资讯')
		}

	 	var jsonpurlstr = 'http://my.xcar.com.cn/interface/fav_article/fav_article_cms_jsonp.php?';
	 	for(var jsonp_data_i in jsonp_data){
	 		jsonpurlstr += "&" + jsonp_data_i + "=" + jsonp_data[jsonp_data_i];
	 	}
	 	jsonp({
		 	url:jsonpurlstr,
	 		callback:function(data){
	 			bool='had';
		 	},err:function(data){

		 	}});
	}

}

