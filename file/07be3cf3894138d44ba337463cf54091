	});
}



//====================换一换
var ch_one_ch_num=0;
$('.watching .ch_one_ch').click(function(){
	ch_one_ch_num++;
	if(ch_one_ch_num>=$('.watching ul').length){
		ch_one_ch_num=0;
	}
	$('.watching ul').hide();
	$('.watching ul').eq(ch_one_ch_num).show();
})
