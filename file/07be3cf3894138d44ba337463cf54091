<script type="text/javascript">
//==================赞和城市报价,经销商
function praise_callback(data)
{
	data=eval(data);

	if (data.code==1)
	{
		clicklog('50','点赞');
        $("#praise_bottom").addClass('point_praise_hover');

        $("#praise_bottom").css('cursor','default');
        $("#praise_bottom").unbind("click");

        $("#addone_float,#addone_bottom").fadeIn('fast').delay(1000).fadeOut('slow');
        $("i[id^='praise_num_']").html(data.nums);

		if(data.num>999)
		{
			setTimeout(function(){
				$("#praise_bottom").html('已赞 <i>'+data.nums+'+</i>');
			},510)
		}else{
			setTimeout(function(){
				$("#praise_bottom").html('已赞 <i>'+data.nums+'</i>');
			},510)
		}			
	}else{
		if(getCookie('praise_1935380')){
    		if(data.nums>999)
    		{			
    			$("#praise_bottom").html('已赞 <i>'+data.nums+'+</i>');			           
    		}else{		
    			$("#praise_bottom").html('已赞 <i>'+data.nums+'</i>');						
    		}
    	}else{
    		if(data.nums>999)
    		{			
    			$("#praise_bottom").html('点赞 <i>'+data.nums+'+</i><span style="display:none;" id="addone_bottom" >+1</span>');			           
    		}else{		
    			$("#praise_bottom").html('点赞 <i>'+data.nums+'</i><span style="display:none;" id="addone_bottom" >+1</span>');						
    		}
		}
	}
}

