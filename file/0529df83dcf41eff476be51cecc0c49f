  <script>
seajs.use(["jquery","aid","scripts/profiles/index"],function(jQ,Aid){
	jQ.validator.addMethod("mobile", function(value, element, param) {
        var reg = /(^(0|86|17951)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$)/;
        return reg.test(value);
    });
    jQ(document).delegate('#regMemberForm [name]',"focus",function(){
		$('#'+jQ(this).data("target")+'_tr_info .info').show()
	})
	jQ(document).delegate('#regMemberForm [name]',"blur",function(){
		$('#'+jQ(this).data("target")+'_tr_info .info').hide()
	})
	var isValid = true;
	jQ('#checkLookMore').click(function(){
		$($('#jmodal-content').html()).dialog({
			title: "人和网-全球最大中文商务社交网络",
		    width: 550,
		    modal: true,
		    dialogClass:'jmodal-dialog',
		    close: function( event, ui ) {
		    	jQ(this).remove();
		    },
		    create:function( event, ui ){
		    	jQ('#address_tr').cxSelect({ 
				  url: Aid.address,
				  selects: ['country','province', 'city'], 
				  nodata: 'none',
				  firstTitle:"-请选择地区-",
				  firstValue:""
				});
				jQ('#industry_tr').cxSelect({
				  url: Aid.industry,
				  selects: ['primary', 'secondary'], 
				  nodata: 'none',
				  firstTitle:"-请选择行业-",
				  firstValue:""
				});
				jQ('#regMemberForm').validate({
					rules:{
						email:{
							required: true,
							email:true
						},
						name:{
							required: true,
						},
						password:{
							required: true,
							maxlength:16,
							minlength:6
						},
						password2:{
							equalTo:"#password"
						},
						prov:{
							required: true
						},
						city:{
							required: true
						},
						industry_Parents:{
							required: true
						},
						industry:{
							required: true
						},
						company:{
							required: true
						},
						title:{
							required: true
						},
						mobile:{
							required:true,
							mobile:true
						},
						tongyi:{
							required: true
						}
					},
					messages:{

						email:{
							required: "请输入邮箱",
							email:"请输入有效邮箱"
						},
						name:{
							required: '真实姓名是开通账号的首要条件',
						},
						password:{
							required: "密码不能为空",
							maxlength:"密码由6-16位字符组成",
							minlength:"密码由6-16位字符组成"
						},
						password2:{
							required: "请输入",
							equalTo:"确认密码不一致，请重新输入"
						},
						country:{
							required: "请选择国家"
						},
						prov:{
							required: "请选择省份"
						},
						city:{
							required: "请选择城市"
						},
						industry_Parents:{
							required: "请选择行业"
						},
						industry:{
							required:"请选择行业"
						},
						company:{
							required: '请填写您当前的公司全名'
						},
						title:{
							required: '请填写您目前的职务'
						},
						mobile:{
							required:'手机号码为11位数字',
							mobile:'手机号码为11位数字'
						},
						tongyi:{
							required: '是否同意人和网服务条款'
						}
					},
					errorPlacement: function(error, element) {
						jQ('#'+element.data("target")+'_tr_info .error').html(error)
					},
					onkeyup: false,
					focusCleanup:true,
					submitHandler:function(form) {
						if(!isValid) return;
						form.submit();
					},
					//onsubmit:false,
					success:function(label,element){
						if(element.name=="email"){
							jQ.ajax({
								url:'/ajax/testemail.html?e='+element.value,
								success:function(res){
    								if(jQ.trim(res)*1=="1"){
    									isValid = false;
    									jQ('#email_tr_info .error').html('<label id="email-error" class="error" for="email" style="display: inline;">该Email已经存在</label>');
    								}else{
										isValid = true;
									}
								}
							})
						}else{
							isValid = true;
						}
					}
				})
		    },
		    open:function( event, ui ) {

		    }
		})
	})
});
</script> 
