  <script type="text/javascript">
    $('#esnews').addClass('act');
    $.getScript(CMS_ABS + uri2MVC("ajax=yuedu_xinqing&aid=166763&datatype=js&varname=xinqing"),function() {
        $('#xinqing').html(xinqing);
    })
        // 心情
    function zixun_dianping(aid,ziduan) {
        $.getScript(CMS_ABS + 'etools/zvdp.php?action=zvdp&js=1&aid=' + aid + '&ziduan=' + ziduan,
        function() {
            if (face == 0) {
                $.modal.tip('您已经点评过了。','error');
            }else {
                dpnum = parseInt($('#' + ziduan).html().match(/\d+/)) + 1;
                $('#' + ziduan).html('(' + dpnum + ')');
                $('#'+aid+'_'+ziduan).addClass('on');
                $.modal.tip('点评成功！','succeed');
            }
        });
        return false;
    }

</script> 
