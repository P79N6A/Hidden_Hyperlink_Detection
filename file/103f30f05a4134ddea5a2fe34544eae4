  <script type="text/javascript"> 
function delu(id,ids){
	$(id).hover(function(){
			$(this).find(ids).slideDown('300');
	},function(){
			$(this).find(ids).slideUp('300');
	});
}

jQuery(function($){
	$('#big').click(function(){zoomDoc(1);}); 
	$('#small').click(function(){zoomDoc(-1);});
	//搜索 
	$('#wd').focus( 
		function () { 
			$(this).removeClass("blur"); 
			$("#button").addClass("blur"); 
		} 
		); 
		$('#wd').blur( 
		function () { 
			if($(this).val()==''){ 
			$(this).addClass("blur"); 
		}else{ 
			$(this).removeClass("blur"); 
		} 
			$("#button").removeClass("blur"); 
		} 
	); 

//滑动块 
	boxs('#m-bbs',0,"mousemove"); 
	boxs('#m-djph',0,"mousemove"); 
	delu('#weixin_s',".weixin_img"); 
	tabs('.bt_recommend .recommend_span','on','.recommend');
	$(window).scroll(function(){
		var htmlTop = $(document).scrollTop();
		if( htmlTop > 0){
			$("#gotop").css('display','block');	
		}else{
			$("#gotop").css('display','none');	
		}
	});
})
</script> 
