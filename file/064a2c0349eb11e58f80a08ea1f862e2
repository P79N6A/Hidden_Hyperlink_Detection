                    $("#qwe").click(function () {
                        if ($("#qwe .btn").attr("id") == "traffic") {
                            alert("你已经喜欢过了！")
                            return;
                        }
                        var id=$("#qwe").attr("value");
                        var traffic=0+1;
                        $.ajax({
                            url: "/news/ChangeLove",
                            dataType: "html",
                            type: "POST",
                            data: "id=" + id,
                            success: function (data) {
                                if (data == "ok") {
                                    $("#qwe").html("<span class='btn' id='traffic'>已喜欢(" + traffic + ")</span>");
                                }
                                else {
                                    alert("点评失败");
                                }
                            }
                        });
                    });
                   
