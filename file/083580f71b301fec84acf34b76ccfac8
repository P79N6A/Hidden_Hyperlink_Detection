  <script type="text/javascript">function open_booking_box(){$("#booking_line").show(200);}function close_booking_box(){$("#booking_line").hide(200);}function CheckMial(){  var mail = $("#mail").val();  var re_mail = $("#re_mail").val();  if(mail == "Email"){  alert("请输入您的邮箱！");  return false;  }else{  var reg =  /^[_a-zA-Z\d\-\.]+@[_a-zA-Z\d\-]+(\.[_a-zA-Z\d\-]+)+$/;  if(!reg.test(mail)){  alert("请您填写正确的邮箱地址！");  return false;  }else{  return true;  }  }}function Check(){  var mail = $("#mail").val();  var re_mail = $("#re_mail").val();  if(mail == "Email"){  alert("请输入您的邮箱！");  return false;  }else{  var reg =  /^[_a-zA-Z\d\-\.]+@[_a-zA-Z\d\-]+(\.[_a-zA-Z\d\-]+)+$/;  if(!reg.test(mail)){  alert("请您填写正确的邮箱地址！");  return false;  }  else if(!reg.test(re_mail)){alert("请您填写正确的重复邮箱地址！");return false;  }  else{var services = $(".service_check");var flag = false;for(var i=0;i<services.length;i++){if(services[i].checked){flag = true;}}if(flag){return true;}else{alert("请选择您需要代办的服务！");return false;}  }  }}    </script> 
