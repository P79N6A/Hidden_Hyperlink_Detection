    window.onload = function () {

        var jump_key = Cookies.get('jump_key') || '';

        var isMobile = device.mobile();

        if(jump_key !== 'desk' && isMobile === true){
            showMobile();
        }
    };

    function  showMobile(){
        var domain = document.domain.replace('www.','');
        Cookies.set('jump_key','mobile',{ domain: domain });
        window.location.href = document.URL.replace('www.','m.');
    }

</script> 
