	//tabs切换 tab*-cont
	$("ul.name-pic").children("li").each(function(i, elem){
		$(elem).hover(function(){
		$(this).removeClass("dq").addClass("dq");
		$(this).siblings("li").removeClass("dq");
		var tabConentSelector = [".tab", i + 1, "-cont"].join("");
		$(tabConentSelector).siblings().hide();
		$(tabConentSelector).show();
		});
	});
	
