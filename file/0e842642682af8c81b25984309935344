  <script type="text/javascript" src="http://www.zedazk.com/skin/js/rollup.min.js"></script> 
  <script src="http://www.zedazk.com/skin/js/lazyload.me.js" type="text/javascript"></script> 
  <script type="text/javascript" src="http://www.zedazk.com/skin/js/jquery-1.7.2.min.js"> </script> 
  <script type="text/javascript">   
$(document).ready(function() {     
//首先将#back-to-top隐藏    
 $("#roll_top").hide();    
  //当滚动条的位置处于距顶部100像素以下时，跳转链接出现，否则消失     
  $(function() {       
  $(window).scroll(function() {         
  if ($(window).scrollTop() > 100) {           
  $("#roll_top").fadeIn(1500);         
  } else {           
  $("#roll_top").fadeOut(1500);         
  }       
  });       
  //当点击跳转链接后，回到页面顶部位置       
  $("#roll_top").click(function() {         
  $('body,html').animate({           
  scrollTop: 0         
  },         
  1000);         
  return false;       
  });     
  });   
  }); 
  </script> 
  <!--footer:end-->  
