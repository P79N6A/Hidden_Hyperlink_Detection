function _login(){
  if(xmlHttp.readyState==4 && xmlHttp.status==200) {
    var status = xmlHttp.responseText;
    if(status == '2'){
      alert('密码错误，请重新输入！');
    }else if(status == '3'){
      alert('请输入用户名！');
    }else if(status == '4'){
      alert('请输入密码！');
    }else if(status == '0'){
      //alert('登陆成功');
      window.location.reload();
    }else if(status == '1'){
      alert('累计6次错误尝试 您在6小时内不能登录系统');
    }else{
      window.location.href="http://www.dlzb.com/member/register.php"
