  <script type+"text="" avascript"="">
        //获取url中的参数
        function getUrlParam(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
            var r = window.location.search.substr(1).match(reg);  //匹配目标参数
            if (r != null) return unescape(r[2]); return null; //返回参数值
        }

        var paramAd = getUrlParam("ad");
        var paramFrom = getUrlParam("from");
        var intCommentCount = ((Math.random() * 40) + 50).toFixed(0);
        $(function() {
            $(".leftvideo").css("width", (screen.width - 86) / 2 + "px");
            $(".leftvideo").css("height", $(".leftvideo").css("width"));
            $(".leftvideo").each(function() {
                var eleWidth = $(this).width();
                var eleHeight = $(this).height();
                $(this).children('.title').css("top", eleWidth - 44);
            });

            digg();
            bury();
            clickVideo();
            NextClick();
            PrevClick();
            buttonApp();
            AdButton();
            UserHeadClick();
            UserNameClick();
            CommentUserHeadClick();
            CommentUserNameClick();
            CommentContentClick();
        });

        function AdButton() {
            if (navigator.userAgent.indexOf("MicroMessenger") >0) {
                $(".Ad .btn").html("<span>+ 关注</span>");
                $(".Ad").bind("tap click", function() {
                _czc.push(["_trackEvent", $("#cs").val(), "关注", "", 1, ""]);
                location.href = "http://mp.weixin.qq.com/s/fwfiUoHJWz5pKEkpgXLpCg";
            });
            //$("#footer").show();
            }
            else {
                $(".Ad").bind("tap click", function() {
                    _czc.push(["_trackEvent", $("#cs").val(), "下载", "", 1, ""]);
                    location.href = "http://a.app.qq.com/o/simple.jsp?pkgname=com.layangjing"
                });
                $("#footer").hide();
            }
        }


        function buttonApp() {
            $("#buttonApp").bind("click", function() {
            _czc.push(["_trackEvent", $("#cs").val(), "底部下载", "", 1, ""]);
            window.location.href = "http://a.app.qq.com/o/simple.jsp?pkgname=com.layangjing";
            });
        }

        ///顶
        function digg() {
            $(".ele_ding").bind("click", function() {
                var p = $(this).parent();
                var id = $(p).attr("data-id");
                _czc.push(["_trackEvent", $("#cs").val(), "赞", "", 1, ""]);
                if (p.attr("data-user") == "") {
                    $(this).css('background-image', 'url(data:image/png;base64,)');
                    $(this).css('color', '#ff819f');
                    $(p).attr("data-user", "digg");
                    $.get("/group/" + id + "/digg", { net: Math.random() }, function(data) {
                    });
                }
                else if (p.attr("data-user") == "digg") {
                    //alert("您已经顶过！");
                }
                else if (p.attr("data-user") == "bury") {
                    //alert("您已经踩过！");
                }
            });
        }
        //踩
        function bury() {
            $(".ele_cai").bind("click", function() {
                var p = $(this).parent();
                var id = $(p).attr("data-id");
                _czc.push(["_trackEvent", $("#cs").val(), "踩", "", 1, ""]);
                if (p.attr("data-user") == "") {
                    $(this).css('background-image', 'url(data:image/png;base64,)');
                    $(this).css('color', '#ff819f');
                    $(p).attr("data-user", "bury");
                    $.get("/group/" + id + "/bury", { net: Math.random() }, function(data) {
                    });
            }
                else if (p.attr("data-user") == "digg") {
                //alert("您已经顶过！");
                }
                else if (p.attr("data-user") == "bury") {
                //alert("您已经踩过！");
                }
            });
        }

        function clickVideo() {
            $('.leftvideo').bind("click", function() {
                var id = $(this).attr("data-id");
                for (var i = 0; i < $('.leftvideo').length; i++) {
                    if ($($('.leftvideo')[i]).data("id") == id) {
                        _czc.push(["_trackEvent", $("#cs").val(), "视频" + i,"",1, ""]);
                        break;
                    }
                }
                window.location.href = "/group/video/" + id + ".html?ad=" + paramAd + "&from=" + paramFrom;
            });
            $('.rightvideo').bind("click", function() {
                var id = $(this).attr("data-id");
                window.location.href = "/group/video/" + id + ".html?ad=" + paramAd + "&from=" + paramFrom;
            });
        }

        function PrevClick() {
            $(".buttonPrv").bind("click", function() {
                var category = $(this).attr("data-category");
                var id = $(this).attr("data-id");
                _czc.push(["_trackEvent", $("#cs").val(), "上一条", "", 1, ""]);
                if (category == "" || category == null || id == 0) window.location.href = "/duanzi.html?ad="+paramAd+"&from="+paramFrom;
                window.location.href = "/group/" + category + "/" + id + ".html?ad=" + paramAd + "&from=" + paramFrom;
            });
        };

        function NextClick() {
            $(".buttonNext").bind("click", function() {
                var category = $(this).attr("data-category");
                var id = $(this).attr("data-id");
                _czc.push(["_trackEvent", $("#cs").val(), "下一条", "", 1, ""]);
                if (category == "" || category == null || id == 0) window.location.href = "/duanzi.html?ad=" + paramAd + "&from=" + paramFrom;
                window.location.href = "/group/" + category + "/" + id + ".html?ad=" + paramAd + "&from=" + paramFrom;
            });
        };

        function UserHeadClick() {
            $(".user img").bind("click", function() {
            _czc.push(["_trackEvent", $("#cs").val(), "用户头像", "", 1, ""]);
            });
        }

        function UserNameClick() {
            $(".user span").bind("click", function() {
            _czc.push(["_trackEvent", $("#cs").val(), "用户名", "", 1, ""]);
            });
        }

        function CommentUserHeadClick() {
            $(".CommentUser img").bind("click", function() {
            _czc.push(["_trackEvent", $("#cs").val(), "评论头像", "", 1, ""]);
            });
        }

        function CommentUserNameClick() {
            $(".CommentUser span").bind("click", function() {
                _czc.push(["_trackEvent", $("#cs").val(), "评论用户名", "", 1, ""]);
            });
        }

        function CommentContentClick() {
            $(".comment_txt").bind("click", function() {
                _czc.push(["_trackEvent", $("#cs").val(), "评论内容", "", 1, ""]);
            });
        }

        var timeOutEvent = 0;
        $(function() {
            $("#footer").on({
                touchstart: function(e) {
                    timeOutEvent = setTimeout("longPress()", 600);
                    e.preventDefault();
                },
                touchmove: function() {
                    clearTimeout(timeOutEvent);
                    timeOutEvent = 0;
                },
                touchend: function() {
                    clearTimeout(timeOutEvent);
                    if (timeOutEvent != 0) {
                    }
                    return false;
                }
            })
        });


        function longPress() {
            timeOutEvent = 0;
            _czc.push(["_trackEvent", $("#cs").val(), "长按二维码", "", 1, ""]);
        }   
        
    </script> 
