  <script>
//防止文章被采集的js方法
$(function () {
	$("#zoom").mouseup(function (e) {//#zoom为功能区域
		var x = 10;
		var y = 10;
		var r = "";
		if (document.selection) {
			r = document.selection.createRange().text;
		}
		else if (window.getSelection()) {
			r = window.getSelection();
		}
		if (r!= "") {
			var bowen = " ";
			var tooltip = "<div id='tooltip' class='tooltip'><a href='javascript:;' onclick=\"openSwt('文章选中-点击咨询')\" target='_blank'>" +'<img src="/public/img/online_20.gif" />'+ bowen + "</a></div>";
			$("body").append(tooltip);
			$("#tooltip").css({
				"top": (e.pageY + y) + "px",
				"left": (e.pageX + x) + "px",
				"position": "absolute"
			}).show("fast");
		}
	}).mousedown(function () {
		$("#tooltip").remove();
	});
})
</script> 
