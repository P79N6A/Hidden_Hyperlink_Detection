                                    var fo =createPlayer("vplayer",width,height,type_video);
                                    fo.addVariable("videoId", "VIDE1470101706097312");//视频id
                                    fo.addVariable("scheduleId", "我的入党志愿书 父亲留给我们的遗产 傅平 党性教育 学习 共产党员网");//关键字
                                    fo.addVariable("filePath", "/flvxml/2009/10/14/");//txt路径
                                    fo.addVariable("sysSource", sysSource);//视频来源
                                    fo.addVariable("url", "http://xuexi.12371.cn/2016/08/02/VIDE1470101706097312.shtml");//视频url
                                    fo.addVariable("videoCenterId","d448d60de663424a96845de04dcfc031");//视频生产中心guid (必要值)
                                    fo.addVariable("isLogin", isLogin);//用户中心相关
                                    fo.addVariable("userId", userId);//用户中心相关
                                    fo.addVariable("adCall", ad_Call);//前贴片广告
                                    fo.addVariable("adPause", ad_Pause);//暂停广告
                                    fo.addVariable("adAfter", ad_After);//后贴片广告
                                    fo.addVariable("adCorner", ad_Corner);//角标广告
                                    fo.addVariable("adCurtain", ad_Curtain);//跑马灯广告
                                    writePlayer(fo,"myFlash",ad_BG,ad_Roll);
                                }
                                setTimeout(loading_video,1000);//延时1秒
                            </script>
                            <div style="display:none"><!--repaste.body.begin-->2014年12月23日，蚌埠市民傅振安向蚌埠市博物馆捐赠了父亲傅平生前一直保存的一份“入党志愿书”。傅平出生于1929年，山东莒南人，1943年就成为八路军第115师686团二营四连一名战士，从此走上革命道路。1945年，傅平光荣地加入了中国共产党。由于战争年代特殊的环境，入党志愿书都由个人保管。此那以后，无论是在炮火纷飞的战场，还是在热血沸腾的建设工地，傅平同志都小心翼翼地珍藏着这份入党志愿书，直到2001年因病去世。这份保存了70年的“入党志愿书”，见证了他对党的感情，也见证了一个时代的变迁。<!--repaste.body.end-->
