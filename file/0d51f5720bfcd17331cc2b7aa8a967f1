<script src="http://j.pengxun.cn/zhibo/livecontent/plugin/require.js" defer="" async="" data-main="http://j.pengxun.cn/zhibo/livecontent/livecommon/app/main_pc.js" type="text/javascript" charset="utf-8"></script>
<div id="geneQRcode" style="display: none;"><canvas width="128" height="128"></canvas></div>
<script>
    $(function () {
        var kaypair_livestatus={
            notbegin:'未开始',
            beginning:'直播中',
            notsignal:'主播暂时离开，稍后再来！',
            endlive:'直播已结束，稍后看回放！',
            playback:'回放中'
        }
        if('playback'!=''){
            var _key='playback';
            var _val=kaypair_livestatus[_key];
            $("#live-status").text(_val).show();
            if(_key=='notbegin'){
                var _countdown='<var class="qlDays">00</var>天 <var class="qlHours">00</var>时 <var class="qlMinutes">00</var>分 <var class="qlSeconds">00</var>秒';

                $("#videomsgtip").show().find("#videomsgtip-content").html("倒计时： "+_countdown);

                function checkTime(i) {
                    if (i < 10) {
                        i = "0" + i
                    }
                    return i
                }
                var startDate = '2017/3/7 20:18:09';
                var now = '2017/4/1 17:17:13';
                var _startTime = new Date(startDate).getTime();
                var _nowTime = new Date(now).getTime();
                if (_nowTime > _startTime) {
                    $(".qlDays").text("00");
                    $(".qlHours").text("00");
                    $(".qlMinutes").text("00");
                    $(".qlSeconds").text("00");
                }
                else {
                    var _timespan = _startTime - _nowTime;
                    var _timehand = setInterval(function () {
                        var a = _startTime - _nowTime,
                            b = parseInt(a / 864E5),
                            c = parseInt(a / 36E5 - 24 * b),
                            d = parseInt(a / 6E4 - 1440 * b - 60 * c),
                            e = parseInt(a / 1E3 - 86400 * b - 3600 * c - 60 * d);

                        $(".qlDays").text(checkTime(b));
                        $(".qlHours").text(checkTime(c));
                        $(".qlMinutes").text(checkTime(d));
                        $(".qlSeconds").text(checkTime(e));

                        _nowTime += 1000;
                        if (a <= 0) {
                            clearInterval(_timehand);
                        }

                    }, 1000)
                }

            }
            else if(_key=='notsignal'||_key=='endlive'){
                $("#videomsgtip").show().find("#videomsgtip-content").text(_val);
            }
            else{
                $("#videomsgtip").hide()
            }
        }
        if(zbvd.isOpenDm){
            $("#dmswitch").show();
            $("#div1").removeClass("close1").addClass("open1");
            $("#div2").removeClass("close2").addClass("open2");
        }
        else{
            $("#dmswitch").hide();
            $("#div1").removeClass("open1").addClass("close1");
            $("#div2").removeClass("open2").addClass("close2");
        }
        $('#liveqr').qrcode({width:128,height:128,correctLevel:0,text:window.location.href});

        $('.direct-three-left-bottom-ma').hover(function () {
            $('.direct-three-erweimasmall').show();
        });
        $('.direct-three-left-bottom-ma').mouseleave(function () {
            $('.direct-three-erweimasmall').hide();
        });
        $('.direct-one-personal').hover(function () {
            $('.direct-one-list').stop().slideDown();
        });
        $('.direct-one-personal').mouseleave(function () {
            $('.direct-one-list').stop().slideUp();
        });
        $('.direct-three-left-follow-end > img').click(function () {
            $('.direct-three-left-follow').hide();
        });
        var div2 = document.getElementById("div2");
        var div1 = document.getElementById("div1");
        div1.onclick = function () {
            div1.className = (div1.className == "close1") ? "open1" : "close1";
            div2.className = (div2.className == "close2") ? "open2" : "close2";
        };
        $('.marquee').animate({ left: "-500px" }, 10000);
        $('.img2').eq(0).hover(function () {
            $(this).attr('src', 'http://j.pengxun.cn/zhibo/livecontent/livecommon/images_pc/direct-three-04.png');
        }).mouseleave(function () {
            $(this).attr('src', 'http://j.pengxun.cn/zhibo/livecontent/livecommon/images_pc/direct-three-01.png');
        });
        $('.img2').eq(1).hover(function () {
            $(this).attr('src', 'http://j.pengxun.cn/zhibo/livecontent/livecommon/images_pc/direct-three-05.png');
        }).mouseleave(function () {
            $(this).attr('src', 'http://j.pengxun.cn/zhibo/livecontent/livecommon/images_pc/direct-three-02.png');
        });
        $('.img2').eq(2).hover(function () {
            $(this).attr('src', 'http://j.pengxun.cn/zhibo/livecontent/livecommon/images_pc/direct-three-06.png');
        }).mouseleave(function () {
            $(this).attr('src', 'http://j.pengxun.cn/zhibo/livecontent/livecommon/images_pc/direct-three-03.png');
        });
        $('.direct-two-co2').click(function () {
            $('.direct-three-left-follow').show();
        });
        //$('.direct-two-co1').click(function () {
        //    $(this).text('已关注');
        //});
        $('#tab_des .direct-four-cooperation').eq(0).click(function () {
            $(this).addClass('active').siblings('.direct-four-cooperation').removeClass('active');
            $('#tab_des .direct-four-words').eq(0).show().siblings('.direct-four-words').hide();
        });
        $('#tab_des .direct-four-cooperation').eq(1).click(function () {
            $(this).addClass('active').siblings('.direct-four-cooperation').removeClass('active');
            $('#tab_des .direct-four-words').eq(1).show().siblings('.direct-four-words').hide();
        });

        //关注/取消关注+-数字
        function followOpt(opt) {
            var prenum = $(".followers").text();
            if (!$.isNumeric(prenum)) {
                return;
            }
            var num = parseInt(prenum);
            switch (opt) {
                case "+":
                    num++;
                    break;
                case "-":
                    num--;
                    break;
            }

            $(".followers").text(num);
        }

        $(document).on("click",".direct-two-co1",function(){
            if('true'=='true'){
                window.location.href='/live/livedetail-87491?oauth=oauths&v=636266638333541246';
                return;
            }
            var cancleFlag = "";
            var amsg = "";
            if ($.trim($(this).text()) == "关注") {
                cancleFlag = "1";
                var amsg = "已关注";
            } else {
                cancleFlag = "0";
                var amsg = "关注";
            }
            var isloading = true;
            if (isloading) {
                $.ajax({
                    type: "POST",
                    url: "/liveajax/savefocus",
                    data: { cancleFlag: cancleFlag ,liveid:87491,"types":"focus",focustype:"click"},
                    success: function (data) {
                        isloading = false;
                        if (data&&data.isok) {
                            $(".direct-two-co1").html(amsg);
                            if (amsg == "关注") {
                                followOpt("-");
                                //$(".direct-two-co1").addClass("not-followed").removeClass("has-followed");
                            } else {
                                followOpt("+");
                                //$(".direct-two-co1").addClass("has-followed").removeClass("not-followed");
                            }
                            $(document).minTipsBox({
                                tipsContent: data.Msg,
                                tipsTime: 1
                            });
                        } else {
                            $(document).minTipsBox({
                                tipsContent: data.Msg,
                                tipsTime: 1
                            });
                        }
                    },
                    error: function () {
                        isloading = false;
                    }
                });
            }
        });
        $(document).on("click","#logout",function(){
            $.cookie('zbvz_userinfo',null,{domain:zbvd.cookiedomain,path:'/'});
            window.location.reload();
        });
    });
</script>



