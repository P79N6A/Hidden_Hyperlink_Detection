 function ajaxGetPage(item,num){
	$.get("/index.php/Home/Index/getIndexItems/"+'item/'+item+"/num/"+num,
     function(data){   
     	if(data.status == 1){
     		if(item == 'around'){  
     		$('#loadingIn').hide();
          	$('#item1').append(data.message);
          	wait = 0;
          }else if(item == 'inland'){
          	$('#loadingIn').hide();
          	$('#item2').append(data.message);
          	wait = 0;
          }else if(item == 'outland'){
          	$('#loadingIn').hide();
          	$('#item3').append(data.message);
          	wait = 0;
          }
     	}else if(data.status == 0){
     		$('#loadingIn').hide();
     		mui.toast('暂无更多');
     	}
          
  },"json");
	
} 
var wait = 0;
var item1count= item2count = item3count = 1;
$(window).on('scroll',function(){
	if(wait == 0){
	var btTop =  $('#page-bt1').offset().top  ; 
	var nowTop = parseInt($(window).scrollTop()+$(window).height()) ; 
	if(nowTop >= btTop){
		//检测当前显示哪部分区域
		var item1 = $('#item1').css('display');
		var item2 = $('#item2').css('display');
		var item3 = $('#item3').css('display');  
		if(item1 == 'block'){
			item1count++;
			wait = 1;
			$('#loadingIn').show(); 
			ajaxGetPage('around',item1count);
		  }else if(item2 == 'block'){
			item2count++;
			wait = 1;
			$('#loadingIn').show();
			ajaxGetPage('inland',item2count);
		  }else if(item3 == 'block'){
			item3count++;
			wait = 1;
			$('#loadingIn').show();
			ajaxGetPage('outland',item3count);
		}
	}
	}
}) 
</script>  
