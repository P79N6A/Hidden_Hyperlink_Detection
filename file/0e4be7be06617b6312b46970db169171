function cqa(){
	$("#qqalert").fadeOut("slow");
}
function sqa(){
	if($("#qqalert").css("display")!="block"){
		$("#qqalert").fadeIn("slow");
	}
	speed=10;
	for(i=0;i<3;i++){
		$("#qqalert").animate({"right":"0"},speed).animate({"bottom":"2px"},speed).animate({"right":"2px"},speed).animate({"bottom":"0"},speed);	
	}
}
