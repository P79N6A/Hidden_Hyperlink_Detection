  <script>
        function show_map(uid)
        {
            art.dialog.open("/map/tjf/"+uid, {title: '来路设置',width: 750, height: 600,lock: true});
        }
        function get_image(uid,room_id,room_num)
        {
            var room_total=4;

            for(i = 0; i <= room_total; i++){
                if($("#fx_name"+i).attr("class")=="choose"){
                    if(room_num != i){
                        $("#fx_name"+i).removeClass("choose");
                    }
                }else{
                    if(room_num == i){
                        $("#fx_name"+i).addClass("choose");
                    }
                }
            }

            var str="";
            var url = '/ajaxjson/get_image/'+uid+'/'+room_id ; //后台处理程
            var pictitle = "";
            $.getJSON(url, function(data){
                if (data.length > 0)
                {
                    for (i = 0; i < data.length; i++)
                    {
                        if(data[i].remark){
                            pictitle= data[i].remark;
                        }else{
                            pictitle="客栈图片";
                        }
                        str+="<li><a href=http://www.3dkezhan.com/uploadfiles/" + data[i].image_url + "><img src=http://www.3dkezhan.com/uploadfiles/" + data[i].thumbnail + " title=\""+pictitle+"\" width=\"90\" height=\"60\"></a></li>";
                    }
                    $(".ad-thumb-list").html(str);
                    var galleries = $('.ad-gallery').adGallery();
                    //alert(str);
                }
            });

        }

        function get_comment(kezhan_id,page)
        {
            var s=(new Date()).getSeconds();
            $.ajax({
                url: '/ajaxjson/get_comment/'+kezhan_id+'/'+page+'/'+s , //后台处理程序
                success:function(msg){
                    $('.dp-one').html(msg);
                }
            });
        }
        function get_zixun(kezhan_id,page)
        {
            var s=(new Date()).getSeconds();
            $.ajax({
                url: '/ajaxjson/get_zixun/'+kezhan_id+'/'+page+'/'+s , //后台处理程序
                success:function(msg){
                   $('.zixun').html(msg);
                }
            });
        }
        function analyseForm(form) // 分析form的元素
        {
             var params = new Array();
              for(var i = 0; i < form.elements.length; i++)
              {
                 var param = form.elements[i].name;
                 param += "=";
                 param += form.elements[i].value;
                 params.push(param);
              }
             return params.join("&");//返回的是一个数组
        }
        function add_zixun()
        {
            var content = $("#ques_content").val();
            if(content=="")
            {
                alert('提问内容内容不能为空');
                return false;
            }
            var form =document.getElementById("zixunform");
            var formBody =analyseForm(form);
            var s=(new Date()).getSeconds();
            $.ajax({
                url: '/ajaxjson/add_zixun/'+s , //后台处理程序
                type:'POST',
                data:formBody,
                success:function(msg){
                    if(msg==0){
                        window.location.href="http://www.3dkezhan.com/login";
                    } else {
                        /*var n=parseInt($('#zixunnum').html())+1;
                        $('#zixunnum').html(n);*/
                        get_zixun(46938,0,1);

                        $('#ques_content').val("");
                        alert('提问成功');
                    }
                }
            });
        }
        function GetDays()
        {
            var sdate=$("#sdate").val();
            var edate=$("#edate").val();
            var startTime = new Date(Date.parse(sdate.replace(/-/g, "/"))).getTime();
            var endTime = new Date(Date.parse(edate.replace(/-/g, "/"))).getTime();
            if(startTime>=endTime || endTime-startTime>7*24*60*60*1000)
            {
                 endTime = startTime+24*60*60*1000;
                 $("#end_date").val(getLocalTime(endTime));
            }
        }
        function getLocalTime(nS) {
              var dd=new Date(nS);
              var m=dd.getMonth()+1;
              if(m<10)
                    m="0"+m;
              var d=dd.getDate();
              if(d<10)
                    d="0"+d;
              var str= dd.getFullYear()+"-"+m+"-"+d;
              return str;

        }
        function get_jianjie() {
            art.dialog({
                width: 640,
                height: 480,
                title: '澳门富豪酒店(Hotel Beverly Plaza)客栈简介',
                content: document.getElementById('jianjie_detail'),
                id: 'art_jianjie',
                lock: true,
                background: '#000', // 背景色
                opacity: 0.87   // 透明度

            });
        }

        function get_notice() {
            art.dialog({
                width: 640,
                        height: 480,
                title: '澳门富豪酒店(Hotel Beverly Plaza)客栈公告',
                content: document.getElementById('notice_detail'),
                id: 'art_notice',
                lock: true,
                background: '#000', // 背景色
                opacity: 0.87   // 透明度

            });
        }




                setTab('one',2,2);
            </script> 
  <script type="text/javascript">
      var panorama = new BMap.Panorama('panorama'); //默认为显示导航控件
      panorama.setPosition(new BMap.Point(113.577302,22.15116));
      var panoramaService = new BMap.PanoramaService();
      panoramaService.getPanoramaByLocation(new BMap.Point(113.577302,22.15116), function(data){
            var panoramaInfo="";
            if (data == null) {
                    return;
            }else{
                $("#juanjing").show();
            }

        });
          $('.dp-txt .more').on('click', function() {
        $(this).closest('.dp-txt').addClass('on')

        $(this).hide()
        $(this).closest('.dp-txt').find('.hidemore').show()
        return false
    })
    $('.dp-txt .hidemore').on('click', function() {
        $(this).closest('.dp-txt').removeClass('on')
        $(this).closest('.dp-txt').find('.more').show()
        return false
    })
    </script> 
