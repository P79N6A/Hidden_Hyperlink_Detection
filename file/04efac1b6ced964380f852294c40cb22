        $(function() {
            $('.tp-list li').on('click', function() {
                $(this).addClass('on').siblings().removeClass('on')
                $('.tp-pic img').attr('src', $(this).find('img').attr('data-src'))
                $('.tp-attr').html($(this).find('img').attr('data-attr'))
                $('.tp-piccur em').html()
            })
            $('.kz-tpmore').on('click', function() {
                $('.tp').show();
            })
            $('.tp .close').on('click', function() {
                $('.tp').hide()
            })
            $(".showmore").on('click',function(){
                $(this).hide().siblings('.hidemore').show().siblings('.show-more').removeClass('show-more-on');
            })
            $(".hidemore").on('click',function(){
                $(this).hide().siblings('.showmore').show().siblings(".show-more").addClass('show-more-on');
            })
            get_image(46938,0,0)
        })
