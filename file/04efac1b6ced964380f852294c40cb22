  function show_info(){
      var Sheight=$('#yd_info').height();
      if(Sheight==48){
           $("#yd_info").hide();
           $("#all_info").show();
           $('#yd_info').height("auto");
           document.getElementById("show_info").innerHTML="收起详情";
          $('#all_info').height("auto");
      }else{
          $("#all_info").hide();
           $("#yd_info").show();
           document.getElementById("show_info").innerHTML="显示详情";
          $('#yd_info').height(48);

      }
  }
