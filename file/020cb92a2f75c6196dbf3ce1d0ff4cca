          <div id="ArticleCnt" zzz="2006973"> 
           <div id="zhan"></div> 
           <div id="ad">
            <!-- 广告位：pc画中画 --> 
            <script>
(function() {
    var s = "_" + Math.random().toString(36).slice(2);
    document.write('<div id="' + s + '"></div>');
