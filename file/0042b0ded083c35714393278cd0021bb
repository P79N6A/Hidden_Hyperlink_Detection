(function(storage){
	if (!storage.canUse()) return;
	var now = new Date().getTime();
	var session = storage.get('polaris:session') || { session_id: Math.random().toString(36).slice(2) + now.toString(36).slice(2) };

	var stats = pageStats(session);
	var pageType = stats.pageType();

	var initAttrs = {pagerVisit: 0, viewads: 0, l2v_num: 0, l2l_num: 0, history: [] };
	for (var name in initAttrs) {
		if (!session[name]) session[name] = initAttrs[name];
	}

	var refererPageType, refererPageFilterNum = 0, lastPageDuration, sessionDuration;
	var refererPage = stats.getRefererPage(session.history);
	if (refererPage) {
		refererPageType = refererPage.type;
		refererPageFilterNum = refererPage.filterNum;
		lastPageDuration = (now - refererPage.time)/1000;
	}
	if (session.history.length) {
		sessionDuration = (now - session.history[0].time)/1000;
	}
	var refererPageType = stats.getRefererPageType(session.history);
	var isL2V = pageType=='viewad' && refererPageType && (refererPageType.indexOf('category-')===0 || refererPageType=='query');
	if (isL2V) session.l2v_num++;
	var isL2L = pageType=='category-2' && refererPageType && refererPageType=='category-1';
	if (isL2L) session.l2l_num++;
	var filterNum = stats.filterNum();

	switch (stats.isPagerVisit()) {
		case 1:
			session.pagerVisit++;
			break;
		case -1:
			session.pagerVisit = 0;
			break;
	}
	if (session.history.length>=100) session.history.shift();
	session.history.push({url:window.location.href, type:pageType, time:now, filterNum:filterNum});
	session.viewads += +(stats.isViewad());
	var source = stats.source();
	if (source) {
		session.source = source;
		session.sourceUrl = document.referrer;
	}
	storage.set('polaris:session',session, {TTL: 20*60*1000});

	var trackData = {
		session_id: session.session_id,
		source: session.source,
		source_url: session.sourceUrl,
		has_query: stats.query() ? 1 : 0,
		contacts: storage.get('polaris:contact') || 0,
		viewads: session.viewads,
		l2v_num: session.l2v_num,
		l2l_num: session.l2l_num,
		timestamp: now / 1000
	};
	trackData.is_l2v = +isL2V;
	trackData.is_l2l = +isL2L;
	trackData.referer_filter_num = refererPageFilterNum;
	trackData.last_page_duration = lastPageDuration;
	trackData.session_duration = sessionDuration;
	if (stats.isListing() && stats.listSize()) {
		trackData.freshness = stats.freshness();
		trackData.pay_rate = stats.pay_rate();
		trackData.organic_rate = stats.organic_rate();
		trackData.pager_visit = session.pagerVisit,
		trackData.list_size = stats.listSize();
		trackData.filter_num = filterNum;
		trackData.exit_rate = predict(trackData);
	}

	tracker.evt('polaris_session',trackData);
	$(document).trigger('polaris_exit_rate_predict', trackData.exit_rate);
	window.polaris_exit_rate_predicted = trackData.exit_rate;
	$('.show-contact').on('click',function() {
		var contacts = storage.get('polaris:contact')||0;
		storage.set('polaris:contact',++contacts, {TTL: 60*60*12*1000});
	});

	function predict(data) {
		var sum = 0;
		var cate = {"cheliang":0.095669661997541,"chongwuleimu":0.15347424643586,"ershou":0.22310248377593,"fang":0.27235233201467,"fuwu":1.1019764086334,"gongzuo":0.39314197045005,"huodong":-0.10106485555046,"jianzhi":0.19803795567028,"jiaoyupeixun":0.76743909042073};
		var cities = {"little":0.10973987764808,"middle":-0.032919934988044,"large":0};
		var hours = [0,0,0.01102544602171,0.27130876041335,0.2838639066182,0.041917004229875,-0.53227323938447,-0.83937132800005,-1.2899486956404,-1.3084972334238,-1.2757869052897,-1.2092643635881,-1.0722976703559,-1.1309938417944,-1.1727401588305,-1.2457513259281,-1.1815974614961,-1.1159209529113,-1.0900425016498,-1.0317279748411,-1.02860404542,-0.99027388774787,-0.93243560856903,-0.55240434108684];
		var numerics = {"list_size":-0.0030157364961732,"freshness":-0.44993156988087,"organic_rate":-0.014461994385492,"pay_rate":0.58518276075629,"session_depth":-0.23704683063496,"filter_num":-0.053488948372691,"is_l2l":-0.13405488159318,"session_is_l2v":0,"has_query":-0.70093683996932,"l2l_num":-0.10172794629467,"l2v_num":0.041135919568125,"referer_filter_num":-0.24518608358387,"viewads":0.17039669979285,"contacts":-0.049322710431733,"":null};
		sum = cate[stats.getCategory()] + cities[getCitySize(stats.getCity())] + hours[ (new Date).getHours() ];
		var map = {
			session_depth: session.history.length,
			viewads: data.viewads,
			l2v_num: data.l2v_num,
			is_l2l: (+isL2L),
			l2l_num: data.l2l_num,
			has_query: data.has_query,
			filter_num: data.filter_num,
			referer_filter_num: data.referer_filter_num,
			contacts: data.contacts,
			freshness: data.freshness,
			organic_rate: data.organic_rate,
			pay_rate: data.pay_rate,
			list_size: data.list_size
		};
		for (var k in map) {
			sum += map[k] * numerics[k];
		}
		var py = 1/(1+Math.pow(Math.E, -1*(sum+1.08039108132)));
		return py;
	}

	function pageStats(session) {
		function isViewad() {
			var path = window.location.pathname;
			if (path.charAt(0)!='/') path = '/'+path;
			return !!path.match(/\/(\w+)\/a(\d+)\.html/i);
		}
		function isListing() {
			return $('body').attr('class').indexOf('category-')!=-1;
		}
		function getCity() {
			return window.location.hostname.split('.')[0];
		}
		function getCategory() {
			var vadPath = window.location.pathname.match(/\/(\w+)\/a(\d+)\.html/i);
			if (vadPath) return vadPath[1];
			var listClass =  $('body').attr('class');
			if (listClass.indexOf('category-')==-1) return null;
			return listClass.split(' ').pop().replace('category-','');
		}
		function pageType() {
			var bodyClass = $('body').attr('class');
			if (bodyClass=='homepage') return 'home';
			if (query()) return 'query';
			var category_count = (bodyClass.match(/category/g) || []).length;
			if (category_count) return 'category-'+category_count;
			if (isViewad()) return 'viewad';
			return 'other';
		}
		function query() {
			var matches;
			if (matches = window.location.href.match(/query=(.+)/i)) {
				return matches[1];
			}
		}
		function source() {
			var link = document.createElement('a');
			link.href = document.referrer;
			var refererHost = link.hostname;
			if (refererHost.indexOf('baixing.com')!=-1 || refererHost.indexOf('baixing.cn')!=-1) {
				return null;
			}
			return refererHost;
		}
		var allList;
		function getAllList() {
			return allList || $('#all-list > ul > li');
		}
		function listSize() {
			return getAllList().length;
		}
		function isPagerVisit() {
			if (!isListing()) return -1;
			if (window.location.href.indexOf('page=')==-1) return -1;
			if (!session.history.length) return -1;
			var lastUrl = session.history[session.history.length-1].url;
			if (lastUrl == window.location.href) return 0;
			lastUrlAttrs = parseUrl(lastUrl);
			urlAttrs = parseUrl(window.location.href);
			if (lastUrlAttrs.hostPath != urlAttrs.hostPath) return -1;
			for (var i in urlAttrs.urlParams) {
				if (['page','afo'].indexOf(i)!=-1) continue;
				if (urlAttrs[i] != lastUrlAttrs[i]) return -1;
			}
			return 1;
		}
		function parseUrl(url) {
			var match,
				search = /([^&=]+)=?([^&]*)/g,
				decode = function (s) { return decodeURIComponent(s.replace(/\+/g, " ")); },
				urlParts  = url.split('?');
			var hostPath = urlParts[0];
			var queryString = urlParts[1] || null;
			urlParams = {};
			while (match = search.exec(queryString)) {
				urlParams[decode(match[1])] = decode(match[2]);
			}
			return {
				hostPath: hostPath,
				queryString: queryString,
				urlParams: urlParams
			}
		}
		function freshness() {
			var allList = getAllList();
			var freshCount = 0;
			var now = new Date;
			var before8 = now.getHours() < 8;
			if (before8) {
				var yesterday = new Date;
				yesterday.setDate(now.getDate() - 1);
				var yesterdayDate = (yesterday.getMonth()+1)+'月'+yesterday.getDate()+'日';
			}
			allList.each(function() {
				var datetime = $(this).find('.pull-right').text();
				var today = datetime.indexOf('今天')==0;
				if (today) {
					freshCount++;
					return;
				}
				if (before8 && yesterdayDate==datetime) freshCount++; 
			});
			return (freshCount/allList.length).toFixed(2);
		}
		function pay_rate() {
			var pinnedItems = $('#all-list > ul > li.item-pinned');
			return (pinnedItems.length/listSize()).toFixed(2);
		}
		function organic_rate() {
			var organicCount = listSize() - $('[data-inorganic=1]').length;
			return (organicCount/listSize()).toFixed(2);

		}
		function getRefererPage(history) {
			if (!history.length) return null;
			for (var i = history.length - 1; i >= 0; i--) {
				if (history[i].url == document.referrer) {
					return history[i];
				}
			};
		}
		function getRefererPageType(history) {
			var page = getRefererPage(history);
			if (!page) return null;
			return page.type;
		}
		function filterNum() {
			var num = 0;
			$('.fieldset>.items').each(function(){
				var selected = $(this).children('strong').get(0);
				if (selected && $(this).children(':not(.more)').get(0) != selected) num++;
			});
			return num;
		}
		return {
			isViewad: isViewad,
			isListing: isListing,
			pageType: pageType,
			query: query,
			source: source,
			listSize: listSize,
			isPagerVisit: isPagerVisit,
			freshness: freshness,
			pay_rate: pay_rate,
			organic_rate: organic_rate,
			getRefererPage: getRefererPage,
			getRefererPageType: getRefererPageType,
			filterNum: filterNum,
			getCategory: getCategory,
			getCity: getCity
		}
	}
	function todayStore() {
		var date = new Date();
		var today = ''+date.getFullYear() + date.getMonth() + date.getDate();
		var store = storage.get('polaris:today');
		if (!store || store.date!=today) {
			storage.set('polaris:today', {date:today, content:{}});
		}
		return {
			get: function(name) {
				return storage.get('polaris:today').content[name];
			},
			set: function(name, value) {
				var store = storage.get('polaris:today');
				store.content[name] = value;
				storage.set('polaris:today', store);
			}
		};
	}
	function getCitySize(city) {
		var large = ['shanghai','beijing','guangzhou','suzhou','shenzhen','shenyang','zhengzhou','wuxi','dalian','xuzhou','dongguan','hangzhou','xian','nanning','zibo','weifang','chengdu','wuhan','nanjing','jinan','shijiazhuang'];
		if ($.inArray(city, large)>-1) return 'large';
		var middle = [
'qingdao','fushun','tianjin','ningbo','hefei','chongqing','changchun','linyi','changzhou','fuxin','xiamen','jilin','yantai','foshan','jinzhou','fuzhou','zhongshan','huludao','changsha','luoyang','jiaozuo','tangshan','haerbin','huaian','zhuhai','jiaxing','kunshan','taiyuan','daqing','quanzhou','jining','lianyungang','tieling','qiqihaer','binzhou','anshan','jinhua','chaoyang','jixi','baotou','yancheng','jiangmen','panjin','wenzhou','huizhou','baoding','nantong','yichang','jiamusi','cangzhou','xingtai','benxi','dezhou','yingkou','yangzhou','zaozhuang','huhehaote','liuzhou','shuangyashan','hegang','changzhi','nanyang','shantou','suqian','handan','taizhou','xinxiang','kunming','xianyang','pingdingshan','wulumuqi','heze','wuhu','qingyuan','dongying','zhumadian','baoji','rizhao','xiangfan','huainan','liaocheng','weihai','taian','liaoyang','jingzhou','xinyang','ganzhou','lanzhou','nanchang','yl','hengshui','qinhuangdao','zhanjiang','luohe','sanmenxia','jieyang','tz','mianyang','yuncheng','shanwei','guiyang','dandong','haikou','guilin','langfang','kaifeng','puyang','zhaoqing','maoming','zhangzhou','weinan','meizhou','hulunbeier','zhoukou','anyang','suihua','shangqiu','mudanjiang','linfen','yangjiang','bengbu','qitaihe','shaoxing','xinzhou','yinchuan','siping','heihe'];
		if ($.inArray(city, middle)>-1) return 'middle';
		return 'small';
	}

})(simpleStorage);
</script><script>
$(function(){
	var stats = {};
	var check = PinnedModuleChecker();
	var index = 0;
	var currentType;
	var indexInModule = 0;
	$('#all-list>ul>li').each(function(){
		index++;
		var $item = $(this);
		if ($item.hasClass('adsense')) return;
		var type = check($item);
		if (type) {
			record(type);
		} else {
			type = 'regular';
		}
		if (currentType!=type) {
			currentType = type;
			indexInModule = 1;
		} else {
			indexInModule++;
		}
		var onclick = (function(index, indexInModule){
			return function(){
				var $link = $(this);
				var href = $link.attr('href');
				if ( href.match(/\/(\w+)\/a(\d+)\.html/i) ) {
					var sep = href.indexOf('?')<0 ? '?' : '&';
					var mod = type=='regular' ? '' : 'm='+type;
					var suffix = '';
										suffix = sep+mod+'&i='+index+'&n='+indexInModule;
										$link.attr('href',href+suffix);
				}
				setTimeout(function(){
					$link.attr('href',href);
				},1);
			};
		})(index, indexInModule);
		$item.on('click','a',onclick);
	});
	var regularItemCount = $('.item-regular').length;
	var insertCount = $('.listing-product-link').length;
	stats.regular = regularItemCount - insertCount;
	stats.insert = insertCount;
		tracker.evt('polaris_modules',stats);
		function record(type) {
		if (!stats.hasOwnProperty(type)) {
			stats[type] = 0;
		}
		stats[type] += 1;
	}
	function PinnedModuleChecker() {
		var pinned = {
			ace: 'img.aceImg',
			huoji: '.tag-huoji',
			ding: '.tag-warn',
			vip: '.vip-icon'
		};
		var types = $.map(pinned, function(v, i){return i;});
		var current = 0;
		return function($item) {
			if (current>=types.length) return false;
			for (var i = current; i < types.length; i++) {
				var type = types[i];
				var selector = pinned[type];
				if ($item.find(selector).length) {
					return type;
				}
				current++;
			};
		}
	}
	function filterNum() {
		var num = 0;
		$('.fieldset>.items').each(function(){
			var selected = $(this).children('strong').get(0);
			if (selected && $(this).children(':not(.more)').get(0) != selected) num++;
		});
		return num;
	}

	if (filterNum()) {
		var mainTop = $('#main').offset().top;
		var $window = $(window);
		var onScroll = function(e){
			if($window.scrollTop() > mainTop) {
				tracker.evt('polaris_valid_filter2');
				$window.off('scroll', onScroll);
			}
		};
		$window.on('scroll', onScroll);
	}
})
