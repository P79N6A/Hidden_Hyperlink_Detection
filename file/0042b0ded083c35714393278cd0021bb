<script type="text/javascript">
_defer.push(function() {
	var $sticky = $('.media-box-fixed');
	var posY = $sticky.offset().top;
	var adHeight = $sticky.outerHeight();
	var $footer = $('#footlinks').length ? $('#footlinks') : $('.copyright');
	if ($('.maincol').height()+600 <= $('.sidecol').height()) return;
	$(window).on('scroll', function(e) {
		var ifFixed = ($sticky.css('position') == 'fixed');
		var posEnd = $footer.offset().top - 20;
		var top = (scrollTop() < (posEnd - adHeight));
		var fixedTop = $('.ershouqiche-view').length ? 64 : 0;
		if (scrollTop() > posY && top) {
			$sticky.css('top', fixedTop);
			$sticky.css('position', 'fixed');
			return;
		}
		if (ifFixed && scrollTop() < posY) {
			$sticky.css('position', 'static');
			return;
		}
		if (!top && ifFixed) {
			$sticky.css('top', (posEnd - scrollTop() - adHeight) + 'px');
		}
	});
	function scrollTop() {
		if (window.scrollY !== undefined) return window.scrollY;
		return $(window).scrollTop();
	}
});
