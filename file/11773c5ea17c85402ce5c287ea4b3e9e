   <script language="javascript" src="http://www.qj.com.cn/js/common/zone.js"></script> 
   <script language="javascript" src="http://www.qj.com.cn/js/common/validate.js"></script> 
   <script language="javascript">

String.prototype.Trim = function() { 
	var m = this.match(/^\s*(\S+(\s+\S+)*)\s*$/); 
	return (m == null) ? "" : m[1]; 
}


function isMobel(value){   
	if(/^1[1-9]\d{9}$/g.test(value)){     
		return true;   
	}else{   
		return false;   
	}   
} 

function isTel(value) {
    return (/^(([0\+]\d{2,3}-)?(0\d{2,3})-)(\d{7,8})(-(\d{3,}))?$/.test(value.Trim()));
}

var issubmit =false;
function submitForm(){
  if(!issubmit) {
	  var f = document.messageForm;
	  if(!checkForm()) return ;
 	  f.zone.value=f.province.value + f.city.value;
	  document.getElementById("lytj").disabled = "disabled";
	  issubmit = true;
	  //f.submit();

	   var param = {};
		$('#messageForm_id').find('input,select,textarea').each(function(){
			param[$(this).attr("name")]=this.value;
		});
		$('.nmessup2').find('input,select,textarea').each(function(){
			param[$(this).attr("name")]=this.value;
		});
		$.ajax({
			url : '/frontMessage!message.action',
			type : 'post',
			dataType: "html",
			data : param,
			success : function(data){
				var result = eval('('+data+')');
				if(result.success) {
					alert('留言成功,我们将尽快审核您的留言，我们会尽快跟您联系！');
					//document.getElementById("dosubmit").disabled = "false";
				} else {
					alert('留言失败，请联系前景工作人员！');
				}
			}
		});
		
  }
}

function checkForm(){
  var f = document.messageForm;
   if(f.investcost.value == "" || f.investcost.value==null){
     alert("请选择您的投资预算！");
     return false;
   }
   if(f.name.value == "" || f.name.value == null){
     alert("请填写您的姓名！");
     f.name.focus();
     return false;
   }

   if(!isEmail(f.mail.value)){
     alert("电子邮件输入格式有误");
     f.mail.focus();
     return false;
   }
   
   if(f.tel.value == "" || f.tel.value==null){
     alert("请填写您的电话/手机！");
     f.tel.focus();
     return false;
   }

   if(!isTel(f.tel.value) && !isMobel(f.tel.value)){
     alert("请填写正确的电话/手机，例如'010-88888888或者13888888888'");
     f.tel.focus();
     return false;
   }
   
   if(!checkObjLength(document.messageForm.name,10,"您的姓名"))  return false;

   if(f.content.value == "" || f.content.value==null){
     alert("请填写您的加盟留言！");
     f.content.focus();
     return false;
   }
   
   return true;

}
function avals(obj){
    document.messageForm.content.value+=obj.innerHTML.replace("・","");
}
