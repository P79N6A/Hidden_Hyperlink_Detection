        });

        $("[id$='headerIbtLogin']").click(function () {
            var u = $("[id$='Header1_tbUid']").val();
            if (u == null || u == "" || u == "请输入帐号……") {
                alert("请输入用户名！");
                return false;
            }
            var p = $("[id$='Header1_tbPwd']").val();
