function ChkTKey() {
    if ($('#sskey').val() == "" || $('#sskey').val() == "请输入关键词") {
        $('#sskey').val("");
        $('#sskey').focus();
        alert('请输入关键词');
        return false;
    }
}
function NullTJs(oNum) {
    if (oNum == 1) {
        if ($('#sskey').val() == "请输入关键词") {
            $('#sskey').val("");
        }
    } else {
        if ($('#sskey').val() == "") {
            $('#sskey').val("请输入关键词");
        }
    }
}
</script> 
