  <script type="text/javascript">
(function(){
	$('.bjdl2').hover(function(){
		
		var obj = $(this).find('.bjdl')[0];
		move(obj,{height:216})
	},function(){
		var obj = $(this).find('.bjdl')[0];
		move(obj,{height:0})	
	})	
})();
(function(){
	$('#keyword').focus(function(){
		if($(this).val()=='输入关键词搜索'){
			$(this).val('').css('color','#333');	
		}	
	}).blur(function(){
		if($(this).val()==''){
			$(this).val('输入关键词搜索').css('color','#ddd')	
		}	
	})
	$('#sub').click(function(){
		$('#keyword').val($.trim($('#keyword').val()));
		if($('#keyword').val()==''||$('#keyword').val()=='输入关键词搜索'){
			alert('请输入关键词！');
			$('#keyword').focus();
			return false;	
		}	
	})	
})();


$('.ser_list_ul li').hover(function(){
$(this).addClass('cur')
},function(){
$(this).removeClass('cur')
})
