  <script type="text/javascript">

var flid=4;

$('#ewmobi').load('/wap/site_ajax.asp?id=' + flid + '&rnd=' + Math.random() * 1000, function(Data) {
    if (Data != '' && ewmdata.isOpen == 1) {
        var ewhtml = '';
        if (ewmdata.mobiUrl1 != "") {
            ewhtml = ewmdata.mobiTit1 + '<img src="' + ewmdata.mobiUrl1 + '" />';
        }
        if (ewmdata.mobiUrl2 != "") {
            ewhtml = ewhtml + ewmdata.mobiTit2 + '<img src="' + ewmdata.mobiUrl2 + '" />';
        }
        if (ewhtml != '') {
            $('#mobibox').show();
            $("#ewmobi").html(ewhtml);
        }
    }
})

$(function() {
    var st = 20;
    $('#bangshoutop').mouseenter(function() {
        $('#bangshoutop > .helpcon').stop(false, true).slideDown(st);
    }).mouseleave(function() {
        $('#bangshoutop > .helpcon').stop(false, true).slideUp(st);
    });
    $('.mobibox').mouseenter(function() {
        $('#ewmobi').stop(false, true).slideDown(st);
    }).mouseleave(function() {
        $('#ewmobi').stop(false, true).slideUp(st);
    });
});

function loadGoTop() {
    var h = $(window).height();
    var t = $(document).scrollTop();
    if (t > 30) {
        $('#gotop').show();
    } else {
        $('#gotop').hide();
    }
}
$(function() {
    loadGoTop();
    $('#gotop').click(function() {
        $(document).scrollTop(0);
    })
});
$(window).scroll(function(e) {
    loadGoTop();
})
$(function() {
    var duilian = $("div.helptop");
    var window_w = $(window).width();
    if (window_w > 100) {
        duilian.show();
    }
    $(window).scroll(function() {
        var scrollTop = $(window).scrollTop();
        duilian.stop().animate({
            top: scrollTop + 30
        });
    });
    //广告标示
    $(".qtz img,.postAd2016 img,.postAd2016 object,.postAd2016 embed").each(function() {
        if ($(this).width() > 150) {
            $(this).parent().parent(".qtz,.postAd2016").css("position", "relative").append('<span class="guanggao" style="position:absolute;z-index:9;left:2px;bottom:10px;margin:0;padding:0;width:19px;height:10px;background:url(http://img.pccoo.cn/v10/images/advtip.png);"></span>');
        }
    });
});
// online
function RefreshOnline() {
    $.get("/post/ajax_post.asp", {
        action: "online"
    });
}
$(function() {
    RefreshOnline();
});
window.setInterval(function() {
    RefreshOnline();
}, (1000 * 60 * 5));
// 
</script> 
