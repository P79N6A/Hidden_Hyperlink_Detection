   <script type="text/javascript">
	function left_fix_op() {
		$("#left_fix").fadeOut(500);
		$("#left_fix_show").fadeIn(500);
		//$(".left_fix").css('height','20px');
		//$("#left_fix").removeClass("left_fix").addClass("left_fix_op");
	}
	function left_fix_show() {
		$("#left_fix_show").fadeOut(500);
		$("#left_fix").fadeIn(500);
		//$(".left_fix").css('height','20px');
		//$("#left_fix").removeClass("left_fix").addClass("left_fix_op");
	}
	$(document).ready(function () {
		var width=window.screen.width;
		if (width<1600)
		{
			$("#right_fix").addClass("right_fix_1600").removeClass("right_fix");
		}
	});
</script>  ;
  </div>
 </body>
</html>