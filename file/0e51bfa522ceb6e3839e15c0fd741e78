<script type="text/javascript">
	function _doForcs(id,value){
			var iden=document.getElementById(id);
		if(iden.value == value){
			iden.value = "";
		}
		iden.style.color="black";
	}

	function _doBlur(id,value){
		var iden=document.getElementById(id);
	if(iden.value==""){
		iden.value=value;
	        iden.style.color="#B1B1B1";
		flag="0";
	}
	if(iden.value==value){
		 iden.style.color="#B1B1B1";
	}
	}


	var searchType=1; //1.企业信用信息 2.经营异常名录 3.严重违法失信企业名单

      //修改搜索方式
      function changeSelectStyle(id){
    	 // alert($("#"+id).attr("data-label"));
    	  searchType=$("#"+id).attr("data-label");
    	  //搜索样式
    	  $("#chose1").find("span").attr("style","");
	    	$("#chose2").find("span").attr("style","");
	    	$("#chose3").find("span").attr("style","");
	    	 $("#qyxy_img").css("visibility","hidden");
	         $("#jyyc_img").css("visibility","hidden");
	         $("#yzwf_img").css("visibility","hidden");
	    	//修改当前导航样式
	    	$("#"+id).find("span").attr("style","font-family: '微软雅黑 Regular', 微软雅黑; font-weight: bold; font-style: normal; text-decoration: none;  opacity: 1;");
	    	if(id=="chose1"){
		    	$("#qyxy_img").css("visibility","visible");
		    	}
		    	if(id=="chose2"){
			    	$("#jyyc_img").css("visibility","visible");
			    	}
		    	if(id=="chose3"){
				    	$("#yzwf_img").css("visibility","visible");
			    	}


	}
</script>
