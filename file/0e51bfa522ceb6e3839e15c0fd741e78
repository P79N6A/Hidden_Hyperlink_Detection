	   var u29 =$("#link");			 
	$("#guide").click(function(e){
		var e = e|| window.event;			
		if(u29.css("display")=="none"){
			u29.css("display","block");
			
		 }else{
			 u29.css("display","none"); 
		 }
       e.stopPropagation();
	});
	$("#link").click(function(e){
		var ev = ev || window.event;
		 e.stopPropagation();
	});
     $(document).click(function(e){
   	  u29.css("display","none");
	});
     
