  //加载logo
      function loadLogoImg(){
    	  $.get("/getLogo.jspx",function(data){
    		  if(data != null && data!=""){
    		 	 $("#headlog_img").attr("src",'/images/index/xz_index_logo/' + data+ ".png");
    		  }
    	  });
      }



  //加载底部
  function  loadEndHtml(){
	   $.get("/company/end.jspx",function(data){
       	 //初始將a.html include div#iframe
   	　　　　$("#dibu").html(data);
   	　　});
  }
  
  //添加手机浏览器自适应
  function  aduptPhone(){
	  $.get("/ifAduptPhone.jspx",function(data){
	       	 //初始將a.html include div#iframe
	       	data=JSON.parse(data)
	       	//alert(data.result);
	       	 if(data.result){
	       		$("head").append("<meta http-equiv='X-UA-Compatible' content='IE=edge'>"+
	       			  "<meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>");
	       	 }
	   	　});
  }
