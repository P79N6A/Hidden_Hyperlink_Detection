  <script>
var n=7;
function showC(id){
  for(j=0;j<n;j++){
  if (id==j){
	    $('#LayerC'+j).show();
	  }else{
	    $('#LayerC'+j).hide();
	  }
  }
}

function hideC(){
  for(j=0;j<n;j++){  
    $('#LayerC'+j).hide();
  }
}
</script> 
