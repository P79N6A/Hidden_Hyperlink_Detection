  <script type="text/javascript">
$(function(){
	var $rotaryArrow = $('#rotaryArrow');
	var $result = $('#result');
	var $resultTxt = $('#resultTxt');
	var $resultBtn = $('#resultBtn');
	var i=0;
	$rotaryArrow.click(function(){
		i++;
		if(i>1){
			
			$.cookie("example", "foo",{expires:1});  
			alert("每天只能抽一次哦");
			return false;
		}
		var data = [0, 1, 2, 3, 4, 5, 6, 7];
		data = data[Math.floor(Math.random()*data.length)];
		switch(data){
			case 1: 
				rotateFunc(1,87,'恭喜您获得了 <em>3000元现金</em>');
	
				break;
			case 2: 
				rotateFunc(2,43,'恭喜您获得了 <em>Ipad</em> 一台 ');
			
				break;
			case 3: 
				rotateFunc(3,134,'恭喜您获得了 <em>Apple watch</em> 一支');
		
				break;
			case 4: 
				rotateFunc(4,177,'很遗憾，这次您未抽中奖，继续加油吧');
				
				break;
			case 5: 
				rotateFunc(5,223,'恭喜您获得了 <em>龙吟智能电动平衡车</em> 一台');
	
				break;
			case 6: 
				rotateFunc(6,268,'恭喜您获得了 <em>Iphone 7</em> 一台');
				
				break;
			case 7: 
				rotateFunc(7,316,'恭喜您获得了 <em>5000元现金</em>');
			
				break;
			default:
				rotateFunc(0,0,'很遗憾，这次您未抽中奖，继续加油吧');
				
			
		}
	});

	var rotateFunc = function(awards,angle,text){  //awards:奖项，angle:奖项对应的角度
		$rotaryArrow.stopRotate();
		$rotaryArrow.rotate({
			angle: 0,
			duration: 10000,
			animateTo: angle + 7200,  //angle是图片上各奖项对应的角度，7200是让指针固定旋转20圈
			callback: function(){
				$resultTxt.html(text);
				$result.show();
				if(awards==0 || awards==4){
					$(".formInfo").hide()
				}else{
					$(".formInfo").show()
				}

				
			}
		});

	};

	$resultBtn.click(function(){
		$result.hide();
	});


	$("#result").css({
		left:( $(".rotary").width()-$("#result").width() )/2,
		top:( $(".rotary").height()-$("#result").outerHeight(true)-30 )/2
	})

if( !$.cookie("example") ){
	$(".rangWin").fadeIn();
}else if( $.cookie("example") ){
	$(".rangWin").hide();
}

	

});
</script> 
