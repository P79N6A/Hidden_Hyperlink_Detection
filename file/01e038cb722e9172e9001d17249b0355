    document.getElementById("stypes").value=chr;
}
function navi()
{
    var content="<div style=\"text-align:center;\">要浏览的网址：<input id=\"urls\" name=\"urls\" class=\"f-text\" type=\"text\" style=\"width:250px;\"></input>";
