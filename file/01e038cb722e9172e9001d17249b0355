        W.ConfirmButton="浏览网页";
        W.Width=400;
        W.Confirm=function()
        {
            var urls=jQuery("#urls").val();
            if(urls=="")
            {
                alert("请填写网址");return;
            }
            window.location.href="/url.html?urls="+urls;                   
        }
        W.Create();     
}
