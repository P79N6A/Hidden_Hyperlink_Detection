   <script type="text/javascript">
function MM_validateForm() { //v4.0
  if (document.getElementById){
    var i,p,q,nm,test,num,min,max,errors='',args=MM_validateForm.arguments;
    for (i=0; i<(args.length-2); i+=3) { test=args[i+2]; val=document.getElementById(args[i]);txt=args[i+1];
      if (val) { nm=val.name; if ((val=val.value)!="") {
        if (test.indexOf('isEmail')!=-1) { p=val.indexOf('@');
          if (p<1 || p==(val.length-1)) errors+='- '+txt+'必须是邮件格式!\n';
        } else if (test!='R') { num = parseFloat(val);
          if (isNaN(val)) errors+='- '+txt+'必须是数字!\n';
          if (test.indexOf('inRange') != -1) { p=test.indexOf(':');
            min=test.substring(8,p); max=test.substring(p+1);
            if (num<min || max<num) errors+='- '+txt+'必须在'+min+'和'+max+'之间!\n';
      } } } else if (test.charAt(0) == 'R') errors += '- '+txt+'不能为空!\n'; }
    } if (errors) alert('提示信息:\n'+errors);
    document.MM_returnValue = (errors == '');
} }
</script> 
   <div class="w992"> 
    <div class="anli f_l"> 
     <div class="zuiz_title c_zise"> 
      <span class="f_l">当前位置：<a href="http://www.topsedu.com/">首页</a> &raquo; <a href="http://www.topsedu.com/news/">留学资讯</a> &raquo; 正文</span> 
      <span class="f_r"><a href="javascript:history.go(-1)" class="zise">返回上一页</a></span> 
