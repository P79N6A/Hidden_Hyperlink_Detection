  <script type="text/javascript">
    //jQuery.noConflict();
    (function($) { 
        $(function() {
            $.fn.tuoScroll = function(options){
                var defaults = {
                    eventType : 'mouseover', //'click || mouseover'
                    speed : 30,// The bigger the slow  
                    other : null,
                    prevBtn : '<div class="tuo_prev prev_next"></div>',
                    nextBtn : '<div class="tuo_next prev_next"></div>'
                };
                var options = $.extend(defaults, options);
                this.each(function(){
                    var $this = $(this),
                    outBox = $('.tuo_out_box',$this),
                    inBox = $('.tuo_in_box',$this),
                    outWidth = outBox.width(),
                    itemWidth = inBox.children().outerWidth() + parseInt(inBox.children().first().css('margin-left')) + parseInt(inBox.children().first().css('margin-right')),
                    itemLen = inBox.children().length,
                    scrollLen = 0,
                    scrollCurrent = 0,
                    direction = 'left';// 'left'|| 'right';
                    addItem();
                    if(itemLen * itemWidth < 3*outWidth){

                    }
                    function addItem(){
                        var chidren = inBox.children().clone(true,true);
                        for(var i = 0; i < 4; i++){
                            inBox.append(chidren.clone(true,true));
                        }
                        var inBoxWidth = 0;//inBox.children().length*itemWidth;
                        for(var i = 0; i< inBox.children().length; i++){
                            var element = $(inBox.children()[i]);
                            inBoxWidth += element.width();
                        }
                        inBox.width(inBoxWidth);
                        scrollLen = inBoxWidth - outWidth;
                    }
                    var auto = setInterval(interval,options.speed)
                    function interval(){
                        if(direction == 'left'){
                            scrollCurrent--
                            if(Math.abs(scrollCurrent) == scrollLen) scrollCurrent = 0;
                        }else{
                            scrollCurrent++;
                            if(Math.abs(scrollCurrent) == 0) scrollCurrent = -scrollLen;
                        }
                        inBox.css({left : scrollCurrent});
                    }
                    function init(){
                        $(options.prevBtn).appendTo($this).bind(options.eventType,function(){
                            direction = 'left';
                        });
                        $(options.nextBtn).appendTo($this).bind(options.eventType,function(){
                            direction = 'right';
                        });
                        outBox.hover(function(){
                            clearInterval(auto);
                        },
                        function(){
                            auto = setInterval(interval,options.speed)
                        })
                    } 
                    init();
                });
            };
            $.getIndex = function(ary,item){
                for(var i = 0; i < ary.length ; i++){
                    if(ary[i] == item) return i;
                }
                return null;
            }
            /*$.fn.tuozi_tab=function(){
            
                function Tab(This){
            
                    var _this = this,
            
                        $this = This;
            
                    _this.navItems = $('.tab_nav_item',$this); 
            
                    _this.conItems = $('.tab_con_item',$this);
            
                    _this.navItems.mouseenter(function(){
            
                        if($(this).attr('class').indexOf('current') >= 0) return false;
            
                        var len = $.getIndex(_this.navItems,this);
            
                        _this.conItems.hide();
            
                        _this.navItems.removeClass('current');
            
                        $(this).addClass('current');
            
                        $(_this.conItems[len]).show();
            
                    })
            
                    function init(){
            
                        _this.navItems.first().addClass('current');
            
                        _this.conItems.hide().first().show();
            
                    }
            
                    init();
            
                }
            
                this.each(function(index){
            
                    $.fn.tuozi_tab['tab' + index] = new Tab($(this));
            
                })
            
            }*/
            //Tab选项卡
            $(function(){

                $(".sta_title_right a:eq(0)").addClass("current").siblings().removeClass("current");
                var sta_title_right_a= $(".sta_title_right a");
                var tab_con_item= $(".tab_con_item");
                sta_title_right_a.mouseover(function(){

                    $(this).addClass('current').siblings().removeClass('current');
                    var sta_title_right_a_index=sta_title_right_a.index(this);
                    tab_con_item.eq(sta_title_right_a_index).show().siblings(".tab_con_item").hide();
                })
            });
            sta_title_right_a.eq(0).trigger('mouseover');
            $(function(){
                $('.tuo_scroll').tuoScroll();
                if($('#tuo_tab').length > 0) {
                    $('#tuo_tab').tuozi_tab();
                }
                $('#tuo_btn1').mouseenter(function(){
                    $('.est').css({'background-position':'0 0'})
                })
                $('#tuo_btn2').mouseenter(function(){
                    $('.est').css({'background-position':'-195px 0'})
                })
                $('#tuo_btn3').mouseenter(function(){
                    $('.est').css({'background-position':'-390px 0'})
                })
                $('#tuo_btn4').mouseenter(function(){
                    $('.est').css({'background-position':'-585px 0'})
                })
            })
        });
    })(jQuery);
</script> 
