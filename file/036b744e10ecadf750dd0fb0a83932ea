        var List = document.getElementById('headerNav');
        var myManage = document.getElementById('myManageCenter');

        var searchBtn = document.getElementById('IndexSearch');
        var curUrl = window.location.href;
        var aTag = List.getElementsByTagName('a');
        var userType;
        
        userType = 3;
        
        var hasurl = false;
        for (var i = 0, len = aTag.length; i < len; i++) {
            //if (window.location.href.indexOf('zujuan') > -1) {
            //    document.getElementById('BuildPaperGroup').className += ' active';
            //    hasurl = true;
            //    break;
            //} else
            if (curUrl.indexOf(aTag[i].getAttribute('href')) > -1 && aTag[i].getAttribute('href') != "/") {
                aTag[i].parentNode.className += ' active';
                hasurl = true;
                break;
            }
            else {
                aTag[i].parentNode.className.replace('active', ' ');
            }
        }
        if (!hasurl) {
            aTag[0].parentNode.className = 'active';
        }
        if (userType == 1) {
            myManage.setAttribute('href', '/teacher/');
        } else if (userType == 2) {
            myManage.setAttribute('href', '/student/');
        }
        else {
            myManage.setAttribute('href', '/login');
