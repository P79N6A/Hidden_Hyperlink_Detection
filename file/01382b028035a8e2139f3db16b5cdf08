  <script>
function checkform(theform)
{
    //姓名验证
    if(theform.name.value=="请输入姓名" || theform.name.value=="")
    {
        alert("请填写您的姓名！");
        theform.name.focus();
        return false;
    }

    str = theform.name.value;
    re = /[\u4E00-\u9FA0]/;
    if (!re.test(str))
    { alert("姓名必须中文填写!");
        theform.name.value = "";
        theform.name.focus();
        return false;
    }
    //联系电话
    if(theform.phone.value=='请输入手机号码' || theform.phone.value=='')
    {
        alert("请输入手机号码");
        theform.phone.focus();
        return false;
    }

    hand = theform.phone.value;
    handre = /^1[3|4|5|7|8]\d{9}$/;
    if (!handre.test(hand))
    { alert("请填写正确的11位手机号码");
        theform.phone.value = "";
        theform.phone.focus();
        return false;
    }
}

</script> 
