        window.addEventListener('message',function(e){
            if(e.origin == 'https://passport.newgame.com' || e.origin == 'http://luck.passport.newgame.com') {
                if (typeof e.data != 'string'){
                    return;
                }
                if (e.data == 'getURL') {
                    e.source.postMessage(window.location.href, e.origin);
                } else {
                    window.location.href = e.data;
                }
            }
        },false);
    </script>


