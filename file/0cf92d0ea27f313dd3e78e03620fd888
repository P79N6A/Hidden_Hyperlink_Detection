	function Favorite_purchase()
	{
		var data = {"areaName":"","areaid":"021","businId":0,"businTitle":"","comeUrl":"","companyName":"","contact":"","isbusin":"","sellerProviderId":100009237055,"supcatId":"","supcatName":"","sysFlag":"","telPhone":""};
		var url = "http://detail.b2b.hc360.com/detail/turbine/action/ajax.Favorite_PurchaseAction/eventsubmit_doFavorite/doFavorite?";
		jQuery.ajax({
		type:"get",
		url:url,
		data:data,
		dataType:"jsonp",  
		jsonp:"jsoncallback",
		timeout:3000,		
		success:function(result){
			var urlmy = "http://my.b2b.hc360.com/my/turbine/action/favorites.Favorite_PurchaseAction/eventsubmit_doPerform/doPerform?";
			result.contact=encodeURIComponent("徐顺");
			result.CompanyName=encodeURIComponent("上海群鼎科技有限公司");
			result.comeUrl=window.location.href;
			result.isbusin=1;
			result.type=13;
			result.buyerSourceId="_";
			jQuery.ajax({
				type:"get",
				url:urlmy,
				data:result,
				dataType:'jsonp',
				jsonp:"jsoncallback",
				timeout:3000,		
				success:function(result1){
				}
				});
		}
		});
	}
