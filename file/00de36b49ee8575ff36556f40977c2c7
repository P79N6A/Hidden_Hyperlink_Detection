  <script type="text/javascript">
$(function(){	
	$("#keyword").bigAutocomplete({
		width:385,
		url:'index.php?act=search&op=aotocomplete',	
		callback:function(data){				
			var key = data.title;
			var url = "index.php?act="+$("#search_act").val()+"&keyword="+key;
			window.location.href = url;
		}
	});

})
</script> 
