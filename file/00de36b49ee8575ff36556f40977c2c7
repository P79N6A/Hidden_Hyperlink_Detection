   <div class="mt10"></div> 
  </div> 
  <script type="text/javascript">
$(function(){
            /*监听滚动条和左侧菜单点击事件 start*/
            var _arr = [];
            //滚动事件
            window.onscroll = function(){
                if(1300 < $(document).scrollTop()){
                    //滚动高度scrollTop小于1300，淡入
                    $('.nav_Sidebar').fadeIn('slow');
                }else{
                    //滚动高度scrollTop大于1300，淡出
                    $('.nav_Sidebar').fadeOut('slow');
                }
                if(($('#faq').offset().top + $('#footer').height() - $('.nav_Sidebar').height() - 50) <= $(document).scrollTop()){//底部的top属性-左侧导航的高度-50 小于等于 当前窗口的滚动高度
                    //左侧导航增加bottom属性：底部的高度；
                    $('.nav_Sidebar').css({'bottom':$('#faq').outerHeight() + 'px'});
                }else{
                    //左侧导航增加bottom属性：100px；
                    $('.nav_Sidebar').css({'bottom':'350px'});
                }
                //每个楼层循环
                $('.home-standard-layout').each(function(index){
                    var that = $(this);
                    //楼层位置
                    that.index = index;
                    //如果滚动高度+(页面的高度/2) 大于 楼层距离页面顶部的像素
                    if($(document).scrollTop() + $(window).height()/2 > that.offset().top){
                        //将位置写入数组
                        _arr.push(index);
                    }
                });
                //如果数组不为空
                if(_arr.length){
                    //查找最后一个左侧导航的A标签，新增样式，添加类名：current。查找他的同胞，修改样式，移除类名：current
                    $('.nav_Sidebar a').eq(_arr[_arr.length-1]).css({'color':'#f31f03','border-bottom':'1px solid #ebebeb;','background':'#fff'}).addClass('current').siblings().css({'color':'#1e1e1e','border-bottom':'1px solid #ebebeb','background':'#fff'}).removeClass('current');
                    //$('.nav_Sidebar a').eq(_arr[_arr.length-1]).css({'backgroundImage':'url(/shop/templates/default/images/jd_side-active.png)'}).addClass('current').siblings().css({'backgroundImage':'url(/shop/templates/default/images/jd_side.png)'}).removeClass('current');
                    //清空数组
                    _arr = [];
                }
            }
            $('.nav_Sidebar a').each(function(index){
                $(this).click(function(){ $(this).css({'color':'#fff','border-bottom':'1px solid #ebebeb','background':'#f31f03'});
                    $('html,body').animate({scrollTop: $('.home-standard-layout').eq(index).offset().top - 20 + 'px'}, 200);
                }).mouseover(function(){
                    //鼠标在A标签上面
                    if($(this).hasClass('current')){
                        //这个A标签有类：current
                        return;
                    }else{
                        //这个A标签没有类：current
                       $(this).css({'color':'#fff','border-bottom':'1px solid #ebebeb','background':'#f31f03'});
                        /*$(this).css({'backgroundImage':'url(/shop/templates/default/images/jd_side-hover.png)'});*/
                    }
                }).mouseout(function(){$(this).css({'color':'#fff','border-bottom':'1px solid #ebebeb;','background':'#f31f03'});
                    //鼠标移开A标签后
                    if($(this).hasClass('current')){
                        //这个A标签有类：current
                        return;
                    }else{
                        //这个A标签没有类：current
                        $(this).css({'color':'#1e1e1e','border-bottom':'1px solid #ebebeb;','background':'#fff'});
                        /*$(this).css({'backgroundImage':'url(/shop/templates/default/images/jd_side.png)'});*/
                    }
                });
            });
            /*end*/
            window.onload = window.onresize = function(){
                if($(window).width() < 1300 || 500 > $(document).scrollTop()){
                    $('.nav_Sidebar').fadeOut('slow');
                }else{
                    $('.nav_Sidebar').fadeIn('slow');
                }
                
            }		   
});



</script>
  <style>
