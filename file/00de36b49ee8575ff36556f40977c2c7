     <script>    
    var onchangesearch = function (name,type){
      if(type==0){
        var url = 'http://qyggo.com/service';
        $('#top_search_form').attr('action',url);
        $('.search_site_task').attr('value','task');
        $('#sea').attr('value',name);
        $('#keyword').attr('data-searchtype',name);
      }else{
        var url = 'http://qyggo.com/mall';
        $('#top_search_form').attr('action',url);
        $('#search_act').val(name);
	     $('#keyword').attr('data-searchtype',name);
      }
    }
    </script> 
