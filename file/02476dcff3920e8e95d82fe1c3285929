			$.ajax({
				url: "/cms/article/articleReport.jhtml",
				type: "POST",
				cache: false,
				data: {
					ip:ip,
					id:id,
					arcId:arcId,
					content:content
				},
				success: function(data) {
					if(data.success == 'success'){
						$(".isreport").val('true');
           				$(".tishicon").text("您的投诉我们已经收到，我们会尽快处理！");
           				$(".faqdiv").hide()
       					$(".tishi").css("display","block");
       					$(".tousu").hide();
           				
           				setTimeout(function () {
				            $(".tishi").css("display","none");
				            $(".faqbg").css("display","none");
				        }, 1000);
           				
					}else{
						$(".isreport").val('false');
						$(".faqdiv").hide()
           				$(".tishi").css("display","block");
           				$(".tishicon").html("<span style='color:red;'>"+data.message+"</span>");
					}
				}
			});
        });
        
        $(".arclist").click(function(){
        
        	var $this = $(this);
        	$this.find('input').attr("checked", "checked");
        });
	
})();

</script> 
