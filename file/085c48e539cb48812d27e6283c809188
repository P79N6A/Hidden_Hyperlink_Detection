function getPageLoadTime()
{ 
    returnValue = '';
    if (typeof(performance) !== 'undefined' && typeof(performance.timing) == 'object')
    {
        timing = performance.timing;
        startTime = timing.redirectStart || timing.fetchStart || timing.requestStart;
        endTime = timing.domContentLoadedEventEnd || timing.domInteractive || timing.domComplete || timing.loadEventComplete;
        if (startTime && endTime && startTime < endTime) returnValue = endTime - startTime;
    }
    return returnValue.toString();
}

$(document).ready(function()
{
    if( typeof(pageData) != 'undefined' && typeof(pageData["page"]) != 'undefined' && typeof(pageData["page"]["loadTime"]) != 'undefined' )
    {
        pageData["page"]["loadTime"]=getPageLoadTime();
    }
}
);


try { pageDataTracker.trackPageLoad(); }
catch(e) { console.warn("There was an error loading or running Adobe DTM:\r\n ", e); }
</script>



