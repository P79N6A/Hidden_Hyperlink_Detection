$(document).ready(function(){
  $(".jiucuo").click(function(){
    $(".jiucuobox").css("display","block");
  });
  $(".jiucuo_guanbi").click(function(){
    $(".jiucuobox").css("display","none");
  });
  $(".jiucuo_qx").click(function(){
    $(".jiucuobox").css("display","none");
  });
});
