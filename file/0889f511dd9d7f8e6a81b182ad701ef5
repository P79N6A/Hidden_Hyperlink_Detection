  <script type="text/javascript">
//检查签到状态
jquery.post('/plugin.php?id=dsu_paulsign:sign&inajax=1&infloat=1&operation=qiandaox&mobiletype=1',
function (data) {
if(jquery(data).find('root').text()=='error') {
                    jquery('#sign-div').hide();
                }else{
                    jQuery("#sign-div").show();
}
}
,'xml'
);

function modalshow(){
jquery.post('/plugin.php?id=dsu_paulsign:sign&inajax=1&infloat=1&operation=qiandaox&mobiletype=1',{'formhash':'e61e3e5e','qdxq':'ybsva','qdmode':'1'},
function (data) {
                    if(jquery(data).find('root').text()=='loggin'){
                        //location.href='/member.php?mod=logging&action=login';
                        jquery('#fast-login').modal('show');
                    }else if(jquery(data).find('root').text()=='error'){
                        jquery('#sign-div').hide();
                    }else{
                        jQuery("#modal-bg").show();
                        jQuery("#sign-modal").show();
                    }
}
,'xml'
);
}
function modalhide(){
jQuery("#modal-bg").hide();
jQuery("#sign-modal").hide();
jQuery("#modal-btn").hide();
}

</script> 
