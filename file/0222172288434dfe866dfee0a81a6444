        var nCode = args.nCode ? args.nCode : "";
        var rType = args.rType ? args.rType : "";
        if (args.action && args.action == "hasLoginResult") {
            if (args.st) {
                location.href = "/login/login_aliyun.htm?st=" + args.st + "&nCode=" + nCode + "&rType=" + rType + "&params=%7B%22site%22%3A%226%22%2C%22umidToken%22%3A%22Y348c5d42f26045017bdb00feafc0dedf%22%2C%22ru%22%3A%22http%3A%2F%2Ficpbeian.aliyun.com%2Forder%2FviewOrderInfo.htm%22%7D";
                return;
