			function zhida(q){

                var data = $('form#zhidaform').serialize();
                $('form#zhidaform').unbind('submit');  
				          
                $.ajax({
                    url: "/zhida/",
                    type: 'POST',
					dataType: "json", 
                    data: data,
                    beforeSend: function() {
						
						   $(".info2").css("display", "block");
					$('.info2').html("<div><img src='/css/loading.gif'></div>");
                        
                    },
                    success: function(data, textStatus, xhr) {
if (data.url.length < 2) {
var bodyScrollTop = document.documentElement.scrollTop || document.body.scrollTop;
imWinOpen('im_win','myselfbox',200+bodyScrollTop);

if (data.reg==1) {
$('.msg').html(""+data.msg+"");
$(".regbutton").css("display", "block");
}else{
$('.msg').html(""+data.msg+"");
$(".regbutton").css("display", "none");
}

//alert(data.msg);
}else{
//alert(data.msg);
 window.location.href=data.url; 
}
				 $(".info2").css("display", "none");
			
                    },
                    error: function(xhr, textStatus, errorThrown) {
                        
                    }
                });
                return false;
            }
		 

 function xiangguanlist(id){
 closeLogin('im_win','myselfbox');

 var data = $('form#zhidaform').serialize();
                $('form#zhidaform').unbind('submit');  
				          
                $.ajax({
                    url: "/xiangguanlist/?page="+id,
                    type: 'POST',
					dataType: "json", 
                    data: data,
                    beforeSend: function() {
						
					
					$('.list1').html("");

                        $(".info3").css("display", "block");
						   $(".info4").css("display", "none");
						   $('.info3').html("<div><img src='/css/loading.gif'></div>");
						   
                    },
                    success: function(data, textStatus, xhr) {

if(id==1){
$('.list1').html("");
$('.list3').html("");
}

if (data.list.length < 1) {
		
 $(".info4").html("<span style='padding:10px 30px;'>全部加载完毕</span>");
				} else {
			
				
		
$.each(data.list, function(key, value) {
	if (value.title.length < 1) {
		$(".list3").append('');
}else{
		$(".list3").append('<li><span><a href="/down/' + value.ccid + '" target="_blank"><img src=' + value.icon + '></a></span><div id=ts><h3><a href="/down/' + value.ccid + '" target="_blank">' + value.title + '</a></h3><p id=p' + value.id +'>' + value.description + ' </p></div><h5><a href="/down/' + value.ccid + '" target="_blank">查看详情</a></h5></li>');
}
				});

				}


		var vd;
		vd=id+1;
		//alert(vd);
	 $(".info4").html("<span style='padding:10px 30px;' onclick='xiangguanlist("+vd+")'>加载更多</span>");
			 $(".info3").css("display", "none");
			    $(".info4").css("display", "block");
			if (data.num==0) {
	 	 $(".info4").html("<span style='padding:10px 30px;'>全部加载完毕</span>");
		 }


                    },
                    error: function(xhr, textStatus, errorThrown) {
                        
                    }
                });
                return false;
 }




		 function xiangguan(q){

                var data = $('form#zhidaform').serialize();
                $('form#zhidaform').unbind('submit');  
				          
                $.ajax({
                    url: "/xiangguan/",
                    type: 'POST',
					dataType: "json", 
