				$(function(){
					$("#language").focus(function(){
						$("#error_logon1_msg").hide();
					});
					$("#language").blur(function(){
						var value = $("#language").val();
						if(value == "0"){
							$("#error_logon1_msg").show();
						}
					});
					$("#logon2_traderName").focus(function(){
						$("#error_logon2_msg").hide();
					});
					$("#logon2_traderName").blur(function(){
						var value = $("#logon2_traderName").val();;
						if(value == "0"){
							$("#error_logon2_msg").show();
						}
					});
				});

				function submitLogon1(){
					var value = $("#language").val();
					if(value == "0"){
						$("#error_logon1_msg").show();
					} else {
						var traderId = $("#language option:selected").attr("trader_id");
						var url = $("#language option:selected").attr("target_url");
						var visitUrl = {"url": url, "sourceUrl": "表单", "traderId": traderId, "type": 4};
						$.ajax({
							type:"POST",
							url:"/f_visit",
							dataType:"json",
							contentType:"application/json",
							data:JSON.stringify(visitUrl),
							success:function(data){
							}
						});
						window.open(url);
					}
				}
