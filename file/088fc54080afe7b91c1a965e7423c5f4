    <script type="text/javascript">
            jQuery(document).ready(function () {
                // 设置藏品搜索类型
                var url = window.location.href.toLowerCase();
                var soUrl = "http://so.cang.com/product/all";
                // 拍卖、书画城 独立控件
                if (url.indexOf("jd.cang.com") > -1) {                  // 鉴定估价
                    soUrl = "http://so.cang.com/search/estimation";
                } else if (url.indexOf("trade.cang.com") > -1 || url.indexOf("www.cang.com/trade") > -1) {        // 地摊藏品
                    soUrl = "http://so.cang.com/product/ditan";
                } else if (url.indexOf("shop.cang.com") > -1 || url.indexOf("www.cang.com/shop") > -1) {         // 古城城藏品
                    soUrl = "http://so.cang.com/product/mall";
                }
                $('#select li').eq(0).attr('data-search', soUrl);

                // 搜索栏目设置
                $("#select li").click(function () {
                    $(this).addClass("currt").siblings().removeClass("currt");
                    $("#search_frm").attr("action", $(this).attr("data-search"));
                    $("#key").focus();
                    $("#key").val($("#key").val());
                })
                // 点击搜索按键            
                $("#btnSo").click(function () {
                    gotoso();
                })
                // 回车事件
                $('#key').bind('keydown', function (event) {
                    if (event && event.keyCode == 13) { // enter 键
                        gotoso();
                        return false;
                    }
                })
                function gotoso() {
                    if ($("#key").val().length > 0 && $("#key").val() != '输入您要搜索的关键字') {
                        for (var i = 0; i < $("#select li").length; i++) {
                            if ($('#select li').eq(i).attr("class") == "currt") {
                                var key_value = encodeURI($("#key").val());
                                if (typeof ($('#select li').eq(i).attr('data-search')) != 'undefined') {
                                    location.href = $("#select li").eq(i).attr("data-search") + "?q=" + key_value;
                                }
                            }
                        }
                    }
                }
                // 搜索标签列表
                $.ajax({
                    contentType: "application/x-www-form-urlencoded; charset=utf-8",
                    type: "get",
                    async: true,
                    url: "http://so.cang.com/base/gethotkey",
                    dataType: "jsonp",
                    jsonp: "jsonpCallBack",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                    success: function (json) {
                        if (json != undefined)
                        {
                            var soHtml = "";
                            var soUrl = "http://so.cang.com";
                            for (var i = 0; i < json.length; i++) {
                                if (json[i].SearchType == "all" || json[i].SearchType == "mall" || json[i].SearchType == "ditan" || json[i].SearchType == "auction") {
                                    soUrl = 'http://so.cang.com/product/' + json[i].SearchType + '?q=' + encodeURI(json[i].KeyWord);
                                } else {
                                    soUrl = 'http://so.cang.com/search/' + json[i].SearchType + '?q=' + encodeURI(json[i].KeyWord);
                                }
                                soHtml += '<a href=' + soUrl + ' target="_blank">' + json[i].KeyWord + '</a>';
                            }
                            if (soHtml.length > 0) {
                                $(".search-ft").html(soHtml);
                            }
                        }
                    }
                });
            });
        </script> 
