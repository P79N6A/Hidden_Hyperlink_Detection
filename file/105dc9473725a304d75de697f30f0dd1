  <script>
    window.bookId = "100466";
    window.chapterId = "3358";
    window.prevChapterUrl = '/book/100466/3357.html'
    ;
    window.nextChapterUrl = '/book/100466/3421.html'
    ;
    window.bookUrl = '/book/detail?id=100466';
    $(document).keyup(function (e) {
        // 避免吐槽的时候触发
        if (e.target.tagName.toUpperCase() == 'TEXTAREA' || e.target.tagName.toUpperCase() == 'INPUT') {
            return;
        }

        if (e.keyCode == 39) {
            location.href = nextChapterUrl ? nextChapterUrl : bookUrl;
        } else if (e.keyCode == 37) {
            location.href = prevChapterUrl ? prevChapterUrl : bookUrl;
        }
    });
</script> 
