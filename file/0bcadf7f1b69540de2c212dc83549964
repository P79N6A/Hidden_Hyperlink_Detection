    $(function() {

        $('.wqsuspend_menu_dj').click(function() {
            $('#menu_mask').toggle();
            $('#wqheadmenu_warp').toggle();
            return false;
        });
        $('#menu_mask').click(function(e) {
            if (!$('#wqheadmenu_warp').is(':hidden')){
                $('#menu_mask').hide();
                $('#wqheadmenu_warp').hide();
            }
        });
    });
