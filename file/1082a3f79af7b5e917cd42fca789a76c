	function trackEvent(category, action, name, value, CustomVariable) {
		//if($instance.FeedbackName!='check')
		if (CustomVariable && typeof CustomVariable == 'object') {
			var idx = 10;
			for ( var key in CustomVariable) {
				_paq.push([ 'setCustomVariable', idx, key, CustomVariable[key],
						'event' ]);
				idx++;
			}
		}
		_paq.push([ 'trackEvent', category, action, name, value ]);
		sleep(100);
		for (var i = 10; i <= idx; i++) {
			_paq.push([ 'deleteCustomVariable', i, "event" ]);
		}
		// end
	};

	function sleep(numberMillis) {
		var now = new Date();
		var exitTime = now.getTime() + numberMillis;
		while (true) {
			now = new Date();
			if (now.getTime() > exitTime)
				return;
		}
	}
	
