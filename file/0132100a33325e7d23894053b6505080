			if(curdata[1]=='1'){
				$("#contents").removeClass('vod-show-id-mode');
				$("#contents").addClass('grid-mode');
			}else{
				$("#contents").addClass('vod-show-id-mode');
				$("#contents").removeClass('grid-mode');
		}
		}
}
);
});
conbread();
function  conbread(){
	var atag = $('.filter-list ul li a');
	var constr="";
$.each(atag, function(i,val){      
       var data = val.data;
	   if(windowurl.indexOf(data)!=-1){
	   	     constr +='<em>'+val.innerHTML+'</em>';
			  $(this).attr('class','current');
	   }
  }); 
  $('.conbread').html(constr);
}
