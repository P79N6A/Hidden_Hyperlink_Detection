  <script>
function checkInput(){
	if($('.search .keyword input').val()==''){
		alert('关键字不能为空!');
		return false;
	}
}
$('.Logo_Search .search .types li').click(function(){
	$('.Logo_Search .search .types li').removeClass('ck');
	$(this).addClass('ck');
	var searchurl='';
	var selectname=$(this).html();
	if(selectname=='资讯') searchurl='http://dezhou.mhcm.net/news/search.php';
	else if(selectname=='分类') searchurl='http://dezhou.mhcm.net/f/search.php';
	else if(selectname=='商品') searchurl='http://dezhou.mhcm.net/shop/search.php';
	else if(selectname=='商家') searchurl='http://dezhou.mhcm.net/hy/search.php';
	else if(selectname=='活动') searchurl='http://dezhou.mhcm.net/tg/search.php';
	else if(selectname=='促销') searchurl='http://dezhou.mhcm.net/coupon/search.php';
	else if(selectname=='招聘') searchurl='http://dezhou.mhcm.net/hr/search.php';
	else if(selectname=='论坛') searchurl='http://dezhou.mhcm.net/bbs/search.php';
	else if(selectname=='上门服务') searchurl='http://dezhou.mhcm.net/fuwu/search.php';
	else if(selectname=='外卖') searchurl='http://dezhou.mhcm.net/waimai/search.php';
	$(".Logo_Search form").attr("action",searchurl);
});
</script> 
