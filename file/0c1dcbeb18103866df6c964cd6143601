/* Fixed Nav ------------- */
var offset_top = 50;
var headline_pos = $('#xzsq').offset();
 $(window).scroll(function()
{
var scroll_top = $(window).scrollTop();

if (scroll_top > (headline_pos.top-offset_top))
{
$('#xzsq').addClass('fixed');
 }
else
{
$('#xzsq').removeClass('fixed');
 }
});
