<script type="text/javascript">

var timeout;

function headerSearchSubmit(ev)  {
  if (window.event && window.event.keyCode == 13 || ev.which == 13) {
   headerSubmit();
   return false;
  } else {
   return true;
  }
 }

var j$ = jQuery.noConflict();
j$(document).ready(function($) {

 var $linkContactYourAccountManager = $('a[id$=popup_contactAccountManager]'),
 $liteBoxContact = $('#liteBoxContact'),
 $descriptionTextArea = $('.topicText'),
 $caseReason = $('[id$=caseReason]'),
 $emailField = $('[id$=emailField]'),
 $subjectField = $('[id$=subjectField]'),
 $topicText = $('[id$=topicText]'),
 $continueButtonManager = $("#continueButtonManager h3"),
 $continueInnerBlock = $("#continueButtonManager div:first"),
 $linkAccountInfo = $('a[id$=popup_accountInfo]'),
 $liteBoxContent = $('#liteBoxContent'),
 $informationIncorrect = $('a[id$=informationIncorrect]'),
 isValid = true,  isTopicSpecified = false, isCaseReasonSelected = false,
 isEmailSpecified = false;

 $continueButtonManager.removeClass("alt").addClass("disabled");
 $continueInnerBlock.removeClass("continue").addClass("continue-disabled");

 $informationIncorrect.click(function(e) {
  e.preventDefault();
  $liteBoxContent.trigger('close');
  $linkContactYourAccountManager.trigger('click');
 });

 $emailField.wrap( "<div class='input'></div>" );
 $subjectField.wrap( "<div class='input'></div>" );

 $linkContactYourAccountManager.click(function(e) {
  e.preventDefault();
  $('#liteBoxContact').lightbox_me({
   centered: true,
   appearEffect: "fadeIn",
   closeClick: false,
   onLoad: function() {
    resetForm();
    validateForm();
   }
  });
 });

 $topicText.on('keyup input change blur', function(e){
  validateForm();
  if (this.value.length > 3200)
   this.value = this.value.substr(0, 3200);
 });

 $caseReason.change(function(e){
  validateForm();
 });

 $emailField.on('keyup input change blur', function(e){
  var validEmail = isEmailValid();

  $('.errorForEmail').html( validEmail ? '' : '未输入电子邮箱或电子邮箱无效');
  $(this).parent().prop('class', validEmail ? 'input' : 'input inputError');

  validateForm();
 });

 function isEmailValid() {
  return $emailField.val().length != 0 && $emailField.val().match( /^[_a-zA-Z0-9-]+(\.[_a-zA-Z0-9-]+)*@[a-zA-Z0-9-]+(\.[a-zA-Z0-9-]+)*(\.[a-zA-Z]{2,4})$/ ) != null;
 }

 var validateForm = function(){

  isTopicSpecified = $topicText.val().length != 0;
  isEmailSpecified = isEmailValid();
  isCaseReasonSelected = $caseReason.find('option:selected').val().length != 0;

  if ( isTopicSpecified && isCaseReasonSelected && isEmailSpecified ) {
   if( !isCaseCreationInProgress() ) {
    isValid = enableSubmitButton();
   }
  } else {
   isValid = disableSubmitButton();
  }
 }

 var resetForm = function(){
  $liteBoxContact.find('textarea').focus();

  //reset all data on the form
  $liteBoxContact.find("select option:first").prop("selected", true);
  $liteBoxContact.find('textarea').val('');
  $subjectField.val('');
  $emailField.val('').parent().prop('class', 'input');
  j$(".textError, .textSuccess").hide();
  j$(".errorForEmail").html('');
  $.uniform.update();
  isTopicSpecified = false;
  isCaseReasonSelected = false;
  isEmailSpecified = false;
 }

 $linkAccountInfo.on('mouseenter', function(e) {
  e.preventDefault();
  $liteBoxContent.lightbox_me({
   centered: false,
   appearEffect: "fadeIn",
   closeClick: true,
   overlayCSS: {background: 'black', opacity: .0},
   modalCSS: {top: '165px'},
   onLoad: function() {
    $liteBoxContent.find('input:first').focus()
   }
  });
 });

 $("body").find("input, select, textarea").uniform();

 $descriptionTextArea.attr('placeholder', '如需进一步协助，请选择一个主题，输入问题，然后点击提交。');
 $emailField.attr('placeholder', '请输入一个电邮地址');
 $subjectField.attr('placeholder', '请输入一个主题');

 $('input').placeholder();

 $("#formHeaderSubmit").click(function(e){
  if (isValid) {
   e.preventDefault();
   isValid = disableSubmitButton();
   submitForm();
  }
 });
});

 function closeLightbox() {
  clearTimeout(timeout);
  j$('#liteBoxContact').trigger('close');
  return false;
 }

 function disableSubmitButton(){
  j$("#continueButtonManager h3").removeClass().addClass("disabled");
  j$("#continueButtonManager div:first").removeClass().addClass("continue-disabled");
  return false;
 }

 function enableSubmitButton(){
  j$("#continueButtonManager h3").removeClass().addClass("alt");
  j$("#continueButtonManager div:first").removeClass('continue-disabled').addClass("continue");
  return true;
 }

 function isCaseCreationInProgress(){
  return j$(".waitingImage").is(":visible") || j$(".textSuccess").is(":visible");
 }

</script>

