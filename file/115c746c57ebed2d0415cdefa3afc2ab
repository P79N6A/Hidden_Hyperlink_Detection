  <script>
//全局变量
var isIE=0;
var isFirefox=0;
var isChrome=0;
var isOpera=0;
var isSafari=0;
var nu=navigator.userAgent;
if(nu.indexOf("MSIE")>0)
{ 
	isIE=1;
} 
else if(nu.indexOf("Firefox")>0)
{ 
	isFirefox=1;
} 
else if(nu.indexOf("Chrome")>0)
{ 
	isChrome=1;
}
else if(nu.indexOf("Safari")>0) 
{ 
	isSafari=1;
}  
else if(nu.indexOf("Opera")>0)
{ 
	isOpera=1;
} 
//获取ie浏览器的版本
function GetIEVersion()
{
	var ua=navigator.userAgent; //获取用户端信息
  	var b=ua.indexOf("MSIE "); //检测特殊字符串"MSIE "的位置
  	if (b<0) 
	{
    	return 0;
    }
    return parseFloat(ua.substring(b+5,ua.indexOf(";",b))); //截取版本号字符串，并转换为数值
}

//获取对象
function $_fmt(id)
{
	 return(document.getElementById(id));
}
function FixElement()
{
	var win_h=$(window).height(),
		scroll_top=$(window).scrollTop();
	if(isIE)
	{
		var ieVersion=GetIEVersion();
		if(ieVersion==6)
		{
			var $_ele=$('page-fixed');
			var top=scroll_top+win_h-$_ele.outerHeight();
			$_ele.css({top:top+'px'});
		}
	}
	
	var $_feedback=$('.feedback'),
		ele_h=$_feedback.outerHeight();

	var top=$(window).scrollTop()+win_h/2-ele_h/2;
	$_feedback.css({top:top+'px'});
}

function FeedbackBindEvent()
{
	$('.feedback-max-a').bind('click',function()
										{
											$('.feedback-min').addClass('nd');
											$('.feedback-max').removeClass('nd');
										});
	$('.feedback-min-a').bind('click',function()
										{
											$('.feedback-max').addClass('nd');
											$('.feedback-min').removeClass('nd');
										});
										
	$('#feedback-sbt').bind('click',function()
							{
								var content=$('#feedback-in').val();
								content=$.trim(content);
								if(content=='')
								{
									alert('感谢您的关注，但内容不可以为空，请填写');
									return;
								}
								if(content.length>300)
								{
									alert('感谢您的关注，但内容太长，超过了300个字符，您可以分多次提交');
									return;
								}
								var url='./?m=netself&a=user_feedback_add';
								$.ajax({url:url,
										type:'post',
										data:{content:content},
										dataType:'json',
										success:function(data)
												{
													if(data.code)
													{
														$('#feedback-in').val('');
													}
													alert(data.msg);
												},
										error:function()
												{
													alert('很抱歉，系统错误，您的建议或意见未发送成功，请稍后再试，再次感谢您的关注.');
												}
									});
							});
}

window.onscroll=function()
 				{
					FixElement();
				};
$(document).ready(function()
				{
					FixElement();
					FeedbackBindEvent();
				});
</script>   
 </body>
</html>