  <script>
function DynamicJs()
{
	var url='./?m=ad&a=load&t='+Math.random();
	var head=document.getElementsByTagName('head')[0];
	var script_el=document.createElement('script');
	script_el.src=url;
	script_el.type='text/javascript';
	head.appendChild(script_el);
}
$(document).ready(function()
					{
						DynamicJs();
						$('.hide-dom').each(function()
											{
												var real_dom_class=$(this).attr('data-real-dom-class'),
													html=$(this).html();
												$(this).prev('.'+real_dom_class).html(html);
												$(this).remove();
											});
					});
</script>
