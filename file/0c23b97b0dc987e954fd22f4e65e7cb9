     </div> 
     <script type="text/javascript">
/**
 * 滑动控件 标签要满足：<div><ul><li>..</li><li>..</li></ul></div> 一种style实现方案:div{width:独立于内容;overflow:hidden;position:relative/*for ie7* /;},ul{white-space:nowrap;font-size:0},li{display:inline-block;white-space:normal;font-size:16px(or you specified)}
 * @param option - {duration:滑动间隔,leftSelector:'',rightSelector:'',loop:false,index:用来设置初始要显示的li（基于0的序数）}
 * @returns {jQuery}
 * @author ykc
 */
$.fn.slider=function(option){
    return this.each(function(){
        if(!option.leftSelector && !option.rightSelector){return false}
        var $div = $(this),$ul = $div.children('ul'),
            divWidth = $div.width(),
            $li = $ul.children(),
            size = $li.length,
            _option = {duration:250,leftSelector:'',rightSelector:'',loop:false,index:0};
        $.extend(_option,option);
        if(_option.loop && size >0){
            $ul.html($li[size-1].outerHTML+$ul.html()+$li[0].outerHTML);
            $li = $ul.children();
        }
        var liWidth = size > 1 ?
                $li.outerWidth()+parseInt($li.css('margin-left').replace('px',''))+parseInt($li.css('margin-right').replace('px',''))
                :$li.width();
        var cf = 0;/*当前可视li的序数*/
        setState(_option.index);
        $(this).css({visibility:'visible'})
        $(_option.leftSelector).click(function fn(){
            if(fn.locked){return}
            if(cf-1<0){
                if(!_option.loop){return}
                $ul.animate({left:'+='+liWidth+'px'},_option.duration,function(){$ul.css({left:-liWidth*size+'px'});fn.locked=false;});
                cf=size-1;
                fn.locked = true;
                return;
            }
            cf-=1;
            $ul.animate({left:'+='+liWidth+'px'},_option.duration);
        });
        $(_option.rightSelector).click(function fn(){
            if(fn.locked){return}
            if(cf+1>=size){
                if(!_option.loop){return}
                $ul.animate({left:'-='+liWidth+'px'},_option.duration,function(){$ul.css({left:-liWidth+'px'});fn.locked=false;});
                cf=0;
                fn.locked = true;
                return;
            }
            cf+=1;
            $ul.animate({left:'-='+liWidth+'px'},_option.duration);
        })

        function setState(index){
            var offset = 0;
            if(_option.loop){offset = -liWidth}
            cf=index;
            $ul.css({left:offset-cf*liWidth+'px'})
        }
    })
}

$(document).ready(function(){
    var $slider =$('.slider');   
    //var index=$slider.find('[style*="#00a5eb"]').closest('.list_lanmu').parent().index();
    //index=index<0?0:index;
    var index=0;
    
    $slider.slider({leftSelector:'#slide_left',rightSelector:'#slide_right',loop:true,duration:400,index:index});
})
</script> 
