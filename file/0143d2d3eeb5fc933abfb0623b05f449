     <div class="page-header"> 
      <h2 class="page-title">Mac配置开发环境</h2> 
      <!-- .page-title --> 
      <!-- .breadcrumb --> 
     </div> 
     <!-- .page-header --> 
     <div id="primary"> 
      <main id="main" class="site-main clearfix"> 
       <article id="post-159" class="post-159 post type-post status-publish format-standard has-post-thumbnail hentry category-12 tag-mac"> 
        <header class="entry-header"> 
        </header> 
        <!-- end .entry-header --> 
        <div class="entry-content clearfix"> 
         <div id="toc_container" class="no_bullets">
          <p class="toc_title">目录</p>
          <ul class="toc_list">
           <li><a href="#1">1&nbsp;环境变量</a></li>
           <li><a href="#2_JDK">2 JDK</a></li>
           <li><a href="#3_IDEA">3 IDEA</a></li>
           <li><a href="#4_MAVEN">4 MAVEN</a></li>
           <li><a href="#5_mysql">5 mysql</a></li>
