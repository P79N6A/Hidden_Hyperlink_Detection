					duilian.stop().animate({top:(scrollTop+40+"px")});
				});
				duilian_close.click(function(){
					jQuery(this).parent().parent().hide();
					return false;
				});
				var businCount;
				jQuery("#companyName,#businCountDiv").bind('mouseover',function(){
					if(businCount){
						clearTimeout(businCount);
					}
					jQuery("#businCountDiv").show();
				}).bind('mouseout',function(){
					businCount=setTimeout(function(){jQuery("#businCountDiv").hide();},300);
				})
			});
