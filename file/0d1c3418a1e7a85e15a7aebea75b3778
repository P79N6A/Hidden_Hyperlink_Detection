  <script>
//返回顶部
(function() {
var Util = {
trim: function(str) {
if (String.prototype.trim) return str.trim();
return str.replace(/^\s+/g, '').replace(/\s+$/g, '');
},
hasClass: function(ele, cls) {
return ele.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)'));
},
addClass: function(ele, cls) {
if (!Util.hasClass(ele, cls)) {
ele.className = Util.trim(ele.className) + " " + cls;
}
},
removeClass: function(ele, cls) {
if (Util.hasClass(ele, cls)) {
var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)');
ele.className = ele.className.replace(reg, ' ');
}
},
addEvent: function(ele, type, handler) {
if (ele.addEventListener) {
ele.addEventListener(type, handler, false);
} else if (ele.attachEvent) {
ele.attachEvent("on" + type, handler);
} else {
ele["on" + type] = handler;
}
},
getEvent: function(event) {
return event ? event : window.event;
},
getTarget: function(event) {
return event.target || event.srcElement;
},
preventDefault: function(event) {
if (event.preventDefault) {
event.preventDefault();
} else {
event.returnValue = false;
}
},
removeEvent: function(ele, type, handler) {
if (ele.removeEventListener) {
ele.removeEventListener(type, handler, false);
} else if (ele.detachEvent) {
ele.detachEvent("on" + type, handler);
} else {
ele["on" + type] = null;
}
},
stopPropagation: function(event) {
if (event.stopPropagation) {
event.stopPropagation();
} else {
event.cancelBubble = true;
}
}
};
var fixedNav = document.getElementById('fixedNav');
var fixedNavItem = fixedNav.getElementsByTagName('li');
var qrcode;
function handler(obj, event) {
event = Util.getEvent(event);
var type = event.type;
if (type === 'mouseover') {
if (Util.hasClass(obj, 'qrcode')) {
Util.addClass(obj, 'open');
}
Util.addClass(obj, 'active');
var imgs = obj.getElementsByTagName("img");
for (var i = 0,len = imgs.length; i < len; i++) {
var img = imgs[i];
if (img.getAttribute("#src1") && !img.getAttribute('loaded')) {
img.src = img.getAttribute("#src1");
img.setAttribute('loaded', 'true')
}
}
} else if (type === 'mouseout') {
if (Util.hasClass(obj, 'qrcode')) {
Util.removeClass(obj, 'open');
}
Util.removeClass(obj, 'active');
}
}
for (var i = 0; i < fixedNavItem.length; i++) {
(function(j) {
Util.addEvent(fixedNavItem[j], 'mouseover', function(event) {
handler(fixedNavItem[j], event);
});
Util.addEvent(fixedNavItem[j], 'mouseout', function(event) {
handler(fixedNavItem[j], event);
});
})(i);
}
var Jtop=document.getElementById("jTop");
Util.addEvent(window, "scroll", function(h) {
var scrTop = document.documentElement.scrollTop+document.body.scrollTop;
if(scrTop>800){
Jtop.style.display="block";
}else{
Jtop.style.display="none";
}
});
Util.addEvent(Jtop, "click", function(h) {
window.scrollTo(0, 0);
});
})();
</script> 
