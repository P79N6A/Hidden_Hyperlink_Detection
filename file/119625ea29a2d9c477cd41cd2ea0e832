                <tr style="PAGE-BREAK-INSIDE: avoid; HEIGHT: 28.25pt"> 
                 <td style="BORDER-BOTTOM: windowtext 1.5pt solid; BORDER-LEFT: windowtext 1pt double; PADDING-BOTTOM: 0pt; PADDING-LEFT: 1.4pt; WIDTH: 42pt; PADDING-RIGHT: 1.4pt; BACKGROUND: rgb(255,255,255); BORDER-TOP: medium none; BORDER-RIGHT: windowtext 1.5pt solid; PADDING-TOP: 0pt; mso-border-left-alt: 0.5000pt double windowtext; mso-border-right-alt: 1.5000pt solid windowtext; mso-border-top-alt: none; mso-border-bottom-alt: 1.5000pt solid windowtext" valign="top" rowspan="2" width="56"> <p style="TEXT-ALIGN: center" class="MsoNormal" align="center"><b><span style="FONT-FAMILY: 宋体; COLOR: rgb(0,0,255); FONT-SIZE: 10.5pt; FONT-WEIGHT: bold; mso-spacerun: 'yes'; mso-font-kerning: 1.0000pt; mso-ascii-font-family: MingLiU">第三天</span></b><b></b></p></td> 
                 <td style="BORDER-BOTTOM: windowtext 1.5pt solid; BORDER-LEFT: medium none; PADDING-BOTTOM: 0pt; PADDING-LEFT: 1.4pt; WIDTH: 31.5pt; PADDING-RIGHT: 1.4pt; BACKGROUND: rgb(127,127,127); BORDER-TOP: windowtext 1.5pt solid; BORDER-RIGHT: windowtext 1.5pt solid; PADDING-TOP: 0pt; mso-border-left-alt: none; mso-border-right-alt: 1.5000pt solid windowtext; mso-border-top-alt: 1.5000pt solid windowtext; mso-border-bottom-alt: 1.5000pt solid windowtext" valign="top" rowspan="2" width="42"> <p class="MsoNormal"><b></b></p></td> 
