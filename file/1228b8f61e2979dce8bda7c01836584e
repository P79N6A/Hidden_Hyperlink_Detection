var search_val1 = '苹果ipad3';
var search_val2 = 'HTC Sensation XE';
var search_val3 = '智能手机';

function selecttype()
{
	var a = jQuery("#slt").val();
	if(jQuery("#keywords").val() == "")
		document.form_ss.action="http://search.sanhaostreet.com/"
	else if(a == "news")
		document.form_ss.action="http://search.sanhaostreet.com/search_news.php";
	else if(a == "price")
		document.form_ss.action="http://search.sanhaostreet.com/search_price.php"
	else if(a == "trader")
		document.form_ss.action="http://search.sanhaostreet.com/search_trader.php"
	else 
		document.form_ss.action="http://search.sanhaostreet.com/index.php"
}
function selectchange()
{
	var a = jQuery("#keywords").val()
	if(a == "" || a == search_val1 || a == search_val2 || a == search_val3)
	{
		a = jQuery("#slt").val();
		if(a == "news")
			jQuery("#keywords").val(search_val1);
		else if(a == "price")
			jQuery("#keywords").val(search_val2);
		else if(a == "trader")
			jQuery("#keywords").val(search_val3);
	}
}
function search_on_focus()
{
	var a = jQuery("#keywords").val()
	if(a == "" || a == search_val1 || a == search_val2 || a == search_val3)
	{
		a = jQuery("#slt").val();
		if(a == "news" && jQuery("#keywords").val() == search_val1)
				jQuery("#keywords").val("");
		else if(a == "price" && jQuery("#keywords").val() == search_val2)
				jQuery("#keywords").val("");
		else if(a == "trader" && jQuery("#keywords").val() == search_val3)
				jQuery("#keywords").val("");
			
	}
}
function search_on_blur()
{
	var a = jQuery("#keywords").val();
	if(a == "")
	{
		a = jQuery("#slt").val();
		if(a == "news" )
				jQuery("#keywords").val(search_val1);
		else if(a == "price" )
				jQuery("#keywords").val(search_val2);
		else if(a == "trader" )
				jQuery("#keywords").val(search_val3);
	}
}


