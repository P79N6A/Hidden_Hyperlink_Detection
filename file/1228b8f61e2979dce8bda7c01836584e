       键盘也能翻页，试试“← →”键
      </div> 
      <script type="text/javascript">
							(function(prev,next){
							window.SanHaoNewsHotKeyEvent = true;/*全局变量，文章页热键功能总开关*/
							function keydownEvent(e){
							var e = e||window.event;
							if(prev&&!e.ctrlKey&&!e.altKey&&!e.shiftKey&&e.keyCode==37&&window.SanHaoNewsHotKeyEvent) window.location.href=prev;
							if(next&&!e.ctrlKey&&!e.altKey&&!e.shiftKey&&e.keyCode==39&&window.SanHaoNewsHotKeyEvent) window.location.href=next;
							}
							
							function bindEvent(obj,ev,fn){
							if(obj.addEventListener){
							obj.addEventListener(ev,fn,false);
							}else {
							obj.attachEvent('on'+ev,fn);
							}
							}
							bindEvent(document,'keydown',keydownEvent);
							})('/NewsData/2012/5/20125885320928_5.shtml','/NewsData/2012/5/20125885320928_7.shtml');
							</script> 
