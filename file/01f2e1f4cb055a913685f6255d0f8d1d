    var hl_para_id = "3", hl_menu_id = "2";
  if(hl_para_id)
  {
    if(1 == hl_menu_id) hl_menu_id = ''
    var hl_class = 'Hover_on'+hl_menu_id, obj = null
    if(isNaN(hl_para_id))
    {
