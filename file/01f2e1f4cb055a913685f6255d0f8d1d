    <script type="text/javascript"> 
gmap_longLat= "";
chinese_address= '北京市朝阳区十八里店南桥56号';
city=1111;
lang='zh_en';

function int_map(id,s1){
	
	mapObj=new AMap.Map(id,{
		view:new AMap.View2D({
			rotation:0,
			zoom:18
		}),
		lang:lang
	})
 
	if(gmap_longLat){
		pos=gmap_longLat.split(',');
		mapObj=new AMap.Map(id,{
			view:new AMap.View2D({
				center:new AMap.LngLat(pos[0],pos[1]),//地图中心点
				rotation:0,
				zoom:18
			}),
			lang:lang
		})
		if(s1){
		 mapObj.plugin(["AMap.ToolBar"],function(){	
				toolBar = new AMap.ToolBar(); //设置地位标记为自定义标记
				mapObj.addControl(toolBar);		
			});
			mapObj.plugin(["AMap.Scale"],function(){
			    var scale = new AMap.Scale();
			    mapObj.addControl(scale);  
			});
			 $('.amap-zoom-labels').remove(); 
		}
		mapObj.clearMap();
		var markerOption = {
	        map:mapObj,
	        draggable:$('.one_item').val() == 1?false:true,
	        icon:"http://webapi.amap.com/images/0.png",
	        position:new AMap.LngLat(pos[0],pos[1]),
	        //clickable:true,
	        raiseOnDrag:false,
	        raiseOnDrag:true
	    };
		
	     mar = new AMap.Marker(markerOption);
	    
		infoWindow = new AMap.InfoWindow({
 	        size:new AMap.Size(300, 0),
 	        autoMove:true, 
 	        offset:new AMap.Pixel(0,-30),
	        content:$('.hid_txt').html()
	    });
    	windowsArr.push(infoWindow);
    	mapObj.setFitView();
    	//infoWindow.open(mapObj, mar.getPosition());
    	if($('.one_item').val() == 1){
    	    AMap.event.addListener(mar, "mouseover", function(e){
    	    	if($(mapObj.t).attr('id') != 'map_canvas'){
    	    		infoWindow.open(mapObj, mar.getPosition());
    	    	}
    	    	
    	    });
    	    AMap.event.addListener(mar, "mouseout", function(){
    	    	infoWindow.close();
    	    });
        }
	}else{
	 if(s1 == 1){
		 mapObj.plugin(["AMap.ToolBar"],function(){	
				toolBar = new AMap.ToolBar(); //设置地位标记为自定义标记
				mapObj.addControl(toolBar);		
			});
			mapObj.plugin(["AMap.Scale"],function(){
			    var scale = new AMap.Scale();
			    mapObj.addControl(scale);  
			});
			 
	 }
		
		placeSearch(chinese_address) 
	}

	mapObj.plugin(["AMap.DistrictSearch"], function() {
	    var opts = {
	        subdistrict: 1,   //返回下一级行政区
	        extensions: 'all',  //返回行政区边界坐标组等具体信息
	        level:'city'  //查询行政级别为 市
	    };
	    //实例化DistrictSearch
	    district = new AMap.DistrictSearch(opts); 
	   //查询成功时的回调函数，定义如何展示请求返回结果
	    AMap.event.addListener(district, 'complete', function(e){
	      var center = e.districtList[0].center;
	      mapObj = new AMap.Map('map_canvas',{
	          layers: [
	              new AMap.TileLayer()
	          ],
	          view: new AMap.View2D({
	              center: new AMap.LngLat(center.lng,center.lat),
	              zoom:8
	          }),
	          lang:lang
	      });
	      
	      $('.dtposmapsc .cur_select_pos').text( $('.dtposmapsc .cur_select_pos').attr('place'))
	      	var clickEventListener=AMap.event.addListener(mapObj,'click',function(e){
				var x=e.lnglat.getLng();
				var y=e.lnglat.getLat();
				$('#getLongLat').val(x+','+y) 
				$('.dtposmapsc .cur_select_pos').text(e.lnglat.lng+','+e.lnglat.lat).show();
				mapObj.clearMap()
				marker = new AMap.Marker({				  
					icon:"http://webapi.amap.com/images/marker_sprite.png",
					position:new AMap.LngLat(x,y)
				});

				marker.setMap(mapObj);  //在地图上添加点
				mapObj.setFitView(); //调整到合理视野

				init_newPoint(x,y);

			})
			 $('.amap-zoom-labels').remove(); 
			$('.amap-toolbar').remove();
	    }); 
	})
	 
	mapObj.plugin(["AMap.DistrictSearch"], function() {
	    var opts = {
	        subdistrict: 1,   //返回下一级行政区
	        extensions: 'all',  //返回行政区边界坐标组等具体信息
	        level:'city'  //查询行政级别为 市
	    };
	    //实例化DistrictSearch
	    district = new AMap.DistrictSearch(opts); 
	    $('.amap-zoom-labels').remove()
	   //查询成功时的回调函数，定义如何展示请求返回结果
	    AMap.event.addListener(district, 'complete', function(e){
	      var center = e.districtList[0].center;
	      mapObj = new AMap.Map('lookBigBox1',{
	          layers: [
	              new AMap.TileLayer()
	          ],
	          view: new AMap.View2D({
	              center: new AMap.LngLat(center.lng,center.lat),
	              zoom:8
	          }),
	          lang:lang
	      });	
	      
		    mapObj.plugin(["AMap.ToolBar"],function(){	
				toolBar = new AMap.ToolBar(); //设置地位标记为自定义标记
				mapObj.addControl(toolBar);		
			});
			mapObj.plugin(["AMap.Scale"],function(){
			    var scale = new AMap.Scale();
			    mapObj.addControl(scale);  
			});
			$('.amap-zoom-labels').remove();  	
	  })
	});
	 
}
int_map('map_canvas',0);
function lookBig(){
	int_map('lookBigBox1',1);
	easyDialog.open({
		container : 'lookBigBox'
	});
}
$(function(){
	
	$('.choice').each(function(){
	   if($(this).text().length<1){
			 $(this).parent().hide();
			 
	  }else{
			  $(this).parent().show();
			  
	  }
		  
	});
});
