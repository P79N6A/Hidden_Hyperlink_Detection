 { $('.intro').show();
   $('.Introduction').show();
 }
else
 {$('.intro').hide();
 $('.Introduction').hide();
 }
</script> 
    <script type="text/javascript">

	function show_edit_info()
	{
		easyDialog.open({
			container : 'testBox1'
		});
	}

	function hide_edit_info()
	{
		easyDialog.close();
	}

	function edit_info()
	{
		var name = $('#name'),
			email = $('#email'),
			subject = $('#subject'),
			message = $('#message'),
			sid = $('#lid'),
			o = {name:name, email:email, subject:subject, message:message,sid:sid},
			data = {};
		for(i in o)
		{
			if('' == o[i].val() && i!='id')
			{
				EAlert('To modify information, please fill out the box. ',1);
				o[i].focus();
				return false;
			}
			data[i] = o[i].val()
		}
		$.post('/listings/edit_apply', data, function(r){
			if(1 == r)
			{
				EAlert('Your modified info has been sent!');
				hide_edit_info();
			}
		});
	}

	function mark()
	{
		var mouse_socre = $('#mouse_socre'),
			mouse_score_result = $('#mouse_score_result'),
			score = $('#score'),
			delta = 0;

		mouse_socre.parent().mousemove(function(e){
			mouse_socre.css('backgroundPosition','0 18px')
			delta = Math.ceil((e.pageX - $(this).offset().left)/16);
			mouse_score_result.html(mark_titles[delta])
			$("input[name='mouse_score_result_value']").val(1);	
			mouse_socre.css('width', delta*16);
		}).mouseleave(function(){
			if(-1 == score.val())
			{
				mouse_socre.css('backgroundPosition','0 18px')
				mouse_score_result.html(mark_titles[0])
			}
			else
			{
				mouse_socre.css('backgroundPosition','0 34px')
				var s = score.val();
				mouse_socre.css('width', s*16);
				mouse_score_result.html(mark_titles[s])
			}
			$("input[name='mouse_score_result_value']").val(1);
		}).click(function(){
			$("input[name='mouse_score_result_value']").val(2);
			score.val(delta)
			mouse_socre.css('backgroundPosition','0 34px')
		});


	}
	mark();

	$('#cmt_submit').click(function(){
		if($('#cmt_submit').attr('disabled')=='true'){
			return;
		}
		var bs = $("#mouse_score_result").html();
		var bl = $("input[name='mouse_score_result_value']").val();
		var ta = $('.PL_con'),
			v = $.trim(ta.val()),
			self = this;
		if((bs == '' && bl == 1) || bs == 'You haven’t given a rating yet'){
			$("#mouse_score_result").html('You haven’t given a rating yet');
			return;
		}
		if('' == v)
		{
			EAlert('Please write something before clicking submit.',1);
			ta.focus();
			return;
		}
		var submit_url = '/add_cmt',
			edit_cmt_id = $('#edit_cmt_id').val();
		if(-1 != edit_cmt_id)
		{
			submit_url += '/'+edit_cmt_id;
		}
		$('#cmt_submit').attr('disabled',true);
		$.post(submit_url, {cnt: v, score:$('#score').val(), is_annoy:$('[name=is_annoy]:checked').val()}, function(r){				
			if(1 == r)
			{
				$('#cmt_submit').attr('disabled',false);
				EAlert('请您先登陆',1)
			}
			else if(222 == r)
			{
				$('#cmt_submit').attr('disabled',false);
				EAlert('请勿提交相同内容',1)
			}
			else if(2 == r)
			{
				msg = -1 != edit_cmt_id ? 'Successful Edit！':'Successful Comment！';
				EAlert({msg:msg,okFun:function(){
					path=location.pathname.split('/');
					window.location.href = '/utils/inner_director/'+path[1]+'/'+path[3];
				},canFun:function(){
					path=location.pathname.split('/');
					window.location.href = '/utils/inner_director/'+path[1]+'/'+path[3];
				},showMask:true})
			}
			else if(6 == r)
			{
				$('#cmt_submit').attr('disabled',false);
				$(self).parents('table').eq(0).find('.warn_text').html('You are unable to post comments at this time.');
			}
		});

	})

				

	
function del_my_confirm(a,b,c,d){
	EConfirm({msg:'Are you sure you want to delete this?',okFun:function(){
		del_cmt(a, b, c, d)
	}})
}
</script> 
