        <!--将要跳转的url--> <script type="text/javascript">
	$('#per_log_username').focus();

	//获取联动城市
	function per_reg_getcheck(app2,app3,me)
	{
		if($(me).val()=='22'){
			$('.'+app2).show();
			$('.'+app3).hide();
		}else if( $(me).val() == '62'){
			$('.'+app2).hide();
			$('.'+app3).show();
		}else{
			$('.'+app2).hide();
			$('.'+app3).hide();
		}
	}

	//ajax登录
	function per_ajax_login(fun)
	{
		var per_log_username = ($('#per_log_username').val());
		var per_log_pw = $('#per_log_pw').val();
		var perlog_warn_text = $('.per_warn_text');
		perlog_warn_text.text('');
		if(per_log_username == '' || per_log_pw == '')
		{
			perlog_warn_text.text('Sorry, invalid username or password. Please try again.');
		}
		else
		{
			if($('#per_log_rememberme').attr("checked")==true)
			{
				var per_log_rememberme = 1;
			}
			else
			{
				var per_log_rememberme = 0;
			}
			var per_to_url = $('#per_to_url').val();
			$.ajax({
		    type:"POST",
		    url:"/site/ajax_login",
		    data:{'username':per_log_username,'pw':per_log_pw,'rememberme':per_log_rememberme},
		    success:function(re){
		    	re=parseInt(re);
				switch(re)
				{
					//个人用户已登录
					case 0:
						perlog_warn_text.text('');
						if(typeof fun == 'function'){
							fun();
						}else if(per_to_url)
						{
							window.location = per_to_url;
						}
						else
						{
							window.location.reload();
						}
						break;
					//用户名或者密码为空
					case 1:
						perlog_warn_text.text('Sorry, invalid username or password. Please try again.');
						break;
					//禁整站
					case 2:
						perlog_warn_text.text('Your account has been blocked due to posts containing spam, advertising or inappropriate content.');
						break;
					//未激活
					case 3:
						perlog_warn_text.text('');
						window.location.href = '/site/unactive';
						break;
					//被删除
					case 4:
						locat=location.host.split('.');
						locat.shift();
						perlog_warn_text.html('The account does not exist or has been deleted. Please <a href="http://www.'+locat.join('.')+'/register">register </a> a new account or <a href="http://www.'+locat.join('.')+'/index.php/main/contact/contact_us/customer_service/Personal?lang=en">send a feedback</a> to us.');
						$('#per_log_username').val('');
						$('#per_log_pw').val('')
						//perlog_warn_text.text('Sorry, but this user has been deleted.');
						break;
					//企业未激活
					case 5:
						perlog_warn_text.text('');
						window.location.href = '/site/companyatt';
						break;
					//登录成功
					case 6:
						perlog_warn_text.text('');
						if(typeof fun == 'function'){
							fun();
						}else if(per_to_url)
						{
							if(per_to_url == 'classifides'){
								easyDialog.close();
								checkinfosubmit();
							}else{
								window.location = per_to_url;
							}
						}
						else
						{
							window.location.reload();
						}
						break;
					//用户名或密码无效
					case 7:
						locat=location.host.split('.');
						locat.shift();
						perlog_warn_text.html('Sorry, invalid username or password. Please try again.');
						$('#per_log_username').val('');
						$('#per_log_pw').val('')
						break;
					default:
						perlog_warn_text.text('Sorry, invalid username or password. Please try again.');
						break;
				}
		     }
		   });
		}

	}

	//切换显示
	function per_change_show(show,hide)
	{
		if(show == 'per_reg'){
			$.get('/utils/regAgain',function(m){
				if(m==1){
					location.href='http://www.echinacities.com/register';
				}
			})
		}
		$('.register-fframe-code-handler:last').click();
		$('.per_warn_text').text('');
		$('.'+hide).css('display','none');
		$('.'+show).css('display','block');
		per_login_reg_tip();
	}

	//关闭弹出层
	$('.per_login_reg_close').click(function(){
		 drop_chang_fun();
		 $('.per_log').css('display','none');
		 $('.per_reg').css('display','none');
		 easyDialog.close();
	});

    //登陆注册弹出层
	function per_login_reg_tip()
	{
		$('.register-fframe-code-handler').click();
		easyDialog.open({
          container : 'per_log_reg_tip'
         });
		if($('.tick-log-box').length > 0){
			$('.CtyGumap').append($('#overlay'))
			$('.CtyGumap').append($('#easyDialogBox'))
			$('#overlay').css({width: '950px',height: '600px',position: 'absolute'});
			$('#easyDialogBox').css({position: 'absolute'});
		}
	}

	//登录调用方法
	function per_login_div(per_to_url,action){
	      $.ajax({
			   type:"GET",
			   url:'/site/ajax_get_sess',
			   async: false,
			   data:null,
			   success:function(re){
			   if(re == 1)
			   {
				   if(per_to_url != 'classifides'){
					window.location = per_to_url;
					return;
				   }else{
					checkinfosubmit();
				   }
			   }
			   else
			   {
				   if(action == 'browse'){
					$(".log_browse_number").css('display','block');
					$(".per_login_reg_close").css('display','none');
				   }
					$('#per_to_url').val(per_to_url);
				    if(action=='reg'){
						per_change_show('per_reg','per_log');
					}else{
						per_change_show('per_log','per_reg');
					}
			   }
		}
		});
	}
	keypressFun=function(e){
		if(13 == e.keyCode)
		{
			per_ajax_login();
			if ( e && e.preventDefault )
			{
0202020202020202		e.preventDefault();
			}
			else
			{
				return false;
			}
		}
	};
	$('#per_log_pw')[0].onkeypress= keypressFun;
	/**
	回调注册成功后的函数
	*/
	function callback_login(fun){
		per_login_div()
		$('.inp_k2 a')[0].onclick=function(){
			per_ajax_login.call(this,fun);
		}
		$('#per_log_pw')[0].onkeypress=function(e){
			if(13 == e.keyCode)
			{
				per_ajax_login.call(this,fun);
				if ( e && e.preventDefault )
				{
	0202020202020202		e.preventDefault();
				}
				else
				{
					return false;
				}
			}
		};
	}

	/**
	清除回调函数
	*/
	function drop_chang_fun(){
		$('.inp_k2 a')[0].onclick=per_ajax_login
		$('#per_log_pw')[0].onkeypress= keypressFun
	}

$('#email_reu_back').val(location.href);
