   <script type="text/javascript">
//back to top
var toTop=$('.back_to_top');
$(window).scroll(function(){
    if($(window).scrollTop()> $('.job _header').height())
     toTop.fadeIn(800);
      else
      toTop.fadeOut(800);
});
toTop.click(function(){
      $(window).scrollTop(0)
      $(this).fadeOut(500);
});

