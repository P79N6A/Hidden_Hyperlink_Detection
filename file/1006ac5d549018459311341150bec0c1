	//下一张
	function dPlayNext()
	{
        var shortname = ".html";
        var npage = 4;
        var totalpage = 4;
		var namehand = '11654';
		var displaytype = 'st';
		var gtimer = null;
        if(npage==totalpage)
		{
			if(namehand!='') location.href = namehand+shortname;
				else location.href = "view.php?aid=11654";
		}
		else
		{
			if(displaytype=='st' && namehand!='') location.href = namehand+"_"+(npage+1)+shortname;
			else location.href = "view.php?aid=11654&pageno="+(npage-1);
		}

	}
