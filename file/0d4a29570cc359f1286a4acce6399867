  <script language="javascript" type="text/javascript">
    function redirecturl(url)
    {
        location=url;
    }
    function my_onload()
	{
		if(GetCookie("number")!=null)
		{
			document.getElementById("number").value=GetCookie("number");
			document.getElementById("docNumber").value=GetCookie("docNumber");
			document.getElementById("issuedProvince").value=GetCookie("issuedProvince");
	
		}
		remove_loading();	
	}
	function remove_loading() {
	   var targelem = document.getElementById('loader_container');
	   targelem.style.display='none';
	   targelem.style.visibility='hidden';
	   SetCookie("tishi",'1',360);
   }
	function remember()
	{
		SetCookie("number",document.getElementById("number").value);
		SetCookie("docNumber",document.getElementById("docNumber").value);
		SetCookie("issuedProvince",document.getElementById("issuedProvince").value);
	}
	function setIssuedProvince(p)
	{
		document.getElementById("issuedProvince").value=p;
	}
	function checkinfo(){
		if(document.getElementById("number").value.length!=18){
			alert("请输入18位驾驶证号码！");
			return false;
		}
		if(document.getElementById("docNumber").value.length<10){
			alert("请输入正确的驾驶证档案编号！");
			return false;
		}
		if(document.getElementById("issuedProvince").value == ""){
			alert("请选择发证地所在省份！勿必正确选择，否则查询结果将不正确！");
			return false;
		}
		document.getElementById('provinceid').value=getprovinceid(document.getElementById("issuedProvince").value);
		SetCookie("number",document.getElementById("number").value);
		SetCookie("docNumber",document.getElementById("docNumber").value);
		SetCookie("issuedProvince",document.getElementById("issuedProvince").value);
		return true;
	}
	function getprovinceid(p)
	{
		switch(p)
		{
			case '北京':
				return 11;
			case '上海':
				return 31;
			case '天津':
				return 12;
			case '重庆':
				return 50;
			case '河北':
				return 13;
			case '山西':
				return 14;
			case '内蒙古':
				return 15;
			case '辽宁':
				return 21;
			case '吉林':
				return 22;
			case '黑龙江':
				return 23;
			case '江苏':
				return 32;
			case '浙江':
				return 33;
			case '安徽':
				return 34;
			case '福建':
				return 35;
			case '江西':
				return 36;
			case '山东':
				return 37;
			case '河南':
				return 41;
			case '湖北':
				return 42;
			case '湖南':
				return 43;
			case '广东':
				return 44;
			case '广西':
				return 45;
