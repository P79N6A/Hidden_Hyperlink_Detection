  <script>	
	$(function(){
	//移入显示二维码
		$(".phone").on("mouseenter",function(){
			$(this).parent(".load_href").siblings(".Code").css('display','block');			
		})
		$(".phone").on("mouseleave",function(){
			$(this).parent(".load_href").siblings(".Code").css('display','none');		
		})
		setNavStyle();		
		function setNavStyle(){
			//设置默认加载项
			$(".nav li").eq(0).addClass("on").siblings().removeClass("on");
			var curIndex = 0;	
			var urlList=[
                                           "index",				
				"NewPcGame",
				"MobileGame",
				"MatchActive",
				"GM"
			];
			//全部转成小写
			var getUrl=window.location.href.toLocaleLowerCase();
			var newUrl=getUrl.split('/')[3];
			var len = $(".nav li").length;	
			if(newUrl!= ""){
				if((newUrl =="" || newUrl.indexOf("shtml") >= 0)){				
					$(".nav li").eq(0).addClass("on").siblings().removeClass("on");
				}else{	
					for(var i=0;i < len;i++){		
						curIndex = i;
						//转成小写比较										
						if(getUrl.indexOf(urlList[i].toLocaleLowerCase()) >= 0){				
							$(".nav li").eq(curIndex ).addClass("on").siblings().removeClass("on");
							return;
						}							
					} 
				}
			}else{
				$(".nav li").eq(0).addClass("on").siblings().removeClass("on");
			}
			
			
			
		}
		
		
	});
	


	</script> 
