  <script>
	$(function(){
    'use strict';
    var clone = $(".move_total li").first().clone();
    
    $(".move_total").append(clone);
    
    var i = 0;
    var len = $(".move_total li").size();
    var spanArr = [];
    
    for(var j = 1; j < len; j++){
        var spans = '<span></span>';
        spanArr.push(spans);
    }
    
    $(".move_index").html(spanArr.join(""));
    
    $(".move_index span").first().addClass("active");
    
    function moveR(){
        i++;
        if(i == len){
            $(".move_total").css({left:0});
            i = 1;
        }
        $(".move_total").stop().animate({left:-i*737},500);
        
        if( i == len - 1){
            $(".move_index span").eq(0).addClass("active").siblings().removeClass("active");
        }else{
            $(".move_index span").eq(i).addClass("active").siblings().removeClass("active");
        }       
    }
    
    $(".move_index span").hover(function(){
        var index = $(this).index();
        i = index;
        $(".move_total").stop().animate({left:-i*737},500);
        $(this).addClass("active").siblings().removeClass("active");
    });
    
    var t = setInterval(function(){
        moveR();
    },2500);
    
    $(".move").hover(function(){
        clearInterval(t)},function(){
        t = setInterval(function(){
            moveR();
        },2500);
    });
    
    //tv
    (function(){
        var hide = $(".botton").siblings(".hide");
        var ul = hide.find(".broadcast_l");
        var lis = ul.find("li");
        var left = parseInt(ul.css("left"),10);
        
        $(".btn_1").on("click",function(){        
        if(left >= 0){
            return;
        }
        left += 248;
        ul.animate({left:left + 'px'},200);        
    });

    $(".btn_2").on("click",function(){    
        if(lis.size() <= 4){
            return;
        }
        var minleft = -(lis.size() - 4) * 248;
        if(left <= minleft){
            return;
        }
        left -= 248;
        ul.animate({left:left+'px'},200);       
    });
    })();
});


        $(".nav li").on("click",function(){
		   $(this).addClass("on").siblings().removeClass("on");
		})
        
        
        //nav hover
        function Change(clas, icon_c, icon_n){
        $(clas).on("mouseenter",function(){            
			$(this).children("span").children("img").attr("src",icon_c);
			$(this).find("p").css("color","#dbdada");                                        
        });
        
        $(clas).on("mouseleave",function(){            $(this).children("span").children("img").attr("src",icon_n);
        $(this).find("p").css("color","#fff");             
        });
        }
        Change(".home a", "/Files/New51v/shouye.png", "/Files/New51v/shouye_n.png");
        Change(".pc_game a", "/Files/New51v/pcyouxi.png", "/Files/New51v/pcyouxi_n.png");
        Change(".phone_game a", "/Files/New51v/shoujiyhouxi.png", "/Files/New51v/shoujiyhouxi_n.png");
        Change(".activities a", "/Files/New51v/saishihuodong.png", "/Files/New51v/saishihuodong_n.png");
        Change(".gm a", "/Files/New51v/kefuhuodong.png", "/Files/New51v/kefuhuodong_n.png");
        
        //Hall download
        $(".download a img").hover(function(){
            $(this).attr("src","/Files/New51v/jinru.btn.up.png");
        },
          function(){
            $(this).attr("src","/Files/New51v/jinru.btn.png"); 
        });
        
        $(".download a img").on("mousedown",function(){
            $(this).attr("src","/Files/New51v/jinru.btn.down.png");
        });
        
        $(".download a img").on("mouseup",function(){
            $(this).attr("src","/Files/New51v/jinru.btn.up.png");
        })
        
        //phone download
        function downLoad(a,b,c){
            $(a).hover(function(){
            $(this).children("img").attr("src",b);
        },function(){
            $(this).children("img").attr("src",c);
        })
        }
        downLoad(".anzhuo","/Files/New51v/anzhuo_up.png","/Files/New51v/anzhuo.png")
        downLoad(".apple","/Files/New51v/apple_up.png","/Files/New51v/apple.png")
        downLoad(".t","/Files/New51v/zzfw.btn.down.png","/Files/New51v/zzfw.btn.png")
        downLoad(".d","/Files/New51v/zxzx.btn.down.png","/Files/New51v/zxzx.btn.png")
        
        //color
        $(".game_info li a").hover(function(){
            $(this).children("p").css("color","#dbdada");
        },function(){
            $(this).children("p").css("color","#333");
        })
        //wechat icon
        $(".vfengxia").on("mouseenter", function() {
            $(".v_block").css("display", "block");
        });
        $(".vfengxia").on("mouseleave", function() {
            $(".v_block").css("display", "none");
        });
    </script>  
