        $(function () {
            var sw = 1;
            var sumCount = jQuery("#featuredWindowCount").val();
            $(".introAD .num a").click(function () {
                sw = $(".num a").index(this);
                myShow(sw)
            });
            function myShow(i) {
                $(".introAD .num a").eq(i).addClass("hover").siblings("a").removeClass("hover");
                $(".introAD ul li").eq(i).stop(true, true).fadeIn(600).siblings("li").fadeOut(600)
            }
            $(".introAD").hover(function () {
                if (myTime) {
                    clearInterval(myTime)
                }
            },
function () {
    myTime = setInterval(function () {
        myShow(sw);
        sw++;
        if (sw == sumCount) {
            sw = 0
        }
    },
3000)
});
