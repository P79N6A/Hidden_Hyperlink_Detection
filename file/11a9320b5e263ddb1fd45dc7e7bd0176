       <script>
        $('#game-android table tr:first').addClass('pophover');
        $('#game-apple table tr:first').addClass('pophover');
        $('#game-android table tr').hover(function(){
            $('#game-android table tr').removeClass('pophover');
            $(this).addClass('pophover');
        },function(){
            $(this).removeClass('pophover');
            $('#game-android table tr:first').addClass('pophover');
        });

        $('#game-apple table tr').hover(function(){
            $('#game-apple table tr').removeClass('pophover');
            $(this).addClass('pophover');
        },function(){
            $(this).removeClass('pophover');
            $('#game-apple table tr:first').addClass('pophover');
        });

        $('#game-android .tables tr:eq(0) td:first span').css('background-color','#ff3a20');
        $('#game-android .tables tr:eq(1) td:first span').css('background-color','#ff7000');
        $('#game-android .tables tr:eq(2) td:first span').css('background-color','#f0ad4e');

        $('#game-apple .tables tr:eq(0) td:first span').css('background-color','#ff3a20');
        $('#game-apple .tables tr:eq(1) td:first span').css('background-color','#ff7000');
        $('#game-apple .tables tr:eq(2) td:first span').css('background-color','#f0ad4e');

        $(".go_more .fa-android").bind('mouseover',function(){
            $("#game-android").addClass('active');
            $("#game-apple").removeClass('active');
            $(".go_more i").css('color','#ddd');
            $(this).css('color','#73D500');
        });
        $(".go_more .fa-apple").bind('mouseover',function(){
            $("#game-android").removeClass('active');
            $("#game-apple").addClass('active');
            $(".go_more i").css('color','#ddd');
            $(this).css('color','#46A0F4');
        });

    </script> 
      </div> 
     </div> 
