     <hgroup> 
      <h1>滴滴顺风车司机强奸女乘客被诉</h1> 
     </hgroup> 
     <script>
			    var str="滴滴顺风车司机强奸女乘客被诉";
			    var num=str.length;
			    if(num<=19)
			       {$("h1").attr("class","f_size1");}
			    else if(num<=23 && num>19)
			     {$("h1").attr("class","f_size2");}
