function get_visit_count(siteid,textid,from,to,day_week,num,is_radom)
{
 var random_num=Math.floor(Math.random()*1000);
 post_url="/top/day.xml?a="+random_num;
 
 jQuery.ajax({  
      type: "GET",  
      url: post_url,  
      dataType: "xml",  
      success: function (ResponseText) {  
             var showtext="";
var i=1;
var list=1;
       jQuery(ResponseText).find('record').each(function () {  
//alert("i:"+i+"from:"+from+"to:"+to+"list:"+list);
if(i>=from && i<=to && list<=num)
{
        var counts = jQuery(this).find('counts').text();  
        var c_url = jQuery(this).find('c_url').text();  
        var c_name = jQuery(this).find('c_name').text();  
        showtext+="<li><a href='"+c_url+"' target=\'_blank\' title=\'"+c_name+"\'>"+c_name+"</a></li>";
i++;
list++;
}
else
{
i++;
}
       }) 
       jQuery(textid).append(showtext);
      }  
     });  
}
