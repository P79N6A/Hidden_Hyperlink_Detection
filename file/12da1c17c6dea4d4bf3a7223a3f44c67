  <script type="text/javascript">
    function getCookie(name) {
        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");

        if (arr = document.cookie.match(reg)) {

            return unescape(arr[2]);
        } else {
            return null;
        }
    }
    function getQueryString(name) {
        //截取url的数据
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return decodeURIComponent(r[2]); return null;
    }
    //检测设备并跳转
    var domain = 'http://m.cyzone.cn';
    if (!getCookie('MRGCY_show_pc')) {
        if (device.mobile() || device.ipad() || device.iphone() || device.android()) {
            window.stop();

            if (window.location.pathname == '/') {
                if (window.location.hostname == "kuailiyu.cyzone.cn") {
                    window.location.href = domain + '/category/7/';
                } else {
                    window.location.href = domain;
                }
            } else if( window.location.pathname.indexOf('/product/') != -1 ) {
                
                var path = window.location.pathname;
                var arr = path.split('/product/');

                if( !isNaN( parseInt( arr[1] ) ) ){
                    window.location.href = 'http://shop.cyzone.cn/?promoter='+(getQueryString('promoter') || 1)+'#/product/' + arr[1].replace('.html','');
                }else{
                    window.location.href = 'http://shop.cyzone.cn/?promoter='+(getQueryString('promoter') || 1);
                }
            } else {
                window.location.href = domain + window.location.pathname;
            }
        }
    }
    </script> 
