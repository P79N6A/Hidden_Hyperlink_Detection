             <td width="30" height="80" align="center" valign="middle"><a href="javascript:void(0);" onclick="scroLeft();return false"><img src="/Public/keche/images/left.png" width="9" height="16" /></a></td> 
             <td width="590" align="center" valign="middle">
              <div id="s_img" style="width:590px;overflow:scroll;overflow-y:hidden;overflow-x:hidden;text-align:center"> 
               <table border="0" align="center" cellpadding="0" cellspacing="0"> 
                <tbody>
                 <tr> 
                  <td align="center" valign="middle"> <img id="ss0" src="http://img.chinacar.com.cn/spic/t2s51182830.jpg" width="68" height="51" onmouseover="ChangeImg(0)" style="cursor:pointer;margin:5px" /> </td>
                  <td align="center" valign="middle"> <img id="ss1" src="http://img.chinacar.com.cn/spic/t2s51182831.jpg" width="68" height="51" onmouseover="ChangeImg(1)" style="cursor:pointer;margin:5px" /> </td>
                  <td align="center" valign="middle"> <img id="ss2" src="http://img.chinacar.com.cn/spic/t2s51182832.jpg" width="68" height="51" onmouseover="ChangeImg(2)" style="cursor:pointer;margin:5px" /> </td> 
                 </tr> 
                </tbody>
               </table> 
              </div></td> 
             <td width="30" align="center" valign="middle"><a href="javascript:void(0);" onclick="scroRight();return false"><img src="/Public/keche/images/right.png" width="9" height="16" /></a></td> 
            </tr> 
           </tbody>
          </table> 
          <script>
                            var indexCount=2;
                            var timer=null;
                            var offset=3000;
                            var index=0;
                            function ChangeImg(id)
                            {
                                for(var l=0;l<=indexCount;l++)
                                {
                                    var e=document.getElementById("d"+l);
                                    var f=document.getElementById("ss"+l);
                                    if(l==id)
                                    {
                                        e.style.display="";
                                        f.className='thumbs_img_current';
                                    }
                                    else
                                    {
                                        e.style.display="none";
                                        f.className='thumbs_img';
                                    }
                                }
                            }
                            function NextButton(but)
                            {
                                if(timer){clearTimeout(timer);}
                                if(but=='big_play_prev')
                                {
                                    index--;
                                    if(index<0)
                                    {
                                        scroRight();
                                        index=indexCount;
                                    }
                                    if(index<3)
                                    {
                                        scroLeft();
                                    }
                                }
                                else
                                {
                                    index++;
                                    if(index>indexCount)
                                    {
                                        scroLeft();
                                        index=0;
                                    }
                                    if(index>6)
                                    {
                                        scroRight();
                                    }
                                }
                                ChangeImg(index);
                                timer=window.setTimeout(autoChangeImg,offset);
                            }
                            function autoChangeImg()
                            {
                                index++;
                                if(index>indexCount)
                                {
                                    scroLeft();
                                    index=0;
                                }
                                if(index>6)
                                {
                                    scroRight();
                                }
                                ChangeImg(index);
                                timer=window.setTimeout(autoChangeImg,offset);
                            }
                            window.onload=function(){autoChangeImg();}
                            function scroRight()
                            {
                                var obj=document.getElementById("s_img");
                                obj.scrollLeft+=parseInt(obj.style.width);
                            }
                            function scroLeft()
                            {
                                var obj=document.getElementById("s_img");
                                obj.scrollLeft-=parseInt(obj.style.width);
                            }
                        </script> 
          <!--图片切换--> 
         </div></td> 
       </tr> 
      </tbody>
     </table> 
