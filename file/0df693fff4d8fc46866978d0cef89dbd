  <script language="javascript">
    //获取cookie
    function getSiteGGCookie1(c_name){
        if (document.cookie.length>0){
            c_start=document.cookie.indexOf(c_name + "=");
            if (c_start!=-1){
                c_start=c_start + c_name.length+1;
                c_end=document.cookie.indexOf(";",c_start);
                if (c_end==-1) c_end=document.cookie.length;
                return unescape(document.cookie.substring(c_start,c_end));
            }
        }
        return "";
    }
    //设置cookie
    function setSiteGGCookie1(c_name,value,expirehours){
        var exp = new Date();
        exp.setTime(exp.getTime() + expirehours*60*60*1000);
        document.cookie=c_name+ "=" +escape(value)+";path=/;"+((expirehours==null) ? "" : "expires="+exp.toGMTString())+";";
    }
    //关闭内容
    function Close_Site_GG_1(){
        $(".left_pouple_box").animate({left:"-508"},600);
        setSiteGGCookie1("ck_gg_1","y",2);
    }

    var ck_gg_1=getSiteGGCookie1('ck_gg_1');
    if (ck_gg_1=="y"){
        //关闭
        //$(".left_pouple_box").animate({left:"-508"},600);
    }else{
        //从左侧滑出
        $(".left_pouple_box").animate({left:"2"},1000);
    }
</script>  
