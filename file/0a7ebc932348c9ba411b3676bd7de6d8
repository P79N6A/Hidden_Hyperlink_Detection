// 关键词排名分布
function divide(a,b){
	if(b == 0 || b == undefined){
		return 0;
	}
	var x=0;
	x=a/b*100;
	return x.toFixed(2);  
}
function toggleId(id){
	$("#" + id ).toggle()
}

