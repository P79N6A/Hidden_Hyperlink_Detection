				$('#baidu_shoulu_7days').html('<a href="http://www.baidu.com/s?wd=site%3A'+domain+'&rn=10&lm=7&gpc=stf%3D'+(now-604800)+','+now+'|stftype%3D1" class="red" target="_blank"><span class="red">'+data.baidu_7days+'</span></a>');
