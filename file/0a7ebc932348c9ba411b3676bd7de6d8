			$("#f_9").html('<b>'+json['mWL'][5]+'</b><i><em>'+divide(json['mWL'][5],total)+'%</em></i>');
			if(divide(json['mWL'][1],total) < 20){$("#f_5 i").addClass("red")}else{$("#f_5 i").addClass("green")}
			if(divide(json['mWL'][2],total) < 20){$("#f_6 i").addClass("red")}else{$("#f_6 i").addClass("green")}
