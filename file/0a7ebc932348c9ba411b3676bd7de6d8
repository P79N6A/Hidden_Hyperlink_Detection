	$('.intro').slideUp();
	$.cookie('baidurankIntro', 1, {path: '/', domain: '.aizhan.com',expires:60});
});

var j = 0;
$('#box_brothersites a').each(function(i) {
	if ($(this).attr('rel') == 'currentsite') {
		j = i;
	}
});
if (j > 0) {
	$('#box_brothersites').scrollTop(j * 28);
}	
function check_form() {
	var obj = document.getElementById('site')
			msg = $('.total'),
			query_domain = url2domain(obj.value),
			type = document.getElementById('ob').value;
	if(query_domain){
		if(isDomain(query_domain)){
			$.cookie('baidurankform', 1, {path: '/', domain: '.aizhan.com'});
			if(type=='position'){
				location.href = '/mobile/'+query_domain+'/';
			}else{
				location.href = '/mobile/'+query_domain+'/-1/0/1/'+type+'/';
			}
		}else{
			alert('域名格式错误！');
			obj.focus();
		}
	}else{
		msg.html('请输入要查询的域名！');
		obj.focus();
	}
	return false;
}
var obj = document.getElementById('site'),
		query_domain = url2domain(obj.value);

if(query_domain){
	if(isDomain(query_domain)){
		addAllSites(query_domain);
	}
}
