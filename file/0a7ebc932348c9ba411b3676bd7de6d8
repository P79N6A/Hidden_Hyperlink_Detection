$.ajax({
	type:"get",
	url:"http://baidurank.aizhan.com/api/wordlocation",
	data:{domain:domain,ajaxKey:'f640dce47b70aaf3905427bb767609fb',rn:'1491060824'},
	dataType: "json",
	success: function(json){
			var total = json['pcWL'][1]+json['pcWL'][2]+json['pcWL'][3]+json['pcWL'][4]+json['pcWL'][5];
			$("#cc1").html(total);
			$("#f_0").html('<b>'+json['pcWL'][1]+'</b><i><em>'+divide(json['pcWL'][1],total)+'%</em></i>');
			$("#f_1").html('<b>'+json['pcWL'][2]+'</b><i><em>'+divide(json['pcWL'][2],total)+'%</em></i>');
			$("#f_2").html('<b>'+json['pcWL'][3]+'</b><i><em>'+divide(json['pcWL'][3],total)+'%</em></i>');
			$("#f_3").html('<b>'+json['pcWL'][4]+'</b><i><em>'+divide(json['pcWL'][4],total)+'%</em></i>');
			$("#f_4").html('<b>'+json['pcWL'][5]+'</b><i><em>'+divide(json['pcWL'][5],total)+'%</em></i>'); 
			if(divide(json['pcWL'][1],total) < 20){$("#f_0 i").addClass("red")}else{$("#f_0 i").addClass("green")}
			if(divide(json['pcWL'][2],total) < 20){$("#f_1 i").addClass("red")}else{$("#f_1 i").addClass("green")}
			if(divide(json['pcWL'][3],total) < 20){$("#f_2 i").addClass("red")}else{$("#f_2 i").addClass("green")}
