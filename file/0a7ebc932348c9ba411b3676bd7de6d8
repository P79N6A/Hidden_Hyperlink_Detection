				$('#baidu_shoulu_all').html('<a href="http://www.baidu.com/s?wd=site%3A'+domain+'" target="_blank"><span class="red">点击查询</span></a>');
			}
			var now = Math.round(new Date().getTime()/1000);
			if (data.baidu_1days != undefined && data.baidu_1days != '-1') {
				$('#baidu_shoulu_1days').html('<a href="http://www.baidu.com/s?wd=site%3A'+domain+'&rn=10&lm=1&gpc=stf%3D'+(now-86400)+','+now+'|stftype%3D1" class="red" target="_blank"><span class="red">'+data.baidu_1days+'</span></a>');
