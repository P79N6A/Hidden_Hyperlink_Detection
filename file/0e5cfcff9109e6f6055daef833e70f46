data:{'deletesubmit':'true', 'formhash':'b5e5a171'},
dataType:'xml',
})
.success(function(s) {
window.location.reload();
})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
});
return false;
});
</script> 
