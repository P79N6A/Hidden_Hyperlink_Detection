	<script type="text/javascript">
		function shareInit(e){var t=function(t,r){this.elem=e(t),this.options=r};t.prototype={constructor:t,toggle:function(){return this[this.isShown?"hide":"show"]()},show:function(){var t=this,r=e.Event("show");t.elem.trigger(r),t.isShown||r.isDefaultPrevented()||(t.isShown=!0,t.elem.show().trigger("shown"),"click"==t.options.trigger&&setTimeout(function(){e(document).on("click.overlay.d",function(r){var a=e(r.target).closest(t.elem);0==a.length&&a!=t.elem&&(t.hide(),e(document).off("click.overlay.d"))})},0))},hide:function(t){t&&t.preventDefault();var r=this;t=e.Event("hide"),r.elem.trigger(t),r.isShown=!1,r.elem.hide().trigger("hidden"),e(document).off("click.overlay.d")}},e.fn.overlay=function(r){return this.each(function(){var a=e(this),n=a.data("overlay"),o=e.extend({},e.fn.overlay.defaults,a.data(),"object"==typeof r&&r);n||a.data("overlay",n=new t(this,o)),"string"==typeof r?n[r]():o.show&&n.show()})},e.fn.overlay.Constructor=t,e.fn.overlay.defaults={show:!0},e(function(){e(document).on("click.overlay.data-api",'[data-trigger="click"][data-toggle="overlay"]',function(){var t,r=e(this),a=e(r.attr("data-target")||(t=r.attr("href"))&&t.replace(/.*(?=#[^\s]+$)/,"")),n=a.data("overlay")?"toggle":e.extend({},a.data(),r.data());a.overlay(n)}),e(document).on("hover.overlay.data-api",'[data-trigger="hover"][data-toggle="overlay"]',function(t){var r,a,n=e(this),o=e(n.attr("data-target")||(r=n.attr("href"))&&r.replace(/.*(?=#[^\s]+$)/,"")),i=function(){a=o.data("overlay"),a&&(a._hoverTimer=setTimeout(function(){o.overlay("hide")},50))};switch(t.type){case"mouseenter":n.data("timer",setTimeout(function(){o.overlay("show"),a=o.data("overlay"),a.hasHoverEvent||(o.hover(function(){a._hoverTimer&&clearTimeout(a._hoverTimer)},i),a.hasHoverEvent=!0)},n.attr("data-delay")||1));break;case"mouseleave":clearTimeout(n.data("timer")),i()}})});var r=function(t,r){var a=this;a.$elem=e(t),a.$weixin=e(".share-weixin",a.$elem),a.$weibo=e(".share-weibo",a.$elem),a.$qzone=e(".share-qzone",a.$elem),a.$friend=e(".share-friend",a.$elem),a.$qq=e(".share-qq",a.$elem),a.options=r,a.init()};r.prototype={constructor:r,init:function(){var t=this;t.$weixin.click(function(t){bShare.share(t.originalEvent,"weixin",0);var r=e("#bsWXBox"),a=e(".bsTop span",r);return a.eq(0).html("扫描分享到微信好友或微信朋友圈"),a.eq(0).css("marginLeft",8),a.eq(1).html(""),!1}),t.$weibo.click(function(e){return bShare.share(e.originalEvent,"sinaminiblog",0),!1}),t.$qzone.click(function(e){return bShare.share(e.originalEvent,"qzone",0),!1}),t.$friend.click(function(t){bShare.share(t.originalEvent,"weixin",0);var r=e("#bsWXBox"),a=e(".bsTop span",r);return a.eq(0).html("扫描分享到微信好友或微信朋友圈"),a.eq(0).css("marginLeft",8),a.eq(1).html(""),!1}),t.$qq.click(function(e){return bShare.share(e.originalEvent,"qqim",0),!1}),e(document).click(function(t){0==e(t.target).closest("#bsWXBox").length&&e("#bsWXBox").hide()})}},e.fn.share=function(t){return this.each(function(){var a=e(this),n=a.data("share"),o=e.extend({},e.fn.share.defaults,a.data(),"object"==typeof t&&t);n||a.data("share",n=new r(this,o)),"string"==typeof t?n[t]():o.show&&n.show()})},e.fn.share.Constructor=r};shareInit($);$("#share_pop").share();$("#share1").share();
	</script>

	<script>
		$(function() {
			//修正引用定位楼层问题 zouxh add at 20140224
			$('.relyhfcon').find("p:first>a:last").live('click',function() {
				var href = $(this).attr('href');
				var m = href.match(/(\d+).html#(\d+)/i);
				var floor = parseFloat(m[2]);
				var p = parseInt(m[1]);
				var val = Math.ceil(floor / 20);
				if (val == p)
					return true;
				else {
					$(this).attr('href', href.replace(p + ".html", val + ".html"));
					return true;
				}
			});

			//最终页内容中除楼层锚点链接在本窗口打开，其他链接均在新窗口中打开 zouxh add at 20140228
			$('div[xname] a').live('click', function() {
				var href = $(this).attr('href')||'';
				href = href.replace("http://club.autohome.com.cn","");
				if (href.length == 0 || /javascript:/i.test(href)) return false;
				if (/^(?:\/bbs\/thread-\w-\d+-\d+-\d+\.html)?#(?:\d+)?$/i.test(href)) {
					window.location.href=href; //当前页楼层跳转后，触发tz.autoLift();
					tz.autoLift();
					return false;
				}
				$(this).attr('target', '_blank');
				return true;
			});
			// 奥运会活动
		});

		//返回顶部悬浮框js
		$(function () {
			var gotop = $("#gotop");
			if (gotop.length == 0) return;
			var ie6 = $.browser.msie && (parseFloat($.browser.version || 6) < 7);
			var $window = $(window);
			if (ie6) {
				var $gotop1 = $('#gotop1').css({ position: "absolute" });
				gotop.css("visibility", "hidden").css("display", "");
				$window.bind("scroll touchend", function () {
					var st = $window.scrollTop();
					if (st > 200) gotop.css("visibility", "visible");
					else { gotop.css("visibility", "hidden").css("display", ""); }
					$gotop1.css({ 'bottom': '', 'top': st + $window.height() - $gotop1.outerHeight() - 10 });
				});
			}
			else {
				$window.bind("scroll touchend", function () {
					$window.scrollTop() > 300 ? gotop.fadeIn(300) : gotop.fadeOut(200);
				});
			}

			$('#gotop1 a.menu-list').live('mouseover', function () {
				$(this).next().show().one('mouseover', function () {
					$(this).data('overin', 1).one('mouseleave', function () {
						$(this).hide().removeData('overin');
					});
				});
			}).live('mouseleave', function () {
				var $pop = $(this).next();
				setTimeout(function () {
					$pop.toggle($pop.data('overin') == 1);
				}, 100);
			});
		});


		(function(){
			var _tm=null;
			$('div.lv-icon').mouseover(function(){ var k=$(this);window.clearTimeout(_tm); _tm=window.setTimeout(function(){k.find('.lv-tips').show(150);},500)}).mouseout(function(){
				var k=$(this); window.clearTimeout(_tm);_tm=window.setTimeout(function(){k.find('.lv-tips').hide(300);},300)});
		})();


		if(document.getElementsByName("clubvideo").length>0){
			window.__pv_video = new Image().src="http://pv.alert.autohome.com.cn/click_count.php?val=1&eid=1|3|47|0|0|300022";
		}

		var _icar = Z.getCookie('tip-_icar');
		if (_icar != 1) {$("#tip-_icar_c").show();}
		function _icarHide() {
			$("#tip-_icar_c").hide();
			Z.setCookie("tip-_icar", 1, 999);
		}
	</script>
