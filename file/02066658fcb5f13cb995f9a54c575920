<script type="text/javascript">
	(function() {
		var open = 0;
		var task = new HelpRdTask();
		task.Show('任务卡', open, 2, autohome.loginUser.UID, 'DivIdHrml');
		var clock;
		function mouseout() {
			clock = setTimeout(function () { $('#DivIdHrml .wrapper').fadeOut(); }, 5000);
		}

		$('#DivIdHrml').on('mouseover', function () {
			if (clock) {
				clearTimeout(clock);
			}
		})
		.on('mouseout', mouseout);
	})();
	(function() {
		/*判断浏览器版本是否过低*/ 
		var appName = navigator.appName; 
		var appVersion = navigator.appVersion; 
		var version = appVersion.split(";"); 
		var trimVersion = version[1].replace(/[ ]/g, ""); 
		if (appName == "Microsoft Internet Explorer") { 
			/*如果是IE6或者IE7*/ 
			if (trimVersion == "MSIE7.0" || trimVersion == "MSIE6.0") {
				$("#public_top").css("height", "68px");
				$(".content").css("padding-top", "73px");
				$("#ieUpdateAlert").show();
			} 
			$("#ieUpdateAlert i.icon16-close").click(function() {
				$("#ieUpdateAlert").hide();
				$("#public_top").css("height", "36px");
				$(".content").css("padding-top", "42px");
			});
		}
	})();
</script>

<script type="text/javascript">
    $("#shangWrapper").size() && (function ($) {
        $('#zan-tip').on({
            'mouseenter': function() {
                $(this).find('.pop').show();
            },
            'mouseout': function() {
                $(this).find('.pop').hide();
            }
        });
        var $mask = $('#AutodisShow');
        var $shangWrapper = $("#shangWrapper");
        var $shangPop = $("#shang-pop");
        var $shangAlert = $("#shang-alert");
        var $shanglist = $shangWrapper.find('.shang-list');
        var $btnShangOk = $("#btnShangOk");
        var $seeAll = $shangWrapper.find('.see-all');
        var meIsNotLouZhu = autohome.loginUser.UID != tz.topTopicOwnerId;
        var meIsLouZhu = !meIsNotLouZhu;
        var buNengShangMsg = $shangWrapper.attr("shang-msg");
        $shanglist.find('li').length < 21 ? $shanglist.removeClass('show1line') : ($shanglist.addClass('show1line'), $seeAll.show());
        $seeAll.click(function () {
            $(this).hasClass('see-part') ? ($shanglist.addClass('show1line'), $(this).removeClass('see-part').html('查看全部')) : ($shanglist.removeClass('show1line'), $(this).addClass('see-part').html('收起全部'));
        });
        var firstClick = true;
        var $shangInput = $shangPop.find('.shang-input input').on('input focus propertychange', function () {
            $(this).val().trim() === '' ? (function() {
                $shangPop.find('.placeholder').show();
                if (!$btnShangOk.hasClass("dis-zan")) {
                    $btnShangOk.addClass("dis-zan");
                }
            })() : $shangPop.find('.placeholder').hide();
        }).click(function () {
            if (firstClick && $(this).val() === "1.00") {
                $(this).val('').trigger("propertychange");
                firstClick = false;
            }
        });
        //赞赏点击
        var $shangBtn = $shangWrapper.find('.shang-btn').click(function () {
            if (!autohome.loginUser.isLogin) {
                return tz.checkLoginA(this);
            }
            _trackEvent && _trackEvent.push({ 'eid': (tz.fake_bbsid || tz.bbsid) == 200042 ? '1|3|247|1697|203361|300928' : '1|3|247|1696|203360|300927', 'val': 1 });
            if ($shangBtn.hasClass("cantshang")) {
                if (buNengShangMsg) {
                    $mask.show();
                    $shangAlert.show().find(".line1").text(buNengShangMsg);
                }
                else if ($shangBtn.shanged) {
                    $mask.show();
                    $shangAlert.show().find(".line1").text("您已经赞赏过该帖子！");
                }
                else if (meIsLouZhu) {
                    $mask.show();
                    $shangAlert.show().find(".line1").text("您无法赞赏自己的帖子！");
                }
                return false;
            }
            if (!$shangPop.find(".shang-box img.user-circle").attr("src")) {
                $shangPop.find(".shang-box img.user-circle").attr("src", $("#maxwrap-maintopic div.conleft p.imguser img").attr("src"));
            }
            $btnShangOk.removeClass("dis-zan");
            $mask.show();
            $shangPop.show();
            $shangInput.trigger("propertychange");
            $shangInput.focus();
            return false;
        });
        if (meIsLouZhu || buNengShangMsg) {
            $shangBtn.addClass("cantshang").attr("title", buNengShangMsg || "您无法赞赏自己的帖子！");
        }
        $shangPop.find('.public-layer-close').click(function () {
            $shangPop.hide();
            $mask.hide();
        });
        $shangAlert.find('.public-layer-close').click(function () {
            $shangAlert.hide();
            $mask.hide();
        });
        function validateShangInput(e) {
            if (!e.value) {
                if (!$btnShangOk.hasClass("dis-zan")) {
                    $btnShangOk.addClass("dis-zan");
                }
                return false;
            }
            var v = parseFloat(e.value || "");
            if (e.value && v > 200) {
                $shangPop.find(".money-tip").text("金额须在1~200元之间").show();
                if (!$btnShangOk.hasClass("dis-zan")) {
                    $btnShangOk.addClass("dis-zan");
                }
                return false;
            }
            else if (e.value && v < 1) {
                $shangPop.find(".money-tip").text("金额须在1~200元之间").show();
                if (!$btnShangOk.hasClass("dis-zan")) {
                    $btnShangOk.addClass("dis-zan");
                }
                return false;
            } else {
                $shangPop.find(".money-tip").text("").hide();
                if ($btnShangOk.hasClass("dis-zan")) {
                    $btnShangOk.removeClass("dis-zan");
                }
                return true;
            }
        }

        window.shangInputOnkeypress = function (e) {
            var dotIndex = e.value.trim().indexOf('.');
            var dotCount = e.value.trim().substring(dotIndex + 1).length;
            if (!e.value.match(/^[\+]?\d*?\.?\d*?$/) || (dotIndex != -1 && dotCount > 2)) {
                e.value = e.t_value || "";
            } else {
                if (dotIndex != -1 && dotCount == 0) {
                    e.t_value = e.value.trim().substring(0, dotIndex);
                    e.o_value = e.t_value;
                } else {
                    e.t_value = e.value.trim();
                }
            }
            if (e.value.match(/^(?:[\+]?\d+(?:\.\d+)?)?$/)) {
                e.o_value = e.value.trim();
            }
            validateShangInput(e);
        };
        window.shangInputOnkeyup = function (e) {
            shangInputOnkeypress(e);
            if (!e.value) {
                $shangInput.val('').trigger("propertychange");
            }
        };
        window.shangInputOnblur = function (e) {
            if (!e.value.match(/^(?:[\+]?\d+(?:\.\d+)?|\.\d*?)?$/)) {
                e.value = e.o_value;
            } else {
                if (e.value.match(/^\.\d+$/)) {
                    e.value = 0 + e.value;
                }
                if (e.value.match(/^\.$/)) {
                    e.value = 0;
                }
                e.o_value = e.value;
            }
        };
        window.shangHeadNotFind = function (img) {
            img.src = "http://x.autoimg.cn/club/v1Content/images/Detail/user120_default.gif";
        };
        function sendRequest(fun) {
            var data = { topicId: tz.topicId, reward: $shangPop.find(".shang-input input").val() };
            $.post("/ajax/ShangStart", data, function (result) {
                if (result && result.result) {
                    location.href = result.result;
                }
                else if (result && result.message) {
                    $shangPop.find(".money-tip").text(result.message).show();
                }
                fun && fun();
            });
        }
        $btnShangOk.click(function () {
            if (validateShangInput($shangPop.find(".shang-input input").get(0))) {
                $btnShangOk.addClass("dis-zan");
                sendRequest(function () {
                    $btnShangOk.removeClass("dis-zan");
                });
            }
        });
        $.get("/ajax/GetRewardList", { topicId: tz.topicId }, function (result) {
            if (result && result.list) {
                var meYiShang = false;
                if (result.count) {
                    var a = [];
                    for (var i = 0; i < result.list.length; i++) {
                        var item = result.list[i];
                        if (item.memberid == autohome.loginUser.UID) {
                            meYiShang = true;
                        }
                        if (!item.headpic) {
                            item.headpic = "http://x.autoimg.cn/club/v1Content/images/Detail/user120_default.gif";
                        }
                        a.push('<li><a href="http://i.autohome.com.cn/' + item.memberid + '" target="_blank" title="' + item.membername + '"><img src="' + item.headpic + '" onerror="shangHeadNotFind(this)" alt=""></a></li>');
                    }
                    $("#shangUsers").html(a.join(""));
                    $("#shangList").show();
                    if (meYiShang) {
                        $shangBtn.attr("title", "您已赞赏本帖");
                        $shangBtn.shanged = true;
                    }
                }
                if (!meYiShang && meIsNotLouZhu && !buNengShangMsg) {
                    $shangBtn.removeClass("cantshang");
                }

                $("#shangsum").text(result.sum);
                $("#shangUserCount").text(result.count);
            }
        });
    })($);
</script>


	</div>
	<script type="text/javascript">
		$("#headDiv").css("position", "relative");
		//tz.initAutoLoadImg();
