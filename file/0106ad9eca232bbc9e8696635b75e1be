var commentorder = 'new';
var fromindextop = "0";
var pageKey = 'comic_base';

// 收费js
function check_charge(cid) {
    var page = 1;
    if(cid > 0) {
        $.get('/ajax/check_charge.php', {'cid':cid,'rand':Math.random()}, function(res) {
            if(1 == res.code) {
                if(-1 == res.data || 0 == res.data) {
                    jump('/read/'+cid+'.html');
                }else if(0 < res.data) {
                    showmessage('啊哦...还需要<font color="#CC0000">'+res.data+'</font>个尚漫点阅读下面的章节，5秒后带你去<a href="http://home.ishangman.com/cp.php?ac=payonline">充值</a><br /><span>一次购买，永久阅读！</span>', 0);
                    delayJump('http://home.ishangman.com/cp.php?ac=payonline', 5000);
                }else {
                    log('请求出现异常');
                }
            }else if(2 == res.code) {
                showmessage('请先登录');
                openlogin();
            }else {
                showmessage(res.data);
            }
        }, 'json');
    }else {
        log('参数错误');
    }
}
