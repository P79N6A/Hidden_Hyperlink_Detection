    var scroll_Top;
    $(function() {
        $('.favbtn').on('click', function() {
            var obj = $(this);
            $.ajax({
                type: 'POST',
                url: obj.attr('href') + '&handlekey=favbtn&inajax=1',
                data: {
                    'favoritesubmit': 'true',
                    'formhash': '2016f602'
                },
dataType: 'html'
            }).success(function(s) {
                popup.open(wqXml(s));
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });

        $('.grades').on('click', function() {
            var obj = $(this);
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {},
dataType: 'html'
            }).success(function(s) {
$('#popup_grade').html(wqXml(s));
                if ($('#popup_grade #rateform').length) {
                    $('#popup_content').attr("style", 'height:' + ($(window).height()-50) + 'px').html(wqXml(s));
                    if ($('#popup_fullscreen').is(':hidden')) {
                        scroll_Top = $(window).scrollTop();
                        $('#popup_title').text("评分");
                        $('#popup_fullscreen').show();
                    }
                    $(".my_rate,#view_big,header,.page,.footer,.h50,#wq_sign_img,a[class='scrolltop']").hide();
                } else {
                    popup.open(wqXml(s));
                }
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.rate_viewratings').on('click', function() {
            var obj = $(this);
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {},
dataType: 'html'
            }).success(function(s) {
                $('#popup_content').attr("style", 'height:' + ($(window).height()-50) + 'px').html(wqXml(s));
                $('#popup_title').text("评分详情");
                $('#popup_fullscreen').show();
                var pid = obj.attr('pid');
                if (pid) {
                    $('.my_rate').attr('href', 'forum.php?mod=misc&action=rate&tid=2406238&pid=' + pid).show();
                    $('#rate_h50').show();
                }
                $("#view_big,header,.page,.footer,.h50").hide();
                scroll_Top = $(window).scrollTop();
            }).error(function(s) {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.recommend').on('click', function() {
            var obj = $(this);
            var handlekey = obj.attr('dataid');
