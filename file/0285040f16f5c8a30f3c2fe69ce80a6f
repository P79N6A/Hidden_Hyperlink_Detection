function checkform(){
	if (document.loginbox.userid.value.length ==0){
		alert("出错了，您没有填写用户名。");
		document.loginbox.userid.focus();
		return false;
	}
	
	if (document.loginbox.password.value.length==0){
		alert("出错了，您没有填写密码。");
		document.loginbox.password.focus();
		return false;
	}
	
	if (document.loginbox.yan.value.length==0){
		alert("出错了，验证码有误。");
		document.loginbox.yan.focus();
		return false;
	}
	//alert()
	var t1=parseInt(document.loginbox.yan.value.toString());
	var t2=yzz
	if (t1 != t2){
		alert("出错了，验证码有误!");
		document.loginbox.yan.focus();
		return false;
	}
	
	//return true;
}	
</script> 
