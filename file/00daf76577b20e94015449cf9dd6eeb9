     <div class="main-content-wrapper-inner"> 
      <section id="main-content"> 
       <div class="main"> 
        <div class="main-inner alpha grid_9"> 
         <div class="region region-content"> 
          <div id="block-system-main" class="block block-system  ">
           <div class="block-inner"> 
            <div class="content"> 
             <div about="/fungi-ergot.htm" typeof="sioc:Item foaf:Document" class="ds-2col-stacked node node-article view-mode-full clearfix"> 
              <div class="group-header"> 
               <div class="field field-name-title field-type-ds field-label-hidden"> 
                <div class="field-items"> 
                 <div class="field-item even" property="dc:title">
                  <h2>麦角</h2>
                 </div> 
                </div> 
               </div> 
              </div> 
              <div class="group-left"> 
               <div class="field field-name-detail-left-ad field-type-ds field-label-hidden"> 
                <div class="field-items"> 
                 <div class="field-item even">
                  <!-- BWW_categ_lifestyle_left_160x600 --> 
                  <div id="div-gpt-ad-1397848603376-26" style="width:160px; height:600px;"> 
                   <script type="text/javascript">
googletag.cmd.push(function() { googletag.display('div-gpt-ad-1397848603376-26'); });
</script> 
                  </div>
                 </div> 
                </div> 
               </div> 
              </div> 
              <div class="group-right"> 
               <div class="field field-name-lai-yuan field-type-ds field-label-inline clearfix"> 
                <div class="field-label">
                 来源:&nbsp;
                </div> 
                <div class="field-items"> 
                 <div class="field-item even">
                  博文网
                 </div> 
                </div> 
               </div> 
               <div class="field field-name-share-icons field-type-ds field-label-hidden"> 
                <div class="field-items"> 
                 <div class="field-item even">
                  <div class="shareButton" style="clear:both">
                   <!-- JiaThis Button BEGIN --> 
                   <div class="jiathis_style_24x24"> 
                    <a class="jiathis_button_weixin"></a> 
                    <a class="jiathis_button_tsina"></a> 
                    <a class="jiathis_button_tqq"></a> 
                    <a href="http://www.jiathis.com/share?uid=1895589" class="jiathis jiathis_txt jtico jtico_jiathis" target="_blank"></a> 
                    <a class="jiathis_counter_style"></a> 
                   </div> 
                   <script type="text/javascript">
