</script> 
                   <script type="text/javascript" src="http://v3.jiathis.com/code/jia.js?uid=1895589" charset="utf-8"></script> 
                   <!-- JiaThis Button END -->
                  </div>
                  <script type="text/javascript">
    // specify jiathis title and summary
    jiathis_config.title = jQuery(".quiz_block_content .content .title").text();
    if (jQuery("#quiz_score_possible_value").length > 0) {
        jiathis_config.summary = jQuery("#quiz_score_possible_value").text() + " " +
                    jQuery("#quiz_score_percent_value").text() + " " +
