  <script type="text/javascript">
			var IsMousedown,LEFT,TOP,lggood;
	document.getElementById("Mdown").onmousedown=function(e){
	lggood = document.getElementById("lggoodBox");
	IsMousedown = true;
	e = e||event;
	LEFT = e.clientX - parseInt(lggood.style.left);
	TOP = e.clientY - parseInt(lggood.style.top);
	document.onmousemove = function(e){
	e = e||event;
	if (IsMousedown){
	lggood.style.left = e.clientX - LEFT + "px";
	lggood.style.top = e.clientY - TOP + "px";
}

}
document.onmouseup=function(){
	IsMousedown=false;
}

}

function show_login(site) {
	if(site == 'sina') {
		//window.location = "/member.php?sina_login&forward="+encodeURIComponent(location.href);
	}else if(site == 'qq') {
		window.location = "http://fahao.78187.com/e/member/qq_api?a=qq_login&forward="+encodeURIComponent(location.href);
	}
}
</script> 
  <script>

jQuery(window).bind("scroll",function(){

 var scrolltop = $(document).scrollTop();

 if(scrolltop>0)

 {

	document.getElementById('backtop').style.display = 'block';  

 }

 else

 {

	document.getElementById('backtop').style.display = 'none';

 }



});

						



function showfmt()

{

	document.getElementById('sfmt').style.display = 'block';

	document.getElementById('hfmt').style.display = 'none';

}

function hidefmt()

{

	document.getElementById('hfmt').style.display = 'block';

	document.getElementById('sfmt').style.display = 'none';

}

</script> 
