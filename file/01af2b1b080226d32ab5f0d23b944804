       <script type="text/javascript">    //操作cookie
    function getCookie(name) {
        var strCookie = document.cookie;
        var arrCookie = strCookie.split("; ");
        for (var i = 0, len = arrCookie.length; i < len; i++) {
            var arr = arrCookie[i].split("=");
            if (arr[0] == name) {
                return decodeURIComponent(arr[1]);
            }
        }
        return "";
    }
    function addCookie(name, value, expireHours, path) {
        var cookieString = name + "=" + encodeURIComponent(value);
        if (expireHours > 0) {
            var date = new Date();
            date.setTime(date.getTime() + expireHours * 3600 * 1000);
            cookieString = cookieString + ";expires=" + date.toGMTString();
        }
        //path必须以'/'开头
        if (!!path) {
            cookieString = cookieString + ";path=" + path;
        } else {
            cookieString = cookieString + ";path=" + "/";
        }
        //console.log(cookieString);
        document.cookie = cookieString;
    }
    function deleteCookie(name) {
        var date = new Date();
        date.setTime(date.getTime() - 10000);
        document.cookie = name + "=v;expires=" + date.toGMTString();
    }

    (function () {
        var $supamount = $("#supamount");
        var $oppamount = $("#oppamount");
        var $pingmount = $("#pingmount");
        var $support_percent = $('.support_percent');
        var $oppose_percent = $('.oppose_percent');
        var $ping_percent = $('.ping_percent');

        var doPercent = function (support_num, oppose_num, ping_num) {
            console.log(ping_num);
            var supportPercent;
            var oppamountPercent;
            var pingPercent;
            if ((support_num + oppose_num + ping_num) == 0) {
                supportPercent = 1;
                oppamountPercent = 1;
                pingPercent = 1;
            } else {
                supportPercent = Math.ceil(parseInt(support_num) / (parseInt(support_num) + parseInt(oppose_num) + parseInt(ping_num))  * 225);
                oppamountPercent = Math.ceil(parseInt(oppose_num) / (parseInt(support_num) + parseInt(oppose_num) + parseInt(ping_num))  * 225);
                pingPercent = Math.ceil(parseInt(ping_num) / (parseInt(support_num) + parseInt(oppose_num) + parseInt(ping_num))  * 225);
            }
            /*$support_percent.text(supportPercent + '%');
            $oppose_percent.text(oppamountPercent + '%');
            $ping_percent.text(pingPercent + '%');*/
            $support_percent.css('width', supportPercent + 'px');
            $oppose_percent.css('width', oppamountPercent + 'px');
            $ping_percent.css('width', pingPercent + 'px');
        };

        doPercent(parseInt($supamount.text()), parseInt($oppamount.text()), parseInt($pingmount.text()));
        var ajaxFuc = function (voteType) {
            if (!(getCookie('voted') == '1')) {
                $.ajax({
                    type: 'get',
                    url: '/index.php/Index/Index/vote',
                    data: {voteType: voteType},
                    dataType: 'json',
                    success: function (returnData) {
                        addCookie('voted', 1);
                        alert('投票成功');
                        $supamount.text(returnData.support_num);
                        $oppamount.text(returnData.oppose_num);
                        $pingmount.text(returnData.ping_num);
                        doPercent(returnData.support_num, returnData.oppose_num, returnData.ping_num);
                    },
                    error: function (a, b, c) {
                        console.log(c);
                        alert('投票失败');
                    }
                });
            } else {
                alert('您已投票');
            }
        };

        $('.support').click(function () {
            ajaxFuc('support');
        });
        $('.oppose').click(function () {
            ajaxFuc('oppose');
        });
        $('.ping').click(function () {
            ajaxFuc('ping');
        });
    })();
</script>
      </div>
