var timer;
$(".tl .share").live("mouseover", function () {
	clearTimeout(timer);
	var xqaathis = $(this);
	timer = setTimeout(function () {
	var xqshf = xqaathis.parent(".tl").nextAll(".bdshare_t"), xqshe = xqshf.find("a").size(), xqshg = xqaathis.offset();
	xqshf.show();
	var xqshb = null;
	xqshf.hover(function() { clearTimeout(xqshb); xqshf.show() }, function() { clearTimeout(xqshb); xqshb = setTimeout(function() { xqshf.hide()}, 1E3) });
	$(this).mouseout(function() {clearTimeout(xqshb); xqshb = setTimeout(function() { xqshf.hide()}, 1E3) })
	    }, 300);
  }
)	
