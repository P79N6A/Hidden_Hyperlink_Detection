    var query = "日本 买 乐器 网";
    var keys = query ? query.split(' ') : [];
    var txts = [];
    for (var i = 0; i < keys.length; i++) {
        if (keys[i] == '') {
            continue;
        }
        txts.push(keys[i]);
    }
    $('.text-highlight').highlight(txts);
    $(".info").highlight(txts);
