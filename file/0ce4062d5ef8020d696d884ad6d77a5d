.rqq{z-index:99;right:0;right:5px;width:105px;height:200px;position:absolute;line-height:24px;border:1px solid #ccc; background:#eee;padding:5px 10px;color:#666;}
.qq_title{font-size:14px;font-weight:bold;}
.qqul li{margin:5px 0;}
.weixin{margin-top:15px;}
.weixin img{width:84px;height:84px;}
</style> 
  <script type="text/javascript">
window.onload = window.onresize = window.onscroll = function ()
{
	var oBox = document.getElementById("rqqbox");	
	var iScrollTop = document.documentElement.scrollTop || document.body.scrollTop;
	setTimeout(function ()
	{
		clearInterval(oBox.timer);
		var iTop = parseInt((document.documentElement.clientHeight - oBox.offsetHeight)/2) + iScrollTop;
		oBox.timer = setInterval(function ()
		{
			var iSpeed = (iTop - oBox.offsetTop) / 8;
			iSpeed = iSpeed > 0 ? Math.ceil(iSpeed) : Math.floor(iSpeed);
			oBox.offsetTop == iTop ? clearInterval(oBox.timer) : (oBox.style.top = oBox.offsetTop + iSpeed + "px");
		}, 30)
	}, 100)
};
</script> 
