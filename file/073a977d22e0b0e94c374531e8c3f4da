  <script>
$('.atl img').parents('p').css('text-align','center');
$('.atl span').css('white-space','');
$('.atl').find('*').css('max-width','100%');
$('.atl').find('*').css('min-width','');
$('.atl').find('*').css('position','');
$('.atl').find('*').css('float','');
$('.atl embed').css('max-width','100%');
//$('.atl iframe').remove();

$('.article table').css('width','auto');
$("#searchbox").mouseout(function() {
$("#searchbox").height('34px');
$("#searchbox").css('background','#f5f5f5');
$("#searchbox").css('border-left-color','#f5f5f5');
$("#searchbox").css('border-bottom','0');
});
$( "#searchbox" ).mouseover(function() {
$( "#searchbox" ).height('102px');
$("#searchbox").css('background','#fff');
$("#searchbox").css('border-left-color','#ddd');
$("#searchbox").css('border-bottom','1px solid #ddd');
});
$( "#searchbox li" ).click(function() {
var n = $('#searchbox li').index(this);
var a = $('#searchbox li:eq('+n+')');
$('#searchtype').val(a.attr('name'));
if (eval(a.attr('name'))>1){
$('.search form').attr('action','http://www.gove.cn/search');
}else{
$('.search form').attr('action','/search');
}
$('#searchbox').prepend(a);
$('#searchbox').height('34px');
$('#searchbox').css('background','#f5f5f5');
$("#searchbox").css('border-left-color','#f5f5f5');
$("#searchbox").css('border-bottom','0');
});
$(document).ready(function(){
var sboxPos = $('.sbox_f').offset().top-10;
$(window).scroll(function(){
var srollPos = $(window).scrollTop();
var footPos = $(document).height() - $(window).scrollTop();
if (srollPos>= sboxPos&& footPos>770){
$('.sbox_f').addClass('rf_fixed');
}else{
$('.sbox_f').removeClass('rf_fixed');
}
});
});
</script> 
