  <script>
    (function ($, w) {
        //菜单伸缩
        function menuFlex(id) {
            var box = $('#' + id);
            box.find('p').click(function () {
                if ($(this).is('.sel')) {
                    $(this).removeClass('sel');
                    box.find('div').hide();
                }
                else {
                    $(this).addClass('sel');
                    box.find('div').show();
                }
            });
        }

        //切换
        function tabs(tab, tabCon) {
            var tab = document.getElementById(tab).children;
            var tabCon = document.getElementById(tabCon).children;
            for (var i = 0; i < tab.length; i++) {
                tab[i].index = i;
                tab[i].onclick = function () {
                    var idx = this.index;
                    for (var j = 0; j < tabCon.length; j++) {
                        if (j == idx) {
                            if (tabCon[j].style.display == "block")
                                tabCon[j].style.display = "none";
                            else
                                tabCon[j].style.display = "block";
                        }
                        else {
                            tabCon[j].style.display = "none";
                        }
                    }
                }
            }
        }

        //根据id隐显另一个id
        function IdHideShow(id, id1) {
            var box = document.getElementById(id);
            box.onclick = function () {
                var box1 = document.getElementById(id1);
                if (box1.style.display == "block")
                    box1.style.display = "none";
                else
                    box1.style.display = "block";
            }
        }

        //点击显示2个id，隐藏1个id
        function show2Hide1(idAct, id1, id2, idHide) {
            var box = document.getElementById(idAct);
            box.onclick = function () {
                var allowfastpost = "";
                if(idAct == 'shareInput' && !allowfastpost) { // 点击回复框并且不允许快捷回复，则跳到详细的回复页面
                    w.location.href = "forum.php?mod=post&"+"action=reply&fid=366&tid=5443852&reppost=39461016&page=1&mobile=1";
                } else {
                    var box1 = document.getElementById(id1);
                    box1.style.display = "block";
                    var box2 = document.getElementById(id2);
                    box2.style.display = "block";
                    var box3 = document.getElementById(idHide);
                    box3.style.display = "none";
                }
            }
        }

        //点击隐藏2个id，显示1个id
        function hide2Show1(idAct, id1, id2, idShow, fn) {
            var box = document.getElementById(idAct);
            box.onclick = function () {
                var box1 = document.getElementById(id1);
                box1.style.display = "none";
                var box2 = document.getElementById(id2);
                box2.style.display = "none";
                var box3 = document.getElementById(idShow);
                box3.style.display = "block";
                if (fn) fn();
            }
        }

        //点击子元素输入区域获得data-msg
        function getMsgIn(id, id1) {
            var subs = document.getElementById(id).children;
            var box = document.getElementById(id1);
            for (var i = 0; i < subs.length; i++) {
                subs[i].children[0].onclick = function () {
                    box.value += this.getAttribute('data-msg');
                }
            }
        }

        //removeDom
        function removeSubDom(id) {
            var box = document.getElementById(id);
            var subs = box.children;
            for (var i = 0; i < subs.length; i++) {
                var act = subs[i].getElementsByTagName('em')[0];
                if (act != null) {
                    act.onclick = function () {
                        var dom = this.parentNode;
                        dom.parentNode.removeChild(dom);
                    }
                }
            }
        }

        w.disableSumnitBtn_float = function(){
            document.getElementById('fastpostsubmit_float').disabled = 'disabled';
            setTimeout(function(){
                document.getElementById('tarea').value = '';
                enableSubmit();
            },2000);
        }
        w.enableSubmit_float = function(){
            document.getElementById('fastpostsubmit_float').removeAttribute('disabled');
        }

        w.onload = function () {
            getMsgIn('bq', 'tarea');
            IdHideShow('sharetitImg', 'conpar');
            show2Hide1('shareInput', 'defilade', 'sharepar1', 'ejectShare');
            show2Hide1('shareAct', 'defilade', 'sharepar0', 'ejectShare');
            hide2Show1('sharebtn', 'defilade', 'sharepar0', 'ejectShare');
            hide2Show1('closeBtn0', 'defilade', 'sharepar1', 'ejectShare', function () {
                document.getElementById('conpar').style.display = 'none';
            });
            hide2Show1('closeBtn1', 'defilade', 'sharepar1', 'ejectShare', function () {
                document.getElementById('fastpostform_float').submit();
                document.getElementById('conpar').style.display = 'none';
            });
            var subject = "UC，色情导航中转站",
                    url = "http://bbs.uc.cn/" + "forum.php?mod=viewthread&"+"tid=5443852",
                   pics = '';

            $.each($('a[id^=share_to]'), function(){
                var shareBtn = $(this),
                          id = shareBtn.attr("id"),
                      params = '',
                     openapi = '';
                if(id.indexOf('weibo') != -1) {
                    openapi= 'http://service.weibo.com/share/share.php?';
                    params = $.param({
                        title : subject,
                        url   : url,
                        appkey: '349496973',
                        ralateUid: '3722119112',
                        pic   : ''
                    });
                } else if(id.indexOf('qzone') != -1) {
                    openapi= 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?';
                    params = $.param({
                title : subject,
                        url   : url,
                        appkey: '17860ef8b1fba52dd300229c53ddea67',
                        desc  : '',
                        pic   : ''
                    });
                } else if(id.indexOf('weixin') != -1) {
//                    openapi= 'http://service.weibo.com/share/share.php?';
//                    params = $.param({
//                        title : subject,
//                        url   : url,
//                        appkey: '349496973',
//                        ralateUid:'3722119112',
//                        pic   : ''
//                    });
                } else if(id.indexOf('qqwb') != -1) {
                    openapi= 'http://share.v.t.qq.com/index.php?';
                    params = $.param({
                        c : 'share',
                        a : 'index',
                        title : subject,
                        url   : url,
                        appkey: '17860ef8b1fba52dd300229c53ddea67',
                        pic   : ''
                    });
                } else {

                }
                shareBtn.attr('href', openapi + params);
            });

        }
    })(jQuery, window);
</script> 
