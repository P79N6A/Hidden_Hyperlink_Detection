  <script>
    var platform = navigator.platform;
    var ua = navigator.userAgent;
    var ios = /iPhone|iPad|iPod/.test(platform) && ua.indexOf( "AppleWebKit" ) > -1;
    var andriod = ua.toLocaleLowerCase().indexOf( "android" ) > -1;

    var iframe;
    var INDEX =  false ;
    function showAdd(){
        center();
        iframe.style.display = 'block';
        closeTip();
    }

    function center() {
        iframe.style.top = (document.documentElement.scrollTop + (document.documentElement.clientHeight - 160) / 2) + "px";
        iframe.style.left = (document.documentElement.scrollLeft + (document.documentElement.clientWidth - 320) / 2) + "px";
    }

    function receiveMessage(event){
        var msg = event.data.message;
        if(msg){
            if(msg == 'already exist'){
                iframe.parentNode.removeChild(iframe);
            }else if(msg  == 'close' || msg  == 'success' || msg  == 'cancle'){
                iframe.parentNode.removeChild(iframe);
            }else if(!msg){
                document.getElementById('pop').style.display = 'block';
            }
        }
    }

    window.addEventListener('message',receiveMessage,false);

    function closeTip(){
        document.getElementById('pop').style.display = 'none';
    }

    function noNotice(){
        setCookie('uc_tags_noNotice','ok',10);
        closeTip();
    }

    /**
     @底部版权居底部
     **/
    function footFix(){
        try{
            var footer = document.getElementById("footer");
            var body = document.getElementById("body");

            var bodyHeight = document.body.clientHeight;
            var screenHeight = window.innerHeight;
            if(bodyHeight < screenHeight){
                footer.style.cssText="position:absolute;bottom:0px;left:0;";
                body.style.cssText = "padding:0 0 100px 0";
            }else{
                footer.style.cssText= '';
                body.style.cssText = "";
            }
        }catch(e){}
        setTimeout(footFix,1000);
    }

    function createTipIfram(){
        iframe = document.createElement('iframe');
        iframe.style.position = 'fixed';
        iframe.style.display = 'none';
        iframe.style.borderRadius = '10px';
        iframe.style.backgroundColor = 'transparent';
        iframe.src = 'http://app.uc.cn/appstore/AppCenter/frame?uc_param_str=nieidnutssvebipfcp&api_ver=2.0&id=2222';
        iframe.height='180';
        iframe.width='320';
        iframe.frameborder='no';
        iframe.scrolling='no';
        iframe.allowtransparency = true;
        document.body.appendChild(iframe);
    }

    footFix();
    if(andriod){
        if(checkCookie('uc_tags_oncome')){
            if(!checkCookie('uc_tags_noNotice') && INDEX){
                createTipIfram();
            }
        }else{
            createTipIfram();
            setCookie('uc_tags_oncome','ok',10);
        }
    }

    function getCookie(c_name)
    {
        if (document.cookie.length>0)
        {
            c_start=document.cookie.indexOf(c_name + "=")
            if (c_start!=-1)
            {
                c_start=c_start + c_name.length+1
                c_end=document.cookie.indexOf(";",c_start)
                if (c_end==-1) c_end=document.cookie.length
                return unescape(document.cookie.substring(c_start,c_end))
            }
        }
        return ""
    }

    function setCookie(c_name,value,expiredays)
    {
        var exdate=new Date()
        exdate.setDate(exdate.getDate()+expiredays)
        document.cookie=c_name+ "=" +escape(value)+
                ((expiredays==null) ? "" : ";expires="+exdate.toGMTString())
    }

    function checkCookie(c_name)
    {
        var value=getCookie(c_name)
        return value!=null && value!="";
    }

    //判定三方QQ用户重置密码
    
    var promptMsg = $("#promptMsg").length > 0 ? $("#promptMsg") : $(".promptMsg");
    function showPrompt(p,p2,msg,timeout){
        promptMsg.html(msg);
        promptMsg.parent().show();
        setTimeout(function(){
            promptMsg.parent().hide();
        },timeout);
    }
</script> 
