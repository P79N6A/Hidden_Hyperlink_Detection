//右侧漂浮
$(function () {
	$(window).scroll(function () {
		if ($(window).scrollTop() >= 200) {
			$(".www_topa").addClass("www_topafb");
		} else {
			$(".www_topa").removeClass("www_topafb");
		}
	});
	var b=$(document).height();
	if(b <= 1200){
		$(".rightpf ul").addClass("down");
		}
	else{
		$(window).scroll(function () {
	if ($(window).scrollTop() >= 300) {
		$(".rightpf ul").addClass("down");
	} else {
		$(".rightpf ul").removeClass("down");
	}
	}
  )}
});
</script> 
