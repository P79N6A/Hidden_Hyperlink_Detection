				}
				else{
					target_save.innerHTML = '<a class="post-tag2" target="add_stories" href="/user_add_remove_links.php?action=add&link='+linkid+'" onclick="show_hide_user_links2(document.getElementById(\'stories-'+thedivid+'\'),\''+thedivid+'\',\''+saves+'\',\'1\',\''+linkid+'\',\''+login+'\');">收藏它('+saves+')</a>';
				}
			}
			if(window.Effect){
				if(thediv.style.display == 'none')
				{Effect.Appear(thediv); }
				else
				{Effect.Fade(thediv); }
			}else{
				var replydisplay=thediv.style.display ? '' : 'none';
				thediv.style.display = replydisplay;					
			}
		}

	//-->
	</script> 
  <script type="text/javascript">
		function bookmarksite(title, url){
		if (document.all)
		window.external.AddFavorite(url, title);
		else if (window.sidebar)
		window.sidebar.addPanel(title, url, "");
		}
	</script> 
