var addfavorite = function(title,url){
	try{ window.external.addFavorite(url, title); }catch(e){ try{ window.sidebar.addPanel(title, url, ""); }catch(e){ alert("您所在浏览器不支持，请使用Ctrl+D进行添加"); } }
}

$.fn.selectTab01 = function(){
	var obj = this;
	
	$(obj).find("span").click(function(){ $(obj).find("ul").show(); });
	$(obj).mouseleave(function(){ $(obj).find("ul").hide(); });
	$(obj).find("ul li").click(function(){
		$(obj).find("span").html($(this).html());
		$(obj).find("input[name=siteid]").val($(this).data("siteid"));
		$(obj).find("input[name=catid]").val($(this).data("catid"));
		$(obj).find("ul").hide();
	});
}

$(function($) {
	$('.select').selectTab01();
	
	//把input绑定事件 排除password框  
	$("input[type='text']").each(function(){
		if($(this).val()=="" && $(this).data("placeholder")!=""){
			$(this).data("color",$(this).css("color"));
			$(this).val($(this).data("placeholder")).css("color","#ccc");
			$(this).focus(function(){
				if($(this).val()==$(this).data("placeholder")) $(this).val("").css("color",$(this).data("color"));
			});
			$(this).blur(function(){
				if($(this).val()=="") $(this).val($(this).data("placeholder")).css("color","#ccc");
			});
		}
	});
});
</script>
