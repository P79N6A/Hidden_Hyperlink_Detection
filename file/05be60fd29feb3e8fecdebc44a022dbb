$.fn.agree = function(){
	var savename = "art-454";
	
	this.click(function(){
		var id = $(this).data("id"),
			savedata = $.cookie(savename);
			
		if(savedata != null){
			var is_s = true,
				saveids = savedata.split(",");
			for(var i=saveids.length-1; i>=0; i--){
				if(saveids[i] == id){
					is_s = false;
					break;
				}
			}
