  <script>
function checkForm()
{
	if(formto.LiuName.value=="")
	{
	alert("留言名字不能为空！");
	formto.LiuName.focus();
	return false;
	}

	 if(document.formto.Email.value.length!=0)
  {
    if (document.formto.Email.value.charAt(0)=="." ||        
         document.formto.Email.value.charAt(0)=="@"||       
         document.formto.Email.value.indexOf('@', 0) == -1 || 
         document.formto.Email.value.indexOf('.', 0) == -1 || 
         document.formto.Email.value.lastIndexOf("@")                        　　
  　   ==document.formto.Email.value.length-1 || 
         document.formto.Email.value.lastIndexOf(".")
              ==document.formto.Email.value.length-1)
     {
      alert("Email的格式不正17_！");
      document.formto.Email.focus();
      return false;
      }
   }
 else
  {
   alert("Email的不能为空！");
   document.formto.Email.focus();
   return false;
   }
	
	if(formto.tel.value=="")
	{
	alert("电话不能为空！");
	formto.tel.focus();
	return false;
	}
	
	if(isNaN(formto.tel.value))  //是否为数字
	{
		alert("电话须为数字！");
		formto.tel.value ="";
		formto.tel.focus();
		return false;
	}
	
	if(formto.LiuContent.value=="")
	{
	alert("留言内容不能为空！");
	formto.LiuContent.focus();
	return false;
	}
}
</script>
