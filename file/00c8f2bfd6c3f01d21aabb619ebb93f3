                skinsPath : "/themes/default/global/plugins/slider-layer-slider/skins/",
                skin : "fullwidth",
                thumbnailNavigation : "hover",
                slideDirection : "fade",
                hoverPrevNext : false,
                responsive : false,
                responsiveUnder : 960,
                layersContainer : 960
