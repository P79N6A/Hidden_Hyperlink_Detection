	    $(window).load(function() {
			setGridHeights();
	    });
	    
	    $( window ).resize(function() {
			setGridHeights();
		});
	    
	    function setGridHeights() {
			$('.grid').each(function(){
				var maxHeight = 0;
			    return $(this).find('li,.grid-item').css('height','auto').each(function(index, box){
				    var boxHeight = $(box).height();
					maxHeight = Math.max(maxHeight, boxHeight);
			    }).height(maxHeight);
			})
		    
		};

        jQuery(document).ready(function() {
            Layout.init();    
              Layout.initTwitter();
                        var $container = $('.isotope');
			// init
			$( "a.dropdown-toggle" ).hover(function () {
		        $container.isotope({
		            itemSelector: ".isotope .item"
		        });
			});

  
/*
  			  $('a[href^=#]').click(function(e){
	  			  if($(this).attr('href') != '#pxlink') {
				    var name = $(this).attr('href');
				    var pos = $(name).offset();
				    $('body').animate({ scrollTop: pos.top },1000);
				    e.preventDefault();
				  }
			  });       
*/

		      $('.fa-shopping-cart').mouseover(function(){
		        	$.get('/modules/cart/ajax-basket.php',function(data) {
		        		var obj = jQuery.parseJSON(data);
		        		$('.top-cart-content-wrapper').html(obj.basketList);
		        	});
		        });
		 
			 	$(document).on('click','.removeItem',function(){
					var id = $(this).data('ctid');
					$.get('/modules/cart/cart.php?action=delete&ajax=true&cid='+id,function(reply){
						if(reply == "success") {
				        	$.get('/modules/cart/ajax-basket.php',function(data) {
				        		var obj = jQuery.parseJSON(data);
				        		$('.top-cart-content-wrapper').html(obj.basketList);
				        		$('.top-cart-info-count').html(obj.numItems);
				        		$('.top-cart-info-value').html(obj.basketTotal);
				        	});
						}
					});
					return false;
				});
				
				$('.search-toggler').click(function(){
					if($('.menu-search').is(':visible')) {
						$('.menu-search').slideUp();
					} else {
						$('.menu-search').slideDown();
					}
				})
				
				$(document).on('keyup','#quicksearch-input',function(){
					$('#quicksearch-results').html('');
					var el = $(this);
					if($(this).val().length > 2) {
						el.addClass('loading');
						var q = $(this).val();
						$.get('/modules/products/ajax.php?action=quicksearch&q='+q,function(data){
							if(data != '') {
								$('#quicksearch-results').html(data);
							}
							el.removeClass('loading');
						});
					}
				})
				
 				$('#quicksearch-results').on('click','#btnSearchMore',function(e){
					$('.search-box form').submit();
				});
				
 				$('#quicksearch-results').on('click','a.close',function(e){
					$('#quicksearch-results').html('');
				});
				

				function checkEmpty(el) {
			    	var strng = el.val();
			    	if(strng == "") {
			    		return true;
			    	} else {
			     		return false
			   		}
			    }
			    
			    function checkEmail(el) {
			    	var strng = el.val();
			    	var emailFilter=/^.+@.+\..{2,4}$/;
			    	var emailFilter=/^[^@]+@[^@.]+\.[^@]*\w\w$/;
			       	var illegalChars= /[\(\)\<\>\,\;\:\\\"\[\]]/
			    	if (!(emailFilter.test(strng)) || strng.match(illegalChars)) { 
			       		return true;
			    	} else {
			    		return false;
			    	}
			    }
			    	
			    $(".required").blur(function(){
			    	if(checkEmpty($(this))) {
			    		$(this).parent("label").css("background","#ffa0a0");
			    	} else {
			    		$(this).parent("label").css("background","transparent");
			    	}
			    });
			    
			    $(".checkemail").blur(function(){
			    	if(checkEmail($(this))) {
			    		$(this).parent("label").css("background","#ffa0a0");
			    	} else {
			    		$(this).parent("label").css("background","transparent");
			    	}
			    });
			    		
			    $("a").click(function(event){
				    if($(this).attr('href') == "#pxlink") {
			    		event.preventDefault();
			    		$("#usedformresponse").html("").fadeOut();
			    		$("#usedformcontent .textinput").val("");
			    		$("#usedformcontent .checkbox").attr("checked",false);
			    		$("#usedformcontent #brand").val($("#usedformcontent #brand option:first").val());
			    		$("#usedformcontent #item").val($("#usedformcontent #item option:first").val());
			    		$("#usedformcontent #condition").val($("#usedformcontent #condition option:first").val());
			    		$("#usedformcontent").show();
			    		leftPos = ($(window).width()-$("#usedform").width())/2;
			    		topPos = $(window).scrollTop() + (($(window).height()-$("#usedform").height())/2);
			    		$("#overlay").css("width","100%").css("height",$(document).height()+"px").fadeIn();
			    		$("#usedform").css("top",topPos+"px").css("left",leftPos+"px").fadeIn();	    
				    }
			    });
			    
			     $(document).on("click","#usedform .close, #usedform .closebutton, #overlay",function(){
			    	$("#usedform, #overlay").fadeOut();
			    });
			    
				$("#submit").click(function(){
					closeDialog = true;
				});
			   
				$("#submitplus").click(function(){
					closeDialog = false;
				});
			   
			   $("#submit, #submitplus").click(function(event){
			   		event.preventDefault();
			   		var error=false;
			   		$(".required").each(function(){
			   			if(checkEmpty($(this))) {
			    			$(this).parent("label").css("background","#ffa0a0");
			  				error = true;
			   			}
			   		});
			
			   		$(".checkemail").each(function(){
			   			if(checkEmail($(this))) {
			    			$(this).parent("label").css("background","#ffa0a0");
			  				error = true;
			   			}
			   		});
			   		var formHeight = $("#usedform").height()+"px";
			   		if(!error) {
			   			$.post("ajax.php",$("#usedform form").serialize(),function(response) {
			   				if(closeDialog) {
			    				$("#usedform").css("height",formHeight);
			   					
			   					$("#usedformcontent").fadeOut(function(){
			   						$("#usedformresponse").html(response).fadeIn();
			   					});
			   				} else {
			    				$("#usedformcontent #model, #usedformcontent #actuations, #usedformcontent #othertxt, #usedformcontent #info").val("");
			    				$("#usedformcontent .checkbox").attr("checked",false);
			    				$("#usedformcontent #brand").val($("#usedformcontent #brand option:first").val());
			    				$("#usedformcontent #item").val($("#usedformcontent #item option:first").val());
			    				$("#usedformcontent #condition").val($("#usedformcontent #condition option:first").val());
			   				}
			   			});
			   		}
			   });
			   
			   $('iframe').each(function(){
				   if(!$(this).hasClass('sagepay-iframe') && $(this).attr('name') != 'google_conversion_frame' && $(this).attr('id') != 'embed-360player') {
					   $(this).wrap('<div class="video-container">');
					}
        		});
        		
        		$('#btnNlSubscribe').click(function(e){
	        		e.preventDefault();
		        	$('form#frmSubscription .alert').slideUp();
	        		if($('#subscribe-email').val() != '') {
		        		$.post("ajax.php","action=nlsubscribe&subscribe-email="+$('#subscribe-email').val(),function(reply){
			        		$('form#frmSubscription').append(reply);
			        		$('form#frmSubscription .alert').slideDown();	
							setTimeout(function() {
							  $('form#frmSubscription .alert').slideUp(function(){
								  $(this).remove();
							  });
							  $('#subscribe-email').val('');
							}, 2000);
		        		});
	        		}
        		});
        		
/*
        		$(window).scroll(function(){
	        		if($(window).scrollTop() > 50) {
		        		$(".total-header").addClass('floating');
	        		} else {
		        		$(".total-header").removeClass('floating');
	        		}
        		})
*/
       
   
        });
       
  
