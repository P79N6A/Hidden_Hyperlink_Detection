    $(function() {
        /*详情页配置信息字符串间隔*/

            $(".detail-content .detail-left span").each(function(){
                var str=$(this).html();
                var sLength=str.length;
                if(sLength==2){
                    $(this).css({"letter-spacing":"14px","margin-right":"-14px"});
                }
                else if(sLength==3){
                    $(this).css({"letter-spacing":"7px","margin-right":"-7px"});
                }
                else if(sLength>=4){
                    $(this).css("letter-spacing","0");
                }
            });


        if(getCookie('the-cookie') == null){
            $("#activity-sjf").show(200);
            $(".close-notice").click(function(){
                $("#activity-sjf").hide(400);
                setCookie('the-cookie','1',86400);
            })}

    });
</script> 
