        <script>
var build_query = function() {
	var redirect = [];
	var comment_score = Sizzle('input', Sizzle('#comments_score')[0]);
	var score = '';
	for (var i = 0;i < comment_score.length;i++) {
		if (comment_score[i].checked == true) {
			score = comment_score[i].value;
		}
	}
	if (score) redirect.push('score=' + score);

	var sort = Sizzle('#comment_sort')[0].value;
	if (sort) redirect.push('sort=' + sort);

	var status = 'all';
	if (status) redirect.push('review_status=' + status);
	document.location = '/entries/746020/comments/?' + redirect.join("&");
}
</script> 
