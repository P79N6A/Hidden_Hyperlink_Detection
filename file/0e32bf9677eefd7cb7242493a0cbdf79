        <script type="text/javascript">
// locate comment form
function comment_form_focus(target) {
	var obj = Sizzle('.comment_form_textarea')[0];
    if (!obj) {
        if (!VeryCD.Member.Id) {
        	VeryCD.login();
        }else{
            VeryCD.alert('异常错误，请重新登录！');
        }
        return;
    }
    VeryCD.goToObj(obj);
    obj.focus();
}

//bind event on comment form
var ini_textarea = function(form_id) {
	var textarea = Sizzle('#' + form_id + ' .comment_form_textarea')[0];
	textarea.onfocus = function() {
       	if (!VeryCD.Member.Id) {
    		VeryCD.login();
    		return false;
       }
       if (VeryCD.__check_textarea_status == false) {
           VeryCD.__check_textarea_status = true;
           textarea.style.borderColor = '#CCCCCC';
           textarea.style.color = '#000';
       }
       if (textarea.value == '内容不可为空') {
           textarea.value = '';
       }
	}
}
VeryCD.iniHtmlOnce(function () {
var forms = Sizzle('.block_add_form');
for(var i = 0; i < forms.length; i++) {
   if (typeof(window.ini_textarea)=='function') {
   	ini_textarea(forms[i].id);
   }
}
});

// check comment form
VeryCD.__check_textarea_status = true;
function check_comment_form(obj_form) {
	var obj_textarea = Sizzle('.comment_form_textarea', obj_form)[0];
	if(obj_textarea.value == '') {
		obj_textarea.style.borderColor = '#CC0000';
		obj_textarea.value = '内容不可为空';
		obj_textarea.style.color = '#CC0000';
		VeryCD.__check_textarea_status = false;
	}
	if(typeof(window.check_collection_status) == 'function') {
		if(check_collection_status(obj_form) == false) {
			return false;
		}
	}
	if(VeryCD.__check_textarea_status == false) {
		return false;
	}
	return true;
}

// submit comment
function comment_ajax_submit(obj, form_id) {
    if(!VeryCD.Member.Id) {
		VeryCD.login();
		return false;
    }
	var obj_form = Sizzle('#' + form_id)[0];
	if(!check_comment_form(obj_form) || VeryCD._comment_ajax_submit_loading == true) {
		return false;
	}
	VeryCD._comment_ajax_submit_loading = true;
	if(VeryCD.__comment_widget_type != null) {
        $(obj).children('[name=obj_type]').attr('value', VeryCD.__comment_widget_type);
        $(obj).children('[name=obj_id]').attr('value', VeryCD.__comment_widget_id);
    }
	$.ajax({
        url: '/base/ajax/collect/comment',
        type: 'post',
        data: $(obj).serialize(),
        dataType: 'json',
        success: function(rs) {
        	setTimeout(function(){VeryCD._comment_ajax_submit_loading = false;},1000);
            VeryCD.hidden_loading();
            if (rs.code === 0) {
            	Sizzle('.comment_form_textarea', obj_form)[0].value = '';
                var cmt = JSON.parse(rs.msg);
                var comment_list = Sizzle('#comments_list')[0];
                if(comment_list){
                    var comment_ul = comment_list.getElementsByTagName('ul')[0];
                    var comment_li = comment_ul.getElementsByTagName('li');
                    var comment_lastli =comment_li[comment_li.length-1];
                    var lastli_flag = 0;
                    if(comment_lastli){
                        lastli_flag = comment_lastli.className.indexOf('comment_dark_bg');
                    }
                    var new_li = comment_build_newli(cmt, lastli_flag);
                    comment_ul.appendChild(new_li);
                    if (typeof(window.change_html)=='function') {
                        change_html(cmt.review_status_value,cmt.comment_score_value);
                    }
                    commentList_ini(new_li);
                    VeryCD.iniHtml(comment_ul);
                    var obj_new_li = Sizzle('#comment'+cmt.id)[0];
                    VeryCD.goToObj(obj_new_li);
                }else{
                	window.location.href = window.location.href + '#comments_list';
                    location.reload();
                }
                if(Sizzle('#no-replies')[0]) Sizzle('#no-replies')[0].style.display = 'none';
                VeryCD.popMsg(cmt.achievement_html);
                if (typeof(init_clear_input) == 'function') {
                	init_clear_input();
                }
                if (typeof(init_clear_video_input) == 'function') {
                	init_clear_video_input();
                }
            } else {
                VeryCD.error(rs.msg);
            }
		},
      	beforeSend: function(){
      		VeryCD.show_loading();
      	},
      	error: function() {
      		VeryCD._comment_ajax_submit_loading = false;
            VeryCD.hidden_loading();
            alert('加载页面失败，请重试！');
      	}
	});
}

// generate new comment
function comment_build_newli(comment, flag) {
    var _li = document.createElement("li");
    _li.setAttribute('id', 'comment'+comment.id);
    if(flag >= 0){
        _li.className = 'block comment_new';
    }else{
        _li.className = 'block comment_new comment_dark_bg';
    }
    var comment_li = '<div class="comment_left"><div class="face"><a href="'+VeryCD.Member.i_url(comment.member_id)+'"><img name="lazyload" src="'+VeryCD.Member.avatar(comment.member_id)+'" hoverstyle="3" hovertips="type=2&id=' + comment.member_id + '"></a></div></div>';
    comment_li += '<div class="comment_right">';
    comment_li += '<div class="more"><img align="absmiddle" onclick="scroll(0,0);" style="cursor:pointer;" src="http://v4.vcimg.com/images/v3/to_top.gif?v=6e382663"></div>';
    comment_li += '<div style="height:20px;" class="user_name">';
    comment_li += '<div style="float:left;"><a href="'+VeryCD.Member.i_url(comment.member_id)+'" style="color:#330066" hoverstyle="3" hovertips="type=2&id=' + comment.member_id + '">'+comment.member_name+'</a>&nbsp;'+comment.member_rankimg+comment.review_status+'</div>'+comment.comment_score;
    comment_li += '<span style="float:right;font-weight:normal;margin-right:15px;_display:inline">';
    if (comment.member_id == VeryCD.cookie.get('member_id')) {
        comment_li += '<a href="javascript:void(0)" class="j-cmt-remove-btn" name="'+comment.id+'-tweet" style="color:gray">删除</a>';
    }
    comment_li += '</span>';
    comment_li += '</div>';
    comment_li += '<div class="block" style="overflow:hidden;"><p style="color:#000;" class="p">'+$("<div>").text(comment.content).html()+'</p>';
        comment_li += '</div>';
    comment_li += '<div class="comment_todo">';
    comment_li += '<div style="color:#999;" class="more"><span class="date-time">'+comment.date+'</span></div>';
    comment_li += '<div><span><a name="up-'+comment.id+'-tweet" href="javascript:void(0)" class="digup j-cmt-dig-btn">支持</a> <span class="j-cmt-dig-view">[<b id="normal-dig_'+comment.id+'_up">0</b>]</span></span>&nbsp;&nbsp;<span><a name="down-'+comment.id+'-tweet" href="javascript:void(0)" class="digdown j-cmt-dig-btn">反对</a> <span class="j-cmt-dig-view">[<b id="normal-dig_'+comment.id+'_down">0</b>]</span></span>&nbsp;&nbsp;<a name="normal-'+comment.id+'-0-tweet" class="j-cmt-reply-btn" onclick="VeryCD.Track(\'/stat/entryQuote/\');try{comment_form_focus(this);}catch(e){};return false;" href="javascript:void(0)">回复</a> &nbsp;&nbsp;</div>';
    comment_li += '</div>';
    comment_li += '<div class="clear"></div>';
    comment_li += '<div id="normal-sub_comments_list_'+comment.id+'"><div class="reply_div"><span class="triangle"></span><ul class="ul reply" style="display:none;"></ul></div></div>';
    comment_li += '<div style="display: none;" class="sub_comments_reply_textarea"></div>';
    comment_li += '</div>';

    _li.innerHTML = comment_li;
    return _li;
}
function get_image_comment(comment) {
	var comment_li = '<div class="block other_cmt" style="color:#000">';
	comment_li += '<span class="triangle"></span>';
	comment_li += '<span class="corner lt"></span>';
	comment_li += '<span class="corner rt"></span>';
	comment_li += '<span class="corner lb"></span>';
	comment_li += '<span class="corner rb"></span>';
	comment_li += '<div class="comment_extra block image2" style="width:144px;">';
	comment_li += '<a href="/entries/'+ comment.entry_id +'/images/view/'+ comment.img_id +'/" onclick="VeryCD.Track(\'/stat/entryComImage/\');show_this_image(\''+ comment.img_archived_id + '_' + comment.img_fileid +'\','+ comment.entry_id +');return false">';
	comment_li += '<img src="http://i-' + comment.img_archived_id + '.vcimg.com/resize/' + comment.img_fileid + '(140x200)/thumb.jpg">';
	comment_li += '</a>';
	comment_li += '</div>';
	comment_li += '</div>';
	var hidden_div = '<div style="display:none;">';
	hidden_div += '<a id="hidden_image_' + comment.img_archived_id + '_' + comment.img_fileid + '" onclick="view_image(\'http://i-' + comment.img_archived_id + '.vcimg.com/' + comment.img_fileid + '(600x)/thumb.jpg\',this,' + comment.img_id + ',0,' + comment.img_digups + ');total_image_view('+comment.img_id+');return false;" name="page_view_image"></a>';
	hidden_div += '<span id="total_image_' + comment.img_id + '">' + comment.img_comments + '</span>';
	hidden_div += '</div>';
	comment_li += hidden_div;
    return comment_li;
}
function get_video_comment(comment) {
    var comment_li = '<div class="block other_cmt" style="color:#000">';
	comment_li += '<span class="triangle"></span>';
	comment_li += '<span class="corner lt"></span>';
	comment_li += '<span class="corner rt"></span>';
	comment_li += '<span class="corner lb"></span>';
	comment_li += '<span class="corner rb"></span>';
	comment_li += '<span class="link_line" style="display:block;color:#333;padding:0 0 10px">视频： <a target="_blank" onclick="VeryCD.Track(\'/stat/entryComVideoTitle/\');" href="' + comment.video_url + '">' + comment.video_name + '</a></span>';
	comment_li += '<div class="comment_extra block single_video" style="width:144px;">';
	comment_li += '<a href="' + comment.video_url + '" onclick="VeryCD.Track(\'/stat/entryComVideo/\');" target="_blank">';
	comment_li += '<img src="' + comment.video_thumbnail + '" style="border:1px #d2d2d2 solid; width:140px;height:105px;margin:0 auto"><br><span class="playbar"><span class="bg" style="*margin-top:-55px;"></span></span>';
	comment_li += '</a>';
    comment_li += '<div style="display:none">';
    comment_li += '<a id="hidden_video_' + comment.video_id + '" onclick="view_image("' + comment.video_uri + '",this,' + comment.video_id + ',0,' + comment.video_digups + ');total_video_view('+ comment.video_id+');return false;"></a>';
    comment_li += '<span id="total_video_' + comment.video_id + '">' + comment.video_comments + '</span>';
    comment_li += '</div>';
    if(comment.video_hits > 0){
        comment_li += '<div style="width:144px;line-height:20px;text-align:center;font-size:12px"><span style="color:#7c7c7c;display:block;overflow:hidden;">';
        comment_li += '<span><a target="_blank" style="margin-right:1px;" onfocus="this.blur()" href="' + comment.video_url + '"><img style="border:0;background:none;padding:3px" width="11" height="11" align="absmiddle" src="http://v4.vcimg.com/base/images/face.gif?v=e4b914af" alt="浏览" title="浏览" /></a>' + comment.video_hits + '</span>';
        comment_li += '<span><a target="_blank" style="margin-right:1px;" onfocus="this.blur()" href="' + comment.video_url + '"><img style="border:0;background:none;padding:3px" width="12" height="11" align="absmiddle" src="http://v4.vcimg.com/base/images/talk.gif?v=fec6d9d6" alt="口水" title="口水" /></a>' + comment.video_comments + '</span>';
        comment_li += '<img onclick="dig_me_up(' + comment.video_id + ', this, \'video\');VeryCD.Track(\'/stat/entryZan/\');return false;" style="cursor:pointer;padding:3px" width="11" height="11" align="absmiddle" src="http://v4.vcimg.com/base/images/good.gif?v=d5eb5f35" alt="赞" title="赞" /><span>' + comment.video_digups + '</span>';
        comment_li += '</span></div>';
    }
	comment_li += '</div>';
	comment_li += '</div>';
    return comment_li;
}
