function dig_me_up(id, obj, type){
    obj.setAttribute('onclick','');
    if (obj.nextSibling.innerHTML == ''){
        obj.nextSibling.innerHTML = 1;
    } else {
        obj.nextSibling.innerHTML = parseInt(obj.nextSibling.innerHTML) + 1;
    }
    var url = '/base/ajax/'+ type + '/dig';
    if (type == 'article') {
    	url = '/base/ajax/'+ type + '/digup';
    }
    $.ajax({
        url: url,
        type: 'post',
        data: type + '_id=' + id
	});
}
</script> 
         <style type="text/css">
.user_name .smallstat{padding: 2px 0 4px;}
</style> 
         <script type="text/javascript">
function kill_by_member_id(member_id, member_name) {
    VeryCD.confirm('确认屏蔽“' + member_name + '”，并清除其所有历史？', 350, null, null, function(el){
        if (el!='ok')return false;
            $.ajax({
                url: '/api/account/kill',
                type: 'post',
                data: {'member_id':member_id},
                dataType: 'json',
                success: function(rs) {
                	VeryCD.hidden_loading();
                    if (rs.code == 0) {
                    	VeryCD.Msg(rs.msg);
                    } else {
                    	VeryCD.error(rs.msg, 300);
                    }
        		},
              	beforeSend: function(){
              		VeryCD.show_loading();
              	},
              	error: function() {
              		VeryCD.hidden_loading();
                    VeryCD.error('抱歉，网络故障，请重试！');
              	}
        	});
    });
}
var commentList_ini= function(context) {

var CommentList = {};

CommentList.init = function(context) {
    var re = /^init_/;
    for (var m in this) {
        if (re.test(m)) {
            this[m](context);
        }
    }
}

CommentList.init_reply_btn = function(context) {
    var es = Sizzle('.j-cmt-reply-btn', context);
    for (var i = 0; i < es.length; i++) {
        var e = es[i].name.split("-");
        var comment_id = e[1];
        var comment_replies = e[2];
        if ( (e[0] == 'hot') && (comment_replies > 0)){
            var see_obj = Sizzle('#see_all_kid_'+comment_id+'_hot')[0];
            if (see_obj) {
                var id = comment_id;
                var q = function(event) {
                    var obj = get_event_target(event);
                    var e = obj.id.split("_");
                	CommentList.fetch_sub_comments(e[3],true,'hot-')
                }
            	see_obj.onclick = q;
            }
        } else if (comment_replies > 0) {
            var see_obj = Sizzle('#see_all_kid_'+comment_id)[0];
            if (see_obj) {
                var id = comment_id;
                var q = function(event) {
                    var obj = get_event_target(event);
                    var e = obj.id.split("_");
                	CommentList.fetch_sub_comments(e[3],true,'normal-')
                }
            	see_obj.onclick = q;
            }
        }
        var f = function(event) {
            if (!VeryCD.Member.Id) {
				VeryCD.login();
				return false;
            }
            var obj = get_event_target(event);

            var e = obj.name.split("-");
            var comment_id = e[1];
            var comment_type = e[3];
            var comment_member_name = e[4];
            if ( e[0] == 'hot'){
                CommentList.show_reply_textarea(obj, comment_id,comment_type, comment_member_name, true, 'hot-');
            } else {
                CommentList.show_reply_textarea(obj, comment_id,comment_type, comment_member_name, true, 'normal-');
            }
            return false;
        }
        es[i].onclick = f;
    }
}

CommentList.show_reply_textarea = function(obj, comment_id, comment_type, comment_member_name, expand, comment_hot, isfocus) {
    if (typeof isfocus=='undefined') isfocus=true;
    var member_id = VeryCD.cookie.get('member_id');
    var tmphtml = '';
    tmphtml += '<div class="input">';
    tmphtml += '<form class="j-cmt-submit-btn" onKeyDown="VeryCD.check_hotkey(event, \'submit_button_' + comment_hot + comment_id +'\')">';
    tmphtml += '<input type="hidden" name="obj_id" value="' + comment_id + '" />';
    tmphtml += '<input type="hidden" name="type" value="' + comment_type + '" />';
    tmphtml += '<input type="hidden" name="face" value="0" />';
    tmphtml += '<div class="reply_box"><textarea name="contents" onfocus="if(!VeryCD.Member.Id){VeryCD.login();};if(this.value!=\'\'){this.style.color=\'#000\';};if(this.value==\'回复内容……\'){this.value=\'\';}" onblur="if(this.value==\'\' || this.value==\'回复内容……\'){this.style.color=\'#BBBBBB\';this.value=\'回复内容……\';}">回复内容……</textarea></div>';
    tmphtml += '<div class="action"><span class="fr"><input type="submit" id="submit_button_' + comment_hot + comment_id +'" value="回复" class="button" /></span>';
    tmphtml += '<div class="mood">\
                <a class="mood_btn" href="javascript:void(0);" onclick="$(this).next(\'div\').toggle()"><img width="18" height="18" src="http://v4.vcimg.com/feed/images/comment_face/default/default.png?v=1d089eb1"><span>心情</span></a>\
                <div style="display:none">\
                    <div class="layer_mood">\
                        <p>添加心情图标</p>\
                        <ul class="mood_list clearfix">\
                            <li><a class="default" href="javascript:void(0);" onclick="feed.add_brow(0, this, \'http://v4.vcimg.com/feed/images/comment_face/default/default.png?v=1d089eb1\');return false;">无</a></li>\
                            <li><a href="javascript:void(0);" onclick="feed.add_brow(1, this);return false;"><img width="18" height="18" title="开心" alt="开心" src="http://v4.vcimg.com/feed/images/comment_face/default/kaixin.png?v=b224d705"></a></li><li><a href="javascript:void(0);" onclick="feed.add_brow(2, this);return false;"><img width="18" height="18" title="偷笑" alt="偷笑" src="http://v4.vcimg.com/feed/images/comment_face/default/touxiao.png?v=0f359944"></a></li><li><a href="javascript:void(0);" onclick="feed.add_brow(3, this);return false;"><img width="18" height="18" title="挖鼻" alt="挖鼻" src="http://v4.vcimg.com/feed/images/comment_face/default/wabishi.png?v=6ed3e14e"></a></li><li><a href="javascript:void(0);" onclick="feed.add_brow(4, this);return false;"><img width="18" height="18" title="叹气" alt="叹气" src="http://v4.vcimg.com/feed/images/comment_face/default/tanqi.png?v=a7d2c9a6"></a></li><li><a href="javascript:void(0);" onclick="feed.add_brow(5, this);return false;"><img width="18" height="18" title="哭泣" alt="哭泣" src="http://v4.vcimg.com/feed/images/comment_face/default/kuqi.png?v=76ad5bb7"></a></li>                        </ul>\
                    </div>\
                    <div class="mood_arrow"><em class="addline">◆</em><span>◆</span></div>\
                </div>\
            </div>';
    tmphtml += '</div>';
    tmphtml += '</form>';
    tmphtml += '</div>';
    var objReply = $('div.sub_comments_reply_textarea', $(obj).parents('li.comment_new'))[0];
    $(objReply).html(tmphtml);

    if (comment_member_name) {
        $('textarea', $(objReply)).val("回复" + comment_member_name + "：");
    }

    if (expand) {
        if ($(objReply).is(':hidden')) {
        	VeryCD.resizeDiv(objReply);
        }
        if (isfocus) {
        	$('textarea', $(objReply)).focusEnd();
        }
    } else {
        $(objReply).css({'display':'none'});
    }
    CommentList.init_submit_btn(objReply, comment_hot, comment_member_name);
    bindtweet.ini(objReply);
}

CommentList.reset_reply_box = function(obj) {
	var $obj = $(obj);
	var id = $obj.parents('li').attr('id').replace('comment', '');
	$('input[name="type"]', $obj).val('tweet');
	$('input[name="obj_id"]', $obj).val(id);
};

CommentList.init_submit_btn = function(context, comment_hot, comment_member_name) {
    var es = Sizzle('.j-cmt-submit-btn', context);
    for (var i = 0; i < es.length; i++) {
        var f = function(event) {
        	if (!VeryCD.Member.Id) {
				VeryCD.login();
				return false;
            }
            var obj = get_event_target(event);
            var ajax_url = '/ajax/i/add_comment';
            if ($('input[name="type"]', $(obj)).val() == 'comment') {
            	var contents = $('textarea', $(obj)).val();
            	var reply_name = VeryCD.check_reply(comment_member_name, contents);
            	if(reply_name == '') {
            		CommentList.reset_reply_box(obj);
            	} else {
                	ajax_url = '/ajax/i/reply_comment';
                	$('textarea', $(obj)).val(contents.replace(reply_name,''));
            	}
            }
            if($('textarea', $(obj)).val() == '') {
				return false;
            }
            $.ajax({
                url: ajax_url,
                type: 'post',
                data: $(obj).serialize(),
                dataType: 'json',
                success: function(rs) {
                	VeryCD.hidden_loading();
                    if (rs.code === 0) {
                        var cmt = JSON.parse(rs.msg);
                        // clear textarea
                        $('textarea', $(obj)).val('');
                        if ($('[name=face]', $(obj)).val() > 0) {
                            var face_img = $('.mood_btn img', obj).clone().wrap('<div />').parent().html();
                        }
                        $('.default', obj).click();
                        CommentList.reset_reply_box(obj);
                        var subcomment_list = Sizzle('#' + comment_hot+'sub_comments_list_'+cmt.tweet_id)[0];
                        var subcomment_ul = subcomment_list.getElementsByTagName('ul')[0];
                        var new_li = subcomment_build_newli(cmt, face_img);
                        var subcomment_firstli = subcomment_ul.getElementsByTagName('li')[0];
                        if (subcomment_firstli) {
                            subcomment_ul.insertBefore(new_li,subcomment_firstli);
                        } else {
                            subcomment_ul.appendChild(new_li);
                        }
                        VeryCD.iniHtml(new_li);
                        commentList_ini(new_li);
                        subcomment_ul.style.display = 'block';
                        VeryCD.popMsg(cmt.achievement_html);
                    } else {
                        // TODO: error handler
                        VeryCD.alert(rs.msg);
                    }
        		},
              	beforeSend: function(){
              		VeryCD.show_loading();
              	},
              	error: function() {
              		VeryCD.hidden_loading();
                    alert('加载页面失败，请重试！');
              	}
        	});
            return false;
        }
        es[i].onsubmit = f;
    }
    function subcomment_build_newli(comment, face_img){
        var _li = document.createElement("li");
        _li.className = 'block';

        var comment_li = '<div class="comment_left image2"><a href="'+VeryCD.Member.i_url(comment.member_id)+'"><img name="lazyload" src="'+VeryCD.Member.avatar(comment.member_id)+'" hoverstyle="3" hovertips="type=2&id=' + comment.member_id + '"></a></div>';
        comment_li += '<div class="comment_right">';//
        comment_li += '<div style="padding:0 0 8px 0;">';
        if (comment.member_id == VeryCD.cookie.get('member_id')) {
            comment_li += '<span style="float:right;margin-right:10px;_display:inline;">';
            comment_li += '<a href="javascript:void(0)" class="j-cmt-remove-btn" name="'+comment.comment_id+'-comment" style="color:gray">删除</a>';
			comment_li += '</span>';
        }
        comment_li += '<span class="user_name"><a href="'+VeryCD.Member.i_url(comment.member_id)+'" hoverstyle="3" hovertips="type=2&id=' + comment.member_id + '">'+comment.member_name+'</a></span>&nbsp;'+comment.member_rank;
        comment_li += '</div>';
        comment_li += '<div class="block" style="overflow:hidden;"><p class="p">'+ (comment.replied_id > 0 ? '回复' + comment.replied_member_name + '：' : '') + (face_img ? face_img : '') + comment.contents+'</p></div>';
        comment_li += '<div class="comment_todo">';
        comment_li += '<div class="more"><span class="date-time">'+comment.create_time+'</span></div>';
        comment_li += '<div><span><a name="up-'+comment.comment_id+'-comment" href="javascript:void(0)" class="digup j-cmt-dig-btn">支持</a> <span class="j-cmt-dig-view">[<b id="normal-dig_'+comment.comment_id+'_up">0</b>]</span></span>&nbsp;&nbsp;&nbsp;&nbsp;<span><a name="down-'+comment.comment_id+'-comment" href="javascript:void(0)" class="digdown j-cmt-dig-btn">反对</a> <span class="j-cmt-dig-view">[<b id="normal-dig_'+comment.comment_id+'_down">0</b>]</span></span>&nbsp;&nbsp;&nbsp;&nbsp;<a name="normal-'+comment.comment_id+'-0-comment-'+comment.member_name+'" class="j-cmt-reply-btn" onclick="VeryCD.Track(\'/stat/entryQuote/\');try{comment_form_focus(this);}catch(e){};return false;" href="javascript:void(0)">回复</a> &nbsp;&nbsp;&nbsp;</div>';
        comment_li += '</div>';
        comment_li += '<div class="clear"></div>';
        comment_li += '</div>';

        _li.innerHTML = comment_li;
        return _li;
    }
}

CommentList.fetch_sub_comments = function(comment_id, expand, comment_hot) {
	$.ajax({
        url: '/base/ajax/comment/get',
        type: 'get',
        data: {'tweet_id':comment_id},
        dataType: 'json',
        success: function(rs) {
        	if (rs.code === 0) {
                var comments = JSON.parse(rs.msg);
                var comments_new = [];
                for (var i = 0, elem; (elem = comments[i]) != null; i++ ) {
                    if (elem.deleted == 0) {
                        comments_new.push(elem);
                    }
                }
                parse_comments_into(comment_id, comments_new, expand, comment_hot);
            } else {
                // do nothing
            }
		}
	});
    function parse_comments_into(comment_id, comments, expand, comment_hot) {
        var obj = Sizzle('#' + comment_hot+'sub_comments_list_' + comment_id)[0];
        var tmphtml = '<div class="reply_div"><span class="triangle"></span><ul class="ul reply">';
        var comments_for_display = [];
        if (comments.length > 2) {
            if (!expand) {
                comments_for_display = comments.slice(0, 2);
            } else {
                comments_for_display = comments;
            }
        } else {
            comments_for_display = comments;
        }

        for (var i = 0, comment; (comment = comments_for_display[i]) != null; i++) {
            tmphtml += _comment_to_html(comment, comment_hot);
        }

        if (comments.length > 2 && Sizzle('#comment_replies_'+comment_id)[0]) {
            var comment_replies = Sizzle('#comment_replies_'+comment_id)[0].value;
            if (!expand) {
                tmphtml += '</ul><ul class="reply"><li class="block" style="padding:0;">';
                                tmphtml += '<a href="javascript:void(0)" class="j-expand-all">查看全部' + comment_replies + '条回复</a>';
                                tmphtml += '</li>';
            } else if(comment_replies > 10){
                tmphtml += '</ul><ul class="reply"><li class="block" style="padding:0;">';
                var last_num = comment_replies - 10;
                tmphtml += '<a target="_blank" href="/t/'+comment_id+'/#comments_list" class="j-redirect">后面还有' + last_num + '条口水，点击查看</a>';
                tmphtml += '</li>';
            }
        }
        tmphtml += '</ul></div>';
        obj.innerHTML = tmphtml;

        var es = Sizzle('.j-expand-all', obj);
        for (var i = 0, elem; (elem = es[i]) != null; i++ ) {
            elem.onclick = function() {
                parse_comments_into(comment_id, comments, true, comment_hot);
                return false;
            }
        }
        var es = Sizzle('.j-collapse-all', obj);
        for (var i = 0, elem; (elem = es[i]) != null; i++ ) {
            elem.onclick = function() {
                parse_comments_into(comment_id, comments, false, comment_hot);
                return false;
            }
        }
        CommentList.init(obj);

        CommentList.show_reply_textarea(obj, comment_id, null,null, expand, comment_hot, false);
        VeryCD.iniHtml(obj);
    }
    function _comment_to_html(comment, comment_hot) {
                var comment_face_config = new Array(
            {name: '开心',file: 'http://v4.vcimg.com/feed/images/comment_face/default/kaixin.png?v=b224d705'},{name: '偷笑',file: 'http://v4.vcimg.com/feed/images/comment_face/default/touxiao.png?v=0f359944'},{name: '挖鼻',file: 'http://v4.vcimg.com/feed/images/comment_face/default/wabishi.png?v=6ed3e14e'},{name: '叹气',file: 'http://v4.vcimg.com/feed/images/comment_face/default/tanqi.png?v=a7d2c9a6'},{name: '哭泣',file: 'http://v4.vcimg.com/feed/images/comment_face/default/kuqi.png?v=76ad5bb7'}        );
        if (comment.face > 0) {
            var face_info = comment_face_config[comment.face - 1];
            var face_img = '<img src="' + face_info.file + '" alt="' + face_info.name + '" title="' + face_info.name + '" width="18" height="18" />';
        }
        var tmphtml = '';
        if (comment.digups - (-comment.digdowns) >= 10) {
            tmphtml += '<li class="block comment_hot">';
        } else {
            tmphtml += '<li class="block">';
        }
        tmphtml += '<div class="comment_left image2"><a href="'+VeryCD.Member.i_url(comment.member_id)+'"><img src="'+VeryCD.Member.avatar(comment.member_id)+'" hoverstyle="3" hovertips="type=2&id=' + comment.member_id + '"></a></div>';
        tmphtml += '<div class="comment_right">';
        tmphtml += '<div style="padding:0 0 8px 0;">';
        tmphtml += '<span style="float:right;margin-right:10px;_display:inline;">';
        if (comment.auth_deletion) {
            tmphtml += '&nbsp;&nbsp;<a href="javascript:void(0)" class="j-cmt-remove-btn" name="' + comment.id + '-comment" style="color:gray">删除</a>';
        }
        tmphtml += '</span>';
        tmphtml += '<span class="user_name"><a href="' + VeryCD.Member.i_url(comment.member_id)+'" hoverstyle="3" hovertips="type=2&id=' + comment.member_id + '">' + comment.member_name + '</a></span> ' +comment.member_rank;
        tmphtml += '</div>';
        tmphtml += '<div class="block" style="overflow:hidden;"><p class="p">' + (comment.replied_id > 0 ? '回复' + comment.replied_member_name + '：' : '') + (face_img ? face_img : '') +comment.contents.replace(/\n/g, '<br/>') + '</p></div>';
        tmphtml += '<div class="comment_todo">';
        tmphtml += '<div class="more">';
        tmphtml += '<span class="date-time">'+comment.create_time+'</span>';
        tmphtml += '</div>';
        tmphtml += '<div>';
        tmphtml += '<span><a class="digup j-cmt-dig-btn" href="javascript:void(0)" name="up-' + comment.id + '-comment">支持</a> <span class="j-cmt-dig-view">[<b id="' + comment_hot + 'dig_' + comment.id + '_up">' + comment.digups + '</b>]</span></span>&nbsp;&nbsp;&nbsp;&nbsp;';
        tmphtml += '<span><a class="digdown j-cmt-dig-btn" href="javascript:void(0)" name="down-' + comment.id + '-comment">反对</a> <span class="j-cmt-dig-view">[<b id="' + comment_hot + 'dig_' + comment.id + '_down">' + comment.digdowns + '</b>]</span></span>&nbsp;&nbsp;&nbsp;&nbsp;';
        tmphtml += '<a href="javascript:void(0)" class="j-cmt-reply-btn" name="'+ comment_hot + comment.id + '-0-comment-' + comment.member_name + '">回复</a>';
        tmphtml += '</div>';
        tmphtml += '</div>';
        tmphtml += '</div>';
        tmphtml += '<div class="clear"></div>';
        tmphtml += '</li>';
        return tmphtml;
    }
}

CommentList.init_remove_btn = function(context) {
    var es = Sizzle('.j-cmt-remove-btn', context);
    for (var i = 0; i < es.length; i++) {
        var f = function(event) {
            var obj = get_event_target(event);
            var e = obj.name.split("-");
            var comment_id = e[0];
            var type = e[1];
            var comment_remove_handler = function(rs) {
                if (rs !== 'ok') {
                    return;
                }
                $.ajax({
                    url: '/ajax/i/del_' + type,
                    type: 'post',
                    data: {'id':comment_id},
                    dataType: 'json',
                    success: function(rs) {
                    	VeryCD.hidden_loading();
                        if (rs.code === 0) {
                            if (location.hash) {
                                location.hash = '';
                                VeryCD.Msg('删除成功！',location.href.slice(0, -1));
                            } else {
                                VeryCD.Msg('删除成功！',document.location);
                            }
                        } else {
                            VeryCD.error(rs.msg);
                        }
            		},
                  	beforeSend: function(){
                  		VeryCD.show_loading();
                  	},
                  	error: function() {
                  		VeryCD.hidden_loading();
                        VeryCD.alert('加载页面失败，请重试！');
                  	}
            	});
            }
            VeryCD.confirm({message:'确实要删除？', handler:comment_remove_handler});
            return false;
        }
        es[i].onclick = f;
    }
}
// 初始化口水列表支持、反对按钮
CommentList.init_dig_btn = function() {
	CommentList.bind_dig('.digup', '/stat/comSupport/');
	CommentList.bind_dig('.digdown', '/stat/comAgainst/');
	CommentList.bind_dig('.sup_btn', '/stat/comSupportLeft/');
	CommentList.bind_dig('.opp_btn', '/stat/comAgainstLeft/');
}
CommentList.bind_dig = function(obj, track) {
	var digs = Sizzle(obj, Sizzle('#comments_list')[0]);
	for (var i = 0; i < digs.length; i++) {
		digs[i].onclick = function () {
			// get info
			var name = this.name;
			if (!name) name = this.id;
    		name = name.split("-");
    		var action = name[0];
    		var obj_id = name[1];
    		var type = name[2];

            VeryCD.Track(track);
            CommentList.change_dig_num(obj_id, action, type);
            CommentList.change_dig_disable(obj_id, type);

			// do ajax
            $.ajax({
                url: '/ajax/i/dig_' + type,
                type: 'post',
                data: 'action=' + action + '&' + type + '_id=' + obj_id
        	});
		}
	}
}

CommentList.change_dig_num = function(comment_id, action, type) {
	var dig_btn = Sizzle('#' + action + '-dig-' + comment_id)[0];
	if (dig_btn) {
		var num = Number(dig_btn.innerHTML);
		if (action == 'up') {
			num += 1;
		} else {
			num -= 1;
		}
		dig_btn.innerHTML = num;
	}
	var comment_li = Sizzle('#comment' + comment_id)[0];
    var es = Sizzle('[name=' + action + '-' + comment_id + '-' + type + ']', comment_li)[0];
    if(es) {
        var btn_num = Sizzle('.j-cmt-dig-view', es.parentNode)[0];
        if (btn_num) {
            var matches = /\[<b id="?\b.*"?>(-?\d+)<\/b>\]/i.exec(btn_num.innerHTML);
            var num = Number(matches[1]);
            if (action == 'up') {
                num += 1;
            } else {
                num -= 1;
            }
            btn_num.innerHTML = '[<b>' + num + '</b>]';
        }
    }
}
CommentList.change_dig_disable = function(comment_id, type) {
	var up_dig = Sizzle('#up-' + comment_id)[0];
	if (up_dig) {
    	up_dig.className = up_dig.className.replace('sup_btn', 'sup_btn sup_btn_gray');
    	up_dig.onclick = function() {};
	}
	var down_dig = Sizzle('#down-' + comment_id)[0];
	if (down_dig) {
    	down_dig.className = down_dig.className.replace('opp_btn', 'opp_btn opp_btn_gray');
    	down_dig.onclick = function() {};
	}

	var obj = Sizzle('[name=up-' + comment_id + '-' + type + ']', Sizzle('#comment' + comment_id)[0])[0];
	if(obj) {
    	var comment_div = obj.parentNode.parentNode;
    	var digs = Sizzle('.j-cmt-dig-btn', comment_div);
    	if (digs) {
        	for (var i = 0;i < digs.length; i++) {
        		digs[i].className = digs[i].className.replace('dig', 'digged');
        		digs[i].onclick = function() {};
        	}
    	}
	}
}

CommentList.init_hide_btn = function(context) {
    var es = Sizzle('.j-cmt-hide-btn', context);
    for (var i = 0, btn; (btn = es[i]) != null; i++) {
        var f = function(event) {
            var obj = get_event_target(event);
            var comment_id = obj.name;

            var es = Sizzle('.j-cmt-hide-show-' + comment_id, context);
            if (obj.innerHTML == '-') {
                obj.innerHTML = '+';
                for (var i = 0, e; (e = es[i]) != null; i++) {
                    e.style.display = "none";
                    //VeryCD.resizeDiv(e);
                }
            } else {
                obj.innerHTML = '-';
                for (var i = 0, e; (e = es[i]) != null; i++) {
                    //e.style.display = "block";
                    VeryCD.resizeDiv(e);
                }
            }
        }
        btn.onclick = f;
    }
}

function get_event_target(event) {
    if (event) {
        return event.target;
    } else {
        // IE event
        return window.event.srcElement;
    }
}

CommentList.init(context);
}

commentList_ini(Sizzle('#comments_list')[0]);
</script> 
