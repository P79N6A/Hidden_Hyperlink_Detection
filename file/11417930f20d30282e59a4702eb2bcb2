    root = "";
    $(".top_scall_text").slide({titCell: ".top_scall_dot li", mainCell: ".top_text_ul", autoPlay: true, effect: "left", delayTime: 500});
     //顶部登陆状态
    var url_top_login = "/register/topstate.html?n="+Math.random();
    var url_login = "/register/login.html?ret="+document.location;
    var url_register = "/register/index.html?ret="+document.location;
    var url_member_center = "/member/index.html";
    var url_logout = "/register/logout.html";
    var url_logout2 = "/register/logout2.html";
    function logout() {
        $.ajax({
            url:url_logout,
            success:function(val) {
                if(val == "1") {
                    document.location.reload();
                } else {
                    document.location = url_logout2;
                }
            }
        })
    }
    $.getJSON(url_top_login,function(data){
        if(data.member_id <= 0) {
            $("#top_unlogin_state").html('<div class="resi_btn fr"><a href="'+url_register+'">注册</a></div><div class="login_btn fr"><a href="'+url_login+'">登录</a></div><div class="c"></div>');
            $(".top_right").css('width','472px');
        } else {
            $("#top_unlogin_state").hide();
            var top_msg = '<li class="li_msg"><div class="mymsg" id="top_msg">';
            if(parseInt(data.total_un_read) > 0) {
                top_msg += '<a class="jiao" href="/member/message.html">消息（<font color="#f00">'+data.total_un_read+'</font>）</a>';
            } else {
                top_msg += '<a class="jiao" href="/member/message.html">消息（<span>'+data.total_un_read+'</span>）</a>';
            }
            top_msg += '<div class="top_msg_sub" id="top_msg_sub"><dl>';
            top_msg += '<dd><a href="/member/message.html" target="_blank">私信('+data.total_un_read_1+')</a></dd>';
            top_msg += '<dd><a href="/member/reply.html" target="_blank">回复我的</a></dd>';
            top_msg += '<dd><a href="/member/message_ti.html" target="_blank">@提到我的('+data.total_un_read_2+')</a></dd>';
            top_msg += '<dd><a href="/member/message_tongzhi.html" target="_blank">通知('+data.total_un_read_4+')</a></dd>';
            top_msg += '<dd><a href="/member/message_sys.html" target="_blank">系统消息('+data.total_un_read_3+')</a></dd>';
            top_msg += '</dl></div></div></li>';
            $("#top_login_state").prepend(top_msg);
            var top_member = '<li class="li_member"><div class="mycenter" id="top_member"><a class="jiao" href="/member/index.html" target="_blank"><img src="'+data.member_headpic+'" width="19px" /> '+data.member_nickname+'</a>';
            top_member += '<div class="top_member_sub" id="top_member_sub"><dl>';
            top_member += '<dd><a href="/homepage/index.html" target="_blank">我的圈子</a></dd>';
            top_member += '<dd><a href="/homepage/article.html" target="_blank">我的文章</a></dd>';
            top_member += '<dd><a href="/homepage/fav.html" target="_blank">我的收藏</a></dd>';
            top_member += '<dd><a href="/member/comment.html" target="_blank">我的评论</a></dd>';
            top_member += '<dd><a href="/homepage/guanzhu_m/gid/-1.html" target="_blank">我的好友</a></dd>';
            top_member += '<dd><a href="/member/index.html" target="_blank">会员中心</a></dd>';
            top_member += '<dd><a href="javascript:void(0)" onclick="logout()" >退出</a></dd>';
            top_member += '</dl></div></div></li>';
            $("#top_login_state").prepend(top_member);
            //顶部我的消息下拉
            $("#top_msg").mouseover(function() {
                $("#top_msg_sub").show();
            });
            $("#top_msg").mouseout(function() {
                $("#top_msg_sub").hide();
            });
            //顶部会员昵称下拉
            $("#top_member").mouseover(function() {
                $("#top_member_sub").show();
            });
            $("#top_member").mouseout(function() {
                $("#top_member_sub").hide();
            });
            
            $("#message_member_id_jubao").val(data.member_id);
        }
    });
