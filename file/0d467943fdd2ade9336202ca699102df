<script type="text/javascript">
//新浪微博分享
jQuery(".pa-fx").click(function(){
window.sharetitle = "@华为华为互动平台: 交换机如何查看接口流量";
window.shareUrl = "";
share();
});

function share(){
//d指的是window
(function(s,d,e){try{}catch(e){}
var f;
if (typeof(window.shareUrl) === "undefined")
{
f='http://service.weibo.com/share/share.php?',u=d.location.href,p=['url=',e(u),'&title=',e(window.sharetitle),'&appkey=2b71aS'].join('');
}	
else
{
f='http://service.weibo.com/share/share.php?',u=d.location.href,p=['url=',e(u),'&title=',e(window.sharetitle),'&appkey=2b71aS','&pic=',e(window.shareUrl)].join('');
}	

function a(){if(!window.open([f,p].join(''),'mb',['toolbar=0,status=0,resizable=1,width=620,height=450,left=',(s.width-620)/2,',top=',(s.height-450)/2].join('')))u.href=[f,p].join('');};if(/Firefox/.test(navigator.userAgent)){setTimeout(a,0)}else{a()}})(screen,document,encodeURIComponent);
}
</script>


