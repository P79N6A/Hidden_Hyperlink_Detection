<script type="text/javascript">
_obj = null;
function modifyAuditing(obj,status,sort,id,pid,fid){
_obj = obj;
var reason = '';
if(status==2){
//			modimageaction('banpostImage',id);
//			return ;
showWindow(1,'forum.php?mod=modifyStatus&action=blocked&status='+status+'&sort='+sort+'&id='+id+'&pid='+pid+'&fid='+fid,'get');
return;
}
url = "forum.php";
jQuery.ajax({
type:"post",
url:url,
data:{
"mod":"modifyStatus",
"status":status,
"sort":sort,
"id":id,
"pid":pid,
"reason":reason
},
dataType:"json",
success:function(response){
if(response.status=='success'){
jQuery(response.img).insertBefore(jQuery(obj).parent());
//jQuery(obj).parent().prev().prev().remove();
jQuery(obj).parent().parent().find("img").eq(sort-1).remove();
jQuery(obj).parent().remove();
}
},
error:function(){
alert("网络有问题！请稍后重试！");
}
})
}
function hideOpenedWindow(name,status,sort,id,pid,fid){
var reason = jQuery.trim(jQuery(":input[name='reason']").val());
hideWindow(name);
url = "forum.php";
jQuery.ajax({
type:"post",
url:url,
data:{
"mod":"modifyStatus",
"status":status,
"sort":sort,
"id":id,
"pid":pid,
"fid":fid,
"reason":reason
},
dataType:"json",
success:function(response){
if(response.status=='success'){
jQuery(response.img).insertBefore(jQuery(_obj).parent());
//jQuery(obj).parent().prev().prev().remove();
jQuery(_obj).parent().parent().find("img").eq(sort-1).remove();
jQuery(_obj).parent().remove();
}
},
error:function(){
alert("网络有问题！请稍后重试！");
}
})
}
</script>
