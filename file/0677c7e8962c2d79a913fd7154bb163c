    <script type="text/javascript">
        function addMor(){
            var site = $("#site").val();
            var tid = $("#tid").val();
            var page = $("#page").val();
            var txt = $("#jp-content").html();
            $.ajax({
                type: 'POST',
                dataType:'jsonp',
                url: "//cmsapp.koolearn.com/koowap.php?site="+site+"&tid="+tid+"&a=fenye&page="+page+"&callback=?",
                success: function(obj) {
                    $("#jp-content").html(txt+obj.contents); 
                    $("#page").val(obj.page);
                    if(obj.page == 0){
                        $("#jp-loadMore").css("display","none");
                    }
                }
            });        
        }
    </script>
