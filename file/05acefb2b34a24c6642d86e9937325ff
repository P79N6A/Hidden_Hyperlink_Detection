  <script>
 /* search */
    var oShow1=document.getElementById('searchShow1');
    var oShow2=document.getElementById('searchShow2');

    var timer=null;
    oShow2.onmouseover=oShow1.onmouseover=function ()
    {
        clearTimeout(timer);
        oShow2.style.display='block';
    };
    oShow2.onmouseout=oShow1.onmouseout=function ()
    {
        timer=setTimeout(function (){
            oShow2.style.display='none';
        }, 1000);
    };
	
	function tj(){
		$('#search').submit();
	}
		
	/*登录弹框*/
	function login(){
		art.dialog.open('/index/login',{
			id:'login',
			title:'学生登录',
			lock:true,
			width:550,
			height:327,
			resize: false,
			opacity:0.3 
		});
	}
	function checkskip(){
		var skip = -1;
		if(skip > 0){
			location.href="/stuwork/index";
		}else{
			hidetable();
			art.dialog.open('/index/login?stu=stu',{
				id:'login',
				title:'学生登录',
				lock:true,
				width:550,
				height:327,
				resize: false,
				opacity:0.3 
			});
		}
	}
	function hidetable(){
		$('.popupbox').hide();
		$('.webmap i').css("background","url(/public/images/sxs.png) no-repeat 6px 4px");
	}
</script> 
