   <img src="index.php?cron=comment%3Acommentlist&amp;p=803" width="1" height="1" border="none" /> 
   <div id="template-modal" style="display:none;"> 
    <div class="dialog"> 
     <div class="dialog-title"> 
      <div class="title span-auto">
       {title}
      </div> 
      <div class="dialog-close-btn">
