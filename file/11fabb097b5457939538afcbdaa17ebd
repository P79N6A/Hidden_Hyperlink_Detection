function postViews(sw){
	$.ajax({
		url: document.location.href,
		type:"post",
		data: 'action=postviews&type=' + sw + '&id=14445&rnd=' + Math.random()*1000,
		success: function(data){
			if(data=="no"){
				alert('请不要重复提交！');
				return false;
			}
			if(sw==0){
				$('.ding > span').html(data);
			}else{
				$('.cai > span').html(data);
			}
		}
	});
}
</script> 
