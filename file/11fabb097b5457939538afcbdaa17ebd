	function plDing(id){
		$.ajax({
		url: document.location.href,
		type:"post",
		data: 'action=plDing&ssid='+ id +'&rnd=' + Math.random()*1000,
		success: function(data){
			if(data=="no"){
				alert('请不要重复提交！');
				return false;
			}			
			$('#plding' + id + ' > span').html('['+data+']');			
		}
	});
	}
