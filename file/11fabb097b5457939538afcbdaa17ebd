	function getPingLunPage(strHtml, id, page, act) {
		var strHtmlTitle = '<p style="text-align:center; font-size:12px; color:#000;margin:2px;vertical-align:middle;">';
		strHtmlTitle += '<img src="http://img.pccoo.cn/post/images/loading.gif" style="vertical-align:middle;margin-right:1px;" border="0" />加载中……</p>';
		$("#" + strHtml).html(strHtmlTitle);
		$.post(document.location.href, {
			strHtml: strHtml,
			sid: id,
			action: act,
			page: page
		},
		function(data) {
			if (data != "") {
				$("#" + strHtml).html(data)
			}
		})
	}
	getPingLunPage('pinglun',14445,1,'showList');
	function addPinglun() {
		if ($('#plContent').val() == "" || $('#plContent').val() =="既然路过就说些什么吧（理性上网 文明留言）") {
			alert("评论内容不能为空!");
			$('#plContent').focus();
			return;
		}
		if ($('#plContent').val().length > 2000) {
			alert("评论内容过长，2000字以内!");
			$('#plContent').focus();
			return;
		}
		$.post(document.location.href, {
				action: 'savepinglun',
				plContent: escape($('#plContent').val()),
				sid: 14445,
				reid:escape($('#reid').val())
		},
		function(data) {
			if(data == 2 || data == '2'){
				alert("对不起，您的用户禁止发布留言信息！")
				return false;
			}
			if(data=="no"){
				alert("请不要重复提交！")
				return false;
			}
			if (data == 1 || data == '1') {
				alert('评论提交成功！')
				$("#reid").val("0");
				$("#plContent").val("");
				getPingLunPage('pinglun',14445,1,'showList');
			}
		})
	}
