<script id="fl-theme-custom-js">jQuery(window).scroll(function(){
	if(window.location.search == '?fl_builder') {
		jQuery('.section-links').css('display','block');
	}
	else {
	
		var scroll2 = jQuery(window).scrollTop(), 
		linksBottom = jQuery(".header-main").outerHeight();
	  
	/*	if(scroll2 >= linksBottom) {    
			jQuery(".header-main").addClass("sticky");   
		}
		else {   
			jQuery(".header-main").removeClass("sticky");	   
		}*/
	
		var y = jQuery(this).scrollTop();

		IS_IPAD = navigator.userAgent.match(/iPad/i) != null;
		IS_ANDROID = navigator.userAgent.match(/android/i) != null;
		IS_IPHONE = (navigator.userAgent.match(/iPhone/i) != null) || (navigator.userAgent.match(/iPod/i) != null);

		if( IS_IPAD==false && IS_IPHONE==false && IS_ANDROID==false){
			jQuery('.section-links .page-specific-menu .menu .menu-item a').each(function (event) {
				aboveSection = jQuery(".section-links").prev(".fl-row "); 
				if(jQuery(jQuery(this).attr('href')).length > 0) {
					if (y >= (jQuery(jQuery(this).attr('href')).offset().top - aboveSection.outerHeight()) + 150) { 
						jQuery('.section-links .page-specific-menu .menu .menu-item a').not(this).removeClass('active');
						jQuery(this).addClass('active');
					}
				}
			});
		
			   
			if(jQuery(".section-links").length > 0){
				var scroll = jQuery(window).scrollTop(),	
					aboveSection = jQuery(".section-links").prev(".fl-row ");
					sectionLinksBottom = aboveSection.outerHeight() - 70,
					
				jQuery(window).resize(function(){
					windowHeight = jQuery(window).height();
				});
				
				if(scroll >= (sectionLinksBottom)) {					
					
					jQuery(".fl-page-header").hide();					
					jQuery(".section-links").addClass("sticky");					
				}else {
					
					jQuery(".fl-page-header").show();	
					jQuery(".section-links").removeClass("sticky");
				}
			}
		}
	}
});
jQuery(document).ready(function(){
    
    jQuery('#back-to-top').click(function(e) {
		e.preventDefault();
		jQuery('html, body').animate({scrollTop:0},"slow");
	});
	
	jQuery('#gform_submit_button_21').click(function(e) {
	    jQuery('body').addClass('payment-processing');
		jQuery('body').append('<div class="overlay"></div>');
	});
	
    
});

jQuery(document).ready(function(){

	var a = getUrlVars();
	var href2_val = a['fname'];
	if(href2_val != null) { 

		jQuery('.final-price-button a').each(function() { 
			var href1 = jQuery(this).attr('href');
			var href2 = href1 + '&fname='+href2_val;
			jQuery(this).attr('href',href2);
		});
	}
});

function getUrlVars()
{
    var vars = [], hash;
    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
    for(var i = 0; i < hashes.length; i++)
    {
        hash = hashes[i].split('=');
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];
    }
    return vars;
}
</script>
