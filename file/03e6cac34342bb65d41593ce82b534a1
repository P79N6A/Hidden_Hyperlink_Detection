<!-- =E sumary 摘要 -->                    <script>
    //便利摘要的类目 如果某一条的数据内容为空 那么就删除
    $(document).ready(function(){
        $(".su_con").each(function(i,v){
            if(v!=null){
                try{
                    if($(v).text()!=null&&$.trim($(v).text())==""&&$(v).parent("li")!=null){
                        var isBangBang=false;
                        var child=$(v).children("span");
                        if(child!=null){
                            for(var i=0;i<child.length;i++){
                                if(child[i]!=null&&$(child[i]).attr("id")!=null&&$(child[i]).attr("id")=="bangbangWrapBox"){
                                    isBangBang=true;
                                }
                            }
                        }
                        if(!isBangBang){
                            $(v).parent("li").remove();
                        }
                    }
                }catch(e){
                }
            }
        })
    });
</script>                </div>

