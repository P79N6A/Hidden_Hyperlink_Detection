     <article class="block"> 
      <h1>跑酷训练方法</h1> 
      <time> <span>作者: <a id="author" target="_blank" href="javascript:;"></a></span> <span>2016-03-21 16:26</span> </time> 
      <script type="text/javascript">
var cpro_id="u2413203";
(window["cproStyleApi"] = window["cproStyleApi"] || {})[cpro_id]={at:"3",hn:"1",wn:"2",imgRatio:"1.7",scale:"20.8",pat:"6",tn:"template_inlay_all_mobile_lu_native",rss1:"#FFFFFF",adp:"1",ptt:"0",titFF:"%E5%BE%AE%E8%BD%AF%E9%9B%85%E9%BB%91",titFS:"14",rss2:"#FFFFFF",titSU:"0",ptbg:"70",ptp:"1"}
