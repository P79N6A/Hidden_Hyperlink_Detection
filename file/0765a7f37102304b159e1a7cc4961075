     <script type="text/javascript">
function goto_nomobile_href(){
var f = window.location.href;
if(f.indexOf('mobile=yes') !== -1){
window.location.href = f.replace(/mobile=yes/,'mobile=no');
}else if(f.substr(-12) == '/mobile.html'){
window.location.href = f.replace('/mobile.html', '/index.php?mobile=no');
}else if(f.substr(-1) == '/' || f.substr(-4) == 'html' || f.substr(-3) == 'php'){
window.location.href = f + '?mobile=no';
}else{
window.location.href = f + '&mobile=no';
}
}
</script> 
