(function($) {
var __lastY__ = 0;

  var backTop = $('#backTop');

  function handleScroll() {
    var shown = backTop.hasClass('show');
    var isScrollUp = window.scrollY < __lastY__; //是否向上滚动
    var scrollOverScreen = window.scrollY > 2 * window.innerHeight; //是否滚动超过一屏
    var shouldShowButton = isScrollUp && scrollOverScreen;
    __lastY__ = window.scrollY;
    if (shouldShowButton && !shown) {
      backTop.addClass('show');
    }else if(!shouldShowButton && shown) {
      backTop.removeClass('show');
    }
  }

  window.addEventListener('scroll', handleScroll);
})(jQuery);
</script> 
