     <script type="text/javascript">	
		var w,h,className;
		function getSrceenWH(){
			w = $(window).width();
			h = $(window).height();
			$('#dialogBg').width(w).height(h);
		}
		window.onresize = function(){  
			getSrceenWH();
		}  
		$(window).resize();  
		$(function(){
			getSrceenWH();	
			//显示弹框
			$('#gotop').click(function(){
				
				className = 'flipInX';
				$('#bg_dialog').removeAttr('class').addClass('animated '+className+'').fadeIn();
				
			});
			//关闭弹窗
			$('.clasebgbutton').click(function(){	
				$('#bg_dialog').removeClass('flipInX'); 	
				$('#dialogBg').fadeOut(300,function(){
					$('#bg_dialog').addClass('bounceOutUp').fadeOut();
				});
			});
		});
		</script> 
