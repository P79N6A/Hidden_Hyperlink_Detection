  <script type="text/javascript">
	$.each($('.mycoupon'),function(){
		$(this).css('width',$(this).css('width'));
	});
	var client = new ZeroClipboard($('.mycoupon'));
	client.on("ready",function(readyEvent){
		client.on("aftercopy",function(event){
			$(event.target).html('复制成功');
			});			  
		});
		
		$('.mycoupon').mouseover(function(){
			$(this).html('点击复制');
		});

		$('.mycoupon').mouseout(function(){
			$(this).html($(this).data('clipboard-text'));
		});
		
    function shoucang(ids){
        var title = document.getElementById("post_title").value;
        var n = 1;
        var num = Number(document.getElementById("num").value) + Number(n);
        $.ajax({
            url: '/user/Favorite/do_favorite',
            data: {object_id: ids, title: title},
            type: 'post',
            dataType: 'json',
            success: function(data){
                if(data === 1){
                    alert("亲，你已收藏过了哦");
                    return false;
                }
                if(data === 2){
                    document.getElementById("nums").innerHTML = num;
                    alert("收藏成功");
                    return false;
                }
                if(data === 3){
                    alert("收藏失败");
                    return false;
                }
                else{
                    alert(data.info);
                }
            }});
    }
</script>
