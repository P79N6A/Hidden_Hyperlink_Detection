  <footer id="footer"> 
   <!-- rightLayer --> 
   <div class="" id="rightLayer" style="display: block; right: 91.5px; position: fixed; bottom: 0px;"> 
    <a href="javascript:void(0);" class="goTotop" id="backToTop" title="返回顶部" style="display: none;"> 
     <!--[if lt IE 8]>返回顶部<![endif]--> <img src="/tpl/newindex/Public/images/bacaoo-pic_26.jpg" alt="" /> </a> 
   </div> 
   <script type="text/javascript">
        $(function(){
            slidingFunction();
            $(".goTotop").click(function(){
                $("html, body").animate({scrollTop: 0}, 150)
            });
        })
        $(window).scroll(function () {
            slidingFunction()
        });
        $(window).resize(function () {
            slidingFunction();
        });
        slidingFunction = function () {
                var M = navigator.userAgent.toLowerCase();
                var TestIE = {
                    IE: /msie/.test(M),
                    OPERA: /opera/.test(M),
                    MOZ: /gecko/.test(M),
                    IE5: /msie 5 /.test(M),
                    IE55: /msie 5.5/.test(M),
                    IE6: /msie 6/.test(M),
                    IE7: /msie 7/.test(M),
                    SAFARI: /safari/.test(M)
                };
                var footer_height = $("#footer").height();
                var winw = $(window).width();
                var winh = $(window).height();
                var sectionWidth = $("section.wrap").width();
                sectionWidth = '1050';
                var leftObjHeight = $("#leftLayer").height();
                var rightObjHeight = $("#rightLayer").height();
                var sideNum = winw / 2 - sectionWidth / 2 - 10 - 48;
                var topNum = winh / 2 - leftObjHeight / 2 - 50;
                if (sideNum > 0) {
                    $("#leftLayer").show().css({"left": sideNum, "top": topNum});
                    $("#rightLayer").show().css("right", sideNum)
                } else {
                    if (sideNum = 0) {
                        $("#leftLayer").show().css({"left": 0, "top": topNum});
                        $("#rightLayer").show().css("right", 0)
                    } else {
                        $("#leftLayer").hide();
                        $("#rightLayer").hide()
                    }
                }
                var scrollTop = document.body.scrollTop || document.documentElement.scrollTop;
                (scrollTop > 300) ? $("#backToTop").css("display", "block") : $("#backToTop").css("display", "none");
                if (TestIE.IE6) {
                    $("#rightLayer").css("top", scrollTop + winh - 166)
                }
                var bodyHeight = $("body").height();
                var back_to_top = document.getElementById("rightLayer");
                if (back_to_top) {
                    var top_element_height = $("#rightLayer").height() + 18;
                    if (bodyHeight <= scrollTop + winh + footer_height - top_element_height + 135) {
                        var this_height = scrollTop + winh + footer_height + 20 - bodyHeight;
                        $("#rightLayer").css({position: "fixed", bottom: this_height})
                    } else {
                        $("#rightLayer").css({position: "fixed", bottom: 0})
                    }
                }
            };
    </script> 
   <!-- rightLayer end --> 
