    <!-- firstHeading --> 
    <div class="wiki-tj-tit border-b"> 
     <h3 id="firstHeading">全程高能盘点世界六大最怪枪械</h3> 
     <div class="fn-clear"> 
      <span id="share" class="fn-right share-con fn-clear"></span> 
      <span class="fn-left">最后更新：2015-07-30 14:56</span> 
     </div> 
    </div> 
    <!-- /firstHeading --> 
    <!-- bodyContent --> 
    <div id="bodyContent"> 
     <!-- tagline --> 
     <!-- /tagline --> 
     <!-- subtitle --> 
     <div id="contentSub"></div> 
     <!-- /subtitle --> 
     <!-- catlinks --> 
     <div id="catlinks" class="catlinks"> 
      <div id="mw-normal-catlinks" class="mw-normal-catlinks">
