  <script>
    	var simulator={
				simulatorBox:$('.simulatorBox'),
				yq_btn:$('.yq_btn'),
				bs_btn:$('.bs_btn'),
				clear_btn:$('.clear_btn'),	
				int:function(){
					simulator.main();
				},
				main:function(){
					var i=j=numbar=0;
					simulator.addDom();
					simulator.clearData();
				},
				//创建文档
				addDom:function(){
					//友情
					simulator.yq_btn.on('click',function(){
						var randomNum=parseInt(Math.random()*yqInfo.length);
						var insert_list="<a class='a_box' href='"+yqInfo[randomNum].url+"'><div><b><img src="+yqInfo[randomNum].img+" alt='' title='' /></b><font>"+yqInfo[randomNum].num+"</font><cite>"+yqInfo[randomNum].Level+"</cite></div><span><img src="+yqInfo[randomNum].bg+" alt='' title='' /></span></a>";
						simulator.simulatorBox.prepend(insert_list);
						var $a_box=$('.a_box');
					    $a_box.hide().fadeIn(0,function(){
							$(this).find('span').delay(1000).fadeOut(800);
						});
						//.animate({'left':	len*$a_box.width()}); 
					});
					//宝石
					simulator.bs_btn.on('click',function(){
						var randomNum=parseInt(Math.random()*bsInfo.length);
						var insert_list="<a class='a_box' href='"+bsInfo[randomNum].url+"'><div><b><img src="+bsInfo[randomNum].img+" alt='' title='' /></b><font>"+bsInfo[randomNum].num+"</font><cite>"+bsInfo[randomNum].Level+"</cite></div><span><img src="+bsInfo[randomNum].bg+" alt='' title='' /></span></a>";
						simulator.simulatorBox.prepend(insert_list);
						var $a_box=$('.a_box');
					    $a_box.hide().fadeIn(0,function(){
							$(this).find('span').delay(1000).fadeOut(800);
						});
						//.animate({'left':	len*$a_box.width()}); 
					});
				},
				//清空数据
				clearData:function(){
					simulator.clear_btn.on('click',function(){
						simulator.simulatorBox.empty();
					});
				}
				
			}
		simulator.int();
    
  
  
  
  
  </script> 
