  <script language="javascript">
$("#ewmbox").load('/wap/site_ajax.asp?id=3',function(Data){
			if(ewmdata.isOpen==1){				
				var ewmsj=(ewmdata.mobiUrl1!="")?'<div class="mob_bot"><img src="'+ewmdata.mobiUrl1+'" title="'+ewmdata.mobiTit1+'" /></div>':'';
				var ewmwx=(ewmdata.mobiUrl2!="")?'<div class="wex_bot"><img src="'+ewmdata.mobiUrl2+'" title="'+ewmdata.mobiTit2+'" /></div>':'';		
					$("#ewmbox").html(ewmsj+ewmwx);	
			}				
})
$("#b_close").click(function(){
$("#b_login").stop().fadeOut("slow");
});
</script> 
