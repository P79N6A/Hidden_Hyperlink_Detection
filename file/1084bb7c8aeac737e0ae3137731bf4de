  <script type="text/javascript" src="images/Xslider.js"></script> 
  <script type="text/javascript">
$(function(){
    //左右切换分页效果的实现；
    var totalnum=$(".hzyhshow li").length,
        numperpage=4,
        pages=Math.ceil(totalnum/numperpage),
        temp=$(".hzyhwraper ul");
    
    function app(){
        temp=$("<ul />").append(temp.find("li:gt("+(numperpage-1)+")")).appendTo($(".hzyhwraper"));
        if(temp.find("li").length>numperpage){
            app();
        }
    }
    app();

    $(".hzyhshow").Xslider({
        scrollobj:".scrollwraper",
        unitdisplayed:4,
        numtoMove:1,
        viewedSize:840,
        unitlen:210
    });

    var totalnum_1=$(".zlhzshow li").length,
        numperpage_1=4,
        pages_1=Math.ceil(totalnum_1/numperpage_1),
        temp_1=$(".zlhzwraper ul");
    
    function app_1(){
        temp_1=$("<ul />").append(temp_1.find("li:gt("+(numperpage_1-1)+")")).appendTo($(".zlhzwraper"));
        if(temp_1.find("li").length>numperpage_1){
            app_1();
        }
    }
    app_1();
    
    $(".zlhzshow").Xslider({
        scrollobj:".scrollwraper",
        unitdisplayed:4,
        numtoMove:1,
        viewedSize:840,
        unitlen:210
    });
    
    $("a.abtn").focus(function(){this.blur();});
})
</script> 
