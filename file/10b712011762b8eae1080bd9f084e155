  <script language="javascript">
function trim(str){
	for(var i=0; i<str.length&&str.charAt(i)==" "; i++);
	for(var j=str.length; j>0&&str.charAt(j-1)==" "; j--);
	if(i>j) return  "";
	return str.substring(i,j);
}
	 var ctPage=document.getElementById('news_content4');
	 var ctnews=trim(ctPage.innerHTML).replace(new RegExp("\"","g"),"").replace(new RegExp("'","g"),"");
	 if(ctnews!=null&&ctnews!="")
	 {
		 var content4=new Array();
		 content4=ctnews.split("，");
		 var n_pages=parseInt('1');
		 var n_page=parseInt('1');
		 var news_id='010291191';
		 var url='';
		 if(n_page<=10)
			url='http://news.house365.com/gbk/hfestate/system/2011/04/15/010291191.shtml'.replace(news_id+"_0"+(n_page-1),news_id);
		 else
			url='http://news.house365.com/gbk/hfestate/system/2011/04/15/010291191.shtml'.replace(news_id+"_"+(n_page-1),news_id);
		 var strclass="";
		 var url2='';
		 var daohang_l='';
		 var daohang_r='';
		if(n_pages<=14)
		{
			for(var i=1;i<=n_pages;i++)
			{
				if(i==n_page)
					strclass="red";
				else
					strclass="blue_txt";
				if(i>1)
				{
					if(i<=10)
						url2=url.replace(news_id,news_id+"_0"+(i-1));
					else
						url2=url.replace(news_id,news_id+"_"+(i-1));
				}
				if(i==1)
					url2=url;
				if(i%2==1)
				{
					daohang_l+='<li><a href="'+url2+'" class="'+strclass+'">第'+i+'页 '+content4[i-1].substring(0,38)+'</a></li>';
				}else
				{
					daohang_r+='<li><a href="'+url2+'" class="'+strclass+'">第'+i+'页 '+content4[i-1].substring(0,38)+'</a></li>';
				}
			}
		}else
