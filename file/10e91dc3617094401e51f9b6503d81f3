  <script type="text/javascript">
window.onload = function () {
    recUrlInfo();
}
function getParamVals(){
    var item,value,obj={},
        href=location.href,
        str=href.substr(href.lastIndexOf('?')+1),
        arr=str.split('&');
    for(var i=0;i<arr.length;i++){
        item=arr[i].split('=');
        obj[item[0]]=item[1];
    }
    return obj;
}
/**
 * 记录用户打开网站瞬间的行为信息
 */
function recUrlInfo(){
    //获取当前url，分析并分析参数
    var title = document.title;
    var platform = getParamVals().pla;
    if(platform == '360'){
        platform = '360bid';
    }
    if(platform==null){
        platform = 'self';
    }else{
        //批量修改当前页面href,补充参数？platform=....
        var _el = [], _el= document.getElementsByTagName('a');
        for (var i=0; i<_el.length; i++ ) {
            // 得到的a标签的href
            oldurl = _el[i].href;
            // 得到a标签的内容
            //content = _el[i].innerText;
            var patt1=new RegExp("pla=");
            var isWith = patt1.test(oldurl);
            if(oldurl != undefined){
                var isWork = oldurl.length>10;
            }
            if(!isWith && isWork){    //当前url不包含pla=
                var _url = oldurl+"?pla="+platform;
                _el[i].setAttribute("href",_url);
            }
        }
    }
    //截取非参数部分
    url = window.location.href.split('?')[0];   //原url
    var url2 = window.location.href;  //完整url
    var param = {'url':url,'url2':url2,'platform':platform,'title':title};
    sendPost("http://m.galaxy-immi.com/Index/addUrl",param);
}

function sendPost(url,param) {
    // var param = {
    //     "name1": "value1",
    //     "name2": "value2"};
    param = (function(obj){ // 转成post需要的字符串.
        var str = "";
        for(var prop in obj){
            str += prop + "=" + obj[prop] + "&"
        }
        return str;
    })(param);

    var xhr = new XMLHttpRequest();
    xhr.open("POST", url, true);
    xhr.setRequestHeader("Content-type","application/x-www-form-urlencoded");
    xhr.onreadystatechange = function(){
        var XMLHttpReq = xhr;
        if (XMLHttpReq.readyState == 4) {
            if (XMLHttpReq.status == 200) {
                var text = XMLHttpReq.responseText;
                console.log(text);
            }
        }
    };
    xhr.send(param);
}

</script> 
