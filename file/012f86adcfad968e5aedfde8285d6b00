  <script type="text/javascript">
function toggle_collapse(id,id2){
var id = document.getElementById(id);
var tesc = document.getElementById(id2);
if(id.style.display !='none'){
id.style.display ='none';
tesc.innerHTML ='展开';
}else{
id.style.display ='';
tesc.innerHTML ='收起';
}
}
function overheight(id,id2){
var id = document.getElementById(id);
var tesc = document.getElementById(id2);
if(id.style.height !='auto'){
id.style.height ='auto';
tesc.innerHTML ='收起';
}else{
id.style.height ='';
tesc.innerHTML ='展开';
}
}
