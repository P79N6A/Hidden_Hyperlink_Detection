//键盘左右键控制页面跳转：37左键--39右键
document.onkeydown=function(e){
	e=window.event||e;
	if (e.keyCode==37){
		turnPerPage();
	 }else if(e.keyCode==39){
	 	turnNextPage();
	 }
} 
