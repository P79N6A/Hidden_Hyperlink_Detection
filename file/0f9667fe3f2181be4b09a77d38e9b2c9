 var keywords="黄冈市公安抗洪救灾 黄冈公安救灾 湖北公安抗洪救灾";
 jQuery.getJSON("http://sousuo.cpd.com.cn/jms/relation?r="+keywords+"&j=?",function(data){
  if(data)
  {
   var jsonstr="<div id=\"relationnewsbox\"><div class=\"relation_title\">相关文章</div><ul>";
   jQuery.each(data,function(i,n){
    jsonstr+="<li><a href='"+n.url+"'>"+n.title+"</a></li>";
   });
   jsonstr+="</ul></div>";
      if(jQuery(data).size()>0)
   jQuery("#rrnews").after(jsonstr);
  }  
 });
});
</script> 
