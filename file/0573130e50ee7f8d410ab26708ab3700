		   data: weixinData.data,
		   dataType:"jsonp",
		   success: function(data){
			   wx.config({
			        debug: false,
			        appId: data.AppId,
			        timestamp: data.Timestamp,
			        nonceStr: data.NonceStr,
			        signature: data.Signature,
