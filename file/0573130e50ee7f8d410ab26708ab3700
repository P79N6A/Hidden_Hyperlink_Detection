	/* 统计点击事件 begain　  */
    var _ah_click = _ah_click || [];
    try{
        !function(doc){
        var _as = doc.createElement('script');
        _as.type = 'text/javascript';
        _as.async = true;
        _as.src = '//x.autoimg.cn/bi/common/ah_click_s.min.js?d=' + (new Date()).toLocaleDateString().replace(/\//g, "");
        var s = doc.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(_as, s);
        }(document);
    }catch(e){};

    function readCookie(name) {
        var reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
        var r = document.cookie.match(reg);
        if (r != null) {
            return decodeURIComponent(r[2]);
        }
        return 0;
    }

    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null)return unescape(r[2]);
        r = window.location.hash.substr(1).match(reg);
        if (r != null)return unescape(r[2]);
        return null;
    }
    /* 点击事件统计 end　  */

</script>
<script>
    /**
     * 百万代理人种cookie
     */
    function initMillionAgent(){
        var platformActivityID = getQueryString("PlatformActivityID");
        var platformAgentID = getQueryString("PlatformAgentID");
        var millionAgentJson = "";
        if(platformActivityID != null && platformActivityID != "" && platformAgentID != null && platformAgentID != ""){
            millionAgentJson = "{\"PlatformActivityID\":\""+platformActivityID+"\",\"PlatformAgentID\":\""+platformAgentID+"\"}";
            var date = new Date();
            date.setHours(date.getHours() + 1);
            document.cookie = "millionAgent=" + millionAgentJson + "; expires=" + date.toGMTString() + "; path=/; domain=mall.autohome.com.cn";
        }
    }

    initMillionAgent();
</script>
<script>
    /**
     * 更新默认提车城市
     * @param cityId
     * @returns {boolean}
     */
    function selectNewDefaltCity(cityId) {
        var result = false;
        if (!cityId ||cityId < 1 || cityId == 999999) {
            result =  false;
        }
            if (cityId == 130300) {
                $("#js-city-selected >div >dl >dd").text("秦皇岛");
                $("#select-city-value").attr("value",130300);
                $("#select-city-value").attr("allowInstallment","0");
                $("#select-city-bak").attr("value",130300);
                $("#select-city-bak").attr("name","秦皇岛");
                $("#select-city-bak").attr("allowInstallment","0");

                //设置城市浮层中城市选中状态
                $("#layer-selcity-item-cell-"+cityId).parent().addClass("current");

                //刷新sku浮层中底层按钮的文案
                refreshSkuBuyButtonText(cityId);
                result = true;
            }
        if(!result){
            return false;
        }
        refreshIntentionDealer(cityId);
        return result;
    }

    /**
     * 初始化城市定位
     */
    if(detailPageData.frontData.basic.collectSaleLeads&&detailPageData.frontData.basic.source==4){
    }else{
        if(!selectNewDefaltCity(__CITY_ID)) {
            var curCityID = getCityIdFromCookie();
            if(curCityID == null) {
                curCityID = 0;
            }

            if(!selectNewDefaltCity(curCityID)) {
            }
        }
    }


    //领取红包弹层
    var redPacketInit = false;
    var redPacketScroll = null;
    $('#layerIscrollRedpacket').on('touchmove', function (e) { e.preventDefault(); });
    $("#js-getredpacket").click(function () {
        //iscroll滚动部分
        setTimeout(function(){
            if(!redPacketInit){
                redPacketScroll = new IScroll('#layerIscrollRedpacket',{click:true});
                redPacketInit = true;
            }
            redPacketScroll.refresh();
        },30);

        $("#js-layer-redpacket").layer('show');
        return false;
    });
    
    //双十一文案
    var serverTimeURL = getServerTimeURL();
        $.ajax({
            url:serverTimeURL,
            dataType:'jsonp',
            data:'',
            jsonp:'callback',
            error: function (data) {

            },
            success:function(result) {
                var reusult = eval(result);
                if(reusult && reusult.returncode == 0) {
                    detailServerTime = result.result;
                    if (detailServerTime > 0) {
                    	refreshWords(detailServerTime);
                    }
                }
            },
            timeout:2000
        });
        
  function refreshWords(detailServerTime){
	  try {
	        $.ajax({
	            url:__CONTEXT_PATH + "/detail/getActivityStep",
	            dataType:'json',
	            data:{nowDate:detailServerTime},
	            error: function (data) {
	                
	            },
	            success:function(results) {
	            	var step = 0;
	            	var result = JSON.parse(results);
	                if(result && result.resultCode == "0") {
	                	step = result.step;
	                }
                    if(step == 1){ //预热期显示
                    	$("#infoTipId").show();
                    }else{
                    	$("#infoTipId").hide();
                    }
	            },
	            timeout:3000
	        });
	    } catch (e) {
	        console.log(e);
	    }
  }
</script>

