			                link: weixinData.data.url, // 分享链接
			                imgUrl: weixinImgUrl, // 分享图标
			                success: function () {
			                    // 用户确认分享后执行的回调函数
			                    $(".layer-bg").hide();
			                    $(".submittip-layer").hide();
			                    //window.location.href = window.location.href + "?flag=1";

			                },
			                cancel: function () {
			                    // 用户取消分享后执行的回调函数
			                }
			            });
			            wx.onMenuShareAppMessage({
			            	title: weixinTitle,  // 分享标题
			                desc: weixinDesc, // 分享描述
			                link: weixinData.data.url, // 分享链接
			                imgUrl: weixinImgUrl, // 分享图标
			                type: 'link', // 分享类型,music、video或link，不填默认为link
			                dataUrl: '', // 如果type是music或video，则要提供数据链接，默认为空
			                success: function () {
			                    // 用户确认分享后执行的回调函数
			                    $(".layer-bg").hide();
			                    $(".submittip-layer").hide();
			                    //window.location.href = window.location.href + "?flag=1";
			                },
			                cancel: function () {
			                    // 用户取消分享后执行的回调函数
			                }
			            });
			        });
			        wx.error(function (res) {
			        });
		   }
		}); 
    }
     

    </script>
