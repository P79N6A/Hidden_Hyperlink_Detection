<div class="btn-placeholder"></div><script>
	function getAppUrl(url){
        url = getAdaptiveProtocolFullUrl(url);
        return "autohome://insidebrowser?url="+encodeURIComponent(url);
    }

    /**
     *获取自适应协议url地址。url以//开头
     * @param url
     * @return 返回新url
     */
    function getAdaptiveProtocolUrl(url) {
        if(url==null){
            return url;
        }
        if(url.indexOf("://")>0){
            url = url.substring(url.indexOf("://") + 1);
        }else if(!(url.indexOf("/")==0) && url.indexOf(".")>-1){     // url以domain开头，这么判断准确吗？
            url = "//"+url;
        }
        return url;
    }

    /**
     * 获取自适应协议下的完整url
     * 例：//domain/test/a.html、 --> http(s)://domain/test/a.html
     *     domain/test/a.html --> http(s)://domain/test/a.html
     *     http(s)://domain/test/a.html情况--> protocolFlag=1时，此情况坚定使用自带的http(s)协议，否则取当前页面的协议
     *     不包括 /test/a.html情况
     * @param url
     * @returns {string}
     */
    function getAdaptiveProtocolFullUrl(url,protocolFlag){
        if(protocolFlag!=null && protocolFlag==1){
            if(url.indexOf("http://") == -1 && url.indexOf("https://") == -1 ){
                if(url.indexOf("//")== 0){        // url以//开头
                    url = url.replace("//",location.protocol+"//");
                }else if(!(url.indexOf("/")==0) && url.indexOf(".")>-1){     // url以domain开头，这么判断准确吗？
                    url = location.protocol+"//"+url;
                }
            }
        }else{
            url = getAdaptiveProtocolUrl(url);
            url = location.protocol + url;
        }
        return url;
    }
