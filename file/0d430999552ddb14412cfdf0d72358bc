    $(function () {
        // 如果是web端展示title,未登录就显示登录按钮,登录就隐藏
        if(App.isWeb){
            $(".top_border").show();
            $.ajax({
                url: Esf.contextPath + "/jf-order/auth/checklogin/02",
                type: "get",
                dataType: 'json',
                success: function (result) {
                    if (result.resultcode != "0000") {
                        $("#login").show();
                        return;
                    }
                }
            });

