<script>
    ////附近商圈
    var alldt = $(".hotarea>dt");
    function showNear(dt){
        alldt.each(function(){
            if(this == dt) {$(this).next("dd").show();$(this).addClass("select");}
            else {$(this).next("dd").hide();$(this).removeClass("select");}
        });
    }
    alldt.each(function(){
        $(this).click(function(){showNear(this);}).hover(function(){showNear(this);});
    });
    showNear(alldt[0]);
    boot.require('dom, anim.hover', function(Frame, dom, hover){
        var cc = $('#infolist');
        cc.delegate('tr:not(#jjh_trn)','mouseover',function(){
            $(this).addClass('bg');
            var oJjtab=$(this).find('a.jj_reason');
            if(oJjtab.length>0){
                oJjtab.show();
            }
        }).delegate('tr:not(#jjh_trn)','mouseout',function(){
            $(this).removeClass('bg');
            var oJjtab=$(this).find('a.jj_reason');
            if(oJjtab.length>0){
                oJjtab.hide();
            }
        });
    });
    $(".selector").each(function(){
        $(this).hover(function(){$(this).addClass("hover")},function(){$(this).removeClass("hover")});
    });

    $('#filter_slide').click(function(){
        //show_more_filter=1默认展开状态
        var show_more_filter;
        boot.require('util.cookie', function(Frame, cookie){show_more_filter = cookie.get('show_more_filter');});
        if(show_more_filter == '1'){
            $(this).removeClass('icon_fang_on');
            $('.secitem:gt(6)').hide();
            boot.require('util.cookie', function(Frame, cookie){cookie.set('show_more_filter', '0');});
        }
        else{
            $(this).addClass('icon_fang_on');
            $('.secitem:gt(6)').show();
            boot.require('util.cookie', function(Frame, cookie){cookie.set('show_more_filter', '1');});
        }
    });


    $('#infolist .img_list').hover(function(){
        var _this = $(this);
        var span_img = _this.find('.span_img');
        var href = _this.find('a:eq(0)').attr('href');
        if(_this.attr('show') != '1'){
            var innerHTML = $.trim(span_img.html());
            if('' != innerHTML){
                var html = '<ol class="span_ol">', imgArray = innerHTML.split('|');
                var picHost = 'http://pic.58.com';
                html += '<li><a target="_blank" href="' + href + '"><img width="310" height="210" src="' +　picHost + imgArray[0]　+　'" /></a></li></ol>';
                html += '<ul class="span_ul">';
                imgArray = imgArray.slice(0, 5);
                for(var i = 0, len = imgArray.length ; i < len ; i ++){
                    html += '<li><a target="_blank" href="' + href + '"><img src="'　+ picHost + imgArray[i].replace('big', 'tiny') + '" /></a></li>';
                }
                html += '</ul>';
                _this.attr('show', '1');
                span_img.html(html).show();
                //预加载大图
                for(var i = 0, len = imgArray.length ; i < len ; i ++){
                    var img = new Image();
                    img.src = picHost + imgArray[i];
                }
                _this.find('.span_ul img').mouseover(function(){
                    var src = $(this).attr('src');
                    span_img.find('ol img').attr('src', src.replace('tiny', 'big'));
                });
            }
        }
        else{
            span_img.show();
        }
    },function(){
        $(this).find('.span_img').hide();
    });

    (function(){
        var city = ____json4fe.locallist[0].listname;
        var yiji = ____json4fe.catentry[0].listname;
        var erji = ____json4fe.catentry[1].listname;
        var info = '/' + city + '/' + yiji + '/' + erji + '/';
        $('.list-datu').click(function(){
            _gaq.push(['pageTracker._trackEvent', 'list', 'show_image_link_click', info]);
        });
    })();
    require(['house/normalslide','house/xf_tongji'],function(slide,tongji){
        var s=new slide($('#jjh_ab'),'li',729+27,189,$('#jjh_left'),$('#jjh_right'));
        s.init();
