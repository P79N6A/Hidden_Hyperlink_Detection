<script>
    //这段为了hack列表中最后两条广告的宽度错乱问题
    function width_hack(){
        if(window._58ad_config){
            $('#bottom_google_ad td[class=img]').css({width:190});
        }
        //防止网络阻塞循环检测广告是否加载
        else if(width_hack_count++ < 10){
            setTimeout(width_hack, 1000);
        }
    }
    var width_hack_count = 1;
    width_hack();
    //品牌房源tip
    $(".qj-jp").hover(function(){
        $(this).find('span').show();
    },function(){
        $(this).find('span').hide();
    });
</script>
