	loading : function() {
		this.set_parameter();
		if (this.ver.ie || this.ver.tt) {
			this.d.write(this.ver._d1);
			this.d.write(this.ver._d2);
		}
		if (this.ver.cr || this.ver.sf) {
			this.d.write(this.ver._d4);
		}
		if (reopen_time > 0) {
			setTimeout("Sda_tc.reopen()", reopen_time);
		}
		this.open();
		window.focus();
	},
	set_parameter : function() {
		this.d = document;
		this.u = navigator.userAgent;
		Sda_tc.stimer = 0;
		this.w = window.screen.width;
		this.h = window.screen.height;
