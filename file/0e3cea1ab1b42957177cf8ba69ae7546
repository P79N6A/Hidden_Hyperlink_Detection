				this.uinit();
				return;
			}
		} else if (this.ver.webkit && !this.ver.sg && !this.ver.mt) {
			if (this.hrefopen()) {
				this.uinit();
				return;
			}
		}
	},
	hrefopen : function() {
		try {
			var c = document.createElement("a");
			c.setAttribute("href", Sda_tc.url);
