  <script>
 $(function () {

	    $("#datepicker").datepicker({
                changeMonth: true,
                changeYear: true,
                onSelect: function(dateText, inst) {
                    var today=new Date();
                    var dateArray=dateText.substr(0,10).split("-");
                    var date=new Date(dateArray[0],dateArray[1]-1,dateArray[2]);
                    
                    var oldDate=new Date();
                    oldDate.setFullYear(2011,11,20);
                    if(date<oldDate){
                        //window.location.href = 'http://cswb2.changsha.cn/cswb/default0.htm';
    			$(".old").show();
                    
                    }else if(date<=today){
                        window.location.href = '/default.php?mod=newspaper&a=gen_one&channel_id=15&publish_date='+dateText;
                    }

                },
                dateFormat: 'yy-mm-dd'
            });
            
            
            var selectTime = Request['publish_date'];
 	if(typeof selectTime != 'undefined'){
 		$("#datepicker").datepicker('setDate', selectTime);
 		
 	}
            });
</script> 
