var oldwhichEl;
function showsubmenu(sid){
  whichEl = eval("submenu" + sid);
  whichimg = eval("imgmenu" + sid);

  try{   
 if(whichEl!=oldwhichEl){
     eval("oldwhichEl.style.display=\"none\";");  
	 eval("oldwhichimg.background=\"images/t1.jpg\";");}

  }   
  catch(error){   
       oldwhichEl=whichEl;
	   oldwhichimg=whichimg;

  }   
if (whichEl.style.display == "none")
{
eval("submenu" + sid + ".style.display=\"\";");
eval("imgmenu" + sid + ".background=\"images/t2.jpg\";");
}
else
{
eval("submenu" + sid + ".style.display=\"none\";");
eval("imgmenu" + sid + ".background=\"images/t1.jpg\";");
 }
oldwhichEl=whichEl;
oldwhichimg=whichimg
 }
</script> 
