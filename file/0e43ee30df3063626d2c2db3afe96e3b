<script type="text/javascript">
var currentPage = 'safeSevice';
window.logined = '0';
window.action = '';
var _domain = "http://safe.sdo.com";
$(function(){
	$("#"+currentPage).addClass('cur');
	$(".title,.safe").hover(function () {
	    $(this).find("div").css({'display':'block','z-index':9999});
	  },
	  function () {
	    $(this).find("div").css('display','');
	  }
	);
	$(".nav > li").hover(function(){
		   if ($(this).attr('class') != "g")$(this).addClass('cur');
		},
		function () {
		 if (currentPage != $(this).attr('id')) $(this).removeClass('cur');
		}
	)
	$(".sub_list").find('li').hover(function(){
		$(this).addClass('cur');
		},
		function () {
		  $(this).removeClass('cur');
		}
	)
	if(action != '' && typeof ($_we_safe[action]) == 'function')
	{

		var todo = $_we_safe[action];
		todo();
	}

})
</script>

