<script type="text/javascript">

    function openOSWin()
    {
        //安全页面的gameid定义为9992；
        //参数source，其中静态文字入口的source参数为010103  如为静态图片入口source为010104，
        //开头语是指接通客服后第一句话，可以定义为安全问题
        open_snda_os("9992","010104","您好",{ar:1,sv:1});
    }
</script>
