  <script>

$(function(){
	$("#content").click(function(){
		var con_val = $(this).val();
		if(con_val=='发表一下你的回应吧！'){
			$(this).attr("value", "");
		}
	})

	$("#content").blur(function(){
		var con_val = $(this).val();
		if(con_val==''){
			$(this).attr("value", "发表一下你的回应吧！");
		}
	})

	$("#content").keyup(function(){
		var text = $(this).val(); 
		var counter = text.length; 
		$("#word_num").text(counter);
		if(counter > 800){
			alert("最多不能超过800字符哦！");
			return;
		}
	})

})


function replay_show(pid, username){
	$("#replay_show_"+pid).show();
	if(username){
		$("#text_"+pid).text("@"+username);
	}
	
}
//回复评论
function sub_reply_msg(pid,userid){
	var reply_txt = $("#text_"+pid).val();
	 $.post('/user-child_comment',{articleid:'',message:reply_txt,pid:pid,touserid:userid},function(data){
		   $(".ul_reply_"+pid).after(data);
			$("#text_"+pid).attr("value","");
			alert("回复成功");
			location.reload();
		});

}

function toggle_reply(pid){
	var str = $(".is_reply_"+pid).text();
	if(str=='收起回复'){
		$("#toggle_reply_"+pid).slideUp();
		$(".is_reply_"+pid).text("打开回复");
	}else{
		$("#toggle_reply_"+pid).slideDown("slow");
		$(".is_reply_"+pid).text("收起回复");
	}

}

function is_toggle(pid, num){
	var str = $(".is_hide_"+pid).text();
	if(str=='点击显示'){
		$(".def_show_"+pid).slideDown();
		$(".is_hide_"+pid).text("点击收起");
		$(".num_"+pid).text("0");
	}else{
		$(".def_show_"+pid).slideUp("slow");
		$(".is_hide_"+pid).text("点击显示");
		$(".num_"+pid).text(num);
	}

}
 function p_submit(){
	 //获取发表框内容
	  var messageinfo = $("#content").val();
	  if(messageinfo == '发表一下你的回应吧！'){
		  alert("字数不能为空。");
		  return false;
	  }
	  var messagelen = messageinfo.length;
	  if(messagelen < 5){
		alert("字数不能小于五个字。");
		return false;
	  }
	
  }


function commend_up(pid,num){
	var tmp_pid = getCookie("commend_"+pid);
	 // tmp_pid = $.cookie("commend_"+pid); 
	if(tmp_pid == null){
		$.get( '/book-commend_up?pid='+pid, function(data){
		data = eval("'"+data+"'");
		if(data==1){
			var n = num+1;
			$(".up_"+pid).text('('+n+')');
			setCookie("commend_"+pid, "1", 1); 
		}else if(data==2){
			alert("网络繁忙！");
		}else{
			alert("网络繁忙！");
		}    
		 });
	}else{
		alert("今天已经点过赞了，明天再来！");
	}
	
}
function report(pid){
 $.get( '/book-report?pid='+pid, function(data){
	data = eval("'"+data+"'");
	if(data==1){
		alert("举报成功");
	}else if(data==2){
		alert("举报成功");
	}else{
		alert("网络繁忙！");
	}    
 });
}
