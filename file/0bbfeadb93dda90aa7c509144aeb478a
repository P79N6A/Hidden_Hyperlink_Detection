function get_more_comment(obj){

    $.ajax({
        url:'/Art/get_more_comment',
        data:{'aid':32044,'start':parseInt($("#l1a1s1t1i1d1").text())},
        dataType:'json',
        type:'post',
        success:function(da){
            if (da == 'error') {
                $(obj).replaceWith("<button>加载出错</button>");
            }else{
                var html = "";
                var n = $(da).length;

                $(da).each(function(i,v){
                    html += '<div class="one">';
                    html += '<div class="imgtx"><img src="http://static1.lvgutou.com/static'+v.face+'" /></div>';
                    html += '<dl>';
                    if(v.mid==0){
                        var url = 'javascript:;';
                    }else{
                        var url = '/Member/home/id/'+v.mid;
                    }
                    html += '<dt><a href="'+url+'">'+v.username+'</a>'+v.dtime+'<div class="hf"><a href="javascript:;" onclick="zan('+v.id+');">赞（<span id="zans_'+v.id+'">'+v.good+'</span>）</a><a href="javascript:;" onclick="reply(this,'+v.id+')">回复</a></div></dt>';
                    html += '<dd id="plk_'+v.id+'">'+replace_em(v.msg)+'</dd>';

                    $(v.son).each(function(m,n){
                        html += '<div class="one onezpl">';
                        if(n.mid==0){
                            var url = 'javascript:;';
                        }else{
                            var url = '/Member/home/id/'+n.mid;
                        }
                        html += '<div class="imgtx"><a href="'+url+'"><img src="http://static1.lvgutou.com/static'+n.face+'" /></a></div>';
                        html += '<dl>';
                        html += '<dt><a href="'+url+'">'+n.username+'</a>'+n.dtime;
                        html += '</dt>';
                        html += '<dd>'+replace_em(n.msg)+'</dd>';
                        html += '</dl>';
                        html += '</div>';
                    });


                    html += '</dl>';
                    html += '</div>';
                    if (i <= (n-1)) {
                        $("#l1a1s1t1i1d1").text(v.id);
                    }
                });
                $("#cmt").append(html);
                if(n<2){
                    $(obj).replaceWith('<div id="btnmore">没有更多评论了</div>');
                }

            }
        }
    });
}

function reply(obj,id,s){
    $("#plk").remove();
    if(s==1){
    	var pid=id;
    }else{
    	var pid = $("#parentid").val();
    }

    if (pid!=0) {
        $(".cnhf_"+pid).html('<a href="javascript:;" onclick="reply(this,'+pid+');">回复</a>');
    }

    $("#plk_"+id).append('<div class="pinglun" id="plk" style="width:600px;height:80px;"><textarea style="width:100%;height:30px;padding:5px;" name="msg" id="msg" placeholder="快来说几句吧:"></textarea><div class="bq"><button type="button" class="submitbtn" id="pinglun" onclick="sub_comment();">回复</button></div></div>');
    $(obj).replaceWith('<a href="javascript:;" onclick="cancleReply(this,'+id+');">取消回复</a>');
    $("#parentid").val(id);
}
function cancleReply(obj,id){
    $("#parentid").val(0);
    $(obj).replaceWith('<a href="javascript:;" onclick="reply(this,'+id+');">回复</a>');
    $("#plk").remove();
    $("#pingl").after('<div class="pinglun" id="plk"><textarea name="msg" id="msg" placeholder="快来说几句吧:"></textarea><div class="bq"><ul><a href="#2"><img src="http://static1.lvgutou.com/static/images/ecy/biaoqing.png" /><span>添加表情</span></a></ul><button type="button" class="submitbtn" id="pinglun" onclick="sub_comment();">评 论</button></div></div>');
}

function sub_comment(){
    var msg = $("#msg").val();
    $("#msg").val('');
    if (!msg) {
        alert('评论不能为空');
        return false;
    }
    var parentid = $("#parentid").val();
    $.ajax({
        url:"/Art/comment",
        data:{aid:32044,typeid:153,arctitle:"线上游戏的老婆不可能是女生？",msg:msg,parentid:parentid,url:"/xiaoshuo/arc/id/32044.html"},
        dataType:'json',
        type:'post',
        success:function(d){

            if (d.msg=='success') {
                $("#parentid").val(0);
                var html = "";
                if (parentid>0) {
                    html += '<div class="one onezpl">';
                }else{
                    html += '<div class="one">';
                }
                if (d.mid>0) {
                    var url = '/Member/home/id/'+d.mid;
                }else{
                    var url = 'javascript:;';
                }
                html += '<div class="imgtx"><a href="'+url+'" target="_blank"><img src="http://static1.lvgutou.com/static'+d.face+'" /></a></div>';
                html += '<dl>';
                if(parentid == 0){
                	html += '<dt><a href="'+url+'">'+d.uname+'</a>刚刚<div class="hf cnhf_'+d.cid+'"><a href="javascript:;" onclick="zan('+d.cid+');">赞（<span id="zans_'+d.cid+'">0</span>）</a><a href="javascript:;" onclick="reply(this,'+d.cid+',1);">回复</a></div></dt>';
                }else{
                	html += '<dt><a href="'+url+'">'+d.uname+'</a>刚刚</dt>';
                }
                html += '<dd id="plk_'+d.cid+'">'+replace_em(msg)+'</dd>';
                html += '</dl>';
                html += '</div>';
                if (parentid>0) {
                    $("#plk_"+parentid).append(html);
                }else{
                    $("#cmt").prepend(html);
                }
            }else{
                alert('抱歉，评论失败！');
                return false;
            }
        }
    });
}

function zan(id){ $.post('/Art/good',{id:id}, function(data){if(data=='success'){$("#zans_"+id).text(parseInt($("#zans_"+id).text())+1);show('点赞成功，+1',0,'',1);}else{show('短时间不可以多次点赞哦！',0,'',1);}});}
</script> 
