	jq = jQuery;

	function shareMore(divId){
		var divId = jq("#" + divId);
		divId.click(function(){
			var icons = jq(this).parent().find(".icons");
			var display = icons.css("display") == "none" ? "block" : "none";
			icons.css("display", display);
			return false;
		});
	}

	jq(document).ready(function(){
		shareMore("share_more");
	});

