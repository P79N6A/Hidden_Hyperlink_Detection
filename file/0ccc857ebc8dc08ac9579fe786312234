  <script>
function scrollFixe(){
	 var st = $(document).scrollTop();
	 var winh = $(window).height();
	 var thisH = $("#fixedBox").outerHeight();
	 if (!window.XMLHttpRequest) {
     	$("#fixedBox").css("top", st + winh - thisH - 163);
	 };
};
function showToTop(){
	 var st = $(document).scrollTop();
	 (st > 0)? $("#go_top").css("display","block"): $("#go_top").css("display","none");
};
function goToTop(){
	 $("html, body").animate({ scrollTop: 0 }, 200);
};
$(function(){
	$(window).scroll(function(){
		showToTop();
		scrollFixe();
	});
	$(window).resize(function(e) {
		 scrollFixe();
	});
	$("#two_code").hover(function(){
		$(this).find(".inner").addClass("code_hover");
		$(this).find(".code_info").show();
		if($(window).width()<1350)
		{
			$(this).find(".code_info").addClass("code_info02")
		}
	},function(){
		$(this).find(".inner").removeClass("code_hover");
		$(this).find(".code_info").hide();
		$(this).find(".code_info").removeClass("code_info02")
	})
});
</script> 
