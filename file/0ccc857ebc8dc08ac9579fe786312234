     <script>
				$("#JS_textarea1").keyup(function() {  
					//定义最多输入数字  
					var textMax =40;  
					//取得用户输入的字符的长度  
					var textLength = $("#JS_textarea1").val().length;  
					if (textLength < textMax) {  
						$("#textNum span").html(textMax - textLength);  
