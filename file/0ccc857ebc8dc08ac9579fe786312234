  <script type="text/javascript">
function backToTopFunW(){
	$("<div class='backToTop'><a class='toTopLink'  onclick='clickFunToTop()' title='返回顶部' href='####'>返回顶部</a></div>").appendTo("body");
}
function scrollBackToTop(){
	 var st = $(document).scrollTop(),winh = $(window).height();
	 (st > 0)? $(".backToTop").show(): $(".backToTop").hide();
	 if (!window.XMLHttpRequest) 
     $(".backToTop").css("top", st + winh - 166)
}
function clickFunToTop(){
	 $("html, body").animate({ scrollTop: 0 }, 200);
	 return false;
}
$(function(){
	 backToTopFunW();
	 $(window).bind("scroll", scrollBackToTop);
})
</script> 
