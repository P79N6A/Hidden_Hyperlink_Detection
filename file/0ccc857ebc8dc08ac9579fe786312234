  <script>
	$(function(){
    $("#order_remark").keyup(function() {
	    //定义最多输入数字
		var _max =50;
		//取得用户输入的字符的长度
		var _length = $("#order_remark").val().length;
		if (_length < _max) {
		$("#JS_msgcount_1").html(_max - _length);
		}else if(_length = _max) {
		$("#JS_msgcount_1").html('<font style="color:red;">'+0+'</font>');
		var num = $("#order_remark").val().substr(0,50);
		$("#order_remark").val(num);
		}
	}) 
	 //预约看房源
	$('#btnseehouse').bind('click',function()
	{
		var cookie_pos = document.cookie.indexOf('esfuserid');
		var order_day = $('#order_day').val();
		var order_time = $('#order_time').val();
		var order_name = $('#order_name').val();

		if($('#telno1').val()<=0) {
			var order_telno = $('#telno').val();
		}else{
			var order_telno = $('#telno1').val();
		}

		var order_validcode= $('#order_validcode').val();
		var order_remark = $('#order_remark').val();

		//验证日期
		if(order_day=='' || order_day ==0){
			$('#orderhouse_day').html("请选择时间。").show();
			return false;
		}else{
			$('#orderhouse_day').hide();
		}

		//验证时间段
		if(order_time == ''|| order_time == 0){
			$('#orderhouse_time').html("请选择时段。").show();
			return false;
		}else{
			$('#orderhouse_time').hide();
		}

		//验证预约人姓名
		if(order_name == ''|| order_name == 0){
			$('#orderhouse_name').html("请填写姓名。").show();
		return false;
		}else{
			$('#orderhouse_name').hide();
		}

		//验证手机
		if(order_telno == ''|| order_telno == 0)
		{
			$('#ordermessage_telno').html("请输入手机号码。").show();
			return false;
		}else{
			var partten = /^1\d{10}$/;
			if(!partten.test(order_telno))
			{
				$('#ordermessage_telno').hide();
				$('#ordermessage_telno').html('请正确输入手机号码。').show();
				return false;
			}else{
				$('#ordermessage_telno').hide();
			}
		}

		if($('#telno1').val()<0) {
			//验证验证码
			if(order_validcode == ''|| order_validcode.length != 6){
				$('#ordermessage_code').html("请正确输入验证码。").show();
				return false;
			}else{
				$('#ordermessage_code').hide();
			}
		}

		var param = $('#JS_formDefault').serialize();
		$.ajax({
			type: "post",
			url: "/module/ajax_action.php",
			data: param,
			success: function(msg){
				
				if(msg=='Y_SUCESS')
				{	
					$('#JS_noteHouse15').hide();
					openWin('JS_noteHouse2');
					//用户行为记录统计(add by xz 2014/4/24)
					_ua.push(6,{"infotype":"1","infofrom":"5","district":4,"streetid":"195","blockid":"1474","price":"1800.00","priceunit":"1","buildarea":90,"floor":"6","room":"2","hall":"2","toilet":"2","kitchen":"1","fitment":3,"renttype":1});
					setTimeout("location.reload()",5000);
				}else if(msg=='N_CHECKFAIL'){
					//验证失败
					$('#ordermessage_code').html("请正确输入验证码。").show();
				}else if(msg=='N_YFS'){
					//今日已预约过该房源
					$('#JS_noteHouse15').hide();
					$('#seehouse_tips').html('今日已预约过该房源。');
					openWin('JS_noteHouse3');
				}else if(msg=='N_LIMIT'){
					//今日已预约次数超限
					$('#JS_noteHouse15').hide();
					$('#seehouse_tips').html('今日已预约次数超限。');
					openWin('JS_noteHouse3');
				}else if(msg=='N_FAIL'){
					//预约失败
					$('#JS_noteHouse15').hide();
					$('#seehouse_tips').html('预约失败,刷新页面再试。');
					openWin('JS_noteHouse3');
				}
			}});
		});
	});
       function send_validcode01(){
		   var self = $('#send_code');
			var kind = $("#kind").val();
			var telno = $("#telno").val();
			var time = 0;
			if(time == 0)
			{
			var partten = /^1\d{10}$/;
				if(!partten.test(telno)){
					$('#ordermessage_code').hide();
					$('#ordermessage_telno').html('请正确输入手机号码。').show();
					return false;
				}else{
					$('#ordermessage_telno').hide();
					$.post('/module/ajax_action.php',{act:'getVnum_api',telno:telno,kind:kind},
						function(data){
							if(data == '1'){
								time = 60;
								var oTime = null;
								self.val('重新获取('+time+')');
								self.css({"color":"#999"});
								oTime = setInterval(function(){if(time>0){time--;self.val('重新获取('+time+')');}else{clearInterval(oTime);self.val('获取验证码');self.css({"color":"#000"});$('#sendingnote').hide();}} ,1000);
							}else if(data == '2'){
								 $('#ordermessage_code').html('验证码获取失败，请刷新页面重新获取。').show();
								}else if(data == '3'){
								 $('#ordermessage_code').html('发送房源次数超过上限').show();
							}else if(data == '4'){
                                 $('#ordermessage_code').html("今日操作已超过4次，请明日再来").show();
							}
						}
					);
				}
			}
	   }
	</script> 
