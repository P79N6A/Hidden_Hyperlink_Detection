  <script type="text/javascript">
function saveoption(blockid){
   var cookie_pos = document.cookie.indexOf('esfuserid');
   var title;
   if(cookie_pos < 0) {
      openWin('JS_noteHouse7_7');
   }else{  
	$.ajax({
            type: 'post',
            url : '/module/ajax_action.php',
            data:"action=collecthouse_api&telno="+$("#telno1").val()+"&validcode="+$("#validcode_01").val()+"&kind=6"+"&rowid="+$("#rowid_01").val(),
			dataType:'html',
            success: function(msg){
				if(msg==1){
				 $("#JS_noteHouse7_7").hide();
				 openWin('JS_noteHouse101');
				 //用户行为记录统计-关注房源(add by xz 2014/4/24)
				 _ua.push(8,{"infotype":"1","infofrom":"5","district":4,"streetid":"195","blockid":"1474","price":"1800.00","priceunit":"1","buildarea":90,"floor":"6","room":"2","hall":"2","toilet":"2","kitchen":"1","fitment":3,"renttype":1});
				 setTimeout("location.reload()",3000);
				 }else if(msg==3){
				 $("#JS_noteHouse7_7").hide();
				 $("#sendmsg_tips_01").html('您已关注过此房源。');
				 openWin('JS_noteHouse111');
				 setTimeout("location.reload()",3000);
				 }else{
				  $("#JS_noteHouse7_7").hide();
				 openWin('JS_noteHouse111');
				 }
				}
	     })
   }
}
$(function(){
$('#fasong_01').click(function(){
    var telno = $("#telno_01").val();
	var validcode = $("#validcode_01").val();
	var partten = /^1\d{10}$/;
		if(!partten.test(telno)){
			$('#sendmessage_code_01').hide();
			$('#sendmessage_telno_01').html('请正确输入手机号码。').show();
			return false;
		}
	if( validcode =='' || validcode.length != 6){
		$('#sendmessage_telno_01').hide();
		$('#sendmessage_code_01').html('请正确输入验证码。').show();
		return false;
	}

	$.ajax({
            type: 'post',
            url : '/module/ajax_action.php',
            data:"action=collecthouse_api&telno="+$("#telno_01").val()+"&validcode="+$("#validcode_01").val()+"&kind=6"+"&rowid="+$("#rowid_01").val(),
			dataType:'html',
            success: function(msg){
				if(msg==1){
				 $("#JS_noteHouse7_7").hide();
				 openWin('JS_noteHouse101');
				  setTimeout("location.reload()",3000);
				 }else if(msg==2){
				 $('#sendmessage_telno_01').hide();
				 $('#sendmessage_code_01').html('请正确输入验证码。').show();
				 }else if(msg==3){
				 $("#JS_noteHouse7_7").hide();
				 $("#sendmsg_tips_01").html('您已关注过此房源。');
				 openWin('JS_noteHouse111');
				 setTimeout("location.reload()",3000);
				 }else{
				  $("#JS_noteHouse7_7").hide();
				 openWin('JS_noteHouse111');
				 }
				}
	     })

})
})


       function send_validcode(){
		   var self = $('#send_code_01');
			var kind = 6;
			var telno = $("#telno_01").val();
			var time = 0;
			if(time == 0)
			{
			var partten = /^1\d{10}$/;
				if(!partten.test(telno)){
					$('#sendmessage_code_01').hide();
					$('#sendmessage_telno_01').html('请输入正确的手机号码').show();
					return false;
				}else{
					$('#sendmessage_telno_01').hide();
					$.post('/module/ajax_action.php',{act:'getVnum_api',telno:telno,kind:kind},
						function(data){
							if(data == '1'){
								time = 60;
								var oTime = null;
								self.val('重新获取('+time+')');
								self.css({"color":"#999"});
								oTime = setInterval(function(){if(time>0){time--;self.val('重新获取('+time+')');}else{clearInterval(oTime);self.val('获取验证码');self.css({"color":"#000"});$('#sendingnote').hide();}} ,1000);
							}
						}
					);
				}
			}
	   }
		
	
 

</script>
