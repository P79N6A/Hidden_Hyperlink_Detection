  <script type="text/javascript">
$(document).ready(function(){	
  // 举报房源
  $('#subreport').bind('click',function(){
     var telno = $('#JS_inputText5').val();
	 var content = encodeURIComponent($('#js_tex0').val());
	 var kind = encodeURIComponent($('#js_sel1').val());
	 var bbsname = encodeURIComponent($('#bbsname').val());
     var bbsuid = $('#bbsuid').val();
	 var infofrom = $('#infofrom').val();
	 var uid = $('#getuid').val();
	 var username = $('#getusername').val();
	 var id = $('#id').val();
	 if(kind==''){
        $('#showreporterror').show();  
        $('#showreporterror').html("请选择理由!");  
		return false;
	 }else{
        $('#showreporterror').hide();  
	 }
	 if(content==''){
        $('#showcontenterror').show();  
        $('#showcontenterror').html("请输入详细说明!");  
		return false;
	 }else{
        $('#showcontenterror').hide();  
	 }
     $.ajax({
	  type: "POST",
	  url: "/ajax_action.php",
	  data: "act=report&id="+id+"&infofrom="+infofrom+"&kind="+kind+"&telno="+telno+"&content="+content+"&uid="+uid+"&username="+username+"&bbsname="+bbsname+"&bbsuid="+bbsuid,
	  success: function(msg){
        if(msg=='1'){
		 $('#JS_noteHouse9').hide();
	     $('#showreport').html("举报成功！");  
		 openWin('JS_noteHouse10');
	  }else if(msg=='2'){
	     $('#JS_noteHouse9').hide();
	     $('#showreport').html("举报失败!");  
		 openWin('JS_noteHouse10');
	  }else if(msg=='3'){
	     $('#JS_noteHouse9').hide();
		 $('#showreport').html("请填写完整的举报信息！");  
		 openWin('JS_noteHouse10');
	  }
	   }});
  });
   
   // 短信问房第一步
  $('#findhomebt1').bind('click',function(){
     var telno = $('#JS_inputText1').val();
	 var vnum = $('#JS_inputText2').val();
	 var fhuid1 = $('#fhuid1').val();
	 if(telno==''){
        $('#findhomeerror1').html('手机号码必须填写');
		$('#findhomeerror1').attr('class','error');
		return false;
	 }else{
        $('#findhomeerror1').html('');
		$('#findhomeerror1').removeClass('error');
	 }
	 if(vnum==''){
        $('#findhomeerror2').html('验证码必须填写');
		$('#findhomeerror2').attr('class','error');
		return false;
	 }else{
        $('#findhomeerror2').html('');
		$('#findhomeerror2').removeClass('error');
	 }
     $.ajax({
		  type: "POST",
		  url: "/ajax_action.php",
		  data: "act=fhsendtelno&telno="+telno+"&vnum="+vnum+"&fhuid1="+fhuid1,
		  success: function(msg){
			if(msg=='1'){
			 $('#JS_noteHouse1').hide();
			 openWin('JS_noteHouse2');
			 $('#fhtelno').val(telno);
			 $('#fhuid').val('0');
		  }else if(msg=='2'){ 
			 $('#findhomeerror2').html('验证码不正确');
		     $('#findhomeerror2').attr('class','error');
		  }else if(msg=='3'){
			 $('#findhomeerror1').html('验证码已过期');
		     $('#findhomeerror1').attr('class','error');	 
		  }
		   }});
  });
   // 短信问房第二步 要包括bbsid ，非注册用户
  $('#findhomebt2').bind('click',function(){
     var telno = $('#fhtelno').val();
	 var utelno = $('#utelno').val();
	 var content = encodeURIComponent($('#JS_textarea1').val());
	 var uid = $('#fhuid').val();
	 var id = $('#fhid').val();
	 var fhmsg = encodeURIComponent($('#fhmsg').val());
	 var fhtitle = encodeURIComponent($('#fhtitle').val());
	 if(content==''){
	    $('#showbt2error').show();  
        $('#showbt2error').html("请输入内容!");  
		return false;
	 }
     $.ajax({
	  type: "POST",
	  url: "/ajax_action.php",
	  data: "act=fhquestion&id="+id+"&telno="+telno+"&uid="+uid+"&content="+content+"&fhmsg="+fhmsg+"&fhtitle="+fhtitle+"&utelno="+utelno,
	  success: function(msg){
	  $('#showbt2error').hide();  
        if(msg=='1'){
		 
		 $('#JS_noteHouse2').hide();
		 openWin('JS_noteHouse3');
	  }else if(msg=='2'){
	     $('#JS_noteHouse2').hide();
	     $('#showfindhome').html("您已经问过此房源!");  
		 openWin('JS_noteHouse11');
	  }else if(msg=='3'){
	     $('#JS_noteHouse2').hide();
		 $('#showfindhome').html("问房次数超过上限！");  
