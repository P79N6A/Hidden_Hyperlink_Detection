  <script>
//小区联动
$(function(){
		ESF.autosearch('blockname_top','http://sz.rent.house365.com/ajax_action.php','top_search_form');

	});
	
	$("#blockname_top1").keyup(function(event){
			//$("#blockname_top").css("background-color","#D6D6FF");
			if(event.keyCode==13) {
			$(".search_btn").trigger('click');
			}
			
			return false;
		});
	$(".search_btn").click(function(){
		var urlF = 'http://sz.rent.house365.com/district/dl_kw#KW#.html';
		var kw = $(this).prev().val().trim();
		if(kw&&kw!='请输入关键字..') {
			window.location.href = urlF.replace('#KW#',kw);
			return false;
		}else {
		return false;
		}
	});

	function search_form_func(){
		$("#top_search_form").submit();
	}
</script> 
