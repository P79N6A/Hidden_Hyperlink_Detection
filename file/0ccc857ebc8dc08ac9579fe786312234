		  type: "POST",
		  url: "/module/ajax_action.php",
		  data: "act=sendtelno&id="+sendtelnoid1+"&telno="+sendtelno+"&telno2="+sendtelno2+"&vnum="+vnum+"&infourl="+infourl+"&infotel="+infotel+"&infouname="+infouname,
		  success: function(msg){
			if(msg=='1'){
			 $('#JS_noteHouse7').hide();
			 $('#showsendmsg').html("发送成功！");  
			 openWin('JS_noteHouse8');
			  setTimeout("location.reload()",3000);
			 //用户行为记录统计(add by xz 2014/4/24)
			 _ua.push(7,{"infotype":"1","infofrom":"5","district":4,"streetid":"195","blockid":"1474","price":"1800.00","priceunit":"1","buildarea":90,"floor":"6","room":"2","hall":"2","toilet":"2","kitchen":"1","fitment":3,"renttype":1});
			
		  }else if(msg=='2'){
		     $('#findhomeerror4').html('请正确输入验证码。');
		     $('#findhomeerror4').attr('class','error');
		  }else if(msg=='3'){
			 $('#JS_noteHouse7').hide();
			 $('#showsendmsg').html("每个用户每日最多获取5次验证码");  
			 openWin('JS_noteHouse8');
		  }else if(msg=='4'){
			 $('#findhomeerror4').html('请正确输入验证码。');
		     $('#findhomeerror4').attr('class','error');
		  }
		   }});
		   
	  
  });
});
function addcollect(id,tbl){
   var cookie_pos = document.cookie.indexOf('bbuserid');
   var title;
   if(cookie_pos < 0) {
   openWin('JS_noteHouse4');
   }else{     
	$.ajax({
	  type: "POST",
	  url: "/ajax_action.php",
	  data: "act=collect&id="+id+"&tbl="+tbl,
	  success: function(msg){
	 
	  if(msg=='1'){
	     $('#showtitle').html("关注成功！");  
		 openWin('JS_noteHouse5');
	  }else if(msg=='2'){
	     $('#showtitle').html("您已经关注过此房源! <a href='http://www.house365.com/user/myuser/my_collection_rent.php' target='_blank' >查看详细&gt;&gt;</a>");  
		 openWin('JS_noteHouse5');
	  }else if(msg=='3'){
		 $('#showtitle').html("关注失败，房源不存在或已失效！");  
		 openWin('JS_noteHouse5');
	  }else if(msg=='4'){
		 $('#showtitle').html("您关注的房源已达到关注上限30条，请进入会员中心管理您的房源关注。<a href='http://www.house365.com/user/myuser/my_collection_rent.php' target='_blank' >进入会员中心</a>");  
		 openWin('JS_noteHouse5');
	  }else if(msg=='5'){
		 $('#showtitle').html("登录名无效");  
		 openWin('JS_noteHouse5');
	  }

	
     }
    });
   }
}
function findhome(id,tbl){
   var cookie_pos = document.cookie.indexOf('bbuserid');
   var title;
   if(cookie_pos < 0) {
    openWin('JS_noteHouse1');
   }else{     
	$.ajax({
	  type: "POST",
	  url: "/ajax_action.php",
	  data: "act=findhome&id="+id+"&tbl="+tbl,
	  success: function(msg){
	 
	  if(msg=='1'){
		 openWin('JS_noteHouse2');
	  }else if(msg=='2'){
	     $('#showfindhome').html("您已经问过房了~!");  
		 openWin('JS_noteHouse11');
	  }else if(msg=='3'){
		 $('#showfindhome').html("您今天已经问过5次了哦...");  
		 openWin('JS_noteHouse11');
	  }else if(msg=='4'){
		 $('#showfindhome').html("短信发送失败，请稍后再试！");  
		 openWin('JS_noteHouse11');
	  }else if(msg=='5'){
		 openWin('JS_noteHouse1');
	  }	
     }
    });
   }
}
function report(id,tbl){
   $('#infoid').val(id); 
   openWin('JS_noteHouse9');
}
function sendtelno(){
   openWin('JS_noteHouse7'); 
}

function searchline(){
   var start = $('#startname').val();
   var end = $('#endname').val();
   if(start==''){
      alert('请输入出发点！');
	  return false;
   }else if(end==''){
      alert('对不起，该房源没有小区，无法查询');
	  return false;
   }else{
	  $('#searchlineform').submit();
   }
}
