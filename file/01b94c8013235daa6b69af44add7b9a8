function del(){
		var num = document.getElementById("number");
		var n = parseInt(num.value);
		if(n-1<=0){
				alert("必须选择一个商品");
		}else{
				num.value = n-1;
		}
}
function add(){
		var num = document.getElementById("number");
		var n = parseInt(num.value);
		num.value = n+1;
}
