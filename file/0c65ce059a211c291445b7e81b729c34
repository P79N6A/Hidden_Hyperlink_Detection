function sign_ad() {
var x = window.innerWidth || Math.max(document.body.clientWidth, document.documentElement.clientWidth),
y = window.innerHeight,
mar = (x-599)/2,
tp	= (y-528)/2,
l = (x - 1200)/2 - 82,
t = y -600,
t1 = (t >= 150) ? 150 : (t<=0 ? 0 : (t-26)/2),
lt = (l <= 0) ? 0 : l,
s = -(1920-x)/2,
c =jq("#wp").width(),
p = c >= 600 && (c-600)/2 || (x-600)/2,
len = jq("#wp").find("#sign-ad").length;
jq('#sign-ad').css({"top":tp+"px","position":"fixed","margin-left": mar+"px"});
jq("#location_list").css({"left":lt,"bottom":t1});

if(jq('.qd-kefu').length > 0){
    if(x > 1366){
                if(lt >= 150){
                    jq(".qd-top").css({"right":lt,"bottom":t1});
                    jq(".kefu").css({"right":lt-150,"bottom":t1});
                }else{
                    jq(".qd-top").css({"right":150,"bottom":t1});
                    jq(".kefu").css({"right":0,"bottom":t1});
                }
}else{
                if(lt >= 100){
                    jq(".qd-top").css({"right":lt,"bottom":t1});
                    jq(".kefu").css({"right":lt-100,"bottom":t1});
                }else{
                    jq(".qd-top").css({"right":100,"bottom":t1});
                    jq(".kefu").css({"right":0,"bottom":t1});
                }
}

}else{
            jq(".qd-top").css({"right":lt,"bottom":t1});
}
jq('.heg').css("backgroundPosition",s + "px");
