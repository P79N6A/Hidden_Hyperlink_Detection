function getPage(strHtml, sValue, page) {
    var strHtmlTitle = '<p style="text-align:center; font-size:12px; color:#000;margin:2px;vertical-align:middle;">';
    strHtmlTitle += '<img src="http://img.pccoo.cn/post/images/loading.gif" style="vertical-align:middle;margin-right:1px;" border="0" />加载中……</p>';
    $("#" + strHtml).html(strHtmlTitle);
    $.post("/post/job/job_search.asp", {
        strHtml: strHtml,
        id: sValue,
        page: page
    }, function(data) {
        if (data != "") {
            $("#" + strHtml).html(data);
        }
    });
}

$(function() {
    var temJobTelArea = ' 归属地：甘肃 酒泉市';
    if (temJobTelArea != '') {
        $('#LoadTel').html(temJobTelArea);
    }
    $(".zpzhiwei_nav ul li.xz_zw").hover(function() {
        var nindex = $(".xz_zw").index(this);
        $(".xz_zw").eq(nindex).addClass("zw_xz").siblings().removeClass("zw_xz");
        $(".zhiwei_content").css("display", "none");
        $(".zhiwei_content").eq(nindex).css("display", "block");
        if (nindex == 2 && $(this).attr("jz") == 0 && $("#zhaopinlist").html() != "") {
            $(this).attr("jz", "1");
            getPage('zhaopinlist', '3461066', 1);
        }
        if (nindex == 3 && $(this).attr("jz") == 0 && $("#jllist").html() != "") {
            $(this).attr("jz", "1");
            getPage('jllist', '3461066', 1);
        }
    });
    $(".zpzhiwei_nav ul li").eq(1).mouseover(function() {
        var zpwidth = $('.ad-thumb-list').children().length;
        var outerWidth = $('.ad-thumb-list').children().eq(0).outerWidth();
        $('.ad-thumb-list').css('width', zpwidth * outerWidth);
    });
    $(".gl").hover(function() {
        $(this).addClass("guanli");
    }, function() {
        $(this).removeClass("guanli");
    });
    $(".gl").hover(function() {
        $(".gl p").show();
    }, function() {
        $(".gl p").hide();
    });
    // 收藏
    GetCollectionIs(1, 3461066);
})
