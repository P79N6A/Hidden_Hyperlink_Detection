    <script type="text/javascript">
      function getBookPage(strHtml, sValue, page, strType) {
          var strHtmlTitle = '<p style="text-align:center; font-size:12px; color:#000;margin:2px;vertical-align:middle;">';
          strHtmlTitle += '<img src="http://img.pccoo.cn/post/images/loading.gif" style="vertical-align:middle;margin-right:1px;" border="0" />加载中……</p>';
          $("#" + strHtml).html(strHtmlTitle);
          $.post('/post/ajax/booklist.asp', {
              strHtml: strHtml,
              id: sValue,
              strType: strType,
              page: page
          },
          function(data) {
              if (data != "") {
                  $("#" + strHtml).html(data)
                  if($('.zpyqcon').length>0){$(".zply").show()}
              }
          })
      }
      function addBook() {
          var test = "既然路过就说些什么吧（理性上网 文明留言 200个字符以内）";
          if ($('#HtmlContent').val() == "" || $('#HtmlContent').val() ==test) {
              alert("回复信息不能为空!");
              $('#HtmlContent').focus();
              return;
          }
          if ($('#HtmlContent').val().length > 200) {
              alert("回复信息过长，200字以内!");
              $('#HtmlContent').focus();
              return;
          }
          if ($('#PostCode').val() == "") {
              alert("验证码不能为空!");
              $('#PostCode').focus();
              return;
          }
          $.post('/post/ajax/addbook.asp', {
                  stype: 'zhaopin',
                  HtmlContent: escape($('#HtmlContent').val()),
                  PostCode: escape($('#PostCode').val()),
                  newsid: $('#newsid').val(),
                  reid: 0
          },
          function(data) {
              if (data == 1 || data == '1') {
                  alert('留言提交成功！')
                  $("#formBook")[0].reset();
                  getBookPage('bookpage',3461066,1,'zhaopin');
              } else {
                  alert(data);
              }
          })
      }
</script> 
