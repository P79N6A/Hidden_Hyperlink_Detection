<script type="text/javascript">
    (function(){
        $(".fsearch_outside").show();
		$("#searchform").submit(function(){
			if($('#sw').val() == $('#sw').attr('msg')) $('#sw').val('');
		});
		$("#sw").focusin(function(){
			$(this).val() == $(this).attr('msg') && $(this).val('');
		});
		$("#sw").focusout(function(){
			$(this).val() == '' && $(this).val($(this).attr('msg'));
		});
    })();
    $("#favbtn").mouseover(function(){$(this).addClass('a_fav_hover');}).mouseout(function(){$(this).removeClass('a_fav_hover');}).click(function(){return addfav('动画片大全_动画片_好看的动画片', 'http://www.4399dmw.com/donghua/');});
    var dh4399params = [];dh4399params['cc']='11';dh4399params['ca']='0';dh4399params['cs']='0';dh4399params['cl']='0';dh4399params['n']='0';	function deleteicon(elem) {
		var val = $(elem).attr('msg');
		if (val == 'all' || val == 'sw') {
			for (key in dh4399params) {
				dh4399params[key] = 0;
			}
		} else {
			dh4399params[val] = 0;
		}
		select_vol(0);
	}
	$(function () {
		$(".ico_del, #q_reselect").click(function () {
			deleteicon(this);
		});
	});
	function select_vol(elem, type) {
		var tmpparam = '';
		if (elem != 0) {
			dh4399params[type] = $(elem).attr('val');
		}
		for (key in dh4399params) {
			tmpparam += '-' + dh4399params[key];
		}
		var urlparam = tmpparam + '-1/#tag';
		if (type == 'n' && false) {
			location.href = "/search/dh_cata_search.php?sw=&n=" + dh4399params[type];
			return false;
		}
		location.href = '/search/dh' + urlparam;
		return false;
	}

	$(function () {
		$('#caflag > li').click(function () {
			select_vol(this, 'ca');
		}).not('.cur').hover(function () {
			$(this).addClass('cur');
		}, function () {
			$(this).removeClass('cur');
		});
		$('#ccflag > li').click(function () {
			select_vol(this, 'cc');
		}).not('.cur').hover(function () {
			$(this).addClass('cur');
		}, function () {
			$(this).removeClass('cur');
		});
		$('#clflag > li').click(function () {
			select_vol(this, 'cl');
		}).not('.cur').hover(function () {
			$(this).addClass('cur');
		}, function () {
			$(this).removeClass('cur');
		});
		$('#csflag > li').click(function () {
			select_vol(this, 'cs');
		}).not('.cur').hover(function () {
			$(this).addClass('cur');
		}, function () {
			$(this).removeClass('cur');
		});
		$('#maintab > li').click(function () {
			window.tag = "#tag";
			select_vol(this, 'n');
		});
	});
	//每天更新动画
	var d = ((new Date()).getDay() + 6) % 7;
	var tabelem = document.getElementById('j-tabweek').getElementsByTagName('LI')[d];
	tabelem.innerHTML = "今天";
	ue.tab({
		tab:$("#j-tabweek li"),
		defaultIndex:d,
		tabCurrentClass:"cur",
		only:true,
		delay:0,
		content:$("#q_twbox .q_wlist"),
		trigger:"mouseover",
		afterSwitch:function (index, is_open) {
			var $div = $(this.options.content).eq(index);
			$div.find("ol li").unbind("mouseover").bind("mouseover", function(){
				var sindex = $(this).index();
				$(this).parent().find("li").removeClass("cur").eq(sindex).addClass("cur");
				$div.find("ul").hide().eq(sindex).show();
			}).eq(0).mouseover();
		}
	});
	//头部动画切换
	ue.marquee({
		target : "#j-scrollbox .dm_imgscroll",//滚动对象 一般为 ul
		items : "#j-scrollbox .dm_imgscroll li", //滚动的详细列
		prevbtn : "#j-scrollbox .btn_left",//向前或者 向上按钮
		nextbtn : "#j-scrollbox .btn_right",//向后或者 向下按钮
		delay : 3000,//切换间隔时间
		speed : 600,//切换速度
		visiblenum : 11,//可见个数
		scrollnum : 11,//一次滚动几个
		autoplay : false,//是否自动播放
		trigger : "click",
		loop : 1,//循环模式 1 无限循环 0 不循环
		mode : 1,//0表示竖直方向滚动 1 表示水平方向滚动
		direction : 0//表示默认的滚动方向0向上或者向左 1表示向下或者向右
	});

    $(function(){
		// 返回
		ue.gototop({
			relative:$(".fixed_wrapper"), //相对定位的对象
			target:$("#j-fixed_menu"), //gototop对象
			bottom:200, //距离底部的位置
			left:0, //距离相对定位对象的水平位置
			fade:false,
			btn:$("#j-fixed_menu .gototop"),
			mini_width:1024,
			mini:"mini_fixed_menu"
		});
		if($('#bottombaiduunion').length){
			add_baidu_union_ad(980, 90, '#bottombaiduunion');
		}
    });
</script>
