  <script type="text/javascript">function changeTab(a){var c,d,e,b=0;1==a?(c=window.localStorage,d=c.getItem("links"),null!=d?(e=new Array,e=d.split(","),b=225+65*Math.ceil(e.length/4)):b=225):b=140,document.getElementById("nav1").className="",document.getElementById("nav2").className="",document.getElementById("nav3").className="",document.getElementById("nav"+a).className="current",$("#index-nearby").find(".box").each(function(){$(this).css("height",b)})}function toggle(a,b){el=document.getElementById(a),"none"==el.style.display?(el.style.display="",document.getElementById(b).className="txt-up"):(el.style.display="none",document.getElementById(b).className="txt-down")}function showsearchapibox(){"block"==document.getElementById("mask").style.display?(document.getElementById("searchapibox").style.display="none",document.getElementById("mask").style.display="none",document.body.style.overflow=""):(document.getElementById("mask").style.display="block",document.getElementById("searchapibox").style.display="block",document.body.style.overflow="hidden"),document.getElementById("updown").className="up"}function choosesearchapi(a){var b=a.getAttributeNode("ls").value;b.length&&(document.getElementById("searchapi").value=b,document.getElementById("searchapibox").style.display="none",document.body.style.overflow="",document.getElementById("mask").style.display="none",document.getElementById("updown").className="down",b=b.replace(/^[i|z|m]/,""),document.getElementById("searchIcon"))}function hideMask(){document.getElementById("searchapibox").style.display="none",document.getElementById("mask").style.display="none",document.body.style.overflow=""}window.mySwipe=Swipe(document.getElementById("slider"),{speed:100,transitionEnd:function(a){changeTab(a+1)}}),window.mySwipes=Swipe(document.getElementById("sliders"),{auto:2e3,speed:400,continuous:!0,disableScroll:!1,stopPropagation:!1}),window.scrollTo(0,1),$(document).ready(function(){var b,c,d,e,f,g,h,i,j,k,l,a=new Array(3);for(b=window.localStorage,c=b.getItem("links"),null!=c&&(d=new Array,d=c.split(","),$.merge(a,d)),c=new Array,c=a,e=Math.ceil((c.length+1)/4),f="",g=1;e>=g;g++)$("#custom_links").html(f)});</script> 
