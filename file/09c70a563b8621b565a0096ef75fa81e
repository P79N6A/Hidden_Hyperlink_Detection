  <script>


    function shouCang(postid){
        $.ajax({
            type: 'get',
            dataTyoe: 'json',
            async : false,
            data: {action:'add', postid:postid},
            url: "/cgi-bin/ajax/ajax_user_favor.php?",
            success: function(data) {
                if (data.error == 0) {
                    //收藏成功 junping
                    alert('收藏成功，你可以在用户中心，我收藏的信息 中查看。');
                } else {
                    //用户未登录 junping
                    if (data.data == '99') {
                        $.openDOMWindow({
                            positionType:'centered',
                            height:380,
                            width:640,
                            modal:1,
                            windowSourceID : '#Login_registration_reminder',
                            overlayOpacity:20,
                            windowPadding:5
                        });
                    }
                }
            }
        });
    }
</script> 
