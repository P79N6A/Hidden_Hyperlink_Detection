  <script>
$(window).scroll(function(){
if ($(window).scrollTop()>100){
$(".navbar-fixed-top").addClass("fixed");
}
else
{
$(".navbar-fixed-top").removeClass("fixed");
}
});
var compareHeight=0,realHeight= 0,hideHeader=false;
$(window).scroll(function(){
realHeight = $(window).scrollTop();
if(realHeight<10){
$("#navbar").removeClass("fadeOutUp").addClass("fadeInDown");
compareHeight=realHeight;
return;
}
if (realHeight>=compareHeight){
compareHeight = realHeight;
hideHeader = true;
}
else
{
if(realHeight+80<compareHeight){
hideHeader = false;
compareHeight = realHeight;
}
}
});
var prevHideHeader = false;
setInterval(function(){
if(hideHeader!=prevHideHeader){
if(hideHeader){
$("#navbar").removeClass("fadeInDown").addClass("fadeOutUp");
prevHideHeader = hideHeader;
}else{
$("#navbar").removeClass("fadeOutUp").addClass("fadeInDown");
prevHideHeader = hideHeader;
}
}
},400)
</script> 
