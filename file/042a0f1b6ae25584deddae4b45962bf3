    $(function(){
       /***活动切换 根据当前显示的判断***/
        var act_index_val;
        var act_len = $(".acttab_cont").length;;
        $(".acttab_cont").each(function(domthis,domindex){ 
            if(!$(this).hasClass("hide")){
                act_index_val = domthis; 
            }
        });
        $(".left_move").bind("click",function(){

            act_index_val--;
        
            if(act_index_val >= 0){
                $(".acttab_cont").hide();
                $(".acttab_cont:eq("+act_index_val+")").show();
            }else{
               act_index_val=0 ;
            }
        });
        $(".right_move").bind("click",function(){
            act_index_val++;
          
            if(act_index_val <= act_len-1){
                $(".acttab_cont").hide();
                $(".acttab_cont:eq("+act_index_val+")").show();
            }else{
               act_index_val=act_len-1; 
            }
        });
       /***活动切换***/
		$('#wx').mouseover(function(){
			$("#wechat_show").show();
		});
		$("#wx").mouseout(function(){
			$("#wechat_show").hide();
		});
		$('.a_wx').mouseover(function(){
			$("#awechat_show").show();
		});
		$(".a_wx").mouseout(function(){
			$("#awechat_show").hide();
		});
    });
