  <script type="text/javascript" src="../js/rd.js"></script> 
  <script type="text/javascript">
function disappear(commentId){
    $("#ians_tip"+commentId).hide();
    $("#input_ians"+commentId).focus();
} 
function clearAswDft(commentId){
    $("#ians_tip"+commentId).hide();
} 
function displayAswDft(commentId){
    if ($("#input_ians"+commentId).val() == ""){
        $("#ians_tip"+commentId).show();
    }
}

function replyanswer(topicId,commentType,qeId,parentId){
    var isLogin = "null";
    if(isLogin == 'false' || isLogin == 'null'){
      lgwdsshow();
      return;
    }
    
    var content = $("#commText" + parentId).val();
    
    if(content.length  == 0){
    	$("#commText" + parentId).leanModal({closeButton: ".modal_close",title:'错误提示',content:'请输入评论内容！'});
        return;
    }
    if(content.length  > 800){
    	$("#commText" + parentId).leanModal({closeButton: ".modal_close",title:'错误提示',content:'对不起，评论不能超过800个字符'});
        return;
    }
    content=encodeURIComponent(content||'');
    $.ajax({
  		type : "post",
  		url : "/www/comment.msp",
        data : "ot=3"+"&c=" + topicId + "&commentType=" + commentType + "&content=" + content + "&quoteId=" + parentId + "&parentId=" + qeId,
  		timeout : 30000,
  		dataType : "json",
  		success : function(data) {
  			if (data.resultCode == "1") {
  				$.ajax({          
				   url: "../jsp/ask_topic_piont.jsp",
				   data: "contid="+topicId+"&commentid="+qeId,
				   cache: false,
				   success: function(html){
                        $(".ask_puser_qusans").html(html);
                        location.hash = "#ask_qusans";
				   }
				});                        
                                           
                            
  			}else if(data.resultCode == 2){
                  lgwdsshow();
        } 
  			else {        
  			      if(data.resultCode == 11){
  			          $("#commText" + parentId).val('');
                  $("#commText" + parentId).blur();
              }                        
              $("#commText" + parentId).leanModal({closeButton: ".modal_close",title:'错误提示',content:data.resultMsg});
        }
  		},
  		error : function(XMLHttpRequest, textStatus, errorThrown){
  			$("#commText" + parentId).leanModal({closeButton: ".modal_close",title:'错误提示',content:'对不起，系统忙，请稍后再试！'});
  	  }
	  });	
}

function ianswered(topicId, commentType, parentId){
    var isLogin = "null";
    if(isLogin == 'false' || isLogin == 'null'){
        lgwdsshow();
        return;
    }
	var content = $("#input_ians" + parentId).val();
    if(content == ''){
        $("#iask_pub_btn2").leanModal({closeButton: ".modal_close",title:'错误提示',content:'请输入你的回答！'});
        return ;
    }
    if(content.length  > 800){
        $("#iask_pub_btn2").leanModal({closeButton: ".modal_close",title:'错误提示',content:'对不起，回答不能超过800个字符！'});
        return ;
    }
    content = $.trim(content);
    content=encodeURIComponent(content||'');
	$.ajax( {
		type : "post",
		url : "/www/comment.msp",
        data : "ot=3"+"&c=" + topicId + "&commentType=" + commentType + "&content="+ content + "&parentId="+parentId,
		timeout : 30000,
		dataType : "json",
		success : function(data) {
			if (data.resultCode == "1") {
				$.ajax({          
				   url: "../jsp/ask_topic_piont.jsp",
				   data: "contid="+topicId+"&commentid="+parentId,
				   cache: false,
				   success: function(html){
                        $(".ask_puser_qusans").html(html);
				   }
				}); 
			}else if(data.resultCode == 2){
                  lgwdsshow();
            } else {   
				if(data.resultCode == 11){
					$("#input_ians" + parentId).val('');
					$("#input_ians" + parentId).blur();
				}
				$("#iask_pub_btn2").leanModal({closeButton: ".modal_close",title:'错误提示',content:data.resultMsg});													
			}
		},
		error : function(XMLHttpRequest, textStatus, errorThrown){
			$("#iask_pub_btn2").leanModal({closeButton: ".modal_close",title:'错误提示',content:'对不起，系统忙，请稍后再试！'});  
		}
	});
}
    
var a_pagesize = 10, a_pageidx = 1, a_begin = 6;
function loadMoreA(obj,answerId,a_recordTotal){
answerId = "''";
    var more_bt = $("#hot_ans_more_ans_more2");
    if(more_bt.attr("data-disabled")=="false"){
      return;
    }
    more_bt.attr("data-disabled","false");
  
    if(document.getElementById("packup9600643") != null){
        $('#packup9600643').remove();
        $("#hot_ans_other9600643").html('');
        $('.ask_qusans').append('<div class="ans_cont_more" id="hot_ans_more_ans_more2" onclick="loadMoreA(this,'+answerId+','+a_recordTotal+')">查看此问题的另外'+a_recordTotal+'个回答</div>');
        a_pageidx =1;
        document.location.href="#ask_qusans";
        return;
	}
    $.ajax({
        type : "post",
        url: "../jsp/query_ask_point_moreAq.jsp",
        data:"topicId=10007802"+"&commentId=9600643"+"&pagesize="+a_pagesize+"&pageidx="+a_pageidx+"&begin=" + a_begin+"&isPoint=1",
        timeout : 30000,
        success : function(data) {
            data = $.trim(data);
            more_bt.attr("data-disabled","true");
            if (data == ''){
                
            }else{
                var temp = a_recordTotal - (a_pageidx * a_pagesize) + 6;
            		var a_remainder;
                if(temp > 0){
                	a_remainder = temp;
                	$('#hot_ans_more_ans_more2').remove();
                	$("#hot_ans_other9600643").append(data);
	                $('.ask_qusans').append('<div class="ans_cont_more" id="hot_ans_more_ans_more2" onclick="loadMoreA(this,'+answerId+','+a_recordTotal+')">查看此问题的另外'+a_remainder+'个回答</div>');
	            		a_pageidx ++ ;
	                a_begin = 0 ;
                }
                else{
                	$('#hot_ans_more_ans_more2').remove();
                	$("#hot_ans_other9600643").append(data);
                	$('.ask_qusans').append('<div class="ans_cont_more" id="packup9600643" onclick="loadMoreA(this,'+answerId+','+a_recordTotal+')">收起全部回答</div>');
                }
            }
        },
        error : function(XMLHttpRequest, textStatus, errorThrown) {
            more_bt.attr("data-disabled","true");
        }
    });
}
var q_pagesize = 10, q_pageidx = 2, q_recordTotal = 141;
function loadMoreQ(obj){
    var more_bt = $("#hot_ans_more_ans_more3");
    if(more_bt.attr("data-disabled")=="false"){
      return;
    }
    more_bt.attr("data-disabled","false");
    
    if(document.getElementById("quepackup") != null){
        $('#quepackup').attr("id","hot_ans_more_ans_more3");
        $('#more_que_others').html('');
        $('#hot_ans_more_ans_more3').html('查看此话题的另外'+131+'个提问');
        q_pageidx =2;
        document.location.href="#ask_point_tit";
        return;
    }
    $.ajax({
        type : "post",
        url : "load_ask_topic_pointMoreAn.jsp",
        data : "topicId=10007802&commentId=9600643&pagesize="+q_pagesize+"&pageidx=" + q_pageidx,
        timeout : 30000,
        success : function(data) {
            data = $.trim(data);
            more_bt.attr("data-disabled","true");
            if (data == ''){
                
            }else{
            		var temp = q_recordTotal - (q_pageidx * q_pagesize);
            		var q_remainder;
            		if(temp > 0){
            			q_remainder = temp;
            			$('#more_que_others').append(data);
                        $('#hot_ans_more_ans_more3').html('查看此话题的另外'+q_remainder+'个提问');
                        q_pageidx ++ ;
            		}
            		else{
                        $('#more_que_others').append(data);
                        $('#hot_ans_more_ans_more3').attr("id","quepackup");
                        $('#quepackup').html('收起更多提问');
            		}
                
            }
        },
        error : function(XMLHttpRequest, textStatus, errorThrown) {
            more_bt.attr("data-disabled","true");
        }
    });
}


