  <link href="http://www.cacos.com/statics/css/dialog_simp.css" rel="stylesheet" type="text/css" /> 
  <script type="text/javascript" src="http://www.cacos.com/statics/js/dialog.js"></script> 
  <!--弹窗JS↑--> 
  <script type="text/javascript">
//随页面滚动特效↓
$(function(){
var name = "#kf_html";
var menuYloc = null;
menuYloc = parseInt($(name).css("top").substring(0,$(name).css("top").indexOf("px")))
$(window).scroll(function () {
var offset = menuYloc+$(document).scrollTop()+"px";
$(name).animate({top:offset},{duration:500,queue:false});
});
})
//随页面滚动特效↑
</script> 
