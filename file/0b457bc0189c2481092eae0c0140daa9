  <script>
$(function(){
  if($(window).width()>640){
    if($(window).scrollTop()<120){
      $('.details-fix-bar').hide();
      $('#detail .arting-nav').show();
      $(window).scroll(function(){
        if($(window).scrollTop()>220){
          $('#detail .arting-nav').fadeOut();
          $('#morphsearch').fadeOut();
        }else{
          $('#detail .arting-nav').fadeIn();
          $('#morphsearch').fadeIn();
        }
        if($(window).scrollTop()>350){
          $('.details-fix-bar').fadeIn();
        }else{
          $('.details-fix-bar').fadeOut();
        }
		var main_height = $('#contenth').height();
        if($(window).scrollTop()>600){
		  
          $('.relation-txt').addClass('relation-txt-fix');
        }
		if($(window).scrollTop()<600 || $(window).scrollTop()>main_height){
          $('.relation-txt').removeClass('relation-txt-fix');
        }
		
		
      })
    }
    if($(window).scrollTop()>120){
      $('.details-fix-bar').show();
      $('#detail .arting-nav').hide();
    }
    $('.recommend-type .btn').click(function(){
      $('.recommend-type .btn').removeClass('active');
      $(this).addClass('active');
    });
	
  };

  
  $('.get-search').click(function(){
    $('.mobile-search').show();
  })
  $('.remove').click(function(){
    $('.mobile-search').hide();
  });
  
  
});
</script> 
