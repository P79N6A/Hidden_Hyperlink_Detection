  <div class="{body}">
     <div class="{header} clearfix">
        <h2>{title}</h2>
        <span><button type="button" title="关闭" class="{close}" hidefocus><i>×</i></button></span>
    </div>
    <div class="{content} clearfix">{main}</div>
  </div>
</script> 
