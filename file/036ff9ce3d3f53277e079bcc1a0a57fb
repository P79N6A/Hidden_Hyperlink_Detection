                    jq('.nav').show();
                });
                jq('.all-nav').bind('mouseout',function(){
                    jq('.nav').hide();
                });
            });

            jq('.header .topbar .topbar-container').css({width:'1225px'});
            jq('.header .header-main').css({width:'1225px'});
