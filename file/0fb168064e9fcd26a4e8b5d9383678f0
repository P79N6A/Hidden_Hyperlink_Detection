     <a id="a7" href="#cainixihuan4" style=" padding-left:30px;">3.5 五样东西放一起就想吐了</a> 
     <script type="text/javascript">
        window.onscroll = function(){ 
        //显示目录
           if(document.getElementById('mulucont').getBoundingClientRect().top<0)
           {
            document.getElementById('mulucont').style.position='fixed';
            document.getElementById('mulucont').style.top=0;
           };
          if(document.getElementById('mulutitle').getBoundingClientRect().top>-30)
          {
            document.getElementById('mulucont').style.position='static';
          }
          
          var sonsids=new Array()
          for(var i=0;i<8;i++)
          {
            var sons=document.getElementById('sons'+i);
            if(sons)
            {
                sonsids[i]=sons.getBoundingClientRect().top;
            }
          }
          var selectid=0;
          var selectvalue=Math.abs(sonsids[0]);
          for(var i=1;i<sonsids.length;i++)
          {
              if(Math.abs(sonsids[i])<selectvalue)
              {
                selectid=i;
                selectvalue=Math.abs(sonsids[i]);
              }
          }
          
          for(var i=0;i<sonsids.length;i++)
          {
              document.getElementById('a'+i).style.backgroundImage="";
          }
          document.getElementById('a'+selectid).style.backgroundImage="url(/img/muludoc.png)";
        } 
      </script> 
