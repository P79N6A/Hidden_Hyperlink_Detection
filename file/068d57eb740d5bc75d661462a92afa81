      <script type="text/javascript">
    function onSuccessTalkMessage(jsonResult) {
        if (jsonResult.IsSuccess) {
            document.getElementById('TalkMessageAdd').reset();
        } else {
            alert(jsonResult.Message);
        }
    }
</script> 
