  <script type="text/javascript">
    function getSubArea(pId, name) {
        $('#_List_Area_Select').html(name + '<span class="caret"></span>');
        $('#SubArea').html('');
        $.getJSON("/GeographicalArea/JsonList", { parentId: pId }).done(function (data) {
            $.each(data, function (key, val) {
                if (val.SubCityCount > 0) {
                    $('#SubArea').append("<li><a>" + val.CnName + "</a></li>");
                } else {
                    $('#SubArea').append("<li><a data-dismiss=\"modal\">" + val.CnName + "</a></li>");
                }
                $('#SubArea li').last().click(function () {
                    if (val.SubCityCount > 0) {
                        getSubSubArea(val.Id, val.CnName);
                    } else {
                        selectSubArea(val.Id, val.CnName, $('#_List_Sub_Area_Select'));
                    }
                });
            });
        });
    }
    function getSubSubArea(pId, name) {
        $('#_List_Sub_Area_Select').html(name + '<span class="caret"></span>');
        $('#SubSubArea').html('');
        $.getJSON("/GeographicalArea/JsonList", { parentId: pId }).done(function (data) {
            $.each(data, function (key, val) {
                $('#SubSubArea').append("<li><a data-dismiss=\"modal\">" + val.CnName + "</a></li>");
                $('#SubSubArea li').last().click(function () {
                    selectSubArea(val.Id, val.CnName, $('#_List_Sub_Sub_Area_Select'));
                });
            });
            $('#SubSubArea').prepend("<li><a data-dismiss=\"modal\">" + name + "</a></li>");
            $('#SubSubArea li').first().click(function () {
                selectSubArea(pId, name, $('#_List_Sub_Sub_Area_Select'));
            });
        });
    }
    function selectSubArea(id, name, eleObj) {
        //location.href = "/Speak/List?cityId=" + id.toString();
        location.href = "/Speak/City?cityId=" + id.toString();
        //alert(id);
        //$('#GAName').val(name);
        //$('#GAId').val(id);
        //eleObj.html(name + '<span class="caret"></span>');
    }
</script> 
