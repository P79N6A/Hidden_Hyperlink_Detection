             function SearchInSite_d2_274553134_2a10()
             {
               var obj =document.getElementById('txtKeyword_d2_274553134_2a10');
               var objType = document.getElementById('sel_d2_274553134_2a10');
               var objHidden =document.getElementById('hfSM_d2_274553134_2a10');
               var strUrl ='_d8.htm';
               var strParameter;
               if(strUrl.indexOf('.htm')== strUrl.length-4)
               {
                 strParameter ="?";
               }
               else
               {
                 strParameter ="&";
               }
               
               if(objType!=null)
               {
                 strParameter += "t="+objType.value;
               }
               if(objHidden!=null && objHidden.value!='')
               {
                 if(strParameter.indexOf('&')!= strParameter.length-1)
                 {
                   strParameter += "&";
                 }
                 strParameter += "m="+escape(objHidden.value);
               }
               if(obj!=null && $.trim(obj.value) !='')
               { 
                 if(strParameter.indexOf('&')!= strParameter.length-1)
                 {
                   strParameter += "&";
                 }
                 strParameter += "k="+encodeURIComponent(obj.value);
               }
               else
               {
                 alert('请输入关键词！');
                 return;
               }
              mynet_openLinkTop(strUrl+strParameter);
            }
