    //发现
    $.ajax({
        url: '/Home/PostGetPageMenu',
        type: 'POST',
        dataType: 'json',
        data: {},
        success: function (data) {
            if (data.length > 0) {
                for (var i = 0; i < data.length; i++) {
                    var html = "<li><a href='" + data[i].linkuri + "'  target='_blank'>" + data[i].title + "</a></li>";
                    $(".hyg-c-menu-dropdown-list").append(html);
                }
            }
        }
    })
    //搜索
    function submit(e, i) {
        if (i) {
            //点击关键字
            $("#search").find("input").val($(e).html());
        }
        $("#search").submit();
    }
</script> 
