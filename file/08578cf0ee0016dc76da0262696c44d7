<script type="text/javascript">
var snapshotsID;
var mapping = ['beforeday', 'yesterday', 'today', 'query'];

var selectChildAnimate = false;
$('div.select-child a').click(function(){

	if(parseInt($(this).attr('data-id')) == 1) {
		$(this).attr('data-id', 0).css('background-position', 'top left');
	}else{
		$(this).attr('data-id', 1).css('background-position', 'bottom left');
	}
	if(selectChildAnimate) return ;
	if(!selectChildAnimate) selectChildAnimate = true;
	$('div.lottery.hide-list').stop().slideToggle('slow', function(){
		selectChildAnimate = false;
	});
	
});

function asyncDayQuery(day) {

	if(!$('div.prepare table.' + mapping[day]).length) {
				var tableTPL = '<table cellpadding="0" cellspacing="0" border="0" width="100%" class="'+mapping[day]+'"><tr><th width="95">期号</th><th width="150">开奖时间</th><th width="360">开奖号码</th><th>类型</th><th class="last-child">和值</th></tr></table>';
		colspan = 5;
				$.getJSON('bjk3kjjg.php', {action: 'chart', date: mapping[day], random: Math.random(), id: 814, async: true}, function(response){

			var tr;
			var ball;
			$('div.prepare').append(tableTPL);

			if(!response || !response['data']) {
				var tr = '<tr class="last-child"><td colspan="' + colspan + '" class="last-child"><p style="line-height: 30px; padding: 10px 0; color: #d00;">找不到数据</p></td></tr>';
				$('div.prepare table.' + mapping[day]).append(tr);
				return ;
			}
			
			for(var i = 0; i < response['data'].length; i++) {
				tr = '';
				ball = '';
				for(var k = 0; k < response['data'][i]['list'].length; k++)
				ball += '<em class="ball">' + response['data'][i]['list'][k] + '</em>';

								tr += (response.data.length- 1 == i) ? '<tr class="last-child">' : '<tr>';
				tr += '<td class="nth-child-1">'+response.data[i].dateNumber+'</td>';
				tr += '<td class="nth-child-2">'+response.data[i].dateTime+'</td>';
				tr += '<td class="nth-child-3">'+ ball +'</td>';
				tr += '<td class="nth-child-4"><em class="type '+response.data[i].typeByClass+'">'+response.data[i].typeByLabel+'</em></td>'
				tr += '<td class="nth-child-5 last-child"><em class="remark green common">'  +  array_sum(response.data[i].list) +  '</em></td>';
				tr += '</tr>';
								$('div.prepare table.' + mapping[day]).append(tr);
					
			}
			
			
		});
	}

	$('div.prepare div.loading').hide();
	
	$.each(mapping, function(key, value){

		if(day == key) {
			cursor = key;
			if($('div.prepare table.' + value).length)
			$('div.prepare table.' + value).show();
		}else{
			if($('div.prepare table.' + value).length)
			$('div.prepare table.' + value).hide();
		}
		
	});

}

function asyncDateQuery(dateVal) {

	if(!$('table.query[data-id="' + snapshotsID + '"]').length) {
		
		$.getJSON('bjk3kjjg.php', {action: 'query', date: dateVal, random: Math.random(), id: 814, async: true}, function(response){
			
						var tableTPL = '<table cellpadding="0" cellspacing="0" border="0" width="100%" class="query" data-id="' + snapshotsID + '"><tr><th width="95">期号</th><th width="150">开奖时间</th><th width="360">开奖号码</th><th>类型</th><th class="last-child">和值</th></tr></table>';
			colspan = 5;
			
			var tr;
			var ball;
			//if($('div.prepare table.query').length) $('div.prepare table.query').remove();
			$('div.prepare').append(tableTPL);

			if(!response || !response['data']) {

				var errInfo = response['errorInfo'] ? response['errorInfo'] : '找不到数据';
				var tr = '<tr class="last-child"><td colspan="' + colspan + '" class="last-child"><p style="line-height: 30px; padding: 10px 0; color: #d00; ">' + errInfo  + '</p></td></tr>';
				$('div.prepare table.query[data-id="' + snapshotsID + '"]').append(tr);
				return ;
			}

			for(var i = 0; i < response['data'].length; i++) {
				tr = '';
				ball = '';
				for(var k = 0; k < response['data'][i]['list'].length; k++)
				ball += '<em class="ball">' + response['data'][i]['list'][k] + '</em>';

								tr += (response.data.length- 1 == i) ? '<tr class="last-child">' : '<tr>';
				tr += '<td class="nth-child-1">'+response.data[i].dateNumber+'</td>';
				tr += '<td class="nth-child-2">'+response.data[i].dateTime+'</td>';
				tr += '<td class="nth-child-3">'+ ball +'</td>';
				tr += '<td class="nth-child-4"><em class="type '+response.data[i].typeByClass+'">'+response.data[i].typeByLabel+'</em></td>'
				tr += '<td class="nth-child-5 last-child"><em class="remark green common">'  +  array_sum(response.data[i].list) +  '</em></td>';
				tr += '</tr>';
								$('div.prepare table.query[data-id="' + snapshotsID + '"]').append(tr);
				
			}
			
		});
		
	}else{
		$('div.prepare table.query[data-id="' + snapshotsID + '"]').show();
	}

	$('div.prepare div.loading').hide();
	
}

$('span.search a.button').bind('click', function(){

	
	var colspan = 6;
	var dateVal = $('input[name="date"]').val();
	if(dateVal == '') return ;
	snapshotsID = Math.round(new Date(dateVal + ' 00:00:00').getTime()/1000);
	
	$.each(mapping, function(key, value){
	
		if($('div.prepare table.' + value).length)
		$('div.prepare table.' + value).hide();
		
	});

	if(!$('table.query[data-id="' + snapshotsID + '"]').length) {
		
		$('div.prepare div.loading').show();
		setTimeout(function(){ asyncDateQuery(dateVal); }, 1500);
		
	}else{
		
		asyncDateQuery(dateVal);
		
	}
	
	cursor = 3;
	
});


$('input[name="date"]').datepicker({
	changeMonth: true,
	changeYear: true,
	dateFormat: 'yy-mm-dd',
	prevText:'前一月',
	nextText:'后一月',
	currentText:' ',
	monthNames:['1月','2月','3月','4月','5月','6月','7月','8月','9月','10月','11月','12月'],
	dayNamesMin:['日', '一', '二', '三', '四', '五', '六']
});


var interval = [];
var expires = [];

function countdown(element, id, index) {
	
	if(interval[index]) clearInterval(interval[index]);
	$.getJSON('http://www.icaile.com/modules/gpc/include/ajax.refresh_periodlist.forchart.php?lottery_type='+id+'&num=2&jsoncallback=?', function(response){

		expires[index] = parseInt(response[1].time_c);
		element.find('em.date_number').empty().html(response[1].periodlist_no);
		
		interval[index] = setInterval(function(){
			if(expires[index] <= 0) {
				clearInterval(interval[index]);
				countdown(element, id, index);
			}else{
				expires[index]-=1000;
				var hours = parseInt(expires[index]/1000/3600);
				var minutes = parseInt(expires[index]/1000%3600/60);
				var seconds = parseInt(expires[index]/1000%3600%60%60);

				
				element.find('em.red.hours').empty().html(hours < 10 ? '0' + hours : hours);
				element.find('em.red.minutes').empty().html(minutes < 10 ? '0' + minutes : minutes);
				element.find('em.red.seconds').empty().html(seconds < 10 ? '0' + seconds : seconds);
				
			}
			
		}, 1000);
			
	});
		
}

$('div.list-item').each(function(index, element){
	expires.push(0);
	//countdown($(element), $(element).attr('data-id'), index);
});

		
$('select[name=scaleInfo]').bind('change', function(){

	var parentNode = $(this).parents('table.mini-chart');
	
	var code = parseInt($(this).val());
	switch(code) {

		case 1:
			
			parentNode.find('tr td.result').each(function(index, element){
				var html = max_to_min($(this).attr('data-id'));
				$(this).html(html);
			});
					
		break;

		case 2:
			
			parentNode.find('tr td.result').each(function(index, element){
				var html = prime_to_composite($(this).attr('data-id'));
				$(this).html(html);
			});
					
		break;

		case 3:
			
			parentNode.find('tr td.result').each(function(index, element){
				var html = odd_to_even($(this).attr('data-id'));
				$(this).html(html);
			});
					
		break;

		case 4:
			
			parentNode.find('tr td.result').each(function(index, element){
				var html = combined_value($(this).attr('data-id'));
				$(this).html(html);
			});
					
		break;

		case 5:
			
			parentNode.find('tr td.result').each(function(index, element){
				var html = span_value($(this).attr('data-id'));
				$(this).html(html);
			});
					
		break;

	}
	
});

	$('table.mini-chart tr td.result').each(function(index, element){
		var html = max_to_min($(this).attr('data-id'));
		$(this).html(html);
	});

	$('p.sub-title').bind('click', function(){
		var dataID = $(this).attr('data-id');
		if($('ul[data-id="'+dataID+'"]').length) {
			$('ul[data-id="'+dataID+'"]').slideToggle();
		}
	})
	</script>

