			</ul>
			
		</div>
		<script type="text/javascript">
		var cursor = 2;
		
		Array.prototype.max = function(){ 
			return Math.max.apply({},this); 
		}

		Array.prototype.min = function(){   
			return Math.min.apply({},this);
		}
		
		function max_to_min(str) {


			var max = 0;
			var min = 0;
			
			var array = str.split(',');
			for(var i = 0 in array) {

				if(array[i] > 6) {
					max++;
				}else{
					min++;
				}
				
			}

			return max + ':' + min;
			
		}

		function prime_to_composite(str) {

			var prime = 0;
			var composite = 0;
			var already = 0;
			
			var array = str.split(',');
			for(var i = 0; i < array.length; i++) {

				var number = array[i];
				if(number > 1) {
					for(var k = 1; k <= number; k++) {
						if(number%k == 0) {
							already++;
						}
					}

					if(already == 2) {
						prime++;
					}else{
						composite++;
					}
				}
				already = 0;
			}

			return prime + ':' + composite;
			
		}

		function odd_to_even(str) {

			var odd = 0;
			var even = 0;
			
			
			var array = str.split(',');
			for(var i = 0; i < array.length; i++) {

				array[i]%2 == 0 ? even++ : odd++;
			}

			return odd + ':' + even;
			
		}

		function combined_value(str) {

			var value = 0;
			var array = str.split(',');

			for(var i = 0; i < array.length; i++) {
				if(array[i]) value += parseInt(array[i]);
			}


			return value;
			
		}

		function span_value(str) {

			var value = 0;
			var array = str.split(',');

			return array.max() - array.min();
			
		}

		function array_sum(array) {

			sum = 0;
			
			for(var i = 0; i < array.length; i++) {
				sum += parseInt(array[i]);
			}

			return sum;
			
		}

		var BEFOREDAY = 0;
		var YESTERDAY = 1;
		var TODAY = 2;
		
		var colspan = 5;

		var mapping = ['beforeday', 'yesterday', 'today', 'query'];
		
		var chart = {
			prepare: function(day){

				if(cursor == day) return ;

				$.each(mapping, function(key, value){
					
					if($('div.prepare table.' + value).length)
					$('div.prepare table.' + value).hide();
					
				});

				if(!$('div.prepare table.' + mapping[day]).length) { 

					$('div.prepare div.loading').show();
					setTimeout(function(){ asyncDayQuery(day); }, 1500);

				}else{

					asyncDayQuery(day);
					
				}
				
				
			}
		};
		</script>
