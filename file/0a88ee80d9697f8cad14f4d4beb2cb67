$(function(){
 var bt2 = $("#xxk_top li");
 var nr2 = $("#xxk_ct ul");
 nr2.eq(0).css("display","block");
 bt2.each(function(i){
  $(this).mouseover(
   function(){
   $(this).addClass("cli").siblings().removeClass("cli");
   $(nr2[i]).show().siblings().hide();
  })
});
})
