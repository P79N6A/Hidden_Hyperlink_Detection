    $(function () {
        loaTailHtml();
    });
    function loaTailHtml() {
        $.ajax({
            url:"/getTail.jspx",
            type:'post',
            async:false,
            success: function(data) {
                $("#zbdw").html($.parseJSON(data).copyright);
                $("#addPho").html($.parseJSON(data).footer);
            }
        });
    }
</script>
