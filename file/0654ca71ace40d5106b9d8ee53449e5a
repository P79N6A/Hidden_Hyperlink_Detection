$("#searchKeywords").keyup(function(){

	if(biaoqianval == false){
		biaoqianval  = true;

			 setTimeout("bqzhi()", 500 ); 
			 
			 
			 
			  if(event.keyCode ==13){
				$("#tangram-suggestion--TANGRAM__2-main").hide();
				var searchval = $("#searchKeywords").val();
				if( searchval == '' ){
					$(".dialog-failed").show();
					$(".mask").show();
				}else{
					$("#guider").hide();
					//position: absolute; top: 0px; left: 0px;
					$("#address").css("position","absolute");
					$("#address").animate({top:"0px",left:"0px"}, 'slow');			
					$("#search_result").removeClass("hidden");
					$("#bd-map").removeClass("hidden");
					//local.search(searchval);
					//	$("#tangram-suggestion--TANGRAM__2-main").css("top","141px");
					///	$("#tangram-suggestion--TANGRAM__2-main").css("left","355px");
					$("#tangram-suggestion--TANGRAM__2-main").css("top","230px");
				$("#tangram-suggestion--TANGRAM__2-main").css("left","353px");
				$(".map").css('height','600px');
				$(".guidebox").css('height','850px');
						
					dosearchmap(0);
				}
			}else{
				 
			 
			 
			 
			 
			 
						var searchval  = $("#searchKeywords").val();
				
						$.ajax({
							url: 'http://www.51china.cn/index.php?ctrl=site&action=getsearmap&datatype=json',
							data: 'searchvalue=' + encodeURI(searchval),
							dataType: "json",
							success: function(content) {  
								
								$("#tangram-suggestion--TANGRAM__2-main").show();
								$("#tangram-suggestion--TANGRAM__2-main #tangram-suggestion--TANGRAM__2 table tbody").html('');
								
							//	alert(content.msg.length);
								var tempaddc = content.msg.length;
								if(content.msg.length > 0 ){
									for(var i=0;i<tempaddc;i++){
												//	alert(	content.msg[i].datacontent );		   
						$('#tangram-suggestion--TANGRAM__2-main #tangram-suggestion--TANGRAM__2 table tbody').append('<tr><td id="tangram-suggestion--TANGRAM__2-item0"  class=""><i class="route-icon">'+content.msg[i].datacontent+'&nbsp;<span class="tangram-suggestion-grey">'+content.msg[i].dataaddress+'</span></i></td></tr>');
										  
									 } 
								 }
								 else{
								 
	// $('#tangram-suggestion--TANGRAM__2-main #tangram-suggestion--TANGRAM__2 table tbody').append('<tr><td id="tangram-suggestion--TANGRAM__2-item0"  class=""><i class="route-icon"><span class="tangram-suggestion-grey">未获取到该地址，请换个关键字尝试下...</span></i></td></tr>');

			$("#tangram-suggestion--TANGRAM__2-main").hide();					 }
								  //   $('#search_result .pager').append('<a  onclick="dosearchmap('+i+');" data-page="'+i+'" href="javascript:;" class="active">'+c+'</a>');
							
								
								bindsearch();
							
							}
						});
			}
	
	}
	
});
function bindsearch(){
	$("#tangram-suggestion--TANGRAM__2 table tbody tr").click(function(){
		$("#searchKeywords").val( $(this).text() );
		$("#tangram-suggestion--TANGRAM__2-main").hide();
	});
}

$("#historylist").click(function(){
	$(".dialog-historyaddr").toggle();
});

