	<script>
var biaoqianval = false;
function bqzhi(){
	biaoqianval  = false;
}
	
		var map = new BMap.Map("bd-map");  


var myIcon1 = new BMap.Icon(siteurl+"/upload/map/pepole.png", new BMap.Size(75,75),{offset: new BMap.Size(25,25),imageOffset: new BMap.Size(25,25)});  
var marker = null;   
var infoWindow = new BMap.InfoWindow('');  // 创建信息窗口对象
var makenow = false;
var markerslist = null;

map.setDefaultCursor("url('upload/shop/add-my-shubiao.cur'),default");
map.setDraggingCursor("auto"); 
//加载缩放级别

map.addControl(new BMap.NavigationControl({anchor: BMAP_ANCHOR_TOP_RIGHT, type: BMAP_NAVIGATION_CONTROL_SMALL}));  //右上角，仅包含平移和缩放按钮

var savemapurl = 'http://www.51china.cn/index.php?ctrl=site&action=setuserlng&mapname=@mapname@&lat=@lat@&lng=@lng@&random=@random@';  
map.enableContinuousZoom();//可缩放

	
	// 百度地图API功能

		map.enableScrollWheelZoom(); 
		map.enableContinuousZoom(); 
	
	
	// 创建Map实例
	//map.centerAndZoom(new BMap.Point(116.404, 39.915), 11);
	map.centerAndZoom("郑州市",15);
	
	$("#tipclose").click(function(){
		$("#guider .bg").hide();
	});
	$(".notitle-close").click(function(){
		$(".dialog-failed").hide();
			$(".mask").hide();
	});
	$("#searchKeywords").focus(function(){
		$("#guider .bg").addClass('search');
	
	//	alert(0);
		
	});
	
	
	$("#search").click(function(){
		$("#tangram-suggestion--TANGRAM__2-main").hide();
		var searchval = $("#searchKeywords").val();
		if( searchval == '' ){
			$(".dialog-failed").show();
			$(".mask").show();
		}else{
			$("#guider").hide();
			//position: absolute; top: 0px; left: 0px;
			$("#address").css("position","absolute");
			$("#address").animate({top:"0px",left:"0px"}, 'slow');			
			$("#search_result").removeClass("hidden");
			$("#bd-map").removeClass("hidden");
			//local.search(searchval);
				$("#tangram-suggestion--TANGRAM__2-main").css("top","230px");
				$("#tangram-suggestion--TANGRAM__2-main").css("left","353px");
				$(".map").css('height','600px');
				$(".guidebox").css('height','850px');
			dosearchmap(0);
		}
	});

function dosearchmap(pagenum){

 
	   //调用搜索
	   map.clearOverlays();
	   var  tempurlss = siteurl+'/index.php?ctrl=site&action=ajaxbaidu&random=@random@';  //  searchvalue  cityname
	   var searchvalue = $('#searchKeywords').val();
	   var cityname = $('#cityname').val();
	  $.ajax({
     type: 'post',
     async:false,
     data:{'searchvalue':searchvalue,'cityname':cityname,'pagenum':pagenum},
     url: tempurlss.replace('@random@', 1+Math.round(Math.random()*1000)), 
     dataType: 'script',success: function(content) {    
	 
		//searchbackdata(content);
		
		 },
      error: function(content) { 
       	alert('选择的城市不满足百度地图搜索条件'); 
	    }
     });   
	   
 
  return false;
}
function searchbackdata(datas){  
 //alert(datas.total);
 //alert(datas.pagenum);
	if(datas.status == 0){
	      miaodian(datas.results,datas.total);
	      makedivpage(datas.total,datas.pagenum);
	}
 // alert(datas.length);
}	

/*构造搜索*/
 var markerslist = null;
 	 
function miaodian(datas,shuliang){


	markerslist=new Array();
	$('#search_result').html('');
	var  wordearray = ['A','B','C','D','E','F','G','H','I','J','K','L','M'];
	makenow = false;
	 var tempoint;
	 $('#search_result').append('<div class="jspContainer" style="width: 300px;overflow-y:auto; height: 100%;"><div class="jspPane" style="padding: 0px; top: 0px; left: 0px; width: 278px;"><div class="total-addr">共<span>'+shuliang+'</span>地址</div><ul> </ul><div class="pager"> </div></div></div>  ');
	 $.each(datas, function(i,val){  
	 //alert(val.location.lng);
	 
         		    	   if(i == 0){  
         		    	        tempoint =   new BMap.Point(val.location.lng,val.location.lat);//imageOffset: new BMap.Size(-32,0 - i * 46)}
         		    	        map.centerAndZoom(tempoint,map.getZoom()); 
         		    	   } 
                     var icon = new BMap.Icon("/upload/map/mapIcon.png", new BMap.Size(90,44),{offset: new BMap.Size(45,46),imageOffset: new BMap.Size(-32,0 - i * 47)});//infoWindowAnchor:new BMap.Size(33,44),offset:new BMap.Size(0,0)}
         		         var mkr = new BMap.Marker(new BMap.Point(val.location.lng,val.location.lat), {icon: icon});  
         		        var temp_linksss = savemapurl.replace('@lat@',val.location.lat).replace('@lng@', val.location.lng).replace('@mapname@', encodeURI(val.name)); 
         		         var contentm = '<p class="infoTitle">'+val.name+'</p><p class="infoAddress">'+val.address+'</p><p class="nearshop">附近共有<span class="shopnum"> '+val.shuliang+' </span>家商家</p>';
         		         if(val.shuliang > 0){
         		            contentm += '<a href="'+temp_linksss+'" class="mapBtns">查看商家</a>';
         		         }
         		       
         		    	   var infoWindow2 = new BMap.InfoWindow(contentm); 
                     markerslist.push(infoWindow2);
                     mkr.addEventListener("click", function(){  this.openInfoWindow(infoWindow2);});
					 
					//  map.openInfoWindow(markerslist[checkobj],tempoint);  
					 // map.centerAndZoom(tempoint,map.getZoom()); 
					  
                     map.addOverlay(mkr);  
					 $('#search_result .jspContainer ul').append('<li lng="'+ val.location.lng+'" lat="'+ val.location.lat+'" index="'+i+'" class="field"><div class="poi sprite fl">'+wordearray[i]+'</div><div class="content"><div class="title">'+val.name+'</div><div class="addr">地址：'+val.address+'</div> <div class="nearby">附近有<span>'+val.shuliang+'</span>家商家</div></div></li>  ');
           
   });  
  if( markerslist.length > 0 ){
		map.openInfoWindow(markerslist[0],tempoint);  
		map.centerAndZoom(tempoint,map.getZoom());
		$("#search_result .jspContainer li:first").addClass("first-field");
		$("#search_result .jspContainer li:first").addClass("active");
  }else{
	$('#search_result .jspContainer ul').append('<div class="no-address">  <div class="text">未找到相关地址：</div>  <p>1.请核对地址拼写正确。</p>  <p>2.请尝试其他关键字。</p></div> ');
  
  }

}
function makedivpage(shuliang,pagenum){
	var pagesize = 12;
	var pageall = shuliang/pagesize > parseInt(shuliang/pagesize) ?parseInt(shuliang/12)+1:parseInt(shuliang/12);
   var gotopage = 0;
   //<a data-page="1" href="javascript:;" class="active">1</a>   
	 if(pagenum != 0){
	 	    gotopage = Number(pagenum)-1;
	      $('#search_result .pager').append('<a  onclick="dosearchmap('+gotopage+');" data-page="'+gotopage+'" href="javascript:;" class="active">&lt;</a>');
	 } 
	var tempage = pageall > 10?10:pageall;
	 for(var i=0;i<tempage;i++){
	 	var c = i+1;
	 	   if(i == pagenum){
	 	   	   
	 	       $('#search_result .pager').append('<a  onclick="dosearchmap('+i+');" data-page="'+i+'" href="javascript:;" class="active">'+c+'</a>');
	 	   }else{
	 	   	
	 	   	 $('#search_result .pager').append('<a  onclick="dosearchmap('+i+');" data-page="'+i+'" href="javascript:;" >'+c+'</a>'); 
	 	   }
	 } 
	 if(pagenum != pageall){
	 	   gotopage = Number(pagenum)+1;
	    $('#search_result .pager').append('<a data-page="'+gotopage+'"  onclick="dosearchmap('+gotopage+');"  href="javascript:;" >&gt;</a>'); //每页12个  
	 }
}
$('#search_result  .jspContainer li').live("click", function() {    
	$(this).addClass('active').siblings().removeClass('active');
	
	var checkobj = $(this).attr('index');
	if(checkobj != undefined){
	     var tempoint =   new BMap.Point($(this).attr('lng'),$(this).attr('lat'));//imageOffset: new BMap.Size(-32,0 - i * 46)}
	      map.openInfoWindow(markerslist[checkobj],tempoint);  
        map.centerAndZoom(tempoint,map.getZoom()); 
	}
});






