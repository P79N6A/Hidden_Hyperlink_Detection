														 }
													   
														   var NewinfoWindow = new BMap.InfoWindow(contentm); 
										  
										  
										 // var NewinfoWindow = new BMap.InfoWindow(contentc);   
										
										 marker = new BMap.Marker(newpoint,{icon:myIcon1});
									
										   map.openInfoWindow(NewinfoWindow,newpoint);  
										  $('#cityname').val(content.msg.city); 
										  if(makenow ==  false){
													makenow = true;
												   map.addOverlay(marker);
										  }
									  
							  
							  
								
							}
							},
						error: function(content) { 


						alert('获取失败，可以尝试刷新重试！'); 
						
						
						
						
						}
					   });  
 
					
			
			//		$.getScript('http://api.map.baidu.com/geocoder/v2/?ak=m7Lmfw2pXNQm5enZ2NxKKbzA&callback=renderReverse&location='+e.point.lat+','+e.point.lng+'&output=json&pois=1');   
					
			}
	} 
});

