<script type="text/javascript">

    $(function () {

        $(document).on('click', "#scartlink", function (e) {
            e.preventDefault();
            //alert('scartlink test');
            $(".sub-menu").stop().slideToggle("slow");

            if ($('.c-cart a').closest('a').prop('class') == '') {
                $(".c-cart a").addClass("active");
            }
            else {
                $(".c-cart a").removeClass("active");
            }


        });

        $(document).on('click', function (e) {
            var container = $("div.c-cart");  // $("ul.sub-menu");
            if (!container.is(e.target) && container.has(e.target).length === 0) {
                if ($('#showmycartitems').is(':visible')) {
                    $(".sub-menu").stop().slideToggle("slow");
                }

                // $('#showmycartitems').hide();
            }
        });
    });


</script>

