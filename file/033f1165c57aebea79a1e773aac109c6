    <script>
        (function () {
            // setup your carousels as you normally would using JS
            // or via data attributes according to the documentation
            // http://getbootstrap.com/javascript/#carousel
            $('#carouselcity').carousel({ interval: 2000 });
            $('#carouselob').carousel({ interval: 3000 });
            $('#carouseltb').carousel({ interval: 4000 });
            $('#carouselOffer').carousel({ interval: 4000 });

        }());
        function RepeatElements(e) {
            var itemToClone = e;//$(this);

            for (var i = 1; i < 4; i++) {
                itemToClone = itemToClone.next();

                // wrap around if at end of item collection
                if (!itemToClone.length) {
                    itemToClone = e.siblings(':first');//$(this).siblings(':first');
                }

                // grab item, clone, add marker class, add to collection
                itemToClone.children(':first-child').clone()
                  .addClass("cloneditem-" + (i))
                  .appendTo(e);//.appendTo($(this));
            }
        }

        (function () {
            if ($('#carouselcity .item').length > 4) {
                $('#carouselcity .item').each(function () {
                    RepeatElements($(this));
                });
            }
            if ($('#carouselob .item').length > 4) {
                $('#carouselob .item').each(function () {
                    RepeatElements($(this));
                });
            }
            if ($('#carouseltb .item').length > 4) {
                $('#carouseltb .item').each(function () {
                    RepeatElements($(this));
                });
            }

            if ($('#carouselOffer .item').length > 4) {
                $('#carouselOffer .item').each(function () {
                    RepeatElements($(this));
                });
            }
            //$('.carousel-showmanymoveone .item').each(function () {
            //    var itemToClone = $(this);

            //    for (var i = 1; i < 4; i++) {
            //        itemToClone = itemToClone.next();

            //        // wrap around if at end of item collection
            //        if (!itemToClone.length) {
            //            itemToClone = $(this).siblings(':first');
            //        }

            //        // grab item, clone, add marker class, add to collection
            //        itemToClone.children(':first-child').clone()
            //          .addClass("cloneditem-" + (i))
            //          .appendTo($(this));
            //    }
            //});

        }());
        $(function () {
            $(window).scroll(function () {
                var scroll = $(window).scrollTop();
                // alert(scroll);
                //>=, not <=
                if (scroll >= 50) {
                    //clearHeader, not clearheader - caps H
                    $("#topnavbar").addClass("top-bg");
                }
                else {
                    $("#topnavbar").removeClass("top-bg");
                }
            }); //missing );

            /******* Local Business Tabs Scroll ******/
            var UlWidth = 0;
            var LastLi = $('.Local-Categories-Bar>li').length;
            var ScrollingLocalTab = false;
            $('.Local-Categories-Bar>li').each(function (i, v) {
                $(this).attr('data-width', $(this).outerWidth());
                UlWidth += $(this).outerWidth();
                if (i + 1 == LastLi) {
                    $('.Local-Categories-Bar').outerWidth(UlWidth);
                }
            });
            $('.Local-Categories-Bar-Direction').click(function (e) {
                console.info(ScrollingLocalTab);
                if (!ScrollingLocalTab) {
                    ScrollingLocalTab = true;
                    console.info(ScrollingLocalTab);
                    var leftBtn = $(this).hasClass('icon-directionLeft');
                    var CurrentElement = $('[data-currentfirst="1"]');
                    if (leftBtn) {
                        //Left Button Press
                        if (CurrentElement.attr('data-number') != "0") {
                            var CurrentLeft = $('.Local-Categories-Bar').position().left;
                            var PreviousElement = $('[data-number="' + (CurrentElement.attr('data-number') - 1) + '"]', '.Local-Categories-Bar');
                            var NewLeft = CurrentLeft + parseInt(PreviousElement.attr('data-width'));
                            $('[data-currentfirst="1"]').attr('data-currentfirst', '0');
                            PreviousElement.attr('data-currentfirst', '1');
                            $('.Local-Categories-Bar').animate({ 'left': NewLeft }, 90, function () { setTimeout(function () { ScrollingLocalTab = false; }, 90); });
                        }
                    }
                    else {
                        //Right Button Press
                        if (CurrentElement.attr('data-number') < LastLi) {
                            var CurrentLeft = $('.Local-Categories-Bar').position().left;
                            var NextElement = $('[data-number="' + (parseInt(CurrentElement.attr('data-number')) + 1) + '"]', '.Local-Categories-Bar');
                            var NewLeft = CurrentLeft - parseInt(CurrentElement.attr('data-width'));
                            $('[data-currentfirst="1"]').attr('data-currentfirst', '0');
                            NextElement.attr('data-currentfirst', '1');
                            $('.Local-Categories-Bar').animate({ 'left': NewLeft }, 90, function () { setTimeout(function () { ScrollingLocalTab = false; }, 90); });
                        }
                    }
                }
            });



            $(".popover-nav a").popover({ trigger: "hover" });
        })
    </script>
