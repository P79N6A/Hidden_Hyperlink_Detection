<input id="blankdomain" name="blankdomain" type="hidden" value="" />



<script>


    $(function () {
        var url = 'http://' + document.domain + '/searchlocation';
        document.getElementById('blankdomain').value = "";
        if (document.domain == "localhost") {
            url = 'http://localhost:2993/searchlocation';
        }
        $('.showSearchLocationfooter').click(function () {
            $('#changelocationfooter_popup').show();
        });
        $('#closeSearchLocationFooter').click(function () {
            $('#changelocationfooter_popup').hide();
            $("#searchlocationfooter_input").val('');
        });
        function searchlocationfooter_domain(val) {
            var arr = val.split(':');
            if (arr.length == 2) {
                val = arr[1].replace(" ", "");
            }
            if (arr.length == 3) {
                val = arr[1].replace(" ", "") + arr[2].replace(" ", "");
            }
            if (arr.length == 4) {
                val = arr[1].replace(" ", "") + arr[2].replace(" ", "") + arr[3].replace(" ", "");
            }
            if (arr.length == 5) {
                val = arr[1].replace(" ", "") + arr[2].replace(" ", "") + arr[3].replace(" ", "") + arr[4].replace(" ", "");
            }
            return 'http://' + val;

        }
        function highlightText(text, jqnode) {
            var searchText = $.trim(text).toLowerCase(), currentNode = jqnode.get(0).firstChild, matchIndex, newTextNode, newSpanNode;
            while ((matchIndex = currentNode.data.toLowerCase().indexOf(searchText)) >= 0) {
                newTextNode = currentNode.splitText(matchIndex);
                currentNode = newTextNode.splitText(searchText.length);
                newSpanNode = document.createElement("span");
                newSpanNode.className = "highlight";
                currentNode.parentNode.insertBefore(newSpanNode, currentNode);
                newSpanNode.appendChild(newTextNode);
            }
        }
        jQuery("#searchlocationfooter_input").keypress(function (event) {
            var charCode = (event.which) ? event.which : event.keyCode;
            if (charCode == 8)
                return true;
            if (charCode == 127)
                return true;
            if (charCode == 32)
                return true;
            if ((charCode > 47) && (charCode < 58))
                return true;
            if ((charCode >= 65) && (charCode <= 90))
                return true;
            if ((charCode >= 97) && (charCode <= 122))
                return true;
            else
                return false;
        });
        jQuery("#searchlocationfooter_input").autocomplete({
            source: function (request, response) {
                jQuery('#searchlocationfooter_spiner').show();
                var i = 0;
                document.getElementById('blankdomain').value = "";
                jQuery.ajax({
                    url: url,
                    data: { term: request.term.trim() },
                    dataType: "json", success: function (data) {
                        $('#searchlocationfooter_spiner').hide();
                        response(jQuery.map(data, function (item) {
                            $('#hdfirstcatid').val(data[0].CatId);

                            if (i == 0) {
                                var val = item.domain.replace(" ", "");
                                if (val != "City:" && val != "State:") {
                                    document.getElementById('blankdomain').value = searchlocationfooter_domain(val);
                                    i = 1;
                                }
                            }
                            return { value: item.domain, lable: item.domain, catid: item.CatId };
                        }))
                    }, error: function () { $('#searchlocationfooter_spiner').hide(); }
                });
            },
            focus: function (event, ui) {

                event.preventDefault();
                var val = ui.item.label;
                if (val != "City:" && val != "State:") {

                    $(this).val(val);
                }
                else { $(this).val(''); }
            },
            select: function (event, ui) {

                var val = ui.item.label;
                val = val.replace(" ", "");

                if (val != "City:" && val != "State:") {

                    window.location.href = searchlocationfooter_domain(val);
                }
                else {
                    event.preventDefault();
                    jQuery('#searchlocationfooter_input').val('');
                }

            }
        }).data("ui-autocomplete")._renderItem = function (ul, item) {
            var host = item.label;
            var atag = "<a></a>";
            var jqa;
            if (host == "City:" || host == "State:") {
                atag = "<a class='licolor'></a>";
                jqa = jQuery(atag).text(host);
            }
            else {
                jqa = $(atag).text(host);
                highlightText(this.term, jqa);
            }
            return jQuery("<li></li>").append(jqa).appendTo(ul);
        };
    });
    jQuery("#searchlocationfooter_input").keypress(function (event) {
        if (event.keyCode == '13' && document.getElementById("searchlocationfooter_input").value != "" && document.getElementById('blankdomain').value != "") {
            window.location.href = document.getElementById('blankdomain').value;
        }

    });
</script>
