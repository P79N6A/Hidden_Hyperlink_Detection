<div class="modal fade" id="APCustomModal" tabindex="-1" role="dialog" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body">
                <div class="ap-modal-body">
                </div>
                <div class="clearfix"></div>
                <div class="text-center">
                    <button type="button" class="btn btn-primary ap-modal-body-btn" data-dismiss="modal">Continue</button>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="modal-footer">
                <button type="button" class="ap-modal-left-btn btn btn-default pull-left" data-dismiss="modal">Close</button>

                <button type="button" class="ap-modal-right-btn1 btn btn-default" data-dismiss="modal">OK</button>
                <button type="button" class="ap-modal-right-btn2 btn btn-default " data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    var ModalOkCallback = null;
    function CustomAlertModal(title, headerClass, body, OkCallback) {
        $('#CustomAlertModal').find('.modal-header').addClass('modal-header-' + headerClass);
        $('#CustomAlertModal').find('.modal-title').html(title);
        $('#CustomAlertModal').find('.modal-body').html(body);
        $('#CustomAlertModal .modal-footer .okbtn').attr('class', 'okbtn btn btn-' + headerClass);
        $('#CustomAlertModal').appendTo('body').modal('show');
        if (OkCallback) {
            ModalOkCallback = OkCallback;
        }
        else
            ModalOkCallback = null;
    }
    function CustomConfirmModal(title, headerClass, body, OkCallback, CancelCallback) {
        $('#CustomConfirmModal').find('.modal-header').addClass('modal-header-' + headerClass);
        $('#CustomConfirmModal').find('.modal-title').html(title);
        $('#CustomConfirmModal').find('.modal-body').html(body);
        $('#CustomConfirmModal .modal-footer .okbtn').attr('class', 'okbtn btn btn-' + headerClass);
        $('#CustomConfirmModal').appendTo('body').modal('show');
        if (OkCallback) {
            $('#CustomConfirmModal .modal-footer .okbtn').off('click').click(function () {
                OkCallback();
            });
        }
        if (CancelCallback) {
            $('#CustomConfirmModal .modal-footer .cancelbtn').off('click').click(function () {
                CancelCallback();
            });
        }
    }
    function CustomConfirmModalWithCustomBtns(title, headerClass, body, okBtn, cancelBtn, OkCallback, CancelCallback) {
        $('#CustomConfirmModal').find('.modal-header').addClass('modal-header-' + headerClass);
        $('#CustomConfirmModal').find('.modal-title').html(title);
        $('#CustomConfirmModal').find('.modal-body').html(body);

        $('#CustomConfirmModal .modal-footer .okbtn').text(okBtn ? okBtn : 'OK');
        $('#CustomConfirmModal .modal-footer .cancelbtn').text(cancelBtn ? cancelBtn : 'Cancel');

        $('#CustomConfirmModal .modal-footer .okbtn').attr('class', 'okbtn btn btn-' + headerClass);
        $('#CustomConfirmModal').modal('show');
        if (OkCallback) {
            $('#CustomConfirmModal .modal-footer .okbtn').off('click').click(function () {
                OkCallback();
            });
        }
        if (CancelCallback) {
            $('#CustomConfirmModal .modal-footer .cancelbtn').off('click').click(function () {
                CancelCallback();
            });
        }
    }

    $(function () {
        $('#CustomAlertModal').on('hide.bs.modal', function (e) {
            $('#CustomAlertModal').find('.modal-header').attr('class', 'modal-header');
            $('#CustomAlertModal').find('.modal-title').html('');
            $('#CustomAlertModal').find('.modal-body').html('');
            $('#CustomAlertModal .modal-footer .okbtn').attr('class', 'okbtn btn btn-default');
            $('#CustomAlertModal .modal-footer .okbtn').off("click");
            if (ModalOkCallback != null) {
                ModalOkCallback();
                ModalOkCallback = null;
            }
        });
        $('#CustomConfirmModal').on('hide.bs.modal', function (e) {
            $('#CustomConfirmModal').find('.modal-header').attr('class', 'modal-header');
            $('#CustomConfirmModal').find('.modal-title').html('');
            $('#CustomConfirmModal').find('.modal-body').html('');
            $('#CustomConfirmModal .modal-footer .okbtn').attr('class', 'okbtn btn btn-default');
            $('#CustomConfirmModal .modal-footer .okbtn').off("click");
        });
        $('#APCustomModal').on('hide.bs.modal', function (e) {
            try {
                $('#APCustomModal').find('.modal-header').attr('class', 'modal-header');
                $('#APCustomModal').find('.modal-title').html('Alert');
                $('#APCustomModal').find('.ap-modal-body').html('');

                var bodyBtn = $('#APCustomModal').find('.ap-modal-body-btn');
                var leftBtn = $('#APCustomModal').find('.ap-modal-left-btn');
                var rightBtn1 = $('#APCustomModal').find('.ap-modal-right-btn1');
                var rightBtn2 = $('#APCustomModal').find('.ap-modal-right-btn2');

                bodyBtn.off('click').text('').attr('class', 'ap-modal-body-btn btn btn-default').hide();
                leftBtn.off('click').text('').attr('class', 'ap-modal-left-btn pull-left btn btn-default').hide();
                rightBtn1.off('click').text('').attr('class', 'ap-modal-right-btn1 btn btn-default').hide();
                rightBtn2.off('click').text('').attr('class', 'ap-modal-right-btn2 btn btn-default').hide();

                if (APCustomModalCloseCallback != null) {
                    APCustomModalCloseCallback();
                    APCustomModalCloseCallback = null;
                }
            }
            catch (ex) {
                console.info('APCustomModal Hide Exception : ' + ex);
            }
        });
    });

    //-------------
    var APCustomModalCloseCallback = null;
    function APCustomModal(obj) {
        /*var obj = {
            title: 'Alert',
            headerClass: 'warning',
            body: '',
            bodyBtn: '',
            bodyBtnClass: '',
            bodyBtnCallback: undefined,
            leftBtn: '',
            leftBtnClass: '',
            leftBtnCallBack: undefined,
            rightBtn1: '',
            rightBtn1Class: '',
            rightBtn1CallBack: undefined,
            rightBtn2: '',
            rightBtn2Class: '',
            rightBtn2CallBack: undefined,
            hideCloseIcon:false,
            escClose:false,
            outsideClickClose:false,
            modalCloseCallback:undefined
        };
        */
        try {
            if (obj.modalCloseCallback) {
                APCustomModalCloseCallback = obj.modalCloseCallback;
            }
            $('#APCustomModal').find('.modal-header').addClass('modal-header-' + obj.headerClass);
            $('#APCustomModal').find('.modal-title').html(obj.title);
            $('#APCustomModal').find('.ap-modal-body').html(obj.body);

            var topCloseBtn = $('#APCustomModal .modal-header').find('.close');
            if (obj.hideCloseIcon && topCloseBtn.length > 0) {
                topCloseBtn.hide();
            }

            var bodyBtn = $('#APCustomModal').find('.ap-modal-body-btn');
            var leftBtn = $('#APCustomModal').find('.ap-modal-left-btn');
            var rightBtn1 = $('#APCustomModal').find('.ap-modal-right-btn1');
            var rightBtn2 = $('#APCustomModal').find('.ap-modal-right-btn2');

            if (!obj.bodyBtn) {
                bodyBtn.hide();
            }
            else {
                bodyBtn.text(obj.bodyBtn).attr('class', 'ap-modal-body-btn btn btn-' + (obj.bodyBtnClass ? obj.bodyBtnClass : 'primary')).show();

                if (obj.bodyBtnCallback) {
                    bodyBtn.off('click').click(function () {
                        obj.bodyBtnCallback();
                    });
                }
            }
            if (!obj.leftBtn) {
                leftBtn.hide();
            }
            else {
                leftBtn.text(obj.leftBtn).attr('class', 'ap-modal-left-btn pull-left btn btn-' + (obj.leftBtnClass ? obj.leftBtnClass : 'primary')).show();

                if (obj.leftBtnCallback) {
                    leftBtn.off('click').click(function () {
                        obj.leftBtnCallback();
                    });
                }
            }
            if (!obj.rightBtn1) {
                rightBtn1.hide();
            }
            else {
                rightBtn1.text(obj.rightBtn1).attr('class', 'ap-modal-right-btn1 btn btn-' + (obj.rightBtn1Class ? obj.rightBtn1Class : 'primary')).show();

                if (obj.rightBtn1Callback) {
                    rightBtn1.off('click').click(function () {
                        obj.rightBtn1Callback();
                    });
                }
            }
            if (!obj.rightBtn2) {
                rightBtn2.hide();
            }
            else {
                rightBtn2.text(obj.rightBtn2).attr('class', 'ap-modal-right-btn2 btn btn-' + (obj.rightBtn2Class ? obj.rightBtn2Class : 'primary')).show();

                if (obj.rightBtn2Callback) {
                    rightBtn2.off('click').click(function () {
                        obj.rightBtn2Callback();
                    });
                }
            }
            $('#APCustomModal').modal({
                keyboard: obj.escClose,
                backdrop: obj.outsideClickClose,
                show: true
            });
        }
        catch (ex) {
            console.info('APCustomModal Show Exception : ' + ex);
        }
    }
    //------------------------
</script>
