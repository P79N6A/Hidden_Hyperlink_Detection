<script>
    $(document).ready(function () {

        $("#headtype").hover(
              function () {
                  $("#typepop").fadeIn(200);
                  $(this).addClass('typeover');
              },
              function () {
                  $("#typepop").fadeOut(0);
                  $(this).removeClass('typeover');
              });

        $(".proxxbmore").click(function () {
            $(".proxxbox").css("height", "auto");
            $(".proxxbmore").css("display", "none");
            $(".proxxbless").css("display", "block");
        });
        $(".proxxbless").click(function () {
            $(".proxxbox").css("height", "54px");
            $(".proxxbless").css("display", "none");
            $(".proxxbmore").css("display", "block");
        });
    });
    $.fn.smartFloat = function () {
        var position = function (element) {
            var top = element.position().top, pos = element.css("position");
            $(window).scroll(function () {
                var scrolls = $(this).scrollTop();
                if (scrolls > top) {
                    if (window.XMLHttpRequest) {
                        $(".fixnav").addClass('hide');
                    }
                } else {
                    $(".fixnav").removeClass('hide');
                }
            });
        };
        return $(this).each(function () {
            position($(this));
        });
    };
    $("#float").smartFloat();
    $(function () {
        $("img").delayLoading({
            defaultImg: "/content/loading.jpg",
            errorImg: "",
            imgSrcAttr: "originalSrc",
            beforehand: 0,
            event: "scroll",
            duration: "normal",
            container: window,
            success: function (imgObj) { },
            error: function (imgObj) { }
        });
    });
</script>
