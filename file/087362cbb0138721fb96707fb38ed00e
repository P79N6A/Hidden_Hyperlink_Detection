    function createAd(adId,divId){
        var ad = document.getElementById(adId);
        var div = document.getElementById(divId);
        if(ad){
            ad.parentNode.removeChild(ad);
            if(div){
                div.appendChild(ad);
            }
        }
    }

    $(function(){
			$.ajax({
				url: "http://m.fenlei.iask.sina.com.cn/javascript.php?part=information&id=32720539&inajax=1",  
				dataType: "json",   
				async: true, 
				data: {},    //参数值
				type: "GET",   //请求方式
				success: function(req) {
					$("#viewnum").html(req);
				}
			});

			var strl = $(".about-text").html().length;
			if(strl>50){
				$(".about-text").css({"height":"45","overflow":"hidden","text-align":"left"});
			}
		});
		</script> 
  <script src="http://static.iask.cn/fidjs/M/584_0.js" type="text/javascript"></script>  
