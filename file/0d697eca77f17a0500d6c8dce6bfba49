  <script>
    $(function () {

        $('.pf_e p a').click(function () {
            $(this).parents('.pf').hide();
        });

        $('.st_t a').click(function () {    //切换tab
            var tab = $(this).attr('id');
            $('.st_t a').removeClass('hover');
            $(this).addClass('hover');
            $('.tab1,.tab2,.tab3').hide();
            $('.' + tab).show();

        });

        $('.tab1 a').click(function () {  //选择单选
            $(this).parent().find('a').removeClass('hover');
            $(this).addClass('hover');
            //统计多少个hover
            var count = $('.tab1 a[class="hover"]').length;
            $('#now_count_1').text(count);

        });

        $('.tab2 a').click(function () {  //选择多选
            var count = 0;
            if ($(this).hasClass('hover')) {
                $(this).removeClass('hover');
            } else {
                $(this).addClass('hover');
            }

            $(".tab2 .st_p").each(function () { //计算数量
                if ($(this).find('a').hasClass('hover')) {
                    count++;
                }
            });

            $('#now_count_2').text(count);
        });


        $('textarea').focusout(function () {    //离开textarea
            var count = 0;
            $("textarea").each(function () {
                if ($(this).val()) {
                    count++;
                }
            });
            $('#now_count_3').text(count);
        });


        //获得答题时间
        var answer_time = $('#answer_time').text();
        answer_time = parseInt(answer_time * 60);

        var intDiff = parseInt(answer_time);   //倒计时总秒数量
        s = setInterval(function () {
            start(intDiff)
        }, 1000);

        function start() {
            var day = 0;
            var hour = 0;
            var minute = 0;
            var second = 0;
            if (intDiff > 0) {
                day = Math.floor(intDiff / (60 * 60 * 24));
                hour = Math.floor(intDiff / (60 * 60)) - (day * 24);
                minute = Math.floor(intDiff / 60) - (day * 24 * 60) - (hour * 60);
                second = Math.floor(intDiff) - (day * 24 * 60 * 60) - (hour * 60 * 60) - (minute * 60);
            }
            if (minute <= 9) minute = '0' + minute;
            if (second <= 9) second = '0' + second;
            $('#h').html('0' + hour);
            $('#i').html(minute);
            $('#s').html(second);
            intDiff--;
            if (intDiff == 0) {
                end();
            }
        }

        function end() {
            //倒计时结束，执行表单提交动作
            $("#form1").submit();
        }


        $('.gr_ee').click(function () {    //暂停
            clearInterval(s);
            $('.tz').show();
            $('.em').show();
        });

        $('.tz a').click(function () {     //继续
            $(this).parents('.tz').hide();
            s = setInterval(function () {
                start(intDiff)
            }, 1000);
            $('.em').hide();
        });


        function unique5(array) {
            var r = [];
            for (var i = 0, l = array.length; i < l; i++) {
                for (var j = i + 1; j < l; j++)
                    if (array[i] === array[j]) j = ++i;
                r.push(array[i]);
            }
            return r;
        }

        $('#look_answer').click(function () { //查看答案（提交）

            $('.tab1 a[class="hover"]').each(function () {    //获得单选答案
                var id = $(this).data('id');
                var answer = $(this).text().toLowerCase();
                $('input[name="' + id + '"]').val(answer);
            });

            $('.tab2 .st_i').each(function () {  //获得多选答案
                var answer = '';
                var id = $(this).data('id');
                var answer_tmp = $(this).find('a[class="hover"]').text().toLowerCase();
                if (answer_tmp != 0) {
                    for (var i = 0; i < answer_tmp.length; i++) {
                        answer += answer_tmp[i] + ',';
                    }
                }
                answer = answer.substring(0, answer.length - 1);
                $('input[name="' + id + '"]').val(answer);
            });


            $('.tab3 .st_f').each(function () {  //获得多选答案
                var answer = '';
                var id = $(this).data('id');
                answer = $(this).find('textarea').val();
                $('input[name="' + id + '"]').val(answer);
            });

            $('input[name="use_time"]').val(answer_time - intDiff);

            $("#form1").submit();

        });


        //下次继续
        $('#go_on').click(function () {
            var id = $(this).data('id');
            var use_time = answer_time - intDiff;

            $.post("/index.php?m=Home&c=Exam&a=next", {id: id, use_time: use_time},
                    function (data) {
                        if (data == 1) {
                            location.href = '/index.php?m=Home&c=Question&a=log';
                        }
                    });
        });


        //打印
        $('#do_p').click(function () {
            var el = document.getElementById('print_div');
            var iframe = document.createElement('IFRAME');
            var doc = null;
            iframe.setAttribute('style', 'position:absolute;width:0px;height:0px;left:-500px;top:-500px;');
            document.body.appendChild(iframe);
            doc = iframe.contentWindow.document;
            doc.write('<div>' + el.innerHTML + '</div>');
            doc.close();
            iframe.contentWindow.focus();
            iframe.contentWindow.print();
            if (navigator.userAgent.indexOf("MSIE") > 0) {
                document.body.removeChild(iframe);
            }
        });


    });








</script>
 </body>
