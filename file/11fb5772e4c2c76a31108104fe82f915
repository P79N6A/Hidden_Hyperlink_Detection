<!-- 认证框 start-->
<script type="text/javascript">
	//刷新验证码
	function reloadcode(){
		clientStr = Math.ceil(Math.random()*1000000);
		document.getElementById('validateCode').src = "http://Captchas.fang.com/Display?type=soufangbang&width=100&height=32&r=" + clientStr;
	}
	
	/*获取手机验证码*/
    function get_captcha(){
        var username = document.getElementById('username').value;
		if(username == ''){
			alert("用户名不能为空，请确认已经登录！");
			return false;
        }
        var usermobile = document.getElementById('usermobile').value;
		rexp = /^1[3,4,5,6,7,8]{1}[0-9]{9}$/;
        if(!rexp.test(usermobile)){
			alert("请填写正确的手机号！");
			return false;
        }          
		try{
			var httpsService = "soufun-bbs-web";
                        var verifyCode = jQuery('#verifyCodeId').val();
                        jQuery.ajax({
                            type: "get",
                            url: "https://passport.fang.com/sendsms.api",  //正式
                           //url: "https://passporttest.fang.com/login.api",//测试
                            //url: '/NormalLoginInterface/Login',
                            dataType: "jsonp",
                            jsonp: "callback",
                            data: {"Service": escape(httpsService), "mobilephone":escape(usermobile),"mathcode":verifyCode},
                            xhrFields: {
                                withCredentials: true
                            },
                            crossDomain: true,
                            success: function (json) {
                                if( json.Message=="Success" )
                                {
                                    //验证成功
                                    document.getElementById('lasttime').innerHTML= '<span>验证码发送成功</span>';
                                }
                                else
                                {
                                    if(json.IsShowMathCode == true){
                                        reloadcode();
                                        jQuery('#verifyCodeId').val('');
                                        jQuery('#showVerifyId').css("display","block");;
                                    }
                                    //验证失败
                                    if(json.Tip!=''){
                                        alert(json.Tip);
                                    }else{
                                        alert('验证码发送失败，请重新获取');
                                    }
                                    return false;
                                }
                            },
                            error: function(XMLHttpRequest, textStatus, errorThrown){
                                alert(XMLHttpRequest.status);
                                alert(XMLHttpRequest.readyState);
                                alert(textStatus);
                            }
                        });
		}catch(e){}; 
		//验证码按钮禁用60秒，60秒后可重新点击获取验证码，奶奶的jQuery为啥用不了，害我写原生js
		var i=60;
		var circle=setInterval(showtime,1000);			
		function showtime(){
			document.getElementById('lasttime').setAttribute("onclick", "javascript:void(0)");
			document.getElementById('lasttime').innerHTML= i+'<span>秒后获取验证码</span>';
			i--;
			if(i<1){
				clearInterval(circle);
				document.getElementById('lasttime').setAttribute("onclick", "get_captcha()");
				document.getElementById('lasttime').innerHTML= '<span>重获手机验证码</span>';
			}			
		}
	}
	
	/*校验手机验证码*/
	function check_captcha(){
		var userId = document.getElementById('userid').value;
        var username = document.getElementById('username').value;
		if(username == ''){
			alert("用户名不能为空，请确认已经登录！");
			return false;
        }
        var usermobile = document.getElementById('usermobile').value;
		rexp = /^1[3,4,5,6,7,8]{1}[0-9]{9}$/;
        if(!rexp.test(usermobile)){
			alert("请填写正确的手机号！");
			return false;
        }
		var code = document.getElementById('captchacode').value;
		if(code.length == 0){
			alert("请填写验证码!");
			return false;
        }
		try{
			var httpsService = "soufun-bbs-web";
            jQuery.ajax({
                type: "get",
                url: "https://passport.fang.com/verifysmstobindorchangebind.api",  //正式
                //url: "https://passporttest.fang.com/login.api",//测试
                //url: '/NormalLoginInterface/Login',
                dataType: "jsonp",
                jsonp: "callback",
                data: {"Service": escape(httpsService), "mobilephone":escape(usermobile),"vcode":escape(code)},
                xhrFields: {
                    withCredentials: true
                },
                crossDomain: true,
                success: function (json) {
                    if( json.Message=="Success" )
                    {
                        //验证成功
                        document.getElementById('loginbo-add').style.display='none';
						//清除用户登录缓存
						AJAXCALL(CurJsBbsHref+'ajax/deleteUserCache.php?userid='+userId, function(res){
                            location.replace(location.href);
                            location.reload(true);
							return;
                        });
                    }
                    else
                    {
                        //验证失败
                        if(json.Tip!=''){
                            alert(json.Tip);
                        }else{
                            alert('验证码发送失败，请重新获取');
                        }
                        return false;
                    }
                },
                error: function(XMLHttpRequest, textStatus, errorThrown){
                    alert(XMLHttpRequest.status);
                    alert(XMLHttpRequest.readyState);
                    alert(textStatus);
                }
            });
			/*
			if('undefined' != typeof AJAXCALL) {
				AJAXCALL(CurJsBbsHref+'ajax/checkUserMobile.php?verificationcode='+code+'&mobilephone='+usermobile+'&username='+username, function(doc){
					if(doc.indexOf('{')){
						var s = doc.indexOf('{');
						var result = doc.slice(s);
					}else{
						var result = doc;
					}
					var json = eval('(' + result + ')');
					//console.log(json.succ+CurJsBbsHref+'ajax/deleteUserCache.php?userid='+userId+'&username='+username);
					if(json.succ == 1){
                        document.getElementById('loginbo-add').style.display='none';                                                
						//清除用户登录缓存
						AJAXCALL(CurJsBbsHref+'ajax/deleteUserCache.php?userid='+userId, function(res){
                            location.replace(location.href);
                            location.reload(true);
							return;
						});
					}else{
                        if(json.error == ''){
                            alert('认证失败，请重新认证');
                        }else{
                            alert(json.error);
                        }
						return false;						
					}
				});
			}
			*/
		}catch(e){};		
	}
 
