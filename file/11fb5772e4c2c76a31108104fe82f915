<script>
	_ub.city = '济南'; //所在城市（中文）（这个地方的城市取得是页面左上角那个地方的城市）
	_ub.biz = 'b'; //论坛‘b’
	_ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
	var postTitleUB = encodeURIComponent('家装修成这样！！！');
	var postType = encodeURIComponent('active');
	if(postType=='vote'){
		postType = encodeURIComponent('投票贴');
	}else if('0'==1){
		postType = encodeURIComponent('精华帖');
	}else{
		postType = encodeURIComponent('普通');
	}
	//var postType = encodeURIComponent('');
	var froumId = '56766';
	var postId = '181197092';
	var forumNameUB = encodeURIComponent('华皓英伦联邦');
	//新发帖浏览
	if('199'==1){
		_ub.collect(53,{'vwg.page':'bbswritepost','vwb.posttitle':postTitleUB,'vwb.sentto':forumNameUB,'vwb.posttype':postType,'vwb.postid':postId+"^"+froumId,'vwb.forumid':forumId});
	}else{
		//浏览 
		_ub.collect(0,{'vwg.page':'bbspostpage','vwb.postid':postId+"^"+froumId,'vwb.posttype':postType,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId});	
	}
	//收藏 
	function collectPostUB(){
		_ub.collect(21,{'vwg.page':'bbspostpage','vwb.postid':postId+"^"+froumId,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId});
	}
	//举报 
	function showReportUB(postId){
		_ub.collect(2,{'vwg.page':'bbspostpage','vwb.postid':postId+"^"+froumId,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId});
	}
	//踩 顶
	function beginReadPostsFlowerUB(forumId,postId,spanid,flag){
		if(flag==1){
			var b = 56;
		}else{
			var b = 55;
		}
		//原有的顶或踩函数↓
		beginReadPostsFlower(forumId,postId,spanid,flag)
		_ub.collect(b,{'vwg.page':'bbspostpage','vwb.postid':postId+"^"+froumId,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId});
	}
	//回复 
	function postReplyUB(){
		_ub.collect(18,{'vwg.page':'bbspostpage','vwb.postid':postId+"^"+froumId,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId});
	}
	//发表评论 
	function postCommentUB(postid){
		_ub.collect(16,{'vwg.page':'bbspostpage','vwb.postid':postid+"^"+froumId,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId});
	}
	//投票 
	function voteUB(){
		_ub.collect(109,{'vwg.page':'bbspostpage','vwb.postid':postId+"^"+froumId,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId,});
		return true;
	}
	//分享 22
	function shareUB(){
		_ub.collect(22,{'vwg.page':'bbspostpage','vwb.postid':postId+"^"+froumId,'vwb.posttitle':postTitleUB,'vwb.forumid':forumId});
	}
	//加关注 21
	function attentionUB(userId){
		_ub.collect(21,{'vwg.page':'bbspostpage','vwb.interestuserid':userId});
	}
</script>
