<script type="text/javascript">
	function showtel(tel,fenji) {
		var telobj = document.getElementById('tel_400');
		var tel_num = tel;
		telobj.setAttribute('tel',tel_num);
		telobj.setAttribute('fenji',fenji);
		var tel_form = document.getElementById('pop_login');
		tel_form.style.display = 'block';
	}
</script>




