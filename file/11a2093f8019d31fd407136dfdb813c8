    <script>
        $(function(){

           $.extend({
            getOrderRecord:function(options){
                var defaults = {    
                    page            : 1,
                    totalNumber    : 0,
                    merchantId       : 175404                }; 
                var option      = $.extend(defaults,options);
                var page        = option.page;
                var totalNumer  = option.totalNumber;

                var url  = '/index.php?c=Ajax_MerchantStore_OrderRecord&callback=?&t='+Math.random();
                $.getJSON(
                    url,
                    {page:page,merchantId:merchantId},
                    function(backdata){
                        var str      = '';
                        if(backdata.flag){
                             //返回值处理
                             str         += '<table><tr>';
                             str         += '<th class="cell-buyer">买家</th>';
                             str         += '<th class="cell-sku">款式和型号</th>';
                             str         += '<th class="cell-price">价格</th>';
                             str         += '<th class="cell-amount">数量</th>';
                             str         += '<th class="cell-time">成交时间</th>';
                             str         += '<th class="cell-state">状态</th>';
                             str         += '</tr>';
                             var orderInfo = backdata.orderInfo;
                             for(var i in orderInfo){
                                 str     += '<tr>';
                                 str     += '<td class="cell-buyer">'+orderInfo[i].username+'</td>';
                                 str     += '<td class="cell-type"><p><a href="'+orderInfo[i].goodsUrl+'" target="_blank">'+orderInfo[i].goodsName+'</a></p>';
                                 str     += '<td class="cell-price">&yen;<em>'+orderInfo[i].goodsPrice+'</em>';
                                 if(orderInfo[i].promoId != 0){
                                     str += '<span alt="促销" class="cu-ico">促</span>';
                                 }
                                 str     += '</td>';
                                 str     += '<td class="cell-amount">'+orderInfo[i].goodsNumber+'</td>';
                                 str     += '<td class="cell-time">'+orderInfo[i].formatAddTime+'</td>';
                                 str     += '<td class="cell-state">成交</td>';
                                 str     += '</tr>';
                             }
                             str         += '</table>';
                             //分页
                            var pageStr  = $.zpPaging({page:page,size:10,totalNumber:totalNumer,merchantId:merchantId,pageFun:'getOrderRecord'});
                            str          += pageStr;
                        }else{//交易记录总量小于0时
                             var str     = '<div class="no-prompt">新店开张，暂未有人购买过...</div>';
                        }

                        $("#order-record-list").html(str);
                    }
                );
            }
           });
        });
        $(function(){
            $.getOrderRecord();
        });
    </script>
                    
