        <script>
            var subList             = $("#goods-category-list>dl>dt").click(function(){
                //判断列表状态
                var subList        = $(this).siblings();
                var subListStatus   = subList.is(":visible");//显示状态

                if(subListStatus){
                    subList.hide();
                    subList.parent("dl").removeClass("unfold").addClass("fold");
                }else{
                    subList.show();
                    subList.parent("dl").removeClass("fold").addClass("unfold");
                }
            });
        </script>
