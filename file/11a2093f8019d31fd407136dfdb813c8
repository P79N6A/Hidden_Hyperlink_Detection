                    <input type="hidden" id="merchantId" value="175404">
                <ul id="promoInfo"><div class="no-prompt">暂无促销信息</div></ul>        
        <script>
            $(document).ready(function() {
                var merchantId = $("#merchantId").val();
                var page = $("#page").html();
                var pageTotal = $("#pageTotal").html();
                if (pageTotal == 0) {
                    $(".pageNumber").hide();
                    $("#next").hide();
                }
                if (pageTotal == 1) {
                    $("#next").hide();
                }
                if (page == 1) {
                    $("#prev").hide();
                }
                if (page == pageTotal) {
                    $("#next").hide();
                }
                getPromoInfo(1, merchantId);
                $("#prev").click(function() {
                    $("#next").show();
                    if (page == 2) {
                        $(this).hide();
                    }
                    if (page > 1) {
                        page--;
                        $("#page").html(page);
                        getPromoInfo(page, merchantId);
                    }
                });
                $("#next").click(function() {
                    $("#prev").show();
                    if (page == (pageTotal - 1)) {
                        $(this).hide();
                    }
                    if (page < pageTotal) {
                        page++;
                        $("#page").html(page);
                        getPromoInfo(page, merchantId);
                    }
                });
            });
            function getPromoInfo(page, merchantId) {
                var url = '/index.php?c=MerchantStore_Market&a=AjaxGetPromoInfo&callback=?&t=' + Math.random();
                $.getJSON(url, {page: page, merchantId: merchantId}, function(data) {
                    if (data.flag) {
                        $("#promoInfo").html(data.msg);
                        $("#promo-page").show();
                    }else{
                        $("#promoInfo").html(data.msg);
                    }
                });
            }
        </script>
    </div>
