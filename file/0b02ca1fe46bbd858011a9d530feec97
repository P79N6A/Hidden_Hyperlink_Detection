  <script>
   		var size=14;
   	function changeArticleFont(){
   		if(size==14){
   			document.getElementById("intro-body").style.fontSize='16px';
   			size=16;
   			return;
   		}
   		if(size==16){
   			document.getElementById("intro-body").style.fontSize='12px';
   			size=12;
   			return;
   		}
   		if(size==12){
   			document.getElementById("intro-body").style.fontSize='14px';
   			size=14;
   			return;
   		}
   	}
   	
   	jQuery(function(){
   	//分享链接的缩略图
   	 var mobileImgUrl = '';
   	 //分享链接的链接地址
   	 var mobileLineLink = 'http://m.expreview.com/51953.html';
   	 //分享链接的描述信息
   	 var mobileDescContent = "";
   	 //分享链接的标题
   	 var mobileShareTitle = "奥巴马邀请你前往白宫游览一番，只要你拥有一顶VR头显";
   	 //留空就好
   	 var mobileAppid = '';
   	// 当微信内置浏览器完成内部初始化后会触发WeixinJSBridgeReady事件。
   	 document.addEventListener('WeixinJSBridgeReady', function onBridgeReady() {
   	 	// 发送给好友
   	 	WeixinJSBridge.on('menu:share:appmessage', function(argv) {shareMobileFriend();});
   	 	// 分享到朋友圈
   	 	WeixinJSBridge.on('menu:share:timeline', function(argv) {shareMobileTimeline();});
   	 	// 分享到微博
   	 	WeixinJSBridge.on('menu:share:weibo', function(argv) {shareMobileWeibo();});
   	 }, false);
   	 });
   	
   	function shareMobileFriend() {
   		WeixinJSBridge.invoke('sendAppMessage', {
   			"appid": mobileAppid,
   			"img_url": mobileImgUrl,
   			"img_width": "640",
   			"img_height": "640",
   			"link": mobileLineLink,
   			"desc": mobileDescContent,
   			"title": mobileShareTitle
   		}, function(res) {
   			_report('send_msg', res.err_msg);
   		})
   	}
   	//分享到朋友圈
   	function shareMobileTimeline() {
   		WeixinJSBridge.invoke('shareTimeline', {
   			"img_url": mobileImgUrl,
   			"img_width": "640",
   			"img_height": "640",
   			"link":mobileLineLink,
   			"desc": mobileDescContent,
   			"title":mobileShareTitle
   		}, function(res) {
   			_report('timeline', res.err_msg);
   		});
   	}
   	//分享到腾讯微博
   	function shareMobileWeibo() {
   		WeixinJSBridge.invoke('shareWeibo', {
   			"content": mobileDescContent,
   			"url": mobileLineLink,
   		}, function(res) {
   			_report('weibo', res.err_msg);
   		});
   	}
   </script>
