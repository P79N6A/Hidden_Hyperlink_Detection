<script type="text/javascript">
/*计算图片宽高*/
new Image().src='http://img.pconline.com.cn/images/upload/upc/tx/ladyproduct/1703/14/c0/39472666_1489498014156_medium.jpg';
var imgurl = 'http://img.pconline.com.cn/images/upload/upc/tx/ladyproduct/1703/14/c0/39472666_1489498014156_medium.jpg';
 /*计算图片宽高*/
(function setImgMargin(){
  var img = new Image,w,h,Timer;
    img.src = imgurl;
    if(img.complete || img.width || img.height){
        // 缓存情况
        callBack(img);
    }else{
        w = img.width;
        h = img.height;
        Timer = setInterval(function(){
            var _w = img.width,
                _h = img.height;
            if(_w != w && _h != h){
               callBack(img);
               clearInterval(Timer);
            }
        },40)
   }
  function callBack(obj){
    var $imgWidth=$(".max-pic .picCont img").width();
    var $imgHeight=$(".max-pic .picCont img").height();
    if($imgHeight<=600){
      $(".max-pic .picCont img").css({"position":"absolute","margin-top":-$imgHeight/2,"margin-left":-$imgWidth/2,"top":"50%","left":"50%"}).show();
    }else{
      if($imgWidth>$imgHeight){
          $(".max-pic .picCont img").css({"width":"800px","position":"absolute","margin-top":-(800/$imgWidth*$imgHeight)/2,"margin-left":-800/2,"top":"50%","left":"50%"}).show();
      }else{
          $(".max-pic .picCont img").css({"height":"600px","position":"absolute","margin-top":"-300px","margin-left":-(600/$imgHeight*$imgWidth)/2,"top":"50%","left":"50%"}).show();
      }      
      
    }
  }
  window.setImgMargin=setImgMargin;
})();

var aa=new Slidenew("slide");

//预加载下一张图片
window.onload = function(){
setTimeout(function(){
	var nextpic = new Image();
	nextpic.src = 'http://img.pconline.com.cn/images/upload/upc/tx/ladyproduct/1703/14/c0/39472666_1489498022902_medium.jpg';
	}, 200);
}

//24小时热图有广告时候，隐藏其热图第5张信息
var lilen = $('#hotGroup li').length;
if(lilen > 5){
	$('#hotGroup li').get(5).style.display = "none";
}

/*计算图片宽高*/
$(function(){
    var Timer;
    function loadImage(url, callback) { 
      var img = new Image(); //创建一个Image对象，实现图片的预下载 
      img.src = url; 

      if(img.complete || img.width || img.height){
            // 缓存情况
            callback.call(img);
        }else{
            w = img.width;
            h = img.height;
            Timer = setInterval(function(){
                var _w = img.width,
                    _h = img.height;
                if(_w != w && _h != h){
                    callback.call(img);
                    clearInterval(Timer);
                }
            },40)
       }
    }
    function equalProp(tar,parentClass,setW,setH){
        tar.each(function(){
            var $this=$(this);
            loadImage($(this).attr("src"), function(){
                if(this.width>=this.height){
                    var setMt=(setH-parseInt(this.height*(setW/this.width)))/2;
                    $this.width(setW); 
                    $this.height(this.height*(setW/this.width)); 
                    $this.css("marginTop",setMt)           
                }else{
                    $this.height(setH);
                    $this.width((setH/this.height)*this.width);
                    $this.parents(parentClass).css("text-align","center")  
                }
            })
        })
    }

    /*猜你喜欢等比例压缩*/
    equalProp($(".recommend-list img"),"recommend-pic",180,140);
    /*24小时等比例压缩*/
    equalProp($(".hot-pic .txt-sum img,.hot-top .txt-sum img"),"txt-sum",260,193);
});
</script>

