$(function(){
	startmarquee('announ',22,1,500,3000);
})
$(function() {
	var footerH=$(".footerbox").height(); 
	$("#topcontrol img").css({bottom:footerH-51});
});
function dropMenu(obj){
	var clienwidths=document.body.clientWidth-10;
	var j=0;
	$(obj).each(function(){
		
		var theSpan = $(this);
		var theMenu = theSpan.find(".submenubox");
		var thesubMenu = theSpan.find(".submenu");
		var tarHeight = theMenu.height();
		theMenu.css({height:0,opacity:0});
		thesubMenu.css({width:clienwidths});
		theSpan.hover(
				function(){
				$(".drop-menu-effect").removeClass("selected");
				$(".submenubox").stop().animate({height:0,opacity:0},0,function(){
					$(".submenubox").css({display:"none"});
				});
				$(this).addClass("selected");
				theMenu.stop().show().animate({height:tarHeight,opacity:1},0);
			}
		);
	});
}

$(document).ready(function(){
	
	dropMenu(".drop-menu-effect");
	$("#floatShow").bind("mouseover",function(){
	
		$("#onlineService").stop(true,true).animate({width:"show", opacity:"show"}, "normal" ,function(){
			$("#onlineService").show();
		});
		
		$("#floatShow").attr("style","display:none");
		$("#floatHide").attr("style","display:block");
		
		return false;
	});
	
	$("#online_qq_layer").bind("mouseleave",function(){
	
		$("#onlineService").stop().animate({width:"hide", opacity:"hide"}, "normal" ,function(){
			$("#onlineService").hide();
		});
		$("#floatHide").attr("style","display:none");
		$("#floatShow").attr("style","display:block");
		
		return false;
	});
});
</script> 
