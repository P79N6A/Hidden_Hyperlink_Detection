         <div class="reward_form">
          <div class="reward_line">
           <div class="input_wrap">
            <input type="text" placeholder="请输入正确的金额" node-type="randVal" />
           </div>
           <em class="unit">&yen;</em>
           <span suda-uatrack="key=tblog_weibo_dashang&amp;value=shang_random:1266194363" class="random" node-type="doRand"><em></em></span>
          </div>
