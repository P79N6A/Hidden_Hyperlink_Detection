  <script type="text/javascript" src="http://www.vx.com/statics/js/bootstrap/js/bootstrap.min.js"></script> 
  <script type="text/javascript" src="http://www.vx.com/statics/js/common.js"></script> 
  <script type="text/javascript" src="http://www.vx.com/statics/js/common_public.js"></script> 
  <script>
	$(document).ready(function(){
		Wind.usepath('common/masonry.pkgd.min.js',function(){
			item_callback();  
			$('.item').fadeIn();
		});
	});

	function item_callback(){ 
		$('.item').mouseover(function(){
			$(this).css('box-shadow', '0 1px 5px rgba(35,25,25,0.5)');
		}).mouseout(function(){
			$(this).css('box-shadow', '');	 	
		});
		item_masonry();	
	}
	function item_masonry(){ 
		$('.results').masonry({ 
			itemSelector: '#itemshow',
			columnWidth:226,
			gutter: 10
		});	
	}
</script> 13 
