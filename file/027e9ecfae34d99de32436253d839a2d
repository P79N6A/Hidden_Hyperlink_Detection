$("#div"+obj).css("display","block");
if(obj=='1')
$('#tip1').css("left","93px");
else if(obj=='78')
$('#tip1').css("left","198px");
else if(obj=='3')
$('#tip1').css("left","295px");
else if(obj=='4')
$('#tip1').css("left","383px");
else if(obj=='5')
$('#tip1').css("left","460px");
}
