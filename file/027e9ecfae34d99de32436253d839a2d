//底部提交意见
function footer_yj_commit()
{
var message = document.getElementById('yj_txt').value;
if(message == '')
{
alert("请填写建议");return;
}
var str="&mes=" + message;
    $.ajax({
           type:"POST", //post提交方式默认是get
           url:"/ajaxcheck.php?op=tjjy",
           data:str, 
           timeout:8000,//设置8秒延时
           error:function(request) {      // 设置表单提交出错             
        	   
           },
           success:function(s) {
if(s>0)
{
alert("提交成功");location.reload();
}
else if(s==-1)
alert("检测到危险字符");
else
alert("提交失败,请联系管理员");
           }
     });
}
</script> 
