     <script>
function changephone()
{
var obj = document.getElementById("newlogin_phone");
    if(!(/^1[3458]\d{9}$/.test(obj.value))){ 
        
    	$("#newlogin_tsphone").html("请输入正确的手机号码");
    } 
    else
    	$("#newlogin_tsphone").html("");
}
function fbrenwu()
{
$("#newlogin_tsphone").html("");
var obj = document.getElementById("newlogin_phone");
    if(!(/^1[3458]\d{9}$/.test(obj.value))){ 
        
    	$("#newlogin_tsphone").html("请输入手机号码");
    	//alert("请输入正确的手机号码!"); 
        obj.focus();
        return false; 
    } 
    var str="userno="+obj.value;
    $.ajax({
           type:"POST", //post提交方式默认是get
           url:"/source/quicknum.php?",
           data:str, 
           //data:$("#userform").serialize(),   //序列化   
           //dataTyp:"json",
           error:function(request) {      // 设置表单提交出错                 
               //$("#showMsg").html(request);  //登录错误提示信息
           },
           success:function(s) {
if(s=="ok"){
$("#newlogin_tsphone").html("");
 	alert("提交成功!");
 	obj.val("");
 	//location.reload();
 	return false;
 }else if(s=="3"){
 $("#newlogin_tsphone").html("请输入正确的手机号码");
 	obj.focus();return false;
 }else if(s=="2"){
 $("#newlogin_tsphone").html("同一IP地址一天只能请求2次");
 	obj.focus();return false;
 }
 else{
 $("#newlogin_tsphone").html("对不起，提交失败，请稍候再尝试");
 	obj.focus();return false;
 }
        }            
     });
    return false;
}
</script> 
