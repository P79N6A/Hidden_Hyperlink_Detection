       <script>
(function($){
    $.fn.hoverDelay = function(options){
        var defaults = {
            hoverDuring: 200,
            outDuring: 200,
            hoverEvent: function(){
                $.noop();
            },
            outEvent: function(){
                $.noop();    
            }
        };
        var sets = $.extend(defaults,options || {});
        var hoverTimer, outTimer;
        return $(this).each(function(){
            $(this).hover(function(){
                clearTimeout(outTimer);
                hoverTimer = setTimeout(sets.hoverEvent, sets.hoverDuring);
            },function(){
                clearTimeout(hoverTimer);
                outTimer = setTimeout(sets.outEvent, sets.outDuring);
            });    
        });
    }      
})(jQuery);

$(function(){
$.ajax({
           type:"POST", //post提交方式默认是get
           url:"/ajaxcheck.php?op=getuserlogin1",
           data:"", 
           timeout:8000,//设置8秒延时
           error:function(request) {//设置表单提交出错             
        	   //alert(123);
           },
           success:function(data) {
                   $('.new_header1_1').html(data);
           }
     });

});	
var u = 0;
$("#co_user").hoverDelay({
    hoverEvent: function(){
    	u = 1;
    	$('#h_userdiv').show();
    },
    hoverDuring: 0,
    outDuring: 300,
    outEvent: function(){
    	if(u == 1)
    	{
    		if($('#h_userdiv').is(":visible"))
    		{
    			$('#h_userdiv').hide();
    		}
    	}
    }
});
$("#h_userdiv").hoverDelay({
    hoverEvent: function(){
    	u = 2;
    },
    hoverDuring: 0,
    outDuring: 300,
    outEvent: function(){
    	if(u == 2)
    	{
    		$('#h_userdiv').hide();
}
    }
});
function addFavorite(url, title) {
try {
window.external.addFavorite(url, title);
} catch (e){
try {
window.sidebar.addPanel(title, url, '');
        	} catch (e) {
        		alert("请按 Ctrl+D 键添加到收藏夹");
}
}
}
</script> 
