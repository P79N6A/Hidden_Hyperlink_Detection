          <script>
//头部搜索
function header_search()
{
var txtobj = document.getElementById('search_txt');
if(txtobj.value==""||txtobj.value=="请输入您想要找的关键字")
{
alert("请输入关键字!");return false;
}
var type = document.getElementById('search_type').value;

if(type=="1")
{
location.href="http://www.ppwaibao.com/task/search.php?k="+encodeURIComponent(txtobj.value);return false;
}
else if(type=="2")
{
location.href="http://www.ppwaibao.com/supplier/search.php?k="+encodeURIComponent(txtobj.value);return false;
}
else if(type=="3")
{
location.href="http://www.ppwaibao.com/works/index.php?k="+encodeURIComponent(txtobj.value);return false;
}
}
</script> 
