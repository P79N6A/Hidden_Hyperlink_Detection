    <script type="text/javascript">
function opentbxq(n,uid)
{
$("#looktbxqnn_"+n).hide();
var obj = $("#looktbxq_"+n);
var obj2 = $("#controltb_"+n);
if(obj.css('display') == 'none')
{
obj.slideDown("300");
obj2.html("收缩方案");

loaddp(uid);

}
else
{
obj.slideUp("300");
obj2.html("展开方案");
}
}
function loaddp(uid)
{
var str="&uid="+uid+"&renwuid=1057";

$.ajax({
           type:"POST", //post提交方式默认是get
           url:"/ajaxcheck.php?op=gettbdp",
           data:str, 
           //data:$("#userform").serialize(),   //序列化   
           //dataTyp:"json",
           error:function(request) {      // 设置表单提交出错                 
               //$("#showMsg").html(request);  //登录错误提示信息
           },
           success:function(data) {
        	   var dataObj=eval(data);//转换为json对象 
if(dataObj.length>0)
{
var ret = '';
$.each(dataObj,function(i,n){

ret += '<div style=" color: #A5A5A5"><a href="#">'+n.fromusername+'</a>&nbsp;于&nbsp;'+n.time+'&nbsp;评论</div>';
ret += '<div style=" line-height: 20px; margin-bottom: 15px;_margin-bottom: 15px;">'+n.content+'</div>';
//alert(ret);

//alert(n.fromusername);
       
    });
$('#tbxq_html_'+uid).html(ret);
}
else
{
$('#tbxq_html_'+uid).html("暂无点评!<br /><br />");
}
$('#tbxq_loadcount_'+uid).html(dataObj.length+'条回复');
$("#tbxq_loaddiv_"+uid).css("display","block");
$("#tbxq_loadimg_"+uid).css("display","none");

           }
     });
}
function adddp(uid)
{
if($("#tbxq_txt_"+uid).val()=='')
{
alert("请填写内容");return;
}
var str="&content="+$("#tbxq_txt_"+uid).val()+"&renwuid=1057"+"&uid="+uid;
$.ajax({
           type:"POST", //post提交方式默认是get
           url:"/ajaxcheck.php?op=addtbdp",
           data:str, 
           //data:$("#userform").serialize(),   //序列化   
           //dataTyp:"json",
           error:function(request) {      // 设置表单提交出错                 
               //$("#showMsg").html(request);  //登录错误提示信息
           },
           success:function(data) {
        	   loaddp(uid);
           }            
     });
}
function txt_change(uid,count){
$('#tbxq_txt_'+uid).val();
$('#tbxq_tishi_'+uid).val();
if($('#tbxq_txt_'+uid).val().length<count)
{
$('#tbxq_tishi_'+uid).html('您还能输入'+(count - $('#tbxq_txt_'+uid).val().length)+'个字符');
}
else
{
$('#tbxq_tishi_'+uid).html('您还能输入0个字符');
$('#tbxq_txt_'+uid).val($('#tbxq_txt_'+uid).val().substring(0, count));
}					
}
function looktb(){
$('#tblook').css({
display:"block"
});
$('#tbcontrol').css({
display:"none"
});
}
function addfav()
{
var str="&renwuid=1057";
$.ajax({
           type:"POST", //post提交方式默认是get
           url:"/ajaxcheck.php?op=addfav",
           data:str, 
           //data:$("#userform").serialize(),   //序列化   
           //dataTyp:"json",
           error:function(request) {      // 设置表单提交出错                 
               //$("#showMsg").html(request);  //登录错误提示信息
           },
           success:function(data) {
        	   if(data==1)
        		   alert("收藏成功!");
        	   else
        		   alert("已收藏该任务!");
           }            
     });
}
function setbx(uid)
{

var str="&uid="+uid+"&renwuid=1057";
$.ajax({
           type:"POST", //post提交方式默认是get
           url:"/ajaxcheck.php?op=settb_bx",
           data:str, 
           //data:$("#userform").serialize(),   //序列化   
           //dataTyp:"json",
           error:function(request) {      // 设置表单提交出错                 
               //$("#showMsg").html(request);  //登录错误提示信息
           },
           success:function(data) {
        	   if(data==1)
       		{
       		   alert("设置成功!");
       		   location.reload();
       		}
       	   else
       		   alert("设置失败!请联系管理员");
           }            
     });
}
