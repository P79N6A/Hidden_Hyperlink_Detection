  <script type="text/javascript">
$(function (){
$("#newlogindiv_a").zgydialogdiv({					       
divid : "newlogindiv",
titleid : "logintit",
closeid : "loginclose",
isbg : 0
});
});
    
    function newlogin()
    {
    	if($("#newlogin_name").val()=='')
   		{
    		$("#newlogin_name2").html('请填写帐号');
    		$("#newlogin_name").focus();
    		return false;
   		}
    	else
    		$("#newlogin_name2").html('');
    	if($("#newlogin_pwd").val()=='')
   		{
    		$("#newlogin_pwd2").html('请填写密码');
    		$("#newlogin_pwd").focus();
    		return false;
   		}
    	else
    		$("#newlogin_pwd2").html('');
    	var cbx = 0;
    	if($("#cbxtime").attr("checked")==true)
    		cbx = 1;
    	//alert(cbx);

    	var str="&username="+$("#newlogin_name").val()+"&password="+$("#newlogin_pwd").val()+"&cbx"+cbx;
    $.ajax({
           type:"POST", //post提交方式默认是get
                      url:"/ajaxcheck.php?op=newlogin",
                      data:str, 
           //data:$("#userform").serialize(),   //序列化   
           //dataTyp:"json",
           error:function(request) {      // 设置表单提交出错                 
               //$("#showMsg").html(request);  //登录错误提示信息
           },
           success:function(data) {
        	   //alert(data);return;
        	   if(data=='-2')
        		{
        		   alert("密码错误");
        		   return false;
        		}
        	   else if(data=='-1')
        		{
        		   alert("用户名错误");
        		   return false;
        		}
        	   else if(data=='1')
        		{
        		   location.reload();
        		}
        		   
        	   
           }            
     });
    return false;
    	
    }
</script> 
