            lj: function(u,u2,uc){
                if (window.vellumLjDone) return;
                window.vellumLjDone = true;
                var d = document;
                var s = d.createElement('link');
                s.type = 'text/css';
                s.rel = 'stylesheet';
                s.crossOrigin = 'anonymous';
