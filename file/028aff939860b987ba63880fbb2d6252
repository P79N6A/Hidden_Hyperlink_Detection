<script type="text/javascript">
function getUrlParam (key) {//key为String
	var reg = new RegExp("(^|&)" + key + "=([^&]*)(&|$)");
	var r = window.location.search.substr(1).match(reg);
	if (r != null)return decodeURI(r[2]);
	return null;
}
$(function(){
	var index_category=getUrlParam("sly");
	
	$("#"+index_category).addClass('active');
	$("#"+index_category).parent('li').siblings('li').find('a').removeClass('active');
})
</script>

