    function addfav(id){
        $.post('/pchome/bz/add_to_favorite.html', { infoid: id}, function(data){
            var c_data = $.parseJSON(data);
            if(c_data['status'] == 2){
                parent.ajaxLogin();
            }else{
                if(c_data['status'] == 1){
                    $('#addfav').text('收藏成功!');
                }else{
                    if(c_data['status'] == 0){
                        $('#addfav').text('您已经收藏过了!');
                    }
                }
            }
        });
    }
