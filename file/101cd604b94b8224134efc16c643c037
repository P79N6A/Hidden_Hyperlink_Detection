            var StudentNum = Res.StudentNum;
            document.title = EplatName + "_安全教育平台"
            $("#WebName").html(EplatName + "学校安全教育平台");
            var ShiYanQuName = EplatformID == 20104 ? "创建“国家级学校安全教育示范区”" : "中国教育学会安全教育实验区";
            $("#ShiYanQuName").html(ShiYanQuName);
            $("#WebJoinInfo").html("已有<span style='color:red;'>" + SchoolNum + "</span>所学校开展安全课程、<span style='color:red;'>" + TeacherNum + "</span>名教师参与授课、<span style='color:red;'>" + StudentNum + "</span>名学生参与学习");
            if (PrvID == 3) {
                $(".hebei").show();
                $(".header-rt p").css({ "height": "34px", "margin-top": "16px" });
            }

