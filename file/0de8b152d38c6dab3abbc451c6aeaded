   <script language="javascript" id="script_follow" reload="1">

function follow(uid,ins){
loading();
needlogin();
return false;
}

function delfollow(uid,ins){
loading();
$.ajax({
type:'GET',
url:'home.php?mod=spacecp&ac=follow&op=del&fuid='+uid+'&hash=cf3992ed&inajax=1&check=1',
dataType:'xml',
})
.success(function(s) {

if(s.lastChild.firstChild.nodeValue!='ok'){
popup.open(s.lastChild.firstChild.nodeValue,'html');
}else{
$('#'+ins).attr('href','javascript:follow(\''+uid+'\',\'follow_'+uid+'\')');
$('#'+ins).text('关注Ta');
$('#'+ins).attr('id','follow_'+uid);
popup.close();
}
})
.error(function() {
popup.open('Ajax请求失败', 'alert');
});
return false;

}
evalscripts.push('script_follow');
</script> 
