  <script type="text/javascript">
/* 
$(window).scroll(function(){
　　var scrollTop = $(this).scrollTop();
　　var scrollHeight = $(document).height();
　　var windowHeight = $(this).height();
　　if(scrollTop + windowHeight == scrollHeight){
　　　　$(".mian_menu ul li:last").hide();
　　}else{
		$(".mian_menu ul li:last").show();
	}
});
*/
$(window).resize(function() { 
	
	if(window.outerHeigth==screen.heigth && window.outerWidth==screen.width){ 
		$(".mian_menu div").show();
	}else{
		$(".mian_menu div").hide();
	}});


window.onload=function(){
    var d=document.getElementById("content_con_bg");
    if(screen.width<1240)  //获取屏幕的的宽度
    {
	  
      d.className='undis';
      
    }
    else{
     d.className='mian_menu';
    
    }
    }
</script> 
