 nextHref = $(".morelink").attr("ahref");
 

 $('.morelink').click(function(){

        // 判断下一页链接是否为空
        if( nextHref != undefined ) {
            // 显示正在加载模块
		 
            // Ajax 翻页
            $.ajax( {
                url: $(".morelink").attr("ahref"),
                type: "GET",
                success: function(data) {
                    result = $(data).find(".news_list");
                    nextHref = $(data).find(".morelink").attr("ahref");
                    $(".morelink").attr("ahref", nextHref);
                    $("#result").append(result);
				}
            });
          
       } 
	   else{
		   $('.morelink').text('已经最后一页了')
		   }

})

</script> 
