<script>
var patternSegments = {"fr":0,"ai":1,"hu":2,"pl":3,"ph":4,"bl":5,"bh":6,"ri":7,"tg":8,"to":9,"ll":10,"lh":11,"fl":12,"ad":13,"bt":14,"ha":15,"di":16,"dd":17,"pi":18,"hs":19,"zs":20,"or":21,"lo":22,"sw":23,"st":24,"at":25,"sc":26,"mp":27,"so":28};
function focusAction(Id){
var value = $("#"+Id).val();
if(value=="查找您关注的小区当前房价"||value=="请输入小区名或地址"){
$("#"+Id).val("");
}
$("#"+Id).css("color","#000");
}
function blurAction(Id){
var value = $("#"+Id).val();
if(value==""){
if(Id=="addressSec"){
$("#"+Id).val("请输入小区名或地址");
}else if(Id=="address"){
$("#"+Id).val("查找您关注的小区当前房价");
}
$("#"+Id).css("color","#999");
}
}
$(document).ready(function() {
$("#address").autocomplete("/xiaoqu/floorList.do",{
extraParams:{
pageno : "1",
floor:function(){return $("#address").val();}
},
scrollHeight:300,
parse:function(data){
var parsed = [];
var rows = data.items;
for (var i=0; i < rows.length; i++) {
var row = rows[i];
if (row) {
parsed[parsed.length] = {
value: row.floor,
data: row,
result: row.floor
};
}
}
return parsed;
},
width:258,
formatItem: function(row){
return "<table width=228><tr><td>"+row.floor+"</td><td width=100 align=right>(约<b>"+(row.saleCountHouse+row.saleCountVilla)+"</b>套)</td></tr></table>";
},
dataType : "json"
}).result(function(event, data, formatted){
$("#floorId").val(data.floorId);
$("#checkSearch").click();
});
$("#address").keydown(function(event){
var e = event || window.event;
if(e.keyCode == 13){
$("#checkSearch").click();
}
});
$("#checkSearch").click(function(){
var arrayObj = new Array();
var params = $.purl.params();
params = purlParams(params);
if($("#address").val()!='查找您关注的小区当前房价'){
params["ad"] = $("#address").val();
//地址 楼盘 等中文 编码处理
for(var i in params){
if(params[i]){
var number = patternSegments[i];
if(i=="ad"){
$.ajax(
{type : "post",
url : "/xiaoqu/selectFloorPrice.do",
cache : false,
async : false,
data : {"address":params[i]},
success : function(data){
if(data!="")
arrayObj[number] = data;
},
dataType : "html"}
);
}else{
delete params[name];
}
}
}
var seoUrl="";
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/"+arrayObj[i]+"_price";
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
if(seoUrl!=""){
if(seoUrl=='0_price'){
alert('不存在对应楼盘');
}else{
window.open(seoUrl+".html");
}
}
}else{
window.location.href = "/sale/";
}
});
});
function purlParams(params){
var url=$.purl.attr("path");
var re = /^\/lease\/search\/(.+)\.html/gi;
re.test(url);
var strings = RegExp.$1;
if(strings!=""){
var patternArray = strings.split("_");
for(var i=0; patternArray.length>i; i++){
var search = patternArray[i];
var key = search.substring(0,2);
var value = search.substring(2,search.length);
if(params[key]==null){
params[key]=value;
}
}
}
return params;
}
