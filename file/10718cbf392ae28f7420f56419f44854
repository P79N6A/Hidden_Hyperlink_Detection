<!-- 价格走势图 -->
<script type="text/javascript">
require.config({
paths:{
echarts: '/js/echarts'
}
});
<!-- 新房成交价格走势图 -->
function graph1(r_years1,timespan1) {
var loc = "ie_echart"+timespan1+"1"+r_years1;
if(timespan1=="1")
loc="ie_echart111";
require(
[
'echarts',
'echarts/chart/line'
],
function (ec) {
var name="";
if(timespan1=="1")
name="月-均价:元/平方米";
if(timespan1=="2")
name="周-均价:元/平方米";
if(timespan1=="3")
name="日-均价:元/平方米";
var myChart = ec.init(document.getElementById(loc));
myChart.clear();
var option={
tooltip : {
trigger: 'axis',
formatter:function(params){
if(params.length>0){
var str=params[0].name+'</br>';
for(var i in params){
str+=(params[i].value=="-"?"":params[i].seriesName+':'+(params[i].value=="-"?"无数据":params[i].value+'元/平米')+'</br>');
}
return str;
}
},
textStyle:{
align:'left'
}
},
legend: {
padding:0,
x : 300,
y : 0,
data:[]
},
grid:{
x:60,y:20,x2:10,y2:65
},
xAxis : [
{
type : 'category',
boundaryGap : false,
data : [],
splitLine:false,
axisLabel: {
interval:1
},axisTick:{
show:true,
interval:function(value){
return true;
}
}
}
],
yAxis : [
{
name : name,
min:0,
max:20000,
type : 'value',
axisLabel : {
formatter: '{value}'
}
}
],
series : []
};
$.ajax({
url:'/xiaoqu/charDataLine_newhouseNew.do',
type:'post',
dataType: 'json',
data:{"r_priceareaid":'4_1_3_6_',"type":1,"timespan":timespan1,"r_years":r_years1},
success:function(data){
option.yAxis[0].min=data[3].min;
option.yAxis[0].max=data[4].max;
option.legend.data=data[1].legend;
option.xAxis[0].data = data[2].xAxis;
option.series = data[0].series;
myChart.setOption(option);
}
});
}
);
}
<!-- 新房成交量走势图 -->
function graph2(r_years2,timespan2) {
var loc = "ie_echart"+timespan2+"2"+r_years2;
if(timespan2=="1")
loc="ie_echart121";
require(
[
'echarts',
'echarts/chart/line'
],
function (ec) {
var name="";
if(timespan2=="1")
name="月-数量:套";
if(timespan2=="2")
name="周-数量:套";
if(timespan2=="3")
name="日-数量:套";
var myChart = ec.init(document.getElementById(loc));
var option={
tooltip : {
trigger: 'axis',
formatter:function(params){
if(params.length>0){
var str=params[0].name+'</br>';
for(var i in params){
str+=(params[i].value=="-"?"":params[i].seriesName+':'+(params[i].value==""?"无数据":params[i].value+'套')+'</br>');
}
return str;
}
},
textStyle:{
align:'left'
}
},
legend: {
padding:0,
x : 300,
y : 0,
data:[]
},
grid:{
x:60,y:20,x2:10,y2:65
},
xAxis : [
{
type : 'category',
boundaryGap : false,
data : [],
splitLine:false,
axisLabel: {
interval:1
}
,axisTick:{
show:true,
interval:function(value){
return true;
}
}
}
],
yAxis : [
{
name : name,
min:0,
max:20000,
type : 'value',
axisLabel : {
formatter: '{value}'
}
}
],
series : []
};
$.ajax({
url:'/xiaoqu/charDataLine_newhouseNew.do',
type:'post',
dataType: 'json',
data:{"r_priceareaid":'4_1_3_6_',"type":2,"timespan":timespan2,"r_years":r_years2},
success:function(data){
option.yAxis[0].min=data[3].min;
option.yAxis[0].max=data[4].max;
option.legend.data=data[1].legend;
option.xAxis[0].data = data[2].xAxis;
option.series = data[0].series;
myChart.setOption(option);
}
});
}
);
}
<!-- 二手房挂牌价格走势图 -->
function graph3(r_years3,timespan3) {
var loc = "ie_echart"+timespan3+"3"+r_years3;
if(timespan3=="1")
loc="ie_echart131";
require(
[
'echarts',
'echarts/chart/line'
],
function (ec) {
var name="";
if(timespan3=="1")
name="月-均价:元/平方米";
if(timespan3=="2")
name="周-均价:元/平方米";
if(timespan3=="3")
name="日-均价:元/平方米";
var myChart = ec.init(document.getElementById(loc));
var option={
tooltip : {
trigger: 'axis',
formatter:function(params){
if(params.length>0){
var str=params[0].name+'</br>';
for(var i in params){
str+=(params[i].value=="-"?"":params[i].seriesName+':'+(params[i].value==""?"无数据":params[i].value+'元/平米')+'</br>');
}
return str;
}
},
textStyle:{
align:'left'
}
},
legend: {
padding:0,
x : 300,
y : 0,
data:[]
},
grid:{
x:60,y:20,x2:10,y2:65
},
xAxis : [
{
type : 'category',
boundaryGap : false,
data : [],
splitLine:false,
axisLabel: {
interval:1
},axisTick:{
show:true,
interval:function(value){
return true;
}
}
}
],
yAxis : [
{
name : name,
min:0,
max:20000,
type : 'value',
axisLabel : {
formatter: '{value}'
}
}
],
series : []
};
$.ajax({
url:'/xiaoqu/charDataLine_second_cityNew.do',
type:'post',
dataType: 'json',
data:{"r_priceareaid":'4_1_3_6_',"timespan":timespan3,"r_years":r_years3},
success:function(data){
option.yAxis[0].min=data[3].min;
option.yAxis[0].max=data[4].max;
option.legend.data=data[1].legend;
option.xAxis[0].data = data[2].xAxis;
option.series = data[0].series;
myChart.setOption(option);
}
});
}
);
}
graph3(1,1);
</script>
<script>
function trimblank(str){
return str.replace(/^\s+|\s+$/g, '');
}
$(function(){
$('#r_years1_tb').delegate('td','click',function(){
var r_years1 = $(this).text().substring(0,1);
graph1(r_years1,1);
});
$("#timespan1_ul").delegate('li','click',function(){
var timespan1;
if(trimblank($(this).text())=='月')
timespan1 = "1";
if(trimblank($(this).text())=='周')
timespan1 = "2";
if(trimblank($(this).text())=='日')
timespan1 = "3";
graph1(1,timespan1);
});
$('#r_years2_tb').delegate('td','click',function(){
var r_years2 = $(this).text().substring(0,1);
graph2(r_years2,1);
});
$("#timespan2_ul").delegate('li','click',function(){
var timespan2;
if(trimblank($(this).text())=='月')
timespan2 = "1";
if(trimblank($(this).text())=='周')
timespan2 = "2";
if(trimblank($(this).text())=='日')
timespan2 = "3";
graph2(1,timespan2);
});
$('#s3price').delegate('td','click',function(){
var r_years3 = $(this).text().substring(0,1);
graph3(r_years3,1);
});
$("#timespan3_ul").delegate('li','click',function(){
var timespan3;
if(trimblank($(this).text())=='月')
timespan3 = "1";
if(trimblank($(this).text())=='周')
timespan3 = "2";
if(trimblank($(this).text())=='日')
timespan3 = "3";
graph3(1,timespan3);
});
});
</script>
</body></html>