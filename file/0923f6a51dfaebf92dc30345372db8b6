})	
}
function hideWindow(str){
$('.dialogbox').hide();
$('#mask').hide();
}
function ajaxpost(url,obj){
var tid=obj.tid.value;
var	pid=obj.pid.value;
var	formhash=obj.formhash.value;

if(typeof(obj.score2) != "undefined") {
var	score2=obj.score2.value;
}else{
var	score2='';	
}

if(typeof(obj.score5) != "undefined") {
var	score5=obj.score5.value;
}else{
var	score5='';	
}

if(typeof(obj.score6) != "undefined") {
var	score6=obj.score6.value;
}else{
var	score6='';	
}	

var	reason=obj.reason.value;
var	referer=obj.referer.value;
if(typeof(obj.applay_tie) != "undefined") {
var	applay_tie=obj.applay_tie.value;
}else{
var	applay_tie=0;
}
jQuery.ajax({
type:'POST',
url:url,
data:{'formhash':formhash,'tid':tid,'pid':pid,'score2':score2,'score5':score5,'score6':score6,'reason':reason,'referer':referer,'inajax':1,'applay_tie':applay_tie},// 你的formid
dataType:'xml',
            async:false
}).success(function(s) {
var msg=s.lastChild.firstChild.nodeValue;
if(applay_tie=='on' && msg.indexOf('感谢您的参与')>0){
//$('#fastpostmessage').val(reason);
//$('#fastpostform').submit();
}else{			
popup.open(msg);			
}
popup.open(s.lastChild.firstChild.nodeValue);
evalscript(s.lastChild.firstChild.nodeValue);

});
return false;	
}
    
    function succeedhandle_(url, message) {
        popup.open(message, 'alert');
        setTimeout(function(){
            location.reload();
        }, 3000);
    }
</script>
