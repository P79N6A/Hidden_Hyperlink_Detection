    <script language="Javascript">
function dspagechange(num){
	for(var id = 1;id<=4;id++)
	{
		var MrPage="dspage"+id;
		var MrJin="dspage_con"+id;
		if(id==num){
		    //alert(id)
			//alert(MrPage)
			document.getElementById(MrPage).className="dActive";
			document.getElementById(MrJin).style.display="block";
		}		
		else{
			document.getElementById(MrPage).className="dUnActive";
			document.getElementById(MrJin).style.display="none";
		}
	}
}


