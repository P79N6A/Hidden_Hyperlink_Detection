          <script>
    function setCityData() {
        $.ajax({
            type: "get",
            url: "http://ass.vccoo.com/feed3_index.html",
            dataType: "jsonp",
            jsonp: 'callback',
            async: true,
            jsonpCallback: "success_jsonpCallback",
            success: function (json) {
                if (json) {
                    $("#news-first").html("");
                    var newJson = eval(json);
                    var htmlStr = "";
                    var shortcity = newJson.shortcity;
                    var newList = newJson.data;
                    if (newList != null && newList.length > 0) {
                        var model = newList[0];
                        var nickName = model.NickName;
                        if (nickName != null && nickName.Length > 10) {
                            nickName = nickName.Substring(0, 10) + "…";
                        }
                        htmlStr2 = '<div class="news-first-img"><a href="' + model.MsgUrl + '" target="_blank" title="' + model.MsgTitle + '">' +
                         '<img class="news-pic" src="' + model.ThumbUrl + '" alt="' + model.MsgTitle + '" style="opacity: 1;"><span></span></a></div>';
                        $("#news-first").append(htmlStr2);
                        htmlStr1 = '<div class="news-first-content">' +
                         '<div class="news-first-title"><a href="' + model.MsgUrl + '" target="_blank" title="' + model.MsgTitle + '"><h3>' + model.MsgTitle + '</h3></a></div>' +
                         '<div class="news-first-name"><a href="' + model.PubUrl + '" target="_blank" title="' + model.NickName + '">' + nickName + '</a>' +
                         '<a id="local" class="local" title="' + shortcity + '"><i class="fa-local"></i>' + shortcity + '</a></div></div>';
                        $("#news-first").append(htmlStr1);

                    }
                }

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            }
        });
    }


</script> 
