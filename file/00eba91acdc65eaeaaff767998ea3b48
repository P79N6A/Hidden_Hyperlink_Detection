  <!-- QQ收回滑出效果 --> 
  <script>
$(function () {
	$('.qq_info_open h4 img').bind('click', function () {
		$('.qq_info_open').animate({'right':'-141'},500);
		$('.qq_info_close').show();
	})
	$('.qq_info_close').bind('mouseenter', function () {
		$('.qq_info_open').animate({'right':'1'},500);
		$(this).hide('slow');
	})
})
</script> 
