<script type="text/javascript">
$(function(){
    var related_read=$("#related_read");
    var related_right=$("#related_right");
    var related_zt=$("#related_zt");
    var related_ft=$("#related_ft");
    if( !related_zt.has("strong").length && !related_ft.has("li").length){
        related_right.hide();
        related_read.addClass("w_auto");
    }else if(!related_zt.has("strong").length){
        $("#related_right .artL_block_title:eq(0)").hide();
        related_zt.hide();
    }else if(!related_ft.has("li").length){
        $("#related_right .artL_block_title:eq(1)").hide();
        related_ft.hide();
    }
});
</script>
