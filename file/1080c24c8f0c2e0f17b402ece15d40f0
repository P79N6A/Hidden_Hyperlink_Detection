  <script type="text/javascript">
j.include("Javascript/calendar/Date.js","Javascript/calendar/calendar.js","Javascript/calendar/citys.js","Javascript/calendar/city_menu.js");
j.includecss("Javascript/calendar/city_menu.css");
$(function(){
if(!$("#tm1").val())$("#tm1").val(ZNDate.format(ZNDate.plus(ZNDate.today(),3)));
if(!$("#tm2").val())$("#tm2").val(ZNDate.format(ZNDate.plus(ZNDate.today(),6)));
//alert(ZNDate.getDateTip(ZNDate.format(ZNDate.plus(ZNDate.today(),3))));
tooltip();
});
$("#list_filter dl dd a[class!='more'][class!='more1']").click(function(){$(this).loading({div:"#mainContent"})});
$("#list_filter dl dd span a").click(function(){
	var s=($(this).attr("class")=="more1")?"more":"more1";
	if(s=="more1"){
	$(this).parent().parent().attr("style","height:auto");}else{$(this).parent().parent().attr("style","height:25px");}
	$(this).attr("class",s);
});
function tooltip(){
$(".list_con div.list_box div.lb_pic dl dt img").each(function(){$(this).simpletooltip({html:'<img src="' + $(this).attr("src").replace(/160x120_/i,'500x375_') + '" width="318" height="238"/>',br:false});});
}
$("input[name='mapon']").click(function(){
	if(($("#divmap").css('display')=="none")&&($("input[name='mapon']").attr("checked")==true)){
		$("#divmap").css({"display":""});
		$("#ifrMapURL").attr("src","Hotel_map.asp?hotels=20072,20073,26572,&h=340&w=720&juli=0.18,0.32,0.34,0.35,0.44,0.45,0.46,0.48,0.52,0.54,&cityid=396");
	}else{
	    $("#divmap").attr("style","display:none");
	}});
function map(n){
	if((($("input[name='mapon']:checked").length==0)||($("#divmap").css('display')=="none"))){
		$("input[name='mapon']").attr("checked",true);
		$("#divmap").css({"display":""});
		$("#ifrMapURL").attr("src","Hotel_map.asp?hotels=20072,20073,26572,&h=340&w=720&juli=0.18,0.32,0.34,0.35,0.44,0.45,0.46,0.48,0.52,0.54,&cityid=396");
	}
	if($.browser.msie){
		if(typeof(window.frames.ifrMapURL.document.body.onload)!="function"){setTimeout("map("+n+")",500)}else{window.frames["ifrMapURL"].openMInfo(n)};
	}else{
		if(typeof(window.frames.ifrMapURL.onload)!="function"){setTimeout("map("+n+")",500)}else{window.frames.ifrMapURL.openMInfo(n)};
	}
}
function rollhotel(n){
	setColor(".list_con div.list_box.div"+n+"",5,"#CDEDF8",160);
	if($(".list_con div.list_box").length<2)return;
	$(".list_con div.list_box").each(function(){$(this).removeAttr("style")});
	var s=$(".list_con div.list_box.div"+n+"");
	s.remove();
	s.insertBefore(".list_con div.list_box:eq(0)");
	$('html').animate({scrollTop:$("#divmap").offset().top}, 'fast');
    $('body').animate({scrollTop:$("#divmap").offset().top}, 'fast');
	tooltip();
}

