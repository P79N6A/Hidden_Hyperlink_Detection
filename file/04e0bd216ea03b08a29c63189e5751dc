   <script type="text/javascript">
 $(function(){
    $('.sort_selector').change(function(){
        var ref_link = $(this.options[this.selectedIndex]).attr('ref_link');
        window.location.href = ref_link ;
    });
    $('.filter_more').click(function(){
       var ref = $(this).attr('ref');
       var bg = $('#filter_' + ref).find('div').css('background-color');
       if (bg != 'rgb(255, 255, 255)' && bg != '#ffffff'){
          $('#filter_' + ref).removeClass('suo');
          $('#filter_' + ref).find('div').css('background-color', '#ffffff');
          lvse.cookie('filter_' + ref + '_open',1);
       }else{
          $('#filter_' + ref).addClass('suo');
          $('#filter_' + ref).find('div').css('background-color', '#f3ffee'); 
          lvse.cookie('filter_' + ref + '_open',0);      
       }
    });
    if (lvse.cookie('filter_area_open') == 1){
       $('.filter_more[ref=area]').click();
    }
    if (lvse.cookie('filter_category_open') == 1){
       $('.filter_more[ref=category]').click();
    }    
 }); 
</script> 
