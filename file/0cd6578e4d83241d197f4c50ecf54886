  <script language="javascript">
// SlideShow
$(function(){
	 var index = 0;
	 $("#operate span:first").addClass("hov");
	 $("#operate span").mouseover(function(){
		index  =  $("#operate span").index(this);
		showImg(index);
	});	
	 $('#slideshow').hover(function(){
			  if(MyTime){
				 clearInterval(MyTime);
			  }
	 },function(){
			  MyTime = setInterval(function(){
			    showImg(index)
				index++;
				if(index==6){index=0;}
			  } , 3000);
	 });
	 var MyTime = setInterval(function(){
		showImg(index)
		index++;
		if(index==6){index=0;}
	 } , 3000);
})
function showImg(i){
	$("#showimg img")
		.eq(i).stop(true,true).fadeIn(1000)
		.parent().siblings().find("img").hide();
	$("#msg li")
		.eq(i).stop(true,true).fadeIn(1000)
		.siblings().hide();
	$("#operate span")
		.eq(i).addClass("hov")
		.siblings().removeClass("hov");
}
//Sidebar-Fixed
$(function(){
	//var contntDivHeight = $(".content").height();
	var navH = $("#text-30").offset().top;
	//if(contntDivHeight>=1000){
		$(window).scroll(function(){
			var scroH = $(this).scrollTop();
			if(scroH>=navH){
				$("#text-30").addClass("fixdiv");
			}else if(scroH<navH){
				$("#text-30").removeClass("fixdiv");
			}
		})
	//}
})
</script>
