  <script>
$(function(){
    var codeType="single";
    $('#ul_hot_merchants').children("li").hover(function () {
            $(this).find(".rebate").stop().animate({bottom: '0'}, {queue: false, duration: 160});
            $(this).find(".num").stop().animate({width: '60px'}, {queue: false, duration: 160});
            $(this).find(".num-content").show();
        }, function () {
            $(this).find(".rebate").stop().animate({bottom: '-21px'}, {queue: false, duration: 160});
            $(this).find(".num").stop().animate({width: '10px'}, {queue: false, duration: 160});
        });
       if(codeType=="multiplex") {
           getCouponCode(24956,false);
       }
});
    function getCouponCode(couponId,getCouponCode){
        $.post(getUrl("Restful/getCouponCodeByEmail"), {
            couponId: couponId,
            getCouponCode:getCouponCode
        }, function (json) {
            var obj = jQuery.parseJSON(json);
            var pagefn = doT.template(document.getElementById('userCoupon').text);
           if(getCouponCode==true && obj.reCode == "nologin") {
                window.location="/login";
            } else {
                $("#userCouponContainer").html(pagefn(obj));
            }

            //复制
            if ($(".copycoupon").length > 0 && flashChecker() == 1) {
                var copySuccess = $('#footer').attr('data-copy-success');
                var copyClick = $("#footer").attr("data-copy-hide");
                $(".copycoupon").hover(function() {
                    //复制
                    if ($(this).attr("is_hover") != 1) {
                        $(this).attr("is_hover", 1);
                        $(this).zclip({
                            path: "http://pics.rebatesme.com/newrm/js/copy/ZeroClipboard.swf",
                            copy: function() {
                                return $(this).attr("data-code");
                            },
                            afterCopy: function() {
                                $(this).next(".copy-tip").text(copySuccess);
                            }
                        });
                    }
                    var width = parseInt($(this).width()) + 20;
                    var left = parseInt($(this).offset().left);
                    var top = parseInt($(this).offset().top);
                    $(this).next(".copy-tip").show();
                }, function() {
                    $(".copy-tip").hide().text(copyClick);
                })
            }

        });
    }

</script>
