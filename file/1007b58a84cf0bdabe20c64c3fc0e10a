function autoshow() {
	if(autoflg==false) {
			 clearTimeout(t)
			 document.getElementById("divqqstart").click();
			 autoflg=true;
	}
}

 

function OpenDiv(){
    if(document.getElementById('v').style.display=='none'){
         document.getElementById('v').style.display='';
		 setTimeout('OpenDiv()',10000);
    }
}

