            0[(getElementsByTagName('head')[0] || body).appendChild(createElement('script')).src = 'http://bdimg.share.baidu.com/static/api/js/share.js?v=89860593.js?cdnversion=' + ~(-new Date() / 36e5)];</script> 
  <script>
$(function(){
    $('#praise').click(function(){
        var book_id=$('#book_id').val();
        var dianzannum=$('#dianzannum').html();
        $.post("/Comment/praise.html",{books_id:book_id},function(result){
                    if(result==1){
                        alert('点赞成功，感谢您的支持！');
                        $('#dianzannum').html(dianzannum*1+1);
                    }else if(result==2){
                        alert('您未登录，请登录后点赞！');
                    }else if(result==3){
                        alert('您已经赞过了！');
                    }else if(result==4){
                        alert('点赞失败');
                    }
                });
    })
    $("#comment_submit").click(function(){
        var book_id=$('#book_id').val();
        var content=$('#content').val();
        var level=$('.star .star_lg').length;
        $.post("/Comment/comment.html",{books_id:book_id,content:content,level:level},function(result){
                    if(result==1){
                        alert('评论发布成功！');
                         location.reload();
                    }else if(result==2){
                        alert('您未登录，请登录后点赞！');
                    }else if(result==3){
                        alert('您已经评论过了！');
                    }else if(result==4){
                        alert('评论失败，请重试！');
                    }else if(result==5){
                        alert("评论内容不能为空！");
                    }
                });
    })
})
    </script> 
