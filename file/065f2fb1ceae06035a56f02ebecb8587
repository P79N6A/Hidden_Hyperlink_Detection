  <script>
        $(function(){
            $(".zhidaole").click(function(){
                $('.white_content').hide();
                $('.black_overlay').hide();
                $.post("/Register/changeSession.html",{num:"num2"});
                })
        })
    </script> 
  <script>window._bd_share_config = {"common": {"bdSnsKey": {}, "bdText": "", "bdMini": "2", "bdMiniList": false, "bdPic": "", "bdStyle": "0", "bdSize": "24"}, "share": {}};
