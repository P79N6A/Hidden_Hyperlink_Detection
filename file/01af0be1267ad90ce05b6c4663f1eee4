  <script type="text/javascript">
var qr_coder = null;
$(document).ready(function(){
	qr_coder = new QRCoder($('#qr_container'));
});
function generateQr(url){
	$('#qr_container').html("generating");
	qr_coder.setMode(0);
	qr_coder.draw(url, 'H', 'http://www.newgoal.cn/public/images/fav32_32.ico',null);
	$('#qr-content-modal').modal('toggle');
}
function generateQues(url, title){
	url = 'http://www.newgoal.cn'+url;
	$('#qr_container').html("generating");
	qr_coder.setMode(0);
	qr_coder.draw(url, 'H', 'http://www.newgoal.cn/public/images/fav32_32.ico',null);
	
	url = encodeURIComponent(url);
	sinatitle = encodeURI(title+"（分享自＠牛果网教育问答社区 ）");
	title = encodeURI(title);
	$("#sina_generate").attr("href", "http://v.t.sina.com.cn/share/share.php?appkey=308458378&searchPic=true&style=number&url="+url+"&title="+sinatitle);
	$("#qq_generate").attr("href", "http://connect.qq.com/widget/shareqq/index.html?url="+url+"&desc="+title);
	$("#qzone_generate").attr("href", "http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url="+url+"&title="+title);

	$('#qr-content-modal').modal('toggle');
}
</script> 
