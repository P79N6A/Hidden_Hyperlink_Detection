  <script type="text/javascript">
$(function(){
	$("#accusationsDesc").focus(function(){
		$("#accusationsDescError").hide();
	});
	
	$("#accusationsDesc").blur(function(){
		var content = $("#accusationsDesc").val();
		if(content.length < 1){
			$("#accusationsDescError").show();
		}
	});
});
function submitAccusation(){
	var content = $("#accusationsDesc").val();
	if(content.length < 1){
		$("#accusationsDescError").show();
		return false;
	}
	$("#_accusation_form").submit();
	return true;
}
function accusation(type, id){
	$("#accusationType").val(type);
	$("#accusationId").val(id);
	$("#accusation-modal").modal('toggle');
}
</script> 
