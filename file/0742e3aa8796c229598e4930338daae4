  <script type="text/javascript">
$(document).ready(function() {
	var id = '4051';
    
	$('img[name=FavoriteSB]').click(function(){
		$.post('favorite.htm',{fid:this.id}, function(data){
			alert(data);
		});
	});
	
	//提交
	$('#submit').click(function(){
		var re		= /^[a-z0-9\._\-]+@[a-z0-9\-\.]+\.[a-z]{2,4}$/i;
		var email	= $('#email').val();
		var level	= $('input[name=level]:checked').val();
		var data	= $('#data').val();
		var code	= $('#code').val();
		if(email==''){
			alert('请输入电子邮件');
			$('#email').focus();
			return false;
		}
		if(!re.test(email)){
			alert('电子邮件格式不正确');
			$('#email').focus();
			return false;
		}
		if(data==''){
			alert('请填写评论内容');
			$('#data').focus();
			return false;
		}
		var re	= /'|"|#|<|>/;
		if(re.test(data)){
			alert('评论中不能包含有特殊字符');
			$('#data').focus();
			return false;
		}
		if(code.length!=6){
			alert('验证码长度为6位，请重新输入');
			$('#code').focus();
			return false;
		}
		$.post('comment.htm', {submit:true,email:email,level:level,data:data,code:code,id:id},function(data){
			var data = eval("("+data+")");
			alert(data.data);
			if(!data.error){
				$('#data').val('');
				$('#code').val('');
				location.reload();
			}
		});
	});
	
	//验证码
	$('#verification').click(function(){
		this.src="verification/code.php?"+Math.random();
	});
});
</script> 
