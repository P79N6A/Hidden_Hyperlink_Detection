if(getcookie("urltarget")==2)
    $('target').checked=false;
else if(getcookie("urltarget")==1)
    $('target').checked=true;
else
    $('target').checked=true;
urltarget();
function urltarget(){
    var alllink = $('mainbox').getElementsByTagName('a');
    if($('target').checked===false){
        for(var i=0;i<alllink.length;i++)
        {
            alllink[i].target = "_self";
        }
        setcookie('urltarget', '2', (24*3+8)*3600*1000,'','.55bbs.com');
        $('target').checked=false;
    }else{
        for(var i=0;i<alllink.length;i++)
        {
            alllink[i].target = "_blank";
        }
        setcookie('urltarget', '1', (24*3+8)*3600*1000,'','.55bbs.com');
        $('target').checked=true;
    }
}
</script> 
    <script type="text/javascript">
//滚动通用函数 (b：滚动容器，e：滚动元素，t：容器内显示的行数，h：滚动的距离)
function scrollTop(b,e,t,h) {
   var b = jQuery(b);           //滚动容器
   var c = 0;
   var d = b.find(e);      //滚动元素
   var n = (d.length)-t;   //最大值   t为滚动容器内显示的行数
   var speed = 500;
   var timer = false;
   function slide(j) {
       if (b.is(':animated') == false) {
           c += j;
           if(c != -1 && c != n){
               b.animate({'marginTop': -c * h + 'px'},speed)
           }
           else if(c == n){
               c = 0;
               b.css({"marginTop": -h + "px"});
               b.animate({"marginTop": 0 + "px"},speed)
           }
       }
   }
   //运行
   function start() {
       timer = setInterval(function(){slide(1)},5000);
   }
   //暂停
   function cease() {
       if(timer){
           clearInterval(timer);
           }
   }
   d.hover(
       function() {
           cease();
       },
       function() {
           start();
   });
   start();
}
scrollTop('#cashbackScroll','li','5','51');
function showTao(){
    document.getElementById("pacity-box").style.display='block';
    document.getElementById("opacity-direct").style.display='block';
}
</script> 
