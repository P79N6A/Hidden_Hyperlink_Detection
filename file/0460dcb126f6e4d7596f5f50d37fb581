   <!-- 畅游互联网 --> 
   <script type="text/javascript">
	//本页面的pvuv统计(统计页面pv、uv)
	$(document).ready(
		function(){	
		try{
		   	jQuery.ajax({
		     	type: "GET",
		       	url: "/cgi-bin/counting?rid=415337&rtype=con",
		     	crossDomain : true
			});
		}catch(err){}
	});

 
	//本页面内所有点击类操作触发的统计（例如广告和活动）    rid 内容id,rtype 栏目id 异步操作(统计本页面广告活动等点击量)
	function setPvUv(rid,rtype,adUrl){
		 url = "/cgi-bin/counting?rid="+rid+"&rtype="+rtype;
		 _p_PvUv = url;
		 if(adUrl!=null&&adUrl!=""){
			 window.location.href=adUrl;
		 }
	}
 
	//页面的下载按钮的pvuv方法(统计下载量,应用游戏)
	var _p_PvUv ="";
	function setPvUvApp(rid,rtype,adUrl,imgUrl,type){
		var url = "/cgi-bin/counting?rid="+rid+"&rtype="+rtype;
		 _p_PvUv = url;
		 var goldtype="con_game";
		 var goldMoney=0;
		 if(adUrl!=null&&adUrl!="" && imgUrl!=null && imgUrl!="" && type!=null && type!=""){
			// var clientAppUrl = adUrl+"?img="+imgUrl+"&type="+type;
			 var clientAppUrl = adUrl;
			 window.location.href=clientAppUrl;
		 }
		//调用andriod方法
		 window.demo.addgold(goldtype,rid,goldMoney);
	}

	//ios和安卓
	var _p_isMobile = false ;
	function isMobileUtil(){
		
		var ua = navigator.userAgent;
		var ipad = ua.match(/(iPad).*OS\s([\d_]+)/),
			isIphone = !ipad && ua.match(/(iPhone\sOS)\s([\d_]+)/),
			isAndroid = ua.match(/(Android)\s+([\d.]+)/),
			isMobile = isIphone || isAndroid;
		   _p_isMobile =  isMobile;
	
	}
	//下载的pvuv
	$(document).ready(function(){
		isMobileUtil();
	  if(_p_isMobile){
		 $(".down,#slide,.pvuvdownload,.tiao,.banner,#slider,#ad,.clickPvuv,.apkDown").click(function(){
			 $("#pvUvIframe").attr("src",_p_PvUv);
		});
	  }
	});		
	//下载传 金币,addgold("con",rid,goldMoney),第一个参数必须为con
	function setPvUvStory(rid,rtype,adUrl,goldMoney){
		url = "/cgi-bin/counting?rid="+rid+"&rtype="+rtype;
		_p_PvUv = url;
		var type="con_newStory";
		 if(adUrl!=null&&adUrl!=""){
			 window.location.href=adUrl;
		 }
		//调用andriod方法
		 window.demo.addgold(type,rid,goldMoney);
	}
	//新闻栏目页点击加金币
	function setNewsGold(rid,adUrl,goldMoney){
		var type="con_news";
		 if(adUrl!=null&&adUrl!=""){
			 window.location.href=adUrl;
		 }
		//调用andriod方法
		window.demo.addgold(type,rid,goldMoney);
	}
</script> 
