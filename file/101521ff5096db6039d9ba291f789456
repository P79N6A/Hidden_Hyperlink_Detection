  <script>
		/*
		var type = document.querySelectorAll('.position a')[1].innerText;
		document.querySelector('.type span').innerText = type;
		*/
		var webName = window.location.href
		var index = webName.indexOf('.com')
		webName = webName.substr(0, index + 4)
		var col = document.getElementById('col').innerText
		var list = document.getElementById('list').innerText
		var pageInfo = document.getElementById('pageInfo').innerText
		var start = pageInfo.indexOf('共 ')
		var end = pageInfo.indexOf(' 页 ')
		var max = pageInfo.substring(start + 2, end)
		document.getElementById('href').onclick = function (e) {
			e.preventDefault()
			var num = document.getElementById('pageNum').value
			if (num < 1) {
				alert('请输入页数')
			} else if ( num > parseInt(max)) {
				alert('已超过最大页数')
			} else {
				var href = webName + col + list + '_' + num + '.html'
				window.location.href = href
			}
		}
	</script>   
