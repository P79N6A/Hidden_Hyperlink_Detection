    <!--个人页面登陆注册弹出层end--> 
    <script type="text/javascript">
//Job placement service移入移出效果
$(function(){
	//创建简历
	$('.creat_resume').hover(function(){
		$('.creat_resume_con').show();
	},function(){
		$('.creat_resume_con').hide();
	});
	//求职服务
	$('.jobs_service').hover(function(){
		$('.jobs_service_con1 ').show();
	},function(){
		$('.jobs_service_con1 ').hide();
	});
	//简历更新
	$('li.update_resume').hover(function(){
		$('.update_resume_con').show();
	},function(){
		$('.update_resume_con').hide();
	});
	//职位订阅
	$('.job_alert').hover(function(){
		$('.job_alert_con').show();
	},function(){
		$('.job_alert_con').hide();
	});
	//职场秀
	$('.zhichangxiu').hover(function(){
		$('.zhichangxiu_con').show();
	},function(){
		$('.zhichangxiu_con').hide();
	});
});
    //页面加载完毕自动更细简历
	$(function(){
		auto_refresh_resume();
	 });
	
    //页面加载完毕判断
	 $(function(){
		setTimeout(is_refresh_resume,10000);
	 });

	//判断是否要显示更新
	function is_refresh_resume()
	{
		$.ajax({
		    type:"GET",
			async: false,
		    url:"/job/resume/is_refresh_resume",
		    data:null,
		    success:function(re){
				switch (re)
				{
					case '0'://没登录
						break;
					case '1'://无简历但今天已经提示过了
						break;
					case '2':////无简历且今天还没提示
						resume_timeout(0);
						break;
					case '3'://7天内更新过
						break;
					case '4'://7天内没更新过但今天已经提示过了
						break;
					case '5'://7天内没更新过且今天还没提示
						resume_timeout(1);
						break;
				}
		     } 
		   });
	}

	//定时任务
	function resume_timeout(tips)
	{//tips为0表示创建，为1表示更新
		if(tips == 0)
		{
			create_tip();
		}
		else
		{
			refresh_tip();
		}
		$.ajax({
			   type:"GET",
			   url:'/job/resume/set_resume_cookie',
			   async: false,
			   data:null,
			   success:function(re){
					
				}
		});
	}

	//简历创建提示
	function create_tip()
	{
		close_tip();
		$('#create_resume_tip').css('display','block');
		setTimeout(close_tip,30000);
	}

	//简历更新提示
	function refresh_tip()
	{
		close_tip();
		$('#update_resume_tip').css('display','block');
		setTimeout(close_tip,30000);
	}
	
	//关闭提示层
	function close_tip(flag)
	{
		if(flag == 0)
		{
			$('#update_resume_tip').css('display','none');
			$('#create_resume_tip').css('display','none');
		}
		else
		{
			$('.update_resume_tip').css('display','none');
		}
	}

	//ajax更新简历并增加积分
	function refresh_resume_points(uid)
	{
		$.ajax({
		    type:"GET",
			async: false,
		    url:"/job/resume/refresh_resume_points/"+uid,
		    data:null,
		    success:function(re){
				if(re != 0)
				{
					close_tip(1);
					$('#refresh_tips').css('display','block');
				}
		     } 
		   });
	}

	//简历更新
	function refresh_resume(uid)
	{
		$.ajax({
		    type:"GET",
			async: false,
		    url:"/job/resume/refresh_resume/"+uid,
		    data:null,
		    success:function(re){
				if(re == 1)
				{
					easyDialog.open({
					  container : 'refresh_resume'
					 });
				}
				else
				{
					alert("未知错误！");
				}
		     } 
		   });
	}

	//关闭弹出层
    $('#refresh_resume .close').click(function(){
		easyDialog.close();
		window.location.reload();
    });

	//自动更新简历并加积分
	function auto_refresh_resume()
	{
		$.ajax({
		    type:"GET",
			async: false,
		    url:"/job/resume/auto_refresh_resume",
		    data:null,
		    success:function(re){
				//alert(re);
		     } 
		   });
	}
</script>
