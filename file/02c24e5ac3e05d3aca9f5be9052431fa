   <script type="text/javascript">
$(function(){
   $('.City_pulse').hover(function(){
      $('.CityPulse').show();
     $('.City_in').css('background','#ecf1f8').css('font-weight','bold').css('color','#dd0009').css('border','1px solid #ccc').css('border-right','none');
      
   },function(){
       $('.CityPulse').hide();
     $('.City_in').css('background','none').css('font-weight','normal').css('color','#000').css('border','none');
   });

    $('#AroundChina').hover(function(){
      $('.Pading_btn').css('background','#fff').css('font-weight','bold').css('color','#dd0009');
      
   },function(){
     $('.Pading_btn').css('background','none').css('font-weight','normal').css('color','#000');
   });
  //导航修改
   $('.Menu_two_L li').hover(function(){
      $(this).find('ul').show();
   },function(){
       $(this).find('ul').hide();
   });
   
   $('.Menu_L li').hover(function(){
      $(this).find('ul').show();
   },function(){
       $(this).find('ul').hide();
   });

});
   function showbuttons(i) {
    $("#button"+i).show();
  }
  function hidebuttons(i) {
    $("#button"+i).hide();
  }
  
$(function(){
   $('.loginSpan').hover(function(){
      $(this).find('ul').show();
    $(this).prev('span').css('border-right','none')
   },function(){
     $(this).find('ul').hide();
     $(this).prev('span').css('border-right','1px solid #ccc')
   });
//more city
$('#city dt span').click(function(){$('#city dd').html($(this).html())});
var current=$('.jobs_more_city .currenttab li'),index,tab=$('.moreCity_tab'),select_city=$('.select_city'),sel=$('.moreCity_hd dd'),sel_box=$('.moreCity_hd dt'),lis=$('.moreCity_hd dt span');
tab.not(':first').hide();



current.bind('click',function(){
  
   index=$(this).index();  
   $(this).addClass('current').siblings().removeClass('current');
   li = $(this).parents('.moreCoty_box').find('.moreCity_tab');
   li.hide();
  li.eq(index).show()
 

}); 
 

$('.More_city').hover(function(){$(this).find('ul').show();});
$('.More_city .more_city_a').click(function(){
   if($(this).siblings('.jobs_more_city').is(':visible'))
        $(this).siblings('.jobs_more_city').hide();
     else
     $(this).siblings('.jobs_more_city').show();
    return false;
});



lis.click(function(){
   $(this).parent().siblings(sel).html($(this).html())
}); 

 $('#pro dt span').click(function(){
   var index1=$(this).index();
   $('#city').show();
   $('#city dd').html('请选择');
     $('#city dt div').eq(index1).show().siblings().hide();
 });  
   
 $('.jobs_more_city .colsed').live('click',function(){
  $(this).parent('.jobs_more_city').hide();
 });
   
//增加积分入口
 $('.blog_points').hover(function(){
   $(this).children('.app_inner').show();
 },function(){
   $(this).children('.app_inner').hide();
 });
 
 //快通需求 ：首页快捷发布栏改进
  $('.nocurpt1225').hover(function(){
   $(this).css('color','#000');
 },function(){
   $(this).css('color','#fff')
 });
   
});
