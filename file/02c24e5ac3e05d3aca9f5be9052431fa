     <div class="Bloder space_login"> 
      <h3>Login to User Center</h3> 
      <p class="username"><input id="username" type="text" value="Username or email" /></p> 
      <p class="Psd_sign"><span class="userpsd"><input type="password" id="passwd" /></span><a class="Sign">Sign in</a></p> 
      <p><input type="checkbox" id="rem" /> Remember me<a class="Password" href="http://www.comments.obs.echinacities.com/forget_pwd">Password?</a><br /> <span class="warn_text"></span> </p> 
      <p class="Sign_Up"><strong>New to eChinacities?</strong> Sign up</p> 
      <div class="Register"> 
       <em>Register today to write blogs,ask questions,find jobs and more...</em> 
       <a href="http://www.comments.obs.echinacities.com/register">Register</a> 
       <div class="clear"></div> 
      </div> 
     </div> 
     <script type="text/javascript">
    var username = $('#username'),
      passwd = $('#passwd'),
      warn_text = $('.warn_text');
    username.focus(function(){
      if('Username or email' == this.value)
      {
        this.value='';
      }
      warn_text.html('')
    });
    passwd.keypress(function(e){
      warn_text.html('')
      if(13 == e.keyCode)
      {
        login();
        return false;
      }
    })
    function login()
    {
      var u = username.val(), p = passwd.val(), rem = $('#rem').attr('checked') ? 1 : 0;
      if(u && p)
      {
        $.post('/community/login', {username:u, pw:p, rem:rem}, function(r){
          if(1 == r)
          {
            location.reload();
          }
          else if(2 == r)
          {
            location = 'http://www.' + WEB_DOMAIN + '/site/unactive';
          }else if(3 == r)
          {
            warn_text.html('Your account has been blocked due to posts containing spam, advertising or inappropriate content.');
          }
          else
          {
            warn_text.html('Sorry, invalid username or password. Please try again.');
          }
        });
      }
      else
      {
        warn_text.html('Please fill out the required fields');
      }
    }
    $('.Sign').click(login);
  </script> 
