    <script>
  function show_content(id){
	  url='/23/notice_content';
	  $.getJSON(url,{id:id},function(data){
	    if(data){
	        $("#title_content").html(data.title_en)
	        $("#textarea").html(data.content_en)
	         $('#notice_content').fadeIn('10000')
	    }
	  })
	}

	 function close_notice(){
	  $('#notice_content').fadeOut('10000')
	 }
	$('.bind_close').bind('click',function(){
		$(this).parents('.scroll').fadeOut('10000');
	 })

	$.fn.extend({'scrollTop':function(){
		$(this).each(function(i,item){
				item.timer=null;
				item.i=0;
				item.len=$(item).find('.scroll_con').length;
			item.timer=setInterval(function(){
				item.i+=1;
				if(item.i >= item.len){
					item.i=0;
				}
				$(item).find('.scroll_con').hide()
				$(item).find('.scroll_con:eq('+item.i+')').show()
			},3000);
		})
	}})
	 $('.scroll:eq(0)').scrollTop();
	 setTimeout(function(){
		 $('.scroll:eq(1)').scrollTop();
	},1000)
  </script>
