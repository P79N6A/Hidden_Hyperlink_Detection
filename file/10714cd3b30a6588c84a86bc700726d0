            $(".choose-province").on("click", function (e) {
                var $province_name = $(this).html();
                $.each(cities[$province_name],function(i, city_info){
                    $('<a class="choose-position" href="javascript:" abbr="'+city_info.abbr+'" online="'+city_info.online+'">'+city_info.city+'</a>').appendTo('.cities-area').on('click', function (e) {
                       $positionCity = $(this).attr('abbr');
                        log.push(['_trackEvent', 'pc_offline_change_position', 'click', 'common']);
                        if($(this).attr('online') == 1){
                            app.redirect('/'+ $positionCity + '/ershouche');
                        }
                        $(".nc").html($(this).html());
                        $provinces.addClass('selected');
                        $province.addClass('selected');
                        $cities.removeClass('selected');
                        $city.removeClass('selected');
                        $cities.html('');
                        $('.change-position').hide();
                        e.stopPropagation();
                    });
                });

                $provinces.removeClass('selected');
                $province.removeClass('selected');
                $cities.addClass('selected');
                $city.addClass('selected');
                e.stopPropagation();
            });

            $province.on("click", function (e) {
                $provinces.addClass('selected');
                $province.addClass('selected');
                $cities.removeClass('selected');
                $city.removeClass('selected');
                $cities.html('');
                e.stopPropagation();
            });

        });
    });

</script> <div id="tip-wrapper" class="tip-wrapper "> <div class="tip-title">
