            $(".tip-choose-province").on("click", function (e) {
                var $province_name = $(this).html();
                $.each(cities[$province_name],function(i, city_info){
                    $('<a class="tip-choose-position" href="javascript:" abbr="'+city_info.abbr+'">'+city_info.city+'</a>').appendTo('.tip-cities-area').on('click', function (e) {
                        e.stopPropagation();
                        $positionCity = $(this).attr('abbr');
                        log.push(['_trackEvent', 'pc_tip_change_position', 'click', 'nc-tip']);
                        app.redirect('/'+ $positionCity + '/ershouche');
                    });
                });

                $provinces.removeClass('tip-selected');
                $province.removeClass('tip-selected');
                $cities.addClass('tip-selected');
                $city.addClass('tip-selected');
                e.stopPropagation();
            });

            $province.on("click", function (e) {
                $provinces.addClass('tip-selected');
                $province.addClass('tip-selected');
                $cities.removeClass('tip-selected');
                $city.removeClass('tip-selected');
                $cities.html('');
                e.stopPropagation();
            });
        });
    });


</script> <div class="container common-breadcrumb-wrapper" rrc-event-scope="navigator2"> <ul class="row-fluid breadcrumb"> <li> <a href="/mas" rrc-event-name="position1">马鞍山二手车</a> <em>&gt;</em> </li> <li> <a href="/mas/ershouche" rrc-event-name="position2">马鞍山个人二手车</a> <em>&gt;</em> </li> <li> <h1 rrc-event-name="position3">马鞍山二手宝马</h1> </li> </ul> <form class="js-search-form index-search clearfix" rrc-event-scope="search" action="/mas/ershouche"> <input type="text" placeholder="请输入关键词，如：别克 1.6L" name="keyword" class="search-text js-search-keyword" autocomplete="off" rrc-event-name="input" rrc-event-type="focus"> <div class="js-search-submit search-button btn-base btn-wireframe-small" rrc-event-name="icon"> <div class="search-submit"></div> </div> <div class="js-search-suggest index-search-content" rrc-event-scope="hotsearches"></div> </form> </div> <div class="search-wrapper"> <script>
