    RRC.Q.push(function() {
        require(['jquery', 'log', 'common/app', 'jquery.cookie'], function ($, log, Application) {
            var app = new Application(window.RRC);
            var $positionFlag = true,
                $chooseFlag = true,
                $positionCity = RRC.city;

            $("#tip-position-problem").on("click", function (e) {
                if($positionFlag){
                    $("#tip-change-position").css('display', 'block');
                    $('.tip-close-change-position').on('click', function () {
                        $provinces.addClass('tip-selected');
                        $province.addClass('tip-selected');
                        $cities.removeClass('tip-selected');
                        $city.removeClass('tip-selected');
                        $cities.html('');
                        $(this).parent().hide();
                        $positionFlag = true;
                    });
                    $('body').on('click',function(){
                        $provinces.addClass('tip-selected');
                        $province.addClass('tip-selected');
                        $cities.removeClass('tip-selected');
                        $city.removeClass('tip-selected');
                        $cities.html('');
                        $('#tip-change-position').hide();
                        $positionFlag = true;
                    });
                    $positionFlag = false;
                }
                else{
                    $("#tip-change-position").css('display', 'none');
                    $positionFlag = true;
                }
                e.stopPropagation();
            });

            $("#tip-city-choose").on("click", function (e) {
                if($chooseFlag){
                    $("#tip-area-city").css('display', 'block').on("click",function(event){
                        event.stopPropagation();
                    });
                    $('.tip-close-area-city').on('click', function () {
                        $(this).parent().hide();
                        $chooseFlag = true;
                    });
                    $('body').on('click',function(){
                        $('#tip-area-city').hide();
                        $chooseFlag = true;
                    });
                    $chooseFlag = false;
                }
                else{
                    $("#tip-area-city").css('display', 'none');
                    $chooseFlag = true;
                }
                e.stopPropagation();
            });

            $('.tip-city-select').on('click', function (e) {
                e.stopPropagation();
                var that = this;

                if($(that).data('abbr') == 'cn') {
                    $('.tip-result-left').find('.tip-choose-city').remove();
                    $('.tip-city-select').data('select', '0');
                } else {
                    $('.tip-result-left').find('span[data-cityname="cn"]').remove();
                }
                $('.tip-requirement').html('').addClass('tip-hide');

                if($('.tip-choose-city').length > 2) {
                    $('.tip-requirement').html('最多选择3个城市').removeClass('tip-hide');
                    return false;
                }

                var choose_cities_abbr = '';
                $('.tip-choose-city').each(function(index, el) {
                    choose_cities_abbr  += $(this).data('cityname');
                });
                if( choose_cities_abbr.indexOf($(that).data('abbr')) > -1) {
                    return false;
                }

                if($(this).data('select') == 1) {
                    return false;
                }

                $('<span class="tip-choose-city" data-cityname="'+$(this).data('abbr')+'">'+$(this).html()+'<div class="tip-result-close"></div></span>').on('click', function () {
                    $(this).remove();
                    $(that).data('select', '0');
                    $('.tip-requirement').addClass('tip-hide');
                    if($('.tip-choose-city').length == 0) {
                        $('.tip-requirement').html('最少选择1个城市').removeClass('tip-hide');
                    }
                    $('.tip-active').removeClass('tip-active');
                    $('.tip-choose-city').each(function(){
                        $("a[data-abbr="+$(this).data('cityname')+"]").addClass('tip-active');
                    });
                }).appendTo('.tip-result-left');

                $('.tip-active').removeClass('tip-active');
                $('.tip-choose-city').each(function(){
                    $("a[data-abbr="+$(this).data('cityname')+"]").addClass('tip-active');
                });

                if($(that).data('abbr') == 'cn') {
                    $(that).data('select', '0');
                } else {
                    $(that).data('select', '1');
                }

            });
            $('.tip-result-close').on('click', function () {
                $(this).parent().remove();
                $('.tip-requirement').addClass('tip-hide');
                if($('.tip-choose-city').length == 0) {
                    $('.tip-requirement').html('最少选择1个城市').removeClass('tip-hide');
                }
                $('.tip-active').removeClass('tip-active');
                $('.tip-choose-city').each(function(){
                    $("a[data-abbr="+$(this).data('cityname')+"]").addClass('tip-active');
                });
            });

            $('.tip-source-close').on('click', function () {
                $(this).parent().remove();
                var cities_name = '';
                $('.tip-source-city').each(function(index, el) {
                    cities_name += $(this).data('cityname') + '_';
                });
                $.removeCookie('rrc_muti_city_name');
                $.cookie('rrc_muti_city_name', cities_name, {expires: 365, path: '/'});
                app.redirect('/'+RRC.city + '/ershouche');
            });
            $('#tip-select_mult_cities').on('click', function () {
                if($('.tip-choose-city').length == 0) {
                    $('.tip-requirement').html('最少选择1个城市').removeClass('tip-hide');
                    return false;
                }
                var cities_name = '';
                $('.tip-choose-city').each(function(index, el) {
                    cities_name += $(this).data('cityname') + '_';
                });

                log.push(['_trackEvent', 'pc_nc_tip_search', 'click', 'search']);
                $.removeCookie('rrc_muti_city_name');
                $.cookie('rrc_muti_city_name', cities_name, {expires: 365, path: '/'});
                app.redirect('/'+ $positionCity + '/ershouche');

            });

            $('.tip-cities-result-right').on('click',function(e){
                log.push(['_trackEvent', 'pc_nc_tip_requirement', 'click', 'search']);
                e.stopPropagation();
            });

            //重新定位
