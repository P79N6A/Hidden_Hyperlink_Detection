//加载报价等级
function myselectbaojia(aid)
{
	
$.ajax({
		  async: false,
    type: "Get",
    url: "/add/ajax_select.php?post=myselectbaojia&aid="+aid+"&t=" + Math.random(),
    data: "text",
	complete: function(data){
            //$(".loading").attr("style","display:block")
        },
    success: function(data) {
		
		var t='';
     data=data.split('~~~');
	 for(var i=0;i<=data.length;i++)
	 {
		 if(data[i])
		 {
			 
		   var v=eval(data[i]);
		 // for(var ii=0;ii<=v.length;ii++)
	      //{
			  if(i==0)
			  {
				  var sel='class="selected"';
			  }
			  else
			  {
				  var sel='';
				  
			  }
			   t=t+' <li  '+sel+' id="li_date_type_'+v[4]+'"><a href="javascript:date_type('+v[4]+')" target="_self" title="'+removeHTMLTag(decodeURIComponent(v[2]))+'">'+decodeURIComponent(v[0])+'</a></li>';  
		  //}
		  
		 }
	 }
    if(t=='') 
	{
    $("#selectTimePrice").html('<option value="1">产品已过期或下架，请选择其他产品</option>');
	t='<li><a href="javascript:">产品已过期或下架</a></li>';
	}
	$("#price_type").html(t);
	}
	});
	
}
myselectbaojia('10375');


//加载出发日期
function getchufariqi()
{
	var id='';
	var aid='10375';
	$("#price_type").find('li').each(function(index, element) {
        
		if($(this).attr('class')=='selected') 
		{
			id=$(this).attr('id');
			id=id.split('_');
			id=id[3];
		}
    });
	
	if(id!='')
	{
		
	$.ajax({
		  async: false,
    type: "Get",
    url: "/add/ajax_select.php?post=getchufariqi&aid="+aid+"&id="+id+"&t=" + Math.random(),
    data: "text",
	complete: function(data){
            //$(".loading").attr("style","display:block")
        },
    success: function(data) {
		
		data=data.split(',');
		var t='';
		var a = []; // 有价格的日期数组
		
	 for(var i=0;i<=data.length;i++)
	 {
		var v=data[i];
		
		if(v)
		{
		v=v.split('|');
		
		var date=v[0];//日期
		
		
		
		
		
		v=v[1].split('-');
		
		var yuding=v[0];//预定价
		var yuding_child=v[1];//预定价儿童价
		var kucun=v[4];//库存
		//
		a[date]=yuding; //加入有价格的日期数组
		
		
		
		if(kucun=='max' || kucun>0)
		{
			//alert(kucun);
		
		
		t=t+'<option value="'+date+'">'+date+'（'+getDayOfWeek(date)+'）成人：￥'+yuding+' 儿童：￥'+yuding_child+'</option>';
		}
		}
	 }
	
	
		$("#selectTimePrice").html(t);
		
		
		//重写日历的价格
		$(".time-price-one .no_guoqi").each(function(index, element) {
           //$(this).html();
		   var date=$(this).find('.search_pp_calendar_day_date').html(); //得到日历的日期
		   /*
		   if(a.indexOf(date) >= 0)
		   { //有价格
			   
		   }*/
		   //alert(a[date]);
		   
		   
		   
		   
		   if(a[date])
		   {
			$(this).find(".search_pp_calendar_price").html('06'+a[date]+'');
			$(this).removeClass('search_pp_calendar_d_box_no_hover');
			$(this).addClass('search_pp_calendar_d_box');
			//$(this).append('<span class="search_pp_calendar_price">06'+a[date]+'</span>'); 
		   }
		   else
		   {
			   
			   $(this).find(".search_pp_calendar_price").html('');
		   }
		   
        });
		
			$('.search_pp_calendar_d_box').on('click',function(){


				var startDate=$(this).find('.search_pp_calendar_day_date').text();// 获取点击的日期
				$("#selectTimePrice option").each(function(index, element) {
                  if($(this).val()==startDate)
				  {
					  $(this).attr("selected",true);

				  }
                });
				
				
			});

	}
	
	
	});
	
		
		
	} //if id
	
	
}
getchufariqi();

//切换等级报价
function date_type(type_id)
{
	
	
$('#price_type').find("li").removeClass("selected");
$('#price_type').find("#li_date_type_"+type_id).addClass("selected");
	
	
getchufariqi();//重置出发日期和价格
}


//加载日历表
function myrili(aid)
{
	
$.ajax({
		  async: false,
    type: "Get",
    url: "/add/ajax_select.php?post=rili&aid="+aid+"&t=" + Math.random(),
    data: "text",
	complete: function(data){
            //$(".loading").attr("style","display:block")
        },
    success: function(data) {
		
		
	$(".time-price-one").html(data);
	}
	
	
	
	
	});
	
}


</script>   
