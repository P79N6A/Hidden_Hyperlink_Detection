    <script type="text/javascript">
                    $(function() { 
						if((parseInt(window.screen.width) <= 1024 && 'undefined' != typeof(document.body.style.maxHeight)) || navigator.userAgent.match(/mobile/i)){
							$(".ibar_main_panel").css("background","none");
							for(var i=0; i <= $('.ibar_main_panel ul').children('li').length-1; i++) {
								if(i!==0 && i!==1 && i!==8){$('.ibar_main_panel ul li:eq('+i+')').css("display","none");}
							}
						}
                        $("#iBar").find("li").hover(function() {
                            $(this).addClass("on");
							$(this).find(".mp_tooltip").css("display","block");
                        }, function() {
                            $(this).removeClass("on");
							$(this).find(".mp_tooltip").css("display","none");
                        });
                        $("#ibar_mysee,.ibar_sub_panel .ibar_closebtn").click(function() {
							var title = $(this).find("span:eq(0)").text();
							$(".ibar_sub_panel .ibar_plugin_name").html(title);
                            var left = $(".ibar_sub_panel").css("left");
							if ('undefined' == typeof(document.body.style.maxHeight)) {
                            if (left == '0px') {
								$(".ibar_sub_panel").css({"display":"block","left": "-287px"});
                            } else {
								$(".ibar_sub_panel").css({"display":"none","left": "0"});
                            }
							}else{
                            if (left == '0px') {
								$(".ibar_sub_panel").css("display","block");
								$(".ibar_sub_panel").animate({left: "-287px"});
                            } else {
                                $(".ibar_sub_panel").animate({left: "0"},function(){$(".ibar_sub_panel").css("display","none");});
                            }
							}
                        });
						$("#iBar:eq()").hover(function(){
							if(parseInt(window.screen.width) <= 1024 && 'undefined' != typeof(document.body.style.maxHeight)){
							$(".ibar_main_panel").css("background","#444851");
							for(var i=0; i <= $('.ibar_main_panel ul').children('li').length-1; i++) {
								if(i!==0 && i!==1 && i!==8){$('.ibar_main_panel ul li:eq('+i+')').css("display","block");}
							}}
						},function(){
							if(parseInt(window.screen.width) <= 1024 && 'undefined' != typeof(document.body.style.maxHeight)){
							$(".ibar_main_panel").css("background","none");
							for(var i=0; i <= $('.ibar_main_panel ul').children('li').length-1; i++) {
								if(i!==0 && i!==1 && i!==8){$('.ibar_main_panel ul li:eq('+i+')').css("display","none");}
							}}
						});
                        $("#ibar_avatar,#ibar_loginbox").hover(function() {
                            var top = (parseInt($("#iBar").height()) / 2) - 190;
                            $("#ibar_loginbox").attr("is-hover", 1);
                            $("#ibar_loginbox").css({"display": "block", "top": top, "left": "-287px"});
                        }, function() {
                            $("#ibar_loginbox").attr("is-hover", 0);
                            setTimeout("hideLoginbox()", 100);
                        });
                    });
                    function hideLoginbox() {
                        var is_hover = $("#ibar_loginbox").attr("is-hover");
                        if (is_hover != 1) {
                            $("#ibar_loginbox").css({"display": "none", "top": "50px", "left": "0"});
                        }
                    }
        </script> 
