          <script>
	function run(){
		var key=document.getElementById("keyword").value;
		var doc=document.body.getElementsByTagName("*");
		var i=0;
		while(doc[i]){
			if(doc[i].innerHTML&&doc[i].className!='evtFind'){
				var tmp=doc[i].innerHTML;
				var reg=new RegExp("("+key+")","g");
				doc[i].innerHTML=tmp.replace(reg,'<font color="#ff0000" class="evtFind">$1</font>');
			}
			i++;
		}
		document.getElementById("keyword").value=key;
		return false;
	}
	function submitSwitch()
	{
		var searchmethod = document.getElementById("searchmethod");
		var searchmethodValue = searchmethod.options[searchmethod.selectedIndex].text;
		if(searchmethodValue == "页面搜索")
		{//网页搜索

			for(var i=0;i<searchmethod.options.length;i++)
			{
				if(searchmethod.options[i].text=="页面搜索")
				{
					searchmethod.options[i].selected = true;
				}
			}
			run();
			return false;
		}
	}
</script> 
