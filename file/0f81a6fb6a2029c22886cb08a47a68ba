   <script type="text/javascript">
var searchVal="0";
function setspanbg(E,N)
{
$("#searchfrm span").removeClass("homeSearchspanBG");
$("#searchfrm span").addClass("homeSearchspan");
$(E).addClass("homeSearchspanBG");
searchVal=N;
}
function gowhere()
{
var url;
var keywordStr=$.trim($("#keyword").val());
if(keywordStr==""){alert("请输入查询关键字");$("#keyword").focus();return false};
if(keywordStr.length<2){alert("请输入2个以上查询关键字");$("#keyword").focus();return false};

if (searchVal == "4")
url = "http://news.chinaso.com/search?wd="+encodeURI(keywordStr);
if (searchVal == "0")
url = "http://www.chinaso.com/search/pagesearch.htm?q="+encodeURI(keywordStr);
if (searchVal == "5")
url = "http://image.chinaso.com/so?q="+encodeURI(keywordStr);
if (searchVal == "2")
url = "http://video.chinaso.com/search/so?q="+encodeURI(keywordStr);
window.open(url);
}
</script> 
