     <script type="text/javascript">
(function(){
function getDateUnix(d) {
var UnixTime = d * 1000;
var dateObj = new Date(UnixTime);
var UnixTimeToDate = dateObj.getUTCFullYear() + '-' + (dateObj.getUTCMonth() +1 ) + '-' + dateObj.getUTCDate() + ' ' + (dateObj.getUTCHours()+8) + ':' + dateObj.getUTCMinutes();
return UnixTimeToDate;
}
$.ajax({ 
url: 'http://www.mzyfz.com/template/mzyfz/cms/mzyfz_chkdata.php',
dataType: 'json',
cache: false,
success: function(json){
var commcon="";
if(json!=""){

for(var i in json){
if(i==0){
var oneurl='http://www.mzyfz.com/html/424/'+ json[i].timestamp +'/content-'+ json[i].id +'.html';
$(".childrenclass li").append('&middot; <a id="hudongid" href="'+oneurl+'" target="_blank" title="'+ json[i].content +'">'+ json[i].content +'</a>');
}else{
commcon+="<div class='commentd1'><span class='comment2'>" + getDateUnix(json[i].timestamp) + "</span><span class='commentl'>网友：" + json[i].username + "</span></div><div class='commentd2'>" + json[i].content    + "</div>"
}
}
if(i==0){
commcon="<a href='"+oneurl+"' target='_blank'><img src='http://www.mzyfz.com/skin/mzyfz/cms/2015/hd.jpg' /></a>";
}else{
commcon=commcon+commcon+commcon+commcon;
}

}
$("#commentsarea").append(commcon);
}
});
$("#commentsarea").height($("#commentsarea").height()-$(".childrenclass").height()+30)
var this_Scroll2 = document.getElementById("commentsarea");
var ScrollTimer2=40;
var MyMar2 = setInterval(Scroll2,ScrollTimer2);
this_Scroll2.onmouseover=function(){ clearInterval(MyMar2) };
this_Scroll2.onmouseout=function(){ MyMar2=setInterval(Scroll2,ScrollTimer2) };
function Scroll2(){
this_Scroll2.scrollTop++;
if(parseInt(this_Scroll2.scrollTop)>=parseInt(this_Scroll2.scrollHeight)*.6) {
this_Scroll2.scrollTop=0;
}
}
})()	
</script> 
