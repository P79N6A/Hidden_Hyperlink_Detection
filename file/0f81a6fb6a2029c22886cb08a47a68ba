  <script type="text/javascript">
function b(){
if($(window).scrollTop() >= 100){
$('#goTop').fadeIn(400);
} else {
$('#goTop').fadeOut(200);
}
}
$(document).ready(function(e) {
$('#goTop').hide();
b();
$('#gotop').click(function(){
$(document).scrollTop(0);	
})	
$('.goTop').click(function(){
$('html,body').animate({scrollTop : '0px'}, 100);
});

$("#floatright li:gt(0)").mouseover(function(){
$("#floatright li img").remove();
$(this).append("<img class='shqcode' width='140' height='140' src='"+$(this).find("a").attr("i")+"' />");
});
$("#floatright li:gt(0)").mouseout(function(){
$("#floatright li img").remove();
});
$("#floatright li:gt(0)").click(function(){
window.open($(this).find("a").attr("href"));
});

});
$(window).scroll(function(e){b();})
function fnhidthis(){
$("#floatright").hide();
}
</script> 
