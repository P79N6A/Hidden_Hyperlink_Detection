var textelement = $('.textElement').text();                   
var showBtn = $('.showFullText');                      
showBtnClick(30, showBtn, textelement);
function showBtnClick(num, showBtn, text){                   
    showBtn.toggle(function(){                         
        $(this).text("更多>>");                    
        $(this).prev().text(text.substring(0,num)+'...');                  
    },function(){                                      
        $(this).text("收起<<");
        $(this).prev().text(text);                     
    }).trigger('click')                                
}
