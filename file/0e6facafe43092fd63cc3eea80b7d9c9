   <span class="mengleftcenter mengleftcenter_1002"><img src="http://i1.hdfimg.com/pc3/images/meng2.png"></span>
   <span class="mengleftclose mengleftclose_1002"><img src="http://i1.hdfimg.com/pc3/images/1.12-1fuclose.png"></span>
  </div>
  </div>
-->
    <script type="text/javascript">
      $(document).ready(function() {
           $(".mengclose").click(function() {
              $(".mengleft").hide();
                $(".mengright").hide();
         });
         $(".mengbut").click(function() {
              $(".mengleft").show();
         });
         $(".mengleftclose").click(function() {
              $(".mengleft").hide();
         });
      }); 
     
</script>
