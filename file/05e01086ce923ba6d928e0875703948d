  <div align="center" style="margin-left:6px; margin-right:6px;"> 
   <script type="text/javascript">
var analyseMenu=1;
function selectMenu(index)
{
	if(index==analyseMenu)
		return;
	var obj=$("#menu_"+analyseMenu+"_1");
	obj.removeClass(analyseMenu==1 ? "menu_a_button_xz_a1" : "menu_a_button_xz_a");
	obj.addClass(analyseMenu==1 ? "menu_a_button_a1" : "menu_a_button_a");
	obj=$("#menu_"+analyseMenu+"_2");
	obj.removeClass("menu_a_button_xz_b");
	obj.addClass("menu_a_button_b");
	obj=$("#menu_"+index+"_1");
	obj.removeClass(index==1 ? "menu_a_button_a1" : "menu_a_button_a");
	obj.addClass(index==1 ? "menu_a_button_xz_a1" : "menu_a_button_xz_a");
	obj=$("#menu_"+index+"_2");
	obj.removeClass("menu_a_button_b");
	obj.addClass("menu_a_button_xz_b");
	$("#menu_"+analyseMenu).hide();
	$("#menu_"+index).show();
	analyseMenu=index;
	if(window.Chart)window.Chart.move();
	else if(window.ChartST)window.ChartST.move();
	else if(window.ChartMT)window.ChartMT.move();
	else if(window.Miss)window.Miss.move();
}
</script> 
