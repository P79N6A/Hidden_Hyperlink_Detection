    $.ajax({
        type:'get',
        url:"http://dalian.liebiao.com/ajax/postclicks/?postid=334874972",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
                $('#post_visitors_div').attr('title', '已被'+json.msg.postclicks+'人浏览');
