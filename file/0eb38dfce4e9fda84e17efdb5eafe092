<script type="text/javascript">
$(document).ready(function() {
	$('#hottitle .hoticonhide').show();
	$('#hottitle .hoticonshow').hide();

	$('#hottitle .hoticonshow').click(function(event) {
		event.stopPropagation();
		$(this).hide();
		$('#hottitle .hoticonhide').show();
		$('ol.notes').slideDown();
		return false;
	});
	$('#hottitle .hoticonhide').click(function(event) {
		event.stopPropagation();
		$(this).hide();
		$('#hottitle .hoticonshow').show();
		$('ol.notes').slideUp();
		return false;
	});

	$('#commentstitle .commentsiconhide').show();
	$('#commentstitle .commentsiconshow').hide();
	$('#commentstitle').click(function(event) {
		event.stopPropagation();
		return false;
	});
	$('#commentstitle .commentsiconshow').click(function(event) {
		event.stopPropagation();
		$(this).hide();
		$('#commentstitle .commentsiconhide').show();$('#comment_wrapper').slideDown();
		return false;
	});
	$('#commentstitle .commentsiconhide').click(function(event) {
		event.stopPropagation();
		$(this).hide();
		$('#commentstitle .commentsiconshow').show();
		$('#comment_wrapper').slideUp();
		return false;
	});
})
