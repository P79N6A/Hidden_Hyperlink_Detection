            with (document) 0[(getElementsByTagName('head')[0] || body).appendChild(createElement('script')).src = 'http://bdimg.share.baidu.com/static/api/js/share.js?v=89860593.js?cdnversion=' + ~(-new Date() / 36e5)];
        });

        function emailBiddingSub() {
            $("a.ten_btn").hide();
            $("a.ten_btn_disabled").show();
            $("#error_msg").text("");
            var email = $.trim($("#sub_email").val());
            if (!checkEmail(email)) {
                $("#error_msg").text("请输入正确的订阅邮箱");
                return;
            }
            var info_type = "102001";
            var info_id = "7398429";

            subEmailBidding(info_type, info_id, email, function (result) {
                if (result.success) {
                    window.location.reload();
                } else {
                    $("#error_msg").text(result.msg);
                    $("a.ten_btn").show();
                    $("a.ten_btn_disabled").hide();
                }
            });
        }

        function submitvalid() {
            var auth_code = $.trim($("#auth_code").val());
            if (auth_code == "") {
                alert("请输入验证码！");
                return;
            }

            $("#btn_valid").attr('disabled', "disabled");
            $.ajax({
                type: "post",
                url: "/common/infooutcountlimitvalidatecodevalid",
                data: { type: "2", code: auth_code },
                success: function (result) {
                    if (result.success) {
                        window.location.reload();
                    } else {
                        alert(result.msg);
                        $("#btn_valid").removeAttr("disabled");
                    }
                }
            });
        }
    </script>   
 </body>
</html>