function yunqikan(){
	
	
    
	
     		$.get("/index.php?m=Index&a=yunqikan", { "id": 0 },
                  function(data){
     					if(data>0){
     						 
     					layer.msg('为您选择文章成功！希望是您喜欢的！', 1, 1);
						
     					 
     					 urltmp="/index.php?m=Index&a=show&id="+data;
  						setTimeout(function(){
  							location.href=urltmp;
  							}, 1000);
     					}else{
     						layer.msg('为您选择文章失败！再选一次！', 1, 13);    						
     						
     						  

     					}
     					
                  });
     		
     	
         
   
	
}
 
function getztzr(){
	var list = '';
	$.ajax({ 
	      type:"GET", 
	      url:"/index.php?g=Home&m=Hot&a=getltop", 
	      dataType:"json",
	      success:function(data){
	      	$(data).each(function(i){
	      		title = data[i].title; 
				title_msubstr = data[i].title_msubstr;
	      		view = data[i].view;
	      		id = data[i].a_id;
	      		catid = data[i].catid;
	      		view = data[i].view;
				
				var urltmp="/index.php?m=Index&a=show&cat="+catid+"&id="+id;
	      		list+='<li><i style="">'+(i+1)+'</i><a target="_blank" href='+urltmp+' title="'+title+'">'+title_msubstr+'</a></li>';
	            })
	            $("#ztzr").html(list);  
	      }
	     
	    }); 
}

		function getmps(){
