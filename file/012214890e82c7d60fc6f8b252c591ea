<script type="text/javascript">


		function setOptionValue( val )
		{
			document.getElementById('type').value = val;
		}
		function CheckValue()
		{
			var type = document.getElementById('type').value;
			var keywork=document.getElementById("key_word").value;
			if(keywork == ''){
				document.getElementById("key_word").value='请输入您的搜索关键词';
				document.getElementById("key_word").style.color="red";
				return;
			}
			if(keywork == '请输入您的搜索关键词'){
				return;
			}
			if( type == 0 )
			{
				window.open('http://search.trade2cn.com/product/search.html?q='+keywork);	
			}
			if( type == 1 )
			{
				window.open('http://search.trade2cn.com/company/search.html?q='+keywork);	
			}
			if( type == 2 )
			{
				window.open('http://search.trade2cn.com/supply/search.html?q='+keywork);	
			}
			if( type == 3 )
			{
				window.open('http://search.trade2cn.com/buy/search.html?q='+keywork);	
			}
			if( type == 4 )
			{
				window.open('http://search.trade2cn.com/news/search.html?q='+keywork);	
			}
		}
		function cl(){
			var keywork=document.getElementById("key_word").value;
			
			if(keywork == '请输入您的搜索关键词'){
				document.getElementById("key_word").value='';
				
			}
			document.getElementById("key_word").style.color="#000000";
		}

</script>

