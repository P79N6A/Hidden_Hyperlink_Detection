		(function(){
			YAHOO.util.Connect.transport("/connection.swf");
			var _requestNum = 7;
			YAHOO.util.Connect.xdrReadyEvent.subscribe(function() {
					var _actor = "BAUHAUS";
					_actor = _actor.replace(/<br \/>/," ");
					var _eng = /[\w]/gi;
 					var _brackets = /\((\S|\s)*?\S+\)/gi;///\(\S+\)/gi;
					var _actorArr;
					if(_actor.match(_brackets)){
						// 带括号的名字
						_actor = _actor.replace(_brackets,",");
						_actorArr = _actor.split(",");
						for(var j=0,len=_actorArr.length;j<len;j++){
							if(_actorArr[j]!=""){
								_actor = _actorArr[j];
								break;
							}
						}
					}else{
						if(!_actor.match(_eng)){
							// 有中文名,按空格分开,取第一个名字
							_actorArr = _actor.split(" ");
							_actor = _actorArr[0];
						}

					}
					//console.log("_actor final "+_actor);
					var obj = YAHOO.util.Connect.asyncRequest('GET',	   "http://ws.audioscrobbler.com/2.0/?method=artist.getsimilar&artist="+encodeURIComponent(_actor)+"&limit="+_requestNum+"&format=json&api_key=b181f092333e65d07d82ea5f26f4f399",callback);


			});
		    var handleSuccess = function(o) {
				var lastfmArtist = YAHOO.lang.JSON.parse(o.responseText);
				var _artist = lastfmArtist.similarartists;
				var _errorImgNum = 0;
				if (lastfmArtist&&_artist!=undefined&&typeof _artist["artist"][1]=="object") {
					old_get("similarArtists").style.display = "";
					old_get("similarArtistsImages").innerHTML="";
					for(var i=0, len=Number(_artist["artist"].length);i<len;i++){
						var _name = _artist["artist"][i].name;
						var _img = _artist["artist"][i]["image"]["1"]["#text"].replace(/http:\/\/userserve-ak.last.fm\/serve\/64\//,"http://userserve-ak.last.fm/serve/64s/");
						if(_img!=""){
							// 照片不为空时才显示
							old_get('similarArtistsImages').innerHTML+='<li><a title="'+_name+'" href="http://www.verycd.com/search/folders/'+_name+'" onclick="VeryCD.Track(\'/stat/topicResStar/\');" target="_blank"><span class=""><span class="image"><img src="'+_img+'" /></span></span><br />'+_name+'</a></li>';
						}else{
							_errorImgNum++;
						}
					}
					if(_errorImgNum==_requestNum){
						// 照片全为空时隐藏掉整块
						old_get("similarArtists").style.display = "none";
					}

