function checkCode(n) {
	var verifycode = document.getElementById("verifycode")
	if (n == 0 && verifycode.value == ""){verifycode.value="";verifycode.style.cssText = "color:#000000"}
	if (n == 1 && verifycode.value == ""){verifycode.value = "";verifycode.style.cssText = "color:#aaaaaa"}
}
function baomings_onclick() {
  if((document.BMuseyinfos.StudentsName.value=="") || document.BMuseyinfos.StudentsName.value.length <2){
    alert ("请输入你的称呼");
    document.BMuseyinfos.StudentsName.focus();
    return false;
  }
   if((document.BMuseyinfos.Telephone.value== "") || document.BMuseyinfos.Telephone.value.length <6){
    alert ("你的手机号码");
    document.BMuseyinfos.Telephone.focus();
    return false;
  }
  }
