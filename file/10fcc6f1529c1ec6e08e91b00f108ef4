     <script>
   window.addEvent('domready',function(){
       $('backtotop').inject(document.body).addEvent('click',function(){
		   new Fx.Scroll(window,{link:'cancel'}).toTop();
	   }).setOpacity(.8);
	   
	   this.addEvent('scroll',function(){
	        if(window.getScrollTop()>100){
	          $('backtotop').setStyle('top', window.getScrollTop() - $('backtotop').getSize().y + 600 ).show();	
	         }else{
		     $('backtotop').hide();
		   }
	   });
   });
