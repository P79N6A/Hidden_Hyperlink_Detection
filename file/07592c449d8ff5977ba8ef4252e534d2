   document.getElementById("page2").style.display='none';
   document.getElementById("page3").style.display='block';  
  } 
 }  
 
 function pinyin()
 {
  if(document.getElementById("duo").style.display=='none')
  {
  document.getElementById("yin").style.display='none';
  document.getElementById("duo").style.display='block';
  }
  else
  {
   document.getElementById("yin").style.display='block';
   document.getElementById("duo").style.display='none';
  }  
 } 
  
    function onChange(i)
    {
       childSort=document.all("child" + i);
       childfont=document.all("font" + i)   
	    if(childSort.style.display=="none")
	    {
		 childSort.style.display="";
		 childfont.style.display="none";			    
		}
	    else
	    {
		childSort.style.display="none";
		childfont.style.display="";
		}
    }
