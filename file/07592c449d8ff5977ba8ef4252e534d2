   <script language="javascript" type="text/javascript">
//字符转换为UTF-8编码
function EncodeUtf8(s1)
{
      var s = escape(s1);
      var sa = s.split("%");
      var retV ="";
      if(sa[0] != "")
      {
         retV = sa[0];
      }
      for(var i = 1; i < sa.length; i ++)
      {
           if(sa[i].substring(0,1) == "u")
           {
               retV += Hex2Utf8(Str2Hex(sa[i].substring(1,5)));

           }
           else retV += "%" + sa[i];
      }

      return retV;
}
function Str2Hex(s)
{
      var c = "";
      var n;
      var ss = "0123456789ABCDEF";
      var digS = "";
      for(var i = 0; i < s.length; i ++)
      {
         c = s.charAt(i);
         n = ss.indexOf(c);
         digS += Dec2Dig(eval(n));

      }
      //return value;
      return digS;
}
function Dec2Dig(n1)
{
      var s = "";
      var n2 = 0;
      for(var i = 0; i < 4; i++)
      {
         n2 = Math.pow(2,3 - i);
         if(n1 >= n2)
         {
            s += '1';
            n1 = n1 - n2;
          }
         else
          s += '0';

      }
      return s;

}
function Dig2Dec(s)
{
      var retV = 0;
      if(s.length == 4)
      {
          for(var i = 0; i < 4; i ++)
          {
              retV += eval(s.charAt(i)) * Math.pow(2, 3 - i);
          }
          return retV;
      }
      return -1;
}
function Hex2Utf8(s)
{
     var retS = "";
     var tempS = "";
     var ss = "";
     if(s.length == 16)
     {
         tempS = "1110" + s.substring(0, 4);
         tempS += "10" + s.substring(4, 10);
         tempS += "10" + s.substring(10,16);
         var sss = "0123456789ABCDEF";
         for(var i = 0; i < 3; i ++)
         {
            retS += "%";
            ss = tempS.substring(i * 8, (eval(i)+1)*8);



            retS += sss.charAt(Dig2Dec(ss.substring(0,4)));
            retS += sss.charAt(Dig2Dec(ss.substring(4,8)));
         }
         return retS;
     }
     return "";
}
</script> 
   <script type="text/javascript" language="javascript">
    function tochang(i)
    {
      if(i==0)
        {
         document.getElementById("HeaderUserControl1_xiangxin").checked=true;
         document.getElementById("HeaderUserControl1_google").checked=false;
         document.getElementById("HeaderUserControl1_baidu").checked=false;
         document.getElementById("HeaderUserControl1_put").placeholder='请输入要搜索的单字';
         document.getElementById("Image1").className='bt';

        }
      if(i==1)
        {
         document.getElementById("HeaderUserControl1_xiangxin").checked=false;
         document.getElementById("HeaderUserControl1_google").checked=true;
         document.getElementById("HeaderUserControl1_baidu").checked=false;
         document.getElementById("HeaderUserControl1_put").placeholder='请输入谷歌搜索关键词';
         document.getElementById("Image1").className='bt1';
        }
        if(i==2)
        {
         document.getElementById("HeaderUserControl1_xiangxin").checked=false;
         document.getElementById("HeaderUserControl1_google").checked=false;
         document.getElementById("HeaderUserControl1_baidu").checked=true;
         document.getElementById("HeaderUserControl1_put").placeholder='请输入百度搜索关键词';
         document.getElementById("Image1").className='bt2';
        }
    }

     function search()
   {
   if(document.getElementById("HeaderUserControl1_xiangxin").checked==true)
   {
   if(document.getElementById("HeaderUserControl1_put").value!=""){
        // window.open("../Word.aspx?wd=" + EncodeUtf8(document.getElementById("HeaderUserControl1_put").value) + "");
         window.location.href="../Word.aspx?wd=" + EncodeUtf8(document.getElementById("HeaderUserControl1_put").value) + "";
    }else{
        alert("搜索内容不能为空");
        document.getElementById("HeaderUserControl1_put").focus();
        return false;
    }
    }

     if(document.getElementById("HeaderUserControl1_google").checked==true)
   {
   if(document.getElementById("HeaderUserControl1_put").value!=""){
        window.open("http://www.google.com.hk/search?ie=utf8&hl=zh-CN&source=hp&q=" + EncodeUtf8(document.getElementById("HeaderUserControl1_put").value) + "&btnG=Google+%E6%90%9C%E7%B4%A2&meta=&aq=f&aqi=g10&aql=&oq=&gs_rfai=");
    }else{
        alert("搜索内容不能为空");
        document.getElementById("HeaderUserControl1_put").focus();
        return false;
    }
    }

     if(document.getElementById("HeaderUserControl1_baidu").checked==true)
   {
   if(document.getElementById("HeaderUserControl1_put").value!=""){
        window.open("http://www.baidu.com/s?ie=utf8&wd="+EncodeUtf8(document.getElementById("HeaderUserControl1_put").value));
    }else{
        alert("搜索内容不能为空");
        document.getElementById("HeaderUserControl1_put").focus();
        return false;
    }
    }
}
    </script> 
