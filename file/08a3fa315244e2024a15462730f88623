<script type="text/javascript">
function _tlogin(i){
    var u=window.location.href;
    window.location.href="https://account.sogou.com/connect/login?provider="+i+"&client_id=2002&ru="+encodeURIComponent(u);
}

function init_upload() {
	var html = $("#upload_container").html();
    uploadtop = document.documentElement.scrollTop+document.body.scrollTop+110;
	upload_sleft = document.documentElement.clientWidth/2-280;
    uploadleft = document.documentElement.clientWidth/2-371;
	if(uploadtop<=0) uploadtop = 0;
	if(uploadleft<=0) uploadleft = 0;
	if(upload_sleft<=0) upload_sleft = 0;
	if(html != "") {
		$("#facy").show();
		var c_html_len = $(".upload_edit_container").find("div.upload_edit_img").length;
		if(c_html_len > 0) {
			$("#upload_edit").css({"top":uploadtop,"left":uploadleft}).show();
		} else {
			$("#upload_choose").css({"top":uploadtop,"left":upload_sleft}).show();
		}
		return;
	}
	$.ajax({
	    url: "/user_ofc/upload_img/",
	    type: "GET",
	    dataType: "html",
	    success: function(msg) {
		    if(msg) {
                $("#upload_container").html(msg);
		    	$("#upload_choose").css({"top":uploadtop,"left":upload_sleft});
		    	$("#upload_edit").css({"top":uploadtop,"left":uploadleft});
		    	$("#upload_container").show();
		    } else {
		    	wp_common.setCookie("login_out", 1, 1);
		    	window.location.reload();
		    }
	    }
	});
}

var login_upload = wp_common.getCookie("login_upload");
if(login_upload == 1) {
	wp_common.deleteCookie("login_upload");
	init_upload();
}
var login_out = wp_common.getCookie("login_out");
if(login_out == 1) {
	wp_common.deleteCookie("login_out");
}else if(login_out == 2) {
	wp_common.deleteCookie("login_out");
}

$(function(){
	$("#arrow").click(function(){
		$("#userpassport_bar").slideToggle(200);
		if ($("#arrow_class").attr("src")=="/bizhi/images/arrow_down.jpg")
		{
			$("#arrow_class").attr("src","/bizhi/images/arrow_up.jpg");
		}
		else $("#arrow_class").attr("src","/bizhi/images/arrow_down.jpg");
		return false;
	});
    $(document).click(function(){
        
	    $("#userpassport_bar").slideUp(200);
	    if ($("#arrow_class").attr("src")=="/bizhi/images/arrow_up.jpg")
	    {
		    $("#arrow_class").attr("src","/bizhi/images/arrow_down.jpg");
	    }
        
        /* msg box hide */
        $(".msg_box").removeClass("msg_box_active");
        $("#msg_panel").hide();

        /* user passport hide*/
        $(".passport-box").removeClass("passport-box-active");
        $("#passport-panel").hide();
    });
    
    //$(".passport-box").mouseenter(function(){
    $(".passport-box").live("mouseenter",function(){
        $(".msg_box").removeClass("msg_box_active");
        $("#msg_panel").hide();
        $(this).addClass("passport-box-active");
        $("#passport-panel").show();
    }).live("mouseleave",function(){
        $(this).removeClass("passport-box-active");
        $("#passport-panel").hide();
    }).click(function(e){
        e.stopPropagation();    
    });

	
    $(function () {
		$(".add_and_top").hide();
		$(window).scroll(function(){
			if ($(window).scrollTop()>500){
				$(".add_and_top").fadeIn(1000);
			}
			else{
				$(".add_and_top").fadeOut(1000);
			}
            var scrolltop = $(window).scrollTop();
            if(scrolltop >= 80){
                $(".class_title").css({
                    "position":"fixed",
                    "_position":"absolute",
                    "_top":scrolltop-80+"px",
                    "top":0+"px",
                    "width":"100%"
                });
            }else{
                $(".class_title").css({
                    "position":"relative",
                    "top":0
                });    
            }
		});
		$(".add_and_top .to_top_aside").click(function(){
			$('body,html').animate({scrollTop:0},1000);
			return false;
		});
        
        $(".add_and_top .add_img_aside").click(function(){
                            jQuery.facybox({ajax:'/user/login_box2'})
                    });
	});
    
    $(".class_alta").children(".class_more").attr("lang","1");
    $(".class_alta").hover(function(e) {
        var container = $(this);
        if(container.children(".class_more").attr("lang") != "1")
            return;
        else
            container.children(".class_more").attr("lang","0");
        container.children(".class_more").slideDown(200);
    },function() {
        var container = $(this);
        container.children(".class_more").slideUp(200,function(){
            $(this).attr("lang","1");
        });
    });
    
    $(".topic_dis, .topic_left, .wallpaper_dis,.wallpaper_dis_new,.wallpaper_dis_jp").hover(function(){
        $(this).children("a").children(".tag_info").children(".tag_info_mid").css("background", 'url(/bizhi/images/newpark/tagmid_hover.png)');
    },function(){
        $(this).children("a").children(".tag_info").children(".tag_info_mid").css("background", 'url(/bizhi/images/newpark/likebg.png)');
    });
    /* hover上去轮播 */
    $("#thumbs li a").mouseenter(function(){
        $(this).trigger("click");});

    // css like
    $(".wallpaper_dis,.wallpaper_dis_new,.wallpaper_dis_jp").live({
        mouseenter: function(){
            $(this).children(".like_info").addClass("wallpaper_dis_hover");
            $(this).children(".like_info").children(".author_info_like").show();
            $(this).children(".like_info").children(".like_num").show();
        }, mouseleave: function(){
            if($(this).children(".like_info").children(".author_info_like").hasClass("active")) {
            } else {
                $(this).children(".like_info").children(".author_info_like").hide();
            }
            $(this).children(".like_info").children(".like_num").hide();
            $(this).children(".like_info").removeClass("wallpaper_dis_hover");
        }
    });
    $(".like_info").live({
        mouseenter: function(){
            $(this).addClass("like_info_hover");
            $(this).removeClass("wallpaper_dis_hover");
        }, mouseleave: function(){
            $(this).addClass("wallpaper_dis_hover");
            $(this).removeClass("like_info_hover");
        }
    });
    function checkBalance(){
        if($(".msg_box").hasClass("msg_box_active")){
            $("#msg_panel").show();
        }else{
            $("#msg_panel").hide();    
        }
    }
    
    $(".alink").live('mousedown',function(){pv_click("web_msg_reason");});
    
    /* park like */
    $(".wallpaper_dis_new").live("mouseover",function(){
        $(this).find(".exp_like_info").css("display","block");    
    }).live("mouseout",function(){
        $(this).find(".exp_like_info").css("display","none");    
    });
    
    /* old_class,active_class */
    function process(obj,res,active_class){
        if(res == -2) {
            popup("未审核、审核不通过的壁纸不支持被喜欢");
            return;
        } else if(res == 2 || res == 1) {
            obj.addClass(""+active_class).attr("title","取消喜欢");
            popup("该壁纸已经被您喜欢");
        } else if(res == 3 || res == -1) {
            obj.removeClass(""+active_class).attr("title","喜欢");
            popup("该壁纸已经被您取消喜欢");
        } else {
            popup("网络异常，请刷新页面重试");
            return;
        }
    }

    /*  set like,else if 太多，难以维护 */
    var LIKE_FLAG = false;
    $(".unews_like,.like_info,.exp_info_like").live("click",function(){
        if(LIKE_FLAG) {
            return;
        }
        LIKE_FLAG = true;
        var obj = $(this);
        if(obj.hasClass("like_info")) {
            obj = $(this).children(".author_info_like");
            var wp_id = obj.attr("id");
            wp_id = wp_id.substring(5);
            if(obj.hasClass("active")) {
                var i = 1;
            } else {
                var i = -1;
            }
        } else if(obj.hasClass("unews_like")){
            var wp_id = $(this).attr("id");
            wp_id = wp_id.substring(5);
            if(obj.hasClass("unews_like_active")) {
                var i = 1;
            } else {
                var i = -1;
            }
        }else{
            var wp_id = $(this).attr("id");
            wp_id = wp_id.substring(3);
            if(obj.hasClass("exp_active")) {
                var i = 1;
            } else {
                var i = -1;
            }
        }
        $.ajax({
            url: "/user/isLogin/",
            success: function(msg) {
                if(msg == "suc") {
                    $.ajax({
                        url: "/detail/setLike/"+wp_id+"/"+i+"/",
                        success: function(res) {
                            if(res) {
                                if(obj.hasClass("exp_info_like")){
                                    process(obj,res,"exp_active");
                                    LIKE_FLAG = false;
                                    return;
                                }

                                if(res == -2) {
                                    popup("未审核、审核不通过的壁纸不支持被喜欢");
                                    return;
                                } else if(res == 2) {
                                    res = 1;
                                    obj.addClass("active").attr("title","取消喜欢");
                                    popup("该壁纸已经被您喜欢");
                                } else if(res == 3) {
                                    res = -1;
                                    obj.removeClass("active").attr("title","喜欢");
                                    popup("该壁纸已经被您取消喜欢");
                                } else {
                                    if(obj.hasClass("active")){
                                        obj.removeClass("active").attr("title","喜欢");
                                    }else{
                                        /* pingback */
                                        if(obj.hasClass("author_info_like")){
                                            pv_click('wpLike',wp_id);
                                        }
                                        obj.addClass("active").attr("title","取消喜欢");
                                    }   
                                }
                                
                                if(obj.hasClass("author_info_like")){
                                    var wp_ofc_like = parseInt(obj.next(".like_num").text()) + parseInt(res); 
                                    obj.next(".like_num").text(wp_ofc_like);
                                } else if(obj.hasClass("unews_like")) {
                                    if(res == 1) {
                                        obj.addClass("unews_like_active");
                                        /* pingback */
                                        pv_click('wpLike',wp_id);
                                    } else {
                                        obj.removeClass("unews_like_active");
                                    }
                                    var wp_ofc_like = parseInt(obj.text()) + parseInt(res); 
                                    obj.text(wp_ofc_like);
                                }

                            }
                            LIKE_FLAG = false;
                        },
                        error: function() {
                            LIKE_FLAG = false;
                        }
                    });
                } else {
                    $.facybox({ajax:'/user/login_box'});
                    LIKE_FLAG = false;
                }
            },
            error: function() {
                LIKE_FLAG = false;
            }
        });
    });

    var islogin = "";
    if(islogin){
        $.ajax({
            type:"get",
            url:"/uhome/checkMsg",
            success: function(msg){
                var msg_num  = msg["has_msg"];
                var cart_num = msg["cart_num"];
                if(cart_num > 0){
                    $("#cart_num_index").text(cart_num).show();
                }
                if(msg_num > 0){
                    $("#msg_num_index").text(msg_num).show();
                }
            }
        });
    }
    
    /* 页面pv/uv */
        var pv_flag = "detail";
        var pv_gif;
    switch(pv_flag){
        case 'index':pv_gif = 'web_pv';break;
        case 'changelog':pv_gif = 'web_cl_pv';break;
        case 'terms':pv_gif = 'web_terms_pv';break;
        case 'share':pv_gif = 'web_share_pv';break;
        case 'error_404':pv_gif = 'web_404_pv';break;
        case 'deal':pv_gif = 'web_deal_pv';break;
        case 'ucenter':pv_gif = 'web_ucenter_pv';break;
        case 'park':pv_gif = 'parkPage';break;
        case 'cate':pv_gif = 'catePage';break;
        case 'detail':pv_gif = 'detailPage';break;
        case 'label':pv_gif = 'labelPage';break;
        case 'user':pv_gif = 'userPage';break;
        case 'newly':pv_gif = 'newlyWall';break;
        case 'index_mall':pv_gif = 'index_mall';break;
        case 'mall_check_order':pv_gif = 'mall_check_order';break;
        case 'mall_order_pay':pv_gif = 'mall_order_pay';break;
        case 'userRank':pv_gif = 'userRank';break;
        default:
            pv_gif = '';
    }
    if(pv_gif){
        pv_click(pv_gif);
    }
});
