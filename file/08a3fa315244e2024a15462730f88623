<script type="text/javascript">
function htmlspecialchars(str)  
{  
    str = str.replace(/&/g, '&amp;');
    str = str.replace(/</g, '&lt;');
    str = str.replace(/>/g, '&gt;');
    str = str.replace(/"/g, '&quot;');
    str = str.replace(/'/g, '&#039;');
    return str;
}
var flag = true;
function LimitComment(field) {
    var maxlimit=100;
    if (field.value.length > maxlimit) {
	    field.value = field.value.substring(0, maxlimit);
        var timer = null;
        clearTimeout(timer);
        if(flag){
            popup("亲，最多输入100字");
            flag = false;
            timer = setTimeout(function(){
                flag = true;
            },5000);
        }
    } 
}

$(function(){
    var scrollLen = 160;
    function init(){
        var count = $(".unews_ppt_wrap").find(".wallpaper_ppt").length;
        var len = (count+1) * scrollLen;
        $(".unews_ppt_wrap").css("width",len+"px");

        var cur = 6;
        if(cur >= 6){
            $(".unews_ppt_wrap").css("left","-"+scrollLen*(cur-3)+"px");
        }else{
            $(".unews_ppt_pre,.unews_ppt_next").hide();    
        }
        
        var left = $(".unews_ppt_wrap").css("left");
        left = parseInt(left);
        if(left == 0 || count <6){
            $("#unews_ppt_pre").hide();
        }

        var rmost = -(count - 6) * scrollLen;
        var lmost = -(count - 1) * scrollLen;

        if(left <= rmost && left >=lmost){
           $("#unews_ppt_next").hide();     
        }

        /* scroll */
                    var top = 0;
                if(top >0 && top < 1200 ){
            $(document).scrollTop(top);
        }

        /* add label */
        var curLabelLen = $(".unews_labels p").length;
        if(curLabelLen >= 5){
            $(".unews_label_addwrap").hide();
        }else{
            $(".unews_label_addwrap").show();
        }

    }
    init();
    
    /* wallpaper ppt scroll pre and next */
    $("#unews_ppt_next").click(function(){
        if($(".unews_ppt_wrap").is(":animated")){
            return false;
        }

        var count = $(".unews_ppt_wrap").find(".wallpaper_ppt").length;
        if(count <=6){
            $(this).hide();
            return false;
        }
        /*
         * 1.最远: 最后一屏有1张壁纸。
         * 2.最近：最后一屏有6张壁纸。
         **/
        var rmost = -(count - 6) * scrollLen;
        var lmost = -(count - 1) * scrollLen;
        var left = $(".unews_ppt_wrap").css("left");
        left = parseInt(left);
        if(left < 0){
            $("#unews_ppt_pre").show();
        }
        
        /* 边界 */
        if(left <= rmost && left >=lmost){
            $(this).hide();
            return false;
        }

        $(".unews_ppt_wrap").animate({"left":"-="+ 6*scrollLen+"px"},function(){
            var left = $(".unews_ppt_wrap").css("left");
            left = parseInt(left);
            /*  滚动完之后再次判断边界 */
            if(left < 0){
                $("#unews_ppt_pre").show();
            }
            if(left <= rmost && left >=lmost){
                $("#unews_ppt_next").hide();
                return false;
            }   
        }); 
    });

    $("#unews_ppt_pre").click(function(){
        if($(".unews_ppt_wrap").is(":animated")){
            return false;
        }
        var left = $(".unews_ppt_wrap").css("left");
        left = parseInt(left);
        $("#unews_ppt_next").show();
        if(left == 0){
            $(this).hide();
            return false;
        }
        /* 保证最终滚动到第一张壁纸 */
        if(left >=-800 && left <=0){
            var c = 0 - left / scrollLen;
            $(".unews_ppt_wrap").animate({"left":"+="+scrollLen * c +"px"});
            $(this).hide();
            return false;
        }
        $(".unews_ppt_wrap").animate({"left":"+="+scrollLen * 6 +"px"},function(){
            /* 滚动完之后再次判断边界 */
            var left = $(".unews_ppt_wrap").css("left");
            left = parseInt(left);
            if(left == 0){
                $("#unews_ppt_pre").hide();
                return false;
            }
        });

    });
    
    /* wallpaper img scroll pre and next */
    $("#pre_wall").click(function(){
        var obj = $(".wallpaper_ppt.active").prev();
        if(obj !="" && obj !="undefined"){
            obj.trigger("click");
            //$("#unews_ppt_pre").trigger("click");
        }
    });

    $("#next_wall").click(function(){
        var obj = $(".wallpaper_ppt.active").next();
        if(obj !="" && obj !="undefined"){
            obj.trigger("click");
            //$("#unews_ppt_next").trigger("click");
        }
        
    });
    /* unew ppt link*/
    $(".unews_ppt_link").click(function(){
        var tops = document.documentElement.scrollTop+document.body.scrollTop;
        var data = $(this).attr('data') || this.data || this.attributes.data.nodeValue;
        window.location.href = "/detail/info/"+data+"?scroll="+tops;
        /* ie6 */
        return false;
    });

    /* comment-button */
    $(".more_wall_back").click(function(){
        var _this = $(this);
        $(".comment_all_back").slideToggle(function() {
            if($(".comment_all_back").css("display") == "none") {
                _this.addClass("more_wall_down");
            } else {
                _this.removeClass("more_wall_down");
            }
        });
    });
    
    /* add label */
    $(".unews_label_add").click(function(){
        $(this).hide();
        $.ajax({
            url: "/user/isLogin/",
            success: function(msg) {
                if(msg == "suc") {
                    $("#unews_label_in").toggle();$("#unews_label_cfm").toggle();
                }else{
                    $.facybox({ajax:'/user/login_box'});
                }
            }
        });    
    });

    $(document).click(function(e){
        var tar = e.srcElement || e.target;
        tar = $(tar);
        if(tar.is(".unews_label_add") || tar.is("#unews_label_in") || tar.is("#unews_label_cfm")){
            return false;    
        }
        $(".unews_label_add").show();
        $("#unews_label_in").hide();
        $("#unews_label_cfm").hide();
    });

    var addFlag = false;
    $("#unews_label_cfm").click(function(e){
        if(addFlag){
            popup("亲，标签不要修改太频繁");
            return false;
        }
        addFlag = true;
        setTimeout(function(){
            addFlag = false;
        },1000*8);

        var val = $("#unews_label_in").val();
        if(val == '' || val == null){
            return false;
        }

        var cnt = $(".unews_labels").find("p").length;
        var limit = 5;
        if(cnt >= limit){
            popup("单张壁纸最多添加"+limit+"枚标签");
            return false;
        }
        if(cnt >=0 && cnt < limit){
            var left_cnt = limit - cnt;
            val = val.replace(/，/g,",");
            val = val.replace(/[<>\/]/,"")
            var strArr = val.split(/,|\s+/);
            var len = strArr.length;
            var start = 0;
            var list = '';
            
            for(var i in strArr){
                if(!strArr[i]){
                    continue;
                }
                //var reg = /^(\w|[-_+.@#\$%\u4E00-\u9FA5])*$/;
                //if(!reg.test(strArr[i])){
                    //continue;
                //}
                start ++;
                if(start > left_cnt){
                    popup("单张壁纸最多添加"+limit+"枚标签");
                    break;
                }
                
                list += '<p>';
                list += '   <a class="unews_atlas_label" href="/label/search/?word='+strArr[i]+'" target="_blank">'+strArr[i]+'</a>';
                list += '   <a class="unews_label_del" href="javascript:void(0)" title="删除标签"></a>';
                list += '</p>';
            }
            
            $("#unews_label_in").val("");
            $(".unews_labels").append(list);
            
        }
        var res = [];
        $(".unews_labels p").each(function(){
            var text = $.trim($(this).find(".unews_atlas_label").text());
            if(text){
                res.push(text);
            }
        });
        res = res.join(",");

        var wp_id = 48103;
        sendAjax(wp_id,res);
        var curLabelLen = $(".unews_labels p").length;
        $("#unews_label_in").hide();$("#unews_label_cfm").hide();
        if(curLabelLen >= 5){
            $(".unews_label_addwrap").hide();
        }
        pv_click("addLabel_detail");
        
    });
    
    function sendAjax(wp_id,res){
        var label = encodeURIComponent(res);
        $.ajax({
            url: "/user/isLogin/",
            success: function(msg) {
                if(msg == "suc") {
                    $.ajax({
                        url:"/detail/addLabel/",
                        type:"post",
                        data:{wp_id:wp_id,label:label},
                        success:function(msg){
                            if(msg == "2"){
                                popup("亲，请先登录。");
                            }else if(msg == "3"){
                                popup("参数错误，请重试");
                            }else if(msg == "4"){
                                popup("亲，只能为自己的壁纸添加标签");
                            }else if(msg == "1"){
                                popup("修改标签成功");
                            }else if(msg == '5'){
                                popup("亲，你已经添加了5枚标签了");
                            }
                        },
                        error:function(){
                            popup("网络故障，请重试");
                        }

                    });
                }else{
                    $.facybox({ajax:'/user/login_box'});
                }
            },
            error:function(){
                popup("网络故障，请重试");
            }
        });
    }

    /* del label*/
    $(".unews_label_del").live("click",function(){
        $(this).parent().remove();
        
        var res = [];
        $(".unews_labels p").each(function(){
            var text = $.trim($(this).find(".unews_atlas_label").text());
            if(text){
                res.push(text);
            }
        });
        res = res.join(",");
        
        var wp_id = 48086;
        sendAjax(wp_id,res);
        $("#unews_label_in").hide();$("#unews_label_cfm").hide();
        var curLabelLen = $(".unews_labels p").length;
        if(curLabelLen < 5){
            $(".unews_label_addwrap").show();
        }
        pv_click("delLabel_detail");

    });


    /* 推荐壁纸展开 */
    $("#more_wall_ucenter").click(function(){
        $("#hide_wp").slideDown("normal",function(){
            var win = $(window);
            win.trigger('resize');
        });
        $(this).hide();
    });

    $(".unews_down").click(function(){
        var i = this.id.substring(10);
        var dl_id = "dl_"+i;
        var dl_cookie = wp_common.getCookie(dl_id);               
        if(dl_cookie == null) {
            wp_common.setCookie(dl_id, 1, 1);
            $.ajax({
                url: "/detail/SetDl/"+i+"/",
                success: function(msg) {
                    if(msg == "suc") {
                        var wp_ofc_dl = parseInt($("#unews_down"+i).text()) + 1; 
                        $("#unews_down"+i).text(wp_ofc_dl); 
                    }
                }
            });
        } 
    });
    
    /* 壁纸评论 */
    $("#more_wall_unews").click(function(){
        
        var cnt = 0;
        var cur = $(".unews_wrap_com").find(".comment_all").length;
        if(cur < cnt){
            $.ajax({
                method:"get",
                dataType:"json",
                url:"/detail/getComment/48103/"+cur+"/"+10,
                success:function(msg){
                    var str = '';
                    for(var i in msg){
                        var nikename = strdecode(msg[i].nikename);
                        str += "<div class='comment_all'><p class='comment_author'>";
                        str += "<a target='_blank' class='news_thumb' href='/uhome/index"+msg[i].uid+"'><img src='"+msg[i].picthumb+"' height='20' width='20' class='comment_head'/></a>";
                        str += "<a target='_blank' class='news_nick' href='/uhome/index/"+msg[i].uid+"'>"+nikename+"</a>";
                        if(msg[i].parent_comment_id > 0){
                            var puid = msg[i].uid2;
                            var puname = msg[i].nikename2;
                            puname = strdecode(puname);
                            str += '<span style="float:left;margin-left:10px;margin-top:2px">回复:</span>'; 
                            str += "<a target='_blank' class='news_nick' href='/uhome/index/"+puid+"'>"+puname+"</a>";
                        }
                        str += "</p>";
                        var comment_name = strdecode(msg[i].comment_name);
                        
                        str += '<div class="comment_detail">'+comment_name+'</div>';
                                                str +='</div><p class="comment_line_news"></p>';

                    }

                    //str +='</div><p class="comment_line_news"></p>';
                    $(str).insertBefore($(".unews_wrap_com").find(".more_wall_wrap"));
                    var cur = $(".unews_wrap_com").find(".comment_all").length;
                    if(cur < 0){
                        $("#more_comment_wrap").show();
                    }else{
                        $("#more_comment_wrap").hide();
                    }
                },
                error:function(){
                    popup("加载评论失败，请检查网络设置");    
                }

            });/* ajax end */
        }/* if end*/
        
    });
    
    $(".leave_comment_p").live("click",function(){
        var id = $(this).attr("id");
        $("#parent_id").val(id);
        var pobj = $(this).parent().find(".comment_author")[0];
        var pname = pobj.id;
        //$("#com_input").val("@"+pname+":").focus();
        $("#com_input").focus();
        $("#parent_unike").val(pname);
        var puobj = $(this).find(".leave_comment")[0];
        var puid = puobj.id;
        $("#parent_uid").val(puid);
    });

    $("#comment_submit").click(function(){
        var comment_name = $("#com_input").val();
        if(comment_name == ""){
            popup("你还没有输入评论内容");
            return;
        }
        var parent_id = $("#parent_id").val();
        var parent_uid = $("#parent_uid").val();
        var parent_unike = $("#parent_unike").val();
        
        var base64_comment_name = strencode(comment_name);

        $.ajax({
            url: "/detail/submitCommentNew/"+encodeURIComponent(base64_comment_name)+"/"+48103+"/"+parent_id+"/",
            success: function(msg) {
                if(msg) {
                    msg = eval("("+msg+")");
                    var str = '';
                    str += "<div class='comment_all'><p class='comment_author' id=''>";
                    str += "<a target='_blank' class='news_thumb' href=''><img src='' height='20' width='20' class='comment_head'/></a>";
                    str += "<a target='_blank' class='news_nick' href='/uhome/index/'></a>";
                    if(parent_id > 0){
                        var puid = $("#parent_uid").val();
                        var puname = $("#parent_unike").val();
                        str += '<span style="float:left;margin-left:10px;margin-top:2px">回复:</span>'; 
                        str += "<a target='_blank' class='news_nick' href='/uhome/index/"+puid+"'>"+puname+"</a>";
                    }
                    str += "</p>";

                    comment_name = htmlspecialchars(comment_name);
                    str += '<div class="comment_detail">'+comment_name+'</div>';
                    
                                        str +='</div><p class="comment_line_news"></p>';
                    
                    /*
                    if($(".unews_wrap_com").find(".comment_all")) {
                        $(str).insertBefore($(".unews_wrap_com").find(".more_wall_wrap"));
                    } else {
                        //why?
                        $(str).insertBefore($(".unews_wrap_com").find(".more_wall_wrap"));
                    }
                    */
                    if($(".comment_all_back").text().trim() == '') {
                        $(".comment_all_back").html(str);
                    } else {
                        $(".comment_all_back .comment_all:first-child").before(str);
                    }
                    $(".more_wall_back").show();
                    $(".more_wall_down").trigger("click");
                    $("#parent_id,#parent_uid,#parent_unike").val("0");
                    $("#com_input").val("").focus();
                } else {
                    popup("评论失败");
                }
            },
            error: function(msg) {
                popup("评论失败");
            }
        });
        pv_click('wpComment',48103);
    });
    $(".bds_tsina,.bds_tqq,bds_qzone,.bds_renren,.bds_douban,#bdshare_l_c li a").mousedown(function(){
        setTimeout(function(){
            pv_click("wpShare",48103);
        },700);
    });

});
