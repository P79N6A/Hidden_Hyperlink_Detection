	<a onclick="gameDownloadFunc();" class="btn-gotoInstall"><i>立即安装</i></a>

<script>
	//从中间页URL中获取inviteChannel的值
	//http://gamevip.vivo.com.cn/assets/html/gameCenter.html?inviteOpenid=c597ee4e5c232ceb&inviteTimestamp=1483065621332&inviteType=1&inviteChannel=1

	//自动调起 游戏中心
//	var jump2VipGClink = 'vivogame://game.vivo.com/openjump?j_type=9&t_from=com.vivo.game&h5_link=';
	var jump2VipGClink = 'vivogame://game.vivo.com/openjump?j_type=9&h5_link=';
	jump2VipGClink += encodeURIComponent('https://gamevip.vivo.com.cn/vip/auth/do.do?inviteChannel=' + getParameter('inviteChannel'));

	location.href = jump2VipGClink;

	
	
	//未下载 或 版本过低 用户需自行点击 下载
	var gameDownloadLink = 'http://gamecenter.vivo.com.cn/clientRequest/gameDownload?id=9513&pkgName=com.vivo.game';
	function gameDownloadFunc(){
		location.href = gameDownloadLink;
	}

	function getParameter(name){
		var search = document.location.search;
		var pattern = new RegExp("[?&]"+name+"\=([^&]+)", "g");
		var matcher = pattern.exec(search);
		var items = null;
		if(null != matcher){
			items = decodeURIComponent(matcher[1]);
		}
		return items;
	}
</script></body></html>