  <script>
$(document).ready(function(){
	$('#return').click(function(){
		$("html,body").animate({scrollTop:0},500);
		$(this).show();	
	});
	var $retop = $('.scrollfixed');
	$(window).scroll(function(){
		if ($(document).scrollTop() === 0){
			$retop.fadeOut()
		}else{
			$retop.fadeIn()
		}
		if ($.browser.msie && $.browser.version == 6.0 && $(document).scrollTop() !== 0){
			$retop.css({ 'opacity': 1 })
		} 
	});	
});
</script> 
