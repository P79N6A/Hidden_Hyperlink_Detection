    				if(gets){
    					if(!reg.test(gets)){
    						result = "请填写正确的手机号码";
    					}
    				}
    				return result;
    			},
    			"vcode":function(gets,obj,curform,regxp){
    				var vcodeLength = gets.length;
    				if(vcodeLength == 4){
    					return true;
    				}else{
	    				return "请输入四位验证码";
    				}
    			}
    		},
    		beforeSubmit:function(curform){
    			if($("#nobaseservice").find("div.form-item").size() >= 1 && $("#nobaseservice").attr("jt-base") == "true"){
                	/*window.scrollTo(0,0);
