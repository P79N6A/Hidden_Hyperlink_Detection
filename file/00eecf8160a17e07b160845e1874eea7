        	btnSubmit:"#cy-submit",
    		tiptype:2,
    		datatype:{
    			"carno":function(gets,obj,curform,regxp){
    				var error = true;
    				var reg = /^[\u4E00-\u9FBF][A-Za-z][A-Za-z0-9]{4}[A-Za-z0-9港澳挂学]$/;
    				if(!reg.test(gets)){
    					error = "请输入正确的车牌号码";
    				}
                    return error;
    			},
    			"frameno":function(gets,obj,curform,regxp){
    				var framenoLength = gets.length;
    				if(framenoLength == 6){
    					return true;
    				}else{
	    				return "请输入车架号后六位";
    				}
    			},
    			"engineno":function(gets,obj,curform,regxp){
    				var enginenoLength = gets.length;
    				if(enginenoLength == 6){
    					return true;
    				}else{
	    				return "请输入发动机后六位";
    				}
    			},
