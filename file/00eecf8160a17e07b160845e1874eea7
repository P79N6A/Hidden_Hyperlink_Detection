                	$("#baseservice").hide();
                    $("[jt-control-type=summany] tr.hide").removeClass("hide").addClass("show");
                	$(this).before('<button class="btn btn-big btn-red" type="button" id="before_button">上一步</button>');
                	$("#before_button").click(function(){
                		$("#nobaseservice").hide().attr("jt-base","true");
                    	$("#baseservice").show();
                        $("[jt-control-type=summany] tr.show").removeClass("show").addClass("hide");
                    	$(this).remove();
                	});*/
                }else{
                    $("#cy-subtip").show();
                    var params=$("#cy-form").serialize();     
                    $("#cy-submit").css({'background':"#999"});
                    if(isSubmit === true){
                  	   isSubmit = false;
	                    $.getJSON("http://www.53chewu.com//index.php?jsoncallback=?",params,function(result){
	                    	$("#cy-submit").show().css({'background':'#1c8dd5'});
	                        $("#cy-submit").show();
	                        $("#cy-subtip").hide();
	                        if(result.success){
	                           
	                            window.location.href ="http://www.53chewu.com/"+result.drumpurl;
	                        }
	                        else{
	                        	isSubmit = true;
	                            alert(result.message);
	                        }
	                    });
                    }
                }
    			return false;
    		}
    	});
         
        $("[jt-carnoarea=1]").on("change", function () {
            var v = $(this).val();
            renderBody(v);
        });
        renderBody($("[jt-carnoarea=1]").val());
        $("[data-upchar='1']").bind("keyup", function (e) {
            var v = $(this).val();
            $(this).val(v.toUpperCase());
        });
        form_el = $("#form");
        form_el.on("submit", function (e) {
            e.preventDefault();
            var p = form_el.serialize();
            $.post('http://www.53chewu.com/index.php', p, function (r) {
                r = JSON.parse(r);
                if (r.success) {
                    alert(r.message);
                    window.location.href ="http://www.53chewu.com/"+ r.drumpurl;
                }
                else {
                    alert(r.message);
                }
            });
            return false;
        }); 
        
        $(".tab-menu a").click(function(){
    		var $this = $(this);
    		var hoverId = $(".tab-menu .hover").removeClass("hover").attr("jt-for");
    		var forId = $this.attr("jt-for");
    		
    		$this.addClass("hover");
    		$("#"+hoverId).hide();
    		$("#"+forId).fadeIn();
    	});
    });
</script>  
 </body>
</html>