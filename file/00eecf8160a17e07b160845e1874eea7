    				var reg = /[\u4E00-\u9FA5\uF900-\uFA2D]/;
    				if(gets){
    					if(!reg.test(gets)){
    						result = "请输入中文姓名";
    					}
    				}
    				return result;
    			},
    			"mobile":function(gets,obj,curform,regxp){
    				var result = true;
