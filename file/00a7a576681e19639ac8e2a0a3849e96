  <script type="text/javascript">
	function show_note() {
		$("#del-modal").modal("show");
	}
	function add_note() {
	    var id = $('#news').val(); //文章id
		var news_title = $('#news_title').text(); //文章标题
		var content = $("[name=note_content]").val();
		var phone = $("[name=phone]").val();
		var userid = $("#userid").val();
		$.post("/news/do_note.html",{"aid":id,"content":content,"atitle":news_title,"phone":phone,"uid":userid},function(msg){
		   var html = "";
		   $.each(msg,function(key,value){
		      html += "<li>";
		      html += "<div style='float:left;height:4em;margin-right:0.5em;'><img src='"+value.img+"' width='30'></div>";
		      html += "<div style='font-size:1em;float:left;width:50%;color:#8ac007'>"+value.phone+"</div>";
		      html += "<div style='font-size:1em;float:left;width:60%;color:#3e3e3e;'>"+value.content+"</div>";
			  html += "<div style='float:left;width:70%;font-size:0.5em;color:#8ac007'>"+value.time+"</div>";
			  html += " <div style='clear:both;'></div>";
		      html += "</li>";
			  
		   });
		   $('#action_add').html(html);
		   $("#del-modal").modal("hide");
		});
	}
$(function(){
	//是否可以编辑
	$('#editable').bind('click',function(){
      if($(this).val() == 1){
	   // $(this).val('0');
	   // $(this).text('编辑');
		var content = $('#wx_article').html();
		var news_id = $('#news').val();
		var title = $('.n_title').text();
		$.post("/news/edit.html",{"body":content,"id":news_id,"title":title},function(msg){
		  if(msg){
		   $('#wx_article').html(msg.body);
		   $('#.n_title').text(msg.title);
		  }
		});
		$("#edit-modal").modal("show");
	  }else{
	    $('#wx_article').attr('contenteditable',true);
	    $('.n_title').attr('contenteditable',true);
	    $(this).val('1');
		$(this).text('确定');
	 }
	});
	
	//删除图片
	$('#wx_article').on('click','img',function(){
	    if($('#wx_article').attr('contenteditable') == true){
		   $(this).remove();
		}
	});
	
	//完成编辑
	$('#confirm').bind('click',function(){
	   //$(this).css('display','none');
	   $('#wx_article').attr('contenteditable',false);
	   $('.n_title').attr('contenteditable',false);
	   $('#editable').css('display','none');
	   //将编辑属性保存
	   var news_id = $('#news').val();
	   var content_attr = $('#region-edit').html();  //可编辑属性
	   $.post("/news/edit.html",{"content_attr":content_attr,"id":news_id},function(msg){
	      $('#region-edit').html(msg.content_attr)
	   });
	   $("#edit-modal").modal("hide");
	});
	

	//change uptime
	$("#post-date").html("2017-03-28");
	//change author
	$("#post-user").html("");
	//iframe add class
	$("iframe").addClass('video_iframe');

	
	//complain();

	$.each($("img"), function(index, item) {
		var img_suffix = "http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl=";
		$(item).attr("src", function() {
			if(typeof($(this).attr("data-src")) != "undefined") {
				return img_suffix+$(this).attr("data-src");
			}
			if(typeof($(this).attr("src")) != "undefined" && $(this).attr("src").indexOf("qpic.cn") < 20 && $(this).attr("src").indexOf("qpic.cn") >0) {
				return img_suffix+$(this).attr("src");	
			}
				
			if(typeof($(this).attr("src")) != "undefined" && $(this).attr("src").indexOf("qpic.cn") >= 20) {
				return $(this).attr("src");	
			}	
			
		});
	});
	
	$.each($(".video_iframe, iframe[data-src]"), function(index, item) {
		//$(item).removeAttr("style");
		$(item).attr("width", "100%");
		$(item).attr("height", "260");
		$(item).attr("src", function() {
			if(typeof($(this).attr("data-src")) != "undefined") {
				return $(this).attr("data-src").replace("height", "").replace("width", "").replace("preview.html", "player.html");
			}
			if(typeof($(this).attr("data-supervoteid")) != "undefined") {
				return "";
			}
			return $(this).attr("src").replace("height", "").replace("width", "").replace("auto", "");
		});
	});

	$.each($(".vote_iframe"), function(index, item) {
		$(item).remove();
	});
	
	if($("#all_screen_video").length > 0){
		$("#video_if").attr('src',function(){
			return $(this).attr("src").replace("height", "").replace("width", "").replace("auto=0", "auto=1");
		});

		$("#video_close").click(function(){
			$("#video_if").attr('src',function(){
				return $(this).attr("src").replace("auto=1", "auto=0");
			});
			$("#all_screen_video").hide();
			show();
		});
	}else{
		
	}
	

	window.onscroll = function() {
        var f = document.documentElement.scrollTop || document.body.scrollTop;
        var d = document.documentElement.clientHeight;
        var g = document.documentElement.offsetHeight;
        if (g - (f + d) <= 0) {
            document.getElementById("img_b").style.display = "none";
        } else {
			if(!isC) {
				document.getElementById("img_b").style.display = "block";
			}
        }
    }

	var e = $("#hhhh");
	$("#wx_erweima").click(function(){
		e.show();
	});
	$('#erweima_bor').click(function(){
		e.hide();
	});

	$("#zhan").click(function(){
		var t = $(this).attr('t');
		if(t == 1){
			$("#tu_bor").animate({width:"100%"},1000);
			$(this).children('img').attr('src','/templets/images/jian.png');
			
			$(this).attr('t','2');
		}else{
			$("#tu_bor").animate({width:"52px"},700);
			$(this).children('img').attr('src','/templets/images/jia.png');
			
			$(this).attr('t','1');
		}
	});

	$.each($("section"),function(index,item){
		$(item).attr('style',function(){
			return $(this).attr("style").replace("max-width", "shanchukuan").replace("width", "shanchukuan2");
		});
	});
	$.each($("iframe"),function(index,item){
		$(item).attr('style',function(){
			return $(this).attr("style").replace("max-width", "shanchukuan").replace("width", "shanchukuan").replace("height", "shanchugao");
		});
	});
		$.each($("iframe"),function(index,item){
		$(item).attr('src',function(){
			return $(this).attr("src").replace("width", "shanchukuan").replace("height", "shanchukuan")
		});
	});

});

//append complain button
function complain(){
	var btn = "<a style='margin:0px;float:right' class='media_tool_meta meta_primary' href='/news/complain/id/387580.html'>投诉</a>";

	$("#js_sg_bar").append(btn);
}

//show article
function show(){
	$("#border").removeClass('hidden');
}

var isC = false;
function cB() {
	$("#img_b").hide();
	isC = true;
}
