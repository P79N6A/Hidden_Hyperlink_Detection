                layer.alert("不能对自己的店铺进行点评");
            }
            if (forbid_dianping==2) {
                layer.alert("您今天点评的太多了，明天再来吧");
            }
            if (forbid_dianping==3) {
                layer.alert("您已经点评过此店铺了");
            }
            if (forbid_dianping != 0) {
                return false;
            }

            if(star == 0){
                $(".startip").html("<span style='color:#f00;'>请给店铺评分</span>");
                return false;
            }

            if($.trim(content) == ''){
                $(".dianpingtip").html("<span style='color:#f00;'>请给店铺点评</span>");
                return false;
            }

            $.ajax({
                type:'post',
                dataType: 'json',
                url:"http://wuhan.liebiao.com/article/dianpudpinsert/",
                data: {dianpuid:dianpuid,userid:userid,star:star,content:content},
                success: function(data) {

                    if(data.error == 0){
                        layer.alert("点评成功");

                        var username = getCookie('_u') ? getCookie('_u') : "匿名";
                        var lihtml= "";
                        lihtml = lihtml + "<li><div class='produce'>";
                        lihtml = lihtml + "<p class='produce-title'>";
                        lihtml = lihtml + "<span class='user-name'>" + username + "</span>";
                        lihtml = lihtml + "<i style='background-position:" + svalue + "'></i>";
                        lihtml = lihtml + "<span class='day'>" + data.date + "</span>";
                        lihtml = lihtml + "</p>";
                        lihtml = lihtml + "<p class='word'>" + content + "</p>";

                        lihtml = lihtml + "</div></li>";
                        $(".pingjia li").first().before(lihtml);
                        $(".pingjia li").eq(2).nextAll().hide();
                        $(".pingjia li").last().show();
                        $("#content").val("");
                        $(".forbid_dianping").val("3");
                        $(".in-left i").css("background-position", "0 -75px");
                        $(".star").val("0");
                    } else {

                    }
                }
            });

            return false;
        });

        $("#content").keyup(function(){
            $(".dianpingtip").html("");
        });

        $(".in-left i span strong").click(function(){
            var myindex = $(this).index();
            var posy = "0 0px";
            var star = 5;
            var startip = "";

            switch(myindex){
                case 0 :
                    posy = "0 -61px";
                    startip = "1分：非常不满意";
                    star = 1;
                    break;
                case 1 :
                    posy = "0 -45px";
                    startip = "2分：比较不满意";
                    star = 2;
                    break;
                case 2 :
                    posy = "0 -30px";
                    startip = "3分：一般";
                    star = 3;
                    break;
                case 3 :
                    posy = "0 -15px";
                    startip = "4分：比较满意";
                    star = 4;
                    break;
                default :
                    posy = "0 0px";
                    startip = "5分：非常满意";
                    star = 5;
                    break;
            }

            $(".in-left i").css({"background-position":posy});
            $(".startip").html(startip);
            $(".star").val(star);

        });

        var numli = $(".pingjia li").size();
        var isshow = 0;

        if(numli > 4)
        {
            $(".pingjia li").eq(2).nextAll().hide();
            $(".pingjia li").last().show();

            $(".pingjia li.more").click(function(){
                $(".pingjia li").eq(2).nextAll().toggle();
                $(".pingjia li").last().show();

                if(isshow == 0){
                    $(".pingjia li.more a").html("收起∧");
                    isshow = 1;
                } else {
                    var mtop = $(".dianping").offset().top+38;
                    $("html,body").scrollTop(mtop);
                    $(".pingjia li.more a").html("更多∨");
                    isshow = 0;
                }
            });
        }
        else
        {
            $(".pingjia li.more").css({"display":"none"});
        }
    });

    //宠物详情页电话弹层
    $(function() {

        //去掉后面的“/”
        var info = $.trim($(".lxr_coninfo").html());
        var inlen = info.length;
        if(info[inlen-1] == '/')
        {
            var info_new = info.slice(0,inlen-1);
            $(".lxr_coninfo").html(info_new);
        }

        layer.config({
            extend: ['skin/myskin/style.css'], //加载新皮肤
        });

        $("#btn-pn,#layer-phone").click(function(event){
            event.stopPropagation();
            var dianhuatanceng=layer.open({
                type: 1,
                closeBtn : false,
                shift: false,
                fix: false,
                skin: 'layer-ext-custoa',
                area: ['auto', 'auto'],
                shade: false,
                title: false,
                content: $("#layer-phone")
            });
            $("body,#btn-close").click(function(event){
                event.stopPropagation();
                layer.close(dianhuatanceng);
            });

            var saomasrc = $(".pic-saoma").attr("src");
            if(saomasrc){
                return false;
            }

            var postid = $(this).attr("postid");
            var phone = $(this).attr("phone");
            var url = "/ajax/weixinqrcode/";
            $.ajax({
                type: "post",
                url: url,
                data: {postid:postid, phone:phone},
                dataType: "json",
                success: function(data){
                    $(".pic-saoma").attr("src", data.url);
                }
            });
        });
        var $parameter_table=$('.parameter-table');
        if ($parameter_table.children().get(0)) {
            $parameter_table.show();
        }else {
            $parameter_table.hide();
        }
    });

    /*宠物详情页导航效果*/

    $(function(){

        var divTop = $("#xiangqing").offset().top;

        $("a.xiangqing").click(function () {

            var doctop = window.pageYOffset;

            var top1 = $("#xiangqing").offset().top - 45;
            var top2 = $("#xiangqing").offset().top - 86;
            var f_position = $(".chongwu-detail-div").css("position");
            if (f_position == "fixed") {
                $("html,body").scrollTop(top1);
            } else {
                $("html,body").scrollTop(top2);
            }

            $(".chongwu-detail-div").find('span').eq(0).addClass('current').siblings().removeClass('current');
        });

        $(".tupian").click(function () {

            var doctop = window.pageYOffset;

            var top1 = $("#tupian").offset().top - 45;
            var top2 = $("#tupian").offset().top - 86;
            var f_position = $(".chongwu-detail-div").css("position");
            if (f_position == "fixed") {
                $("html,body").scrollTop(top1);
            } else {
                $("html,body").scrollTop(top2);
            }

            $(".chongwu-detail-div").find('span').eq(1).addClass('current').siblings().removeClass('current');
        });


        function float_detail_menu($float_div_obj,$div_obj,className,$img_obj)
        {
            if($float_div_obj.length == 0 || $div_obj.length == 0) {
                return false;
            }
            $(window).on('scroll', function () {
                var doc_ScrollTop = $(this).scrollTop();
                var div_ScrollTop = $div_obj.offset().top-56;

                var dtop = [];
                dtop[0] = $("#xiangqing").offset().top-50;
                if($img_obj.length != 0){
                    dtop[1] = $("#tupian").offset().top-90;
                }

                if (doc_ScrollTop > div_ScrollTop) {
                    $float_div_obj.addClass(className);
                } else if (doc_ScrollTop <= div_ScrollTop) {
                    $float_div_obj.removeClass(className);
                }
                if($img_obj.length != 0){
                    if(doc_ScrollTop > dtop[1]){
                        $float_div_obj.find('span').eq(1).addClass('current').siblings().removeClass('current');
                    } else if (doc_ScrollTop > dtop[0]) {
                        $float_div_obj.find('span').eq(0).addClass('current').siblings().removeClass('current');
                    }
                } else {
                    if(doc_ScrollTop > dtop[0]){
                        $float_div_obj.find('span').eq(0).addClass('current').siblings().removeClass('current');
                    }
                }

            });
        }

        float_detail_menu($(".chongwu-detail-div"), $("#contentDetail"), "chongwu-detail-div-cur", $('#tupian'));


    });

</script> 
