<script>
    $(window).load(function () {
        $('.qode_carousels').find('.slides').carouFredSel({
            circular: true,
            responsive: true,
            scroll: {
                items: 1,
                duration: 1000,
                pauseOnHover: false
            },
            items: {
                <!-- width: 100, -->
                visible: {
                    min: 1,
                    max: 9
                }
            },
            auto: true,
            mousewheel: false,
            swipe: {
                onMouse: true,
                onTouch: true
            }
        }).animate({
            'opacity': 1
        }, 1000)

        $('.caroufredsel_wrapper').height($('.qode_carousels ul li.item').outerHeight());
    });

    /*******banner图*******/
    $('.flexslider').flexslider({
        animation: "fade",
        slideshow: true,
        animationSpeed: 1000,
        slideshowSpeed: 5000,
        animationSpeed: 1000, //滚动时间ms
        animationLoop: true,
        touch: true //是否支持触屏滑动
    });
    $('.flex-control-nav li a').html('');
    /*************case_sec**************/
    var mySwiper = new Swiper('.swiper-container', {
        nextButton: '.swiper-button-next',
        prevButton: '.swiper-button-prev',
        slidesPerView: 3,
        paginationClickable: true,
    });

    /**********partner_sec*****************/
    $('.flexslider2').flexslider({
        animation: "slide",
        slideshowSpeed: 10000, //展示时间间隔ms
        animationSpeed: 1000, //滚动时间ms
        controlNav: false,
        directionNav: true
    });
    $('.flex-direction-nav li a').html('');


    var $window = $(window);
    $window.scroll(function () {
        var h = $('.counters').offset().top;
        var h2 = $('.business_list').offset().top;
        var h3 = $('.swiper-container').offset().top;

        if ($(this).scrollTop() > h - 300 && $(this).scrollTop() < h + 500) {
            $(".counter1").stop().animate({count: 561}, {
                duration: 500,
                step: function () {
                    $(".counter1").text(String(parseInt(this.count)));
                }
            });
            $(".counter2").stop().animate({count: 97}, {
                duration: 500,
                step: function () {
                    $(".counter2").text(String(parseInt(this.count)));
                }
            });
            $(".counter3").stop().animate({count: 2016}, {
                duration: 500,
                step: function () {
                    $(".counter3").text(String(parseInt(this.count)));
                }
            });
            $(".counter4").stop().animate({count: 982}, {
                duration: 500,
                step: function () {
                    $(".counter4").text(String(parseInt(this.count)));
                }
            });
            $(".counter5").stop().animate({count: 80}, {
                duration: 500,
                step: function () {
                    $(".counter5").text(String(parseInt(this.count)) + "%");
                }
            });
        } else {
            $(".counter1").stop().animate({count: 0}, {
                duration: 100,
                step: function () {
                    $(".counter1").text(String(parseInt(this.count)));
                }
            });
            $(".counter2").stop().animate({count: 0}, {
                duration: 100,
                step: function () {
                    $(".counter2").text(String(parseInt(this.count)));
                }
            });
            $(".counter3").stop().animate({count: 0}, {
                duration: 100,
                step: function () {
                    $(".counter3").text(String(parseInt(this.count)));
                }
            });
            $(".counter4").stop().animate({count: 0}, {
                duration: 100,
                step: function () {
                    $(".counter4").text(String(parseInt(this.count)));
                }
            });
            $(".counter5").stop().animate({count: 0}, {
                duration: 100,
                step: function () {
                    $(".counter5").text(String(parseInt(this.count)) + "%");
                }
            });
        }
        if ($(this).scrollTop() > h2 - 400 && $(this).scrollTop() < h2 + 400) {
            $('.business_txt').addClass('cur');
        }
        if ($(this).scrollTop() > h3 - 300 && $(this).scrollTop() < h3 + 300) {
            $('.swiper-slide').addClass('act');
        }
    });

</script>
