				$(this).children('ul').show();
	},function(){
		$(this).children('ul').hide();
		if(!($.browser.msie && $.browser.version < 9)){
					}
	});

	var ulheight = 0;
	$("#nav_layer3FA50F9C180970DA3D6F7EB342A9711A").find('li.wp_subtop').each(function(){
		ulheight += $(this).outerHeight() + parseInt($(this).css('margin-bottom'));
	});
	$("#nav_layer3FA50F9C180970DA3D6F7EB342A9711A").height(ulheight);
	
	/*
	$("#nav_layer3FA50F9C180970DA3D6F7EB342A9711A").find('li').each(function(){
