  <script>


    function shouCang(postid){
        $.ajax({
            type: 'get',
            dataTyoe: 'json',
            async : false,
            data: {action:'add', postid:postid},
            url: "/cgi-bin/ajax/ajax_user_favor.php?",
            success: function(data) {
                if (data.error == 0) {
                    //收藏成功 junping
                    alert('收藏成功，你可以在用户中心，我收藏的信息 中查看。');
                } else {
                    //用户未登录 junping
                    if (data.data == '99') {
                        $.openDOMWindow({
                            positionType:'centered',
                            height:380,
                            width:640,
                            modal:1,
                            windowSourceID : '#Login_registration_reminder',
                            overlayOpacity:20,
                            windowPadding:5
                        });
                    }
                }
            }
        });
    }
</script> 
  <script>
var url_obj = {"fuli":"","hangye":"","xingzhi":"","worklength":"","xueli":"","salary":"","date":"","key":""};
var fuli_arr = [""];

var dis_pinyin = {"pinyin":null,"cat_pinyin":"siji"};

//批量投递简历

var jobid = [];

$("#apply_job").click(function(){
    var jobid_str = jobid.join(',');

    if (!jobid_str) {
        alert('请选择职位');
        return;
    }

    applyWork(jobid_str);
});

/*全选*/
$(function(){
    var $wsel = $('.wsel');
    $wsel.click(function(){


        var now_jobid = $(this).attr('value');

        if($(this).hasClass('iselect')){
            $(this).removeClass('iselect');
            $('.seleAll').removeClass('iselect');


            for (var i in jobid) {
                if (jobid[i]==now_jobid) {
                    jobid.splice(i,1);
                    break;
                }
            }


        }else{
            $(this).addClass('iselect');


            jobid.push(now_jobid);

            if($(".tablist .iselect").length == $wsel.length){      //每个单选都选中时
                $('.seleAll').addClass('iselect');
            }
        }
    });

    $('.seleAll').find('span').click(function(){
        jobid = [];
        if($(".tablist .iselect").length < $wsel.length){

            $(".jobinfo").each(function(){
                jobid.push($(this).attr('value'));
            });

            $('.seleAll').addClass('iselect');
            if($wsel){
                $wsel.addClass('iselect');
            }
        }else if($(".tablist .iselect").length == $wsel.length){
            $('.seleAll').removeClass('iselect');
            if($wsel){
                $wsel.removeClass('iselect');
            }
        }
    })
})


</script> 
