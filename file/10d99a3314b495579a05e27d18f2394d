  <script>

	var domain = 'http://www.liebiao.com';
	$(function() {
		bindSk();
		if(getCookie('the-cookie') == null){
			$("#activity-sjf").show(200);
			$(".close-notice").click(function(){
				$("#activity-sjf").hide(400);
				setCookie('the-cookie','1',86400);
			})}

	});

	function search() {
		var $sk = $("#search_keyword"),catname = '';
		if ($sk.val() == default_search_word) {
			$sk.val('').focus();
			return false;
		}
				catname =  'zhaopin_';
				location.href = '/sou/' + catname + $sk.val() + '/';
		return false;
	}

	function bindSk () {
		var $sk = $("#search_keyword");
		$("#search_keyword").focus(function() {
			if ($sk.val() == default_search_word) {
				$sk.val('');
			}
		}).blur(function(){
					if ($sk.val().length==0){
						$sk.val(default_search_word);
					}
				});
	}
	$(document).ready(function(){
		default_search_word = ($('.header h1').html() != null) ? '搜索'+ $('.header h1').html().replace('跳蚤市场','二手') + '信息' : '输入类别或关键词试试';
		//$("#search_keyword").val(default_search_word);
	});

</script> 
