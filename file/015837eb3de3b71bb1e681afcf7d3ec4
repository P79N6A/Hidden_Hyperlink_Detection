	$(".small_list").mouseover(function(){
		$(this).removeClass("small_list").addClass('over').find(".small_list_left a").addClass("cf");
	})
	$(".small_list").mouseleave(function(){
		$(this).removeClass("over").addClass('small_list').find(".small_list_left a").removeClass("cf");
