            var sctop = null;
            $(function(){
                sctop = $('section').scrollTop()
                $('section').scroll(function(){
                    var vv = $('section').scrollTop();
                    sctop = vv
                    return sctop
                })
            })

            // 收藏 / 取消收藏
            function favorite(em,id){
                if($("#comment_login").val()=='' ){
                    login();
                    return false;
                }
                //var fav_count = parseInt($(em).html());
                type=$(em).attr("data");
                $.get("/ajax_new/ajax_action.php?action=favorite", {
                    id: id,type:type
                }, function(data) {
                    console.log(data);
                    if (data.is_error==0) {
                        if(type == '0'){
                            $(em).addClass("D_col2");
                            $(em).attr("data","1");
                        }else{
                            $(em).removeClass("D_col2");
                            $(em).attr("data","0");
                        }
                    }else {
                        tips(data.msg);
                        return false;
                    }
                },'json');

                $('section').scrollTop(sctop) 
            }
