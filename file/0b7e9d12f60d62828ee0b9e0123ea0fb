      <script language="javascript"> 
  function gamemsg(){
  	var aid=164742;
  	$.ajax({
  			url: 'http://api.joyme.com/collection/api/gamearchive/gameinfo',
  			type: "post",
  			async: false,
  			data: {archiveid:aid},
  			dataType: "jsonp",
  			jsonpCallback: "getGameInfo",
  			success: function (req) {
  				data = req[0];
  				if(data.rs == 1 && data.result && data.result.gameinfo){
  					var gameinfo = data.result.gameinfo;
  					// 游戏类型
  					var gtype = [];
  					for(var i in gameinfo.categoryTypeSet){
  						gtype.push(gameinfo.categoryTypeSet[i].value);
  					}
  					// 平台类型
  					var platformtype = [];
  					for(var i in gameinfo.platformMap){
  						var platformMapitem = gameinfo.platformMap[i];
  						for(var x in platformMapitem){
  							if(platformMapitem[x].desc == 'iPhone'){
  								platformtype.push('IOS');
  							}else{
  								platformtype.push(platformMapitem[x].desc);
  							}
  						}
  					}
  					
  					var wikimsg = '<div class="tj-wiki"><a href="http://m.joyme'+env+'/collection/'+gameinfo.gameDbId+'"><cite><img src="'+gameinfo.gameIcon+'"></cite></a><div class="wiki-text"><a href="http://m.joyme'+env+'/collection/'+gameinfo.gameDbId+'"><font>'+gameinfo.gameName+'</font></a><span class="wiki-type"><cite>类型：'+gtype.join(',')+'</cite><cite>平台：'+platformtype.join(',')+'</cite></span></div></div>';
  					
  					// wiki
  					if(gameinfo.wikiUrl){
  						wikimsg += '<a href="'+gameinfo.wikiUrl+'" class="link-wiki" target="_blank">进入WIKI</a>';
  					}
  					
  					$('#_wikimsg').html(wikimsg);
  					$('#_wikimsg').show();
  					$('#_wikimsg').next('div').show();
  				}
  				if(data.rs == 1 && data.result && data.result.giftlist){
  					var giftlist = data.result.giftlist;					
  					var giftmsg = '<h3 class="tj-tit">游戏礼包</h3><div>';
  					
  					for(var i in giftlist){
  						if(i > 3) break;
  						giftmsg += '<a href="http://www.joyme'+env+'/gift/'+giftlist[i].gid+'" target="_blank">'+giftlist[i].title+'<cite class="get-gift">领</cite></a >';
  					}
  					giftmsg += '</div>';
  					$('#_giftmsg').html(giftmsg);
  					$('#_giftmsg').show();
  					$('#_giftmsg').next('div').show();
  				}
  			},
  			error: function () {
  				console.log('getGameInfo程序错误');
  			}
  		});
  }
  
      </script> 
