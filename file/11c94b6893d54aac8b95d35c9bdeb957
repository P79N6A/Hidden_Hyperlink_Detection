{"subId":383,"subName":"永川市"},{"subId":384,"subName":"南川市"}]}];

        places = parseCity(places) || places;

        var cId = 0;
        

        setSubCity(cId);

        $(".province").change(function(){
           setSubCity();
        });

        function parseCity(data){
           var ret = {};
            $.each(data,function(i,m){
                 ret[m.parentId] = (function(){
                     var tmp = {};
                     $.each(m.subCity,function(j,n){
                         tmp[n.subId] = n.subName;
                     });
                     return tmp;
                 })();
            });
            return ret;
        }

        function setSubCity(cid){
            var pId = $(".province").val();
            $(".city").html( cityTmp(places[pId],cid) );
        }

        function cityTmp(cities,cId){
            if(!cities) return;

            var ret = [];
            ret = $.map(cities,function(e,i){
                if(i == cId){
                    return '<option value="'+i+'" selected="selected">'+e+'</option>';
                }
                return '<option value="'+i+'">'+e+'</option>'
            });
            return ret.join('');
        }
    });
    </script> 
