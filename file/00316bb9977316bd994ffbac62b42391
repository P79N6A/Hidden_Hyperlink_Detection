<script language="javascript">
    function checkseccode() {
        var seccode = $('#seccode').val();
        if(seccode.length < 5) {
            $('#seccode_msg').html('请填写验证码。');
            return false;
        }
        var return_s = 0;
        $.ajax({
            async:false,
            url:'/login?cmd=checkseccode&code='+seccode,
            type:'GET',
            success:function(s){
                if(s.length != 0) {
                    return_s = s;
                    if(s == 0) {
                        $('#seccode_msg').html('验证码错误。');
                    }else if(s == 1) {
                        $('#seccode_msg').html('验证码已过期。');
                    }else if(s == 2) {
                        $('#seccode_msg').html('验证码正确。');
                    }
                }
            }
        });
        if(return_s == 2) {
            return true;
        }
        return false;
    }
</script>
