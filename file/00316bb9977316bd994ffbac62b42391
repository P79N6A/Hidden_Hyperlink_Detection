    <script type="text/javascript">
    function autocomplete(id, ds, onselect)
    {
        if(!ds.length)
             return;
        if (!this.__ac) this.__ac = new Array;
        if(!this.__ac[id])
            this.__ac[id]=new Ac(id, ds, onselect,true);
        else
            Ac.call(this.__ac[id],id,ds,onselect,false);
    }
    </script>
