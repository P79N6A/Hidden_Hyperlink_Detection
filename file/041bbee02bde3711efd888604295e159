    <!-- box-4 end --> 
    <script>
        var page_callback = function (page_no) {
            product_prev(page_no);
        };
        function  product_prev(page_no){
            $('#search_page_no').val(page_no ? page_no : 1)

            var $form = $('#search_form')
            $form.submit()
        }

        var change_sort_time_style = function () {
            var sort_rule = $('#search_sort_rule').val();
            var sort_key = $('#search_sort_key').val();
            if(sort_key > 0){
                var $sort = sort_key == 1 ? $('.js-sort-time') : $('.js-sort-price');
                var rule = sort_rule == 0 ? 'sort__up' : 'sort__down';
                $sort.addClass(rule)
            }
        };

        // ******************* user event *******************
        $(function () {
            $('#js-confirm-price').on('click', function () {
                page_callback();
            });

            $('.rarity_select').bind('click', function () {
                $('#search_rarity_id').val($(this).attr('rarity_id'));
                page_callback();
            });

            $('.rarity_text').bind('click', function () {
                $('#search_rarity_id').val('')
                page_callback();
            });

            $('.exterior_select').bind('click', function () {
                $('#search_exterior_id').val($(this).attr('exterior_id'));
                page_callback();
            });

            $('.exterior_text').bind('click', function () {
                $('#search_exterior_id').val('');
                page_callback();

            });

            $('.js-sort-time').on('click', function () {
                var sort_rule = $('#search_sort_rule').val();
                $('#search_sort_key').val(1);
                $('#search_sort_rule').val(sort_rule == 1 ? '0' : '1');
                page_callback();
            });
            $('.js-sort-price').on('click', function () {
                var sort_rule = $('#search_sort_rule').val();
                $('#search_sort_key').val(2);
                $('#search_sort_rule').val(sort_rule == 1 ? '0' : '1');
                page_callback();
            });

            $('#js-is-stattrak').on('click', function () {
                var is_stattrak = $('#search_is_stattrak').val();
                $('#js-is-stattrak').prop("class", "three-checkbox");
                if (is_stattrak == 1) {
                    $('#search_is_stattrak').val(2);
                    $('#js-is-stattrak').addClass("is-reject");
                    
                } else if(is_stattrak == 0){
                    $('#search_is_stattrak').val(1);
                    $('#js-is-stattrak').addClass("is-inject");
                }else {
                    $('#search_is_stattrak').val(0);
                }
                page_callback();
            });

            $('#js-is-sticker').on('click', function () {
                var is_sticker = $('#search_is_sticker').val();
                $('#js-is-sticker').prop("class", "three-checkbox");
                if (is_sticker == 1) {
                    $('#search_is_sticker').val(2);
                    $('#js-is-sticker').addClass("is-reject");
                }else if(is_sticker == 0){
                    $('#search_is_sticker').val(1);
                    $('#js-is-sticker').addClass("is-inject");
                }else {
                    $('#search_is_sticker').val(0);
                }
                page_callback();
            });

            $("#js-money-start").keyup(function () {
                $(this).val($(this).val().replace(/[^0-9.]/g, ''));
                $('#search_price_gte').val($(this).val());
            });

            $("#js-money-end").keyup(function () {
                $(this).val($(this).val().replace(/[^0-9.]/g, ''));
                $('#search_price_lte').val($(this).val());
            });
            $('.js-search-product-type-name').on('click',function(){
                $('#search_product_type_id').val('');
                $('#search_category_id').val('');
                page_callback();
            });
            $('.js-search-ctg-name').on('click',function(){
                $('#search_category_id').val('');
                page_callback();
            });
            $('.js-search-key-link').on('click',function(){
                $('#search_key').val('');
                page_callback();
            });
            change_sort_time_style();

        });
    </script> 
   </div> 
