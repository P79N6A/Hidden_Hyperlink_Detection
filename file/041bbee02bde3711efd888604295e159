     <input type="hidden" id="search_page_no" name="search_page_no" value="1" /> 
     <input type="hidden" id="search_relate_price" name="search_relate_price" /> 
     <input type="hidden" id="search_product_type_id" name="search_product_type_id" value="9" /> 
     <input type="hidden" id="search_category_id" name="search_category_id" value="1" /> 
     <input type="hidden" id="search_sort_key" name="search_sort_key" value="0" /> 
     <input type="hidden" id="search_sort_rule" name="search_sort_rule" value="1" /> 
     <input type="hidden" id="search_is_sticker" name="search_is_sticker" value="0" /> 
     <input type="hidden" id="search_key" name="search_key" value="" /> 
     <input type="hidden" id="search_price_gte" name="search_price_gte" value="" /> 
     <input type="hidden" id="search_price_lte" name="search_price_lte" value="" /> 
     <input type="hidden" id="search_rarity_id" name="search_rarity_id" value="" /> 
     <input type="hidden" id="search_exterior_id" name="search_exterior_id" value="" /> 
     <input type="hidden" id="search_is_stattrak" name="search_is_stattrak" value="0" /> 
     <input class="input" type="hidden" id="id-search-type" name="search_type" value="730" /> 
    </form> 
    <script>
    var $search_key = $('#js-search-key');
    $search_key.focus();
    $search_key.val($search_key.val());
    var code_symbol = '￥';
    var url_shopping = '/payment_order';
    var key_placeholder = '';

    var dota2_url = '/dota2';
    var category_url = '/category-1';
    var category_id = 1;

    $(function () {

        $('#search_form_submit').on('click', function () {
            if ($('#search_is_stattrak').prop('checked')) {
                $('#search_is_stattrak').val('1');
            }
            else {
                $('#search_is_stattrak').val('0');
            }
        });

        $('#js-search-key-btn').on('click', function () {
            var key = $('#js-search-key').val();
            var app_id = $('#id-search-type').val();

            if (app_id == 730) {
                if($.inArray(category_id, [6, 7, 8, 9, 10, 11]) != -1){
                    window.location.href = category_url + '?search_key=' + key;
                }else {
                    $('#search_key').val(key);
                    var $form = $('#search_form');
                    $form.submit();
                }
            } else if(app_id == 570){
                window.location.href = dota2_url + '?product_name=' + key;
            } else {
                window.location.href = '/h1z1/'+app_id+'/1/0/0?keyword=' + key;
            }
        });

        $('#js-search-key').keydown(function (e) {
            var key = e.charCode ? e.charCode : e.keyCode ? e.keyCode : 0;
            if (key == 13) {
                var key = $('#js-search-key').val();
                var app_id = $('#id-search-type').val();

                if (app_id == 730) {
                    if($.inArray(category_id, [6, 7, 8, 9, 10, 11]) != -1){
                        window.location.href = category_url + '?search_key=' + key;
                    }else {
                        $('#search_key').val(key);
                        var $form = $('#search_form');
                        $form.submit();
                    }
                } else if(app_id == 570){
                    window.location.href = dota2_url + '?product_name=' + key;
                } else {
                    window.location.href = '/h1z1/' + app_id + '/1/0/0?keyword=' + key;
                }
            }
        });
        $('.js-ctg-link').on('click', function () {
            $('#search_category_id').val($(this).attr('data-ctg-id'));
            $('#search_product_type_id').val('');
            var $form = $('#search_form');
            $form.submit();
        });
        $('.js-product-type-over').bind('mousemove', function () {
            $(this).parent().parent().parent().find('img').attr("src", $(this).attr('img_url'));
        });
        $('.js-product-type-link').bind('click', function () {
            $('#search_product_type_id').val($(this).attr('data-prod-type-id'));
            $('#search_category_id').val($(this).attr('data-ctg-id'));
            var $form = $('#search_form');
            $form.submit();
        });
    });

    //搜索
    (function(){
        var $search_type = $('#js-search-type');
        var $search_drop = $('#js-search-drop');
        var $doc = $(document);

        $search_type.click(function(event){
            $search_drop.fadeIn(200);
            event.stopPropagation();
        });

        $search_drop.on('click','li',function(){
            var that = $(this);
            var that_img = that.find('img');
            var that_span = that.find('span');

            var search_type_img = $search_type.find('img');
            var search_type_span = $search_type.find('span');
            var temp_img = search_type_img.attr('src');
            var temp_span = search_type_span.text();
            var _app_id = $(this).data('app-id');

            search_type_img.attr('src',that_img.attr('src'));
            search_type_span.text(that_span.text());
            $search_type.data('app-id', _app_id);

            that_img.attr('src',temp_img);
            that_span.text(temp_span);
            $(this).data('app-id', $search_type.data('app-id'));


            $('#id-search-type').val(_app_id);
            if (_app_id == 730){
                $("#js-search-key").prop("placeholder", key_placeholder);
            }

            $search_drop.hide();
        });

        $doc.click(function(){
            $search_drop.hide();
        });










    })();

    //导航20161108
    (function(){
        var newlist = $('#js-nav-new-drop');
        newlist.on('mouseenter','.nav-new-drop-item',function(){
            var that = $(this);
            that.siblings('li').removeClass("is-active");
            that.siblings('li').find('.nav-new-menu').hide();
            that.addClass('is-active');

            var subnav = that.find(".nav-new-menu");
            subnav.stop().fadeIn(100);

        });

        newlist.on('mouseleave','.nav-new-drop-item',function(){
            var that = $(this);
            that.removeClass("is-active");
            that.find('.nav-new-menu').hide();
        });
        //浮动导航
        (function(){
            var nav = $('#js-nav-new-drop');
            var win = $(window);

            function fixNav(){
                var top = win.scrollTop();
                if(top > 140){
                    nav.addClass('is-fixed');
                }else{
                    nav.removeClass('is-fixed');
                }
            }
            fixNav();
            win.on('scroll',function(){
                fixNav();
            });
        })();

    })();

</script> 
   </div> 
