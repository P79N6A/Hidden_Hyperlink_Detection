    function showDiv98()
    {
        div1 = document.getElementById("divLink101");
        div2 = document.getElementById("divLink102");
        div3 = document.getElementById("divLink103");
        div1.style.display="none";
        div2.style.display="none";
        div3.style.display="none";

        div11 = document.getElementById("divLink101_all");
        div21 = document.getElementById("divLink102_all");
        div31 = document.getElementById("divLink103_all");
        div11.style.display="block";
        div21.style.display="block";
        div31.style.display="block";
    }
    
    function hideDiv98()
    {
        div1 = document.getElementById("divLink101");
        div2 = document.getElementById("divLink102");
        div3 = document.getElementById("divLink103");
        div1.style.display="block";
        div2.style.display="block";
        div3.style.display="block";

        div11 = document.getElementById("divLink101_all");
        div21 = document.getElementById("divLink102_all");
        div31 = document.getElementById("divLink103_all");
        div11.style.display="none";
        div21.style.display="none";
        div31.style.display="none";
    }
    
    function showDiv99()
    {
        div1 = document.getElementById("divLink101");
        div2 = document.getElementById("divLink102");
        div3 = document.getElementById("divLink103");
        
        t1 = document.getElementById("td101");
        t2 = document.getElementById("td102");
        t3 = document.getElementById("td103");

        obj1 = document.getElementById("img101");
        obj2 = document.getElementById("img102");
        obj3 = document.getElementById("img103");
        
        if (div1.style.overflow == "")
        {
            div1.style.overflow = "hidden";
            div2.style.overflow = "hidden";
            div3.style.overflow = "hidden";
            obj1.src="images/xia.gif";				
            obj2.src="images/xia.gif";				
            obj3.src="images/xia.gif";
            obj1.alt = "展开";
            obj2.alt = "展开";
            obj3.alt = "展开";
            document.getElementById("img101").nextSibling.nodeValue="更多..";
            document.getElementById("img102").nextSibling.nodeValue="更多..";
            document.getElementById("img103").nextSibling.nodeValue="更多..";
        }
        else
        {
            div1.style.overflow = "";
            div2.style.overflow = "";
            div3.style.overflow = "";
            obj1.src="images/shang.gif";				
            obj2.src="images/shang.gif";				
            obj3.src="images/shang.gif";	
            obj1.alt = "收缩";
            obj2.alt = "收缩";
            obj3.alt = "收缩";			
            document.getElementById("img101").nextSibling.nodeValue="";
            document.getElementById("img102").nextSibling.nodeValue="";
            document.getElementById("img103").nextSibling.nodeValue="";
        }
    }
