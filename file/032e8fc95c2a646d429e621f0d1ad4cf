			window.external.addFavorite('http://vgou1.com','【V购】中国互联网电子商务众筹平台-一元夺宝');
		}
		else if(window.sidebar){
		   window.sidebar.addPanel('【V购】中国互联网电子商务众筹平台-一元夺宝','http://vgou1.com', "");
		}else{ 
			alert('您可以通过快捷键' + ctrl + ' + D 加入到收藏夹');
		}
    });
