  <script type="text/javascript">
		//check
		function checkLink(img,aid,pid,to_uid,to_username,to_avatar,dateline){
			var uid = parseInt(0), link_reply = parseInt();
			//登录状态
			if(!uid){
				var poplogin = '<div class="con"><h5><strong>亲，请先登录哦</strong><i onclick="colse(this)"></i></h5><div class="popup-btn"><a href="http://my.55bbs.com/login/index.php" class="login-btn">登录</a><a href="http://my.55bbs.com/reg/index.php">立即注册</a></div><p class="login-more">您也可以用其他账号登陆</p><div class="popup-btn"><a href="http://my.55bbs.com/bind/index.php?act=qqURL" class="qq-btn"></a><a href="http://my.55bbs.com/sinaUrl.php" class="sina-btn"></a><a href="http://my.test.55bbs.com/bind/index.php?act=topURL" class="taobao-btn"></a></div></div>';
				showAlert(poplogin, 'popLogo', 0);
			}else{
				//未回复
				$('popupLink').style.display = 'block';
				showSmily();
				//提交
				$('confirm').onclick=function (){
					addLink(img,aid,pid,to_uid,to_username,to_avatar,dateline);
				};
			}
		}
		//提交求链
		function addLink(img,aid,pid,to_uid,to_username,to_avatar,dateline){
			var addpost = $('addpost').checked ? 1 : 0, message = $('talkBox').value, linksubject = 'SASA官网有假货吗？？？？？？？？';
			var data = '&fid=19&tid=8273547&pid='+ pid +'&aid='+ aid +'&addpost='+ addpost +'&img='+ img + '&message='+ encodeURIComponent(encodeURIComponent(message)) +'&subject='+encodeURIComponent(encodeURIComponent(linksubject))+'&to_uid='+ to_uid +'&to_username='+ encodeURIComponent(encodeURIComponent(to_username))+'&to_avatar='+ to_avatar+'&dateline='+dateline;
			ajaxresponse('inajax=1&action=addlink'+ data);
			$('talkBox').value='';
		}
		//JS ajax
		function ajaxresponse(data) {
			var x = new Ajax('XML'),uid = 0;
				x.post('ajax.php', data, function(s){
					if(s == 'succeed') {
						cancel();
						var popNotAnswer = '<div class="con" style="height:200px;"><h5><strong>提示</strong><i onclick="colse(this)"></i></h5><p class="popup-tip" style="padding-top:80px;">亲，楼主暂未回复宝贝链接，楼主回复后我们会站短通知您！</p></div>';
						showAlert(popNotAnswer, 'popNotAnswer', 2000);
					}else if(s == 'isreply'){
					//已回复
					cancel();
					var popAnswer = '<div class="con"><h5><strong>提示</strong><i onclick="colse(this)"></i></h5><p class="popup-tip">亲，楼主已回复宝贝链接，<a target="_blank" href="http://my.55bbs.com/linkout-'+uid+'-2-1.html">马上查看</a>！</p></div>';
					showAlert(popAnswer, 'popAnswer', 0);
					}else if(s == 'not_login'){
						cancel();
						var poplogin = '<div class="con"><h5><strong>亲，请先登录哦</strong><i onclick="colse(this)"></i></h5><div class="popup-btn"><a href="http://my.55bbs.com/login/index.php" class="login-btn">登录</a><a href="http://my.55bbs.com/reg/index.php">立即注册</a></div><p class="login-more">您也可以用其他账号登陆</p><div class="popup-btn"><a href="#" class="qq-btn"></a><a href="#" class="sina-btn"></a></div></div>';
						showAlert(poplogin, 'popLogo', 0);
					}else{
						cancel();
						var error = '<div class="con"><h5><strong>提示</strong><i onclick="colse(this)"></i></h5><p class="popup-tip">请求失败，请重试！</p></div>';
						showAlert(error, 'popError', 2000);
					}
				});
		}
		//获取求链次数
		function getLinkTimes(aid, uid){
			if($('linktimes_'+aid).innerHTML == ''){
				ajaxget('/ajax.php?inajax=1&action=getlinktimes&aid='+aid+'&uid='+uid, 'linktimes_'+aid);
			}
		}
		//弹窗输出
		function showAlert(info, infoSign, setTime) {
			$(infoSign).innerHTML = info;
			$(infoSign).style.display = 'block';
			//if(setTime){
				setTimeout(function(){$(infoSign).style.display ='none';},2500);
			//}
		}
		//关闭窗口
		function colse(obj){
			if (obj == null) {
				return;
			}
			obj.parentNode.parentNode.parentNode.style.display = 'none';
		}
		//取消求链
		function cancel(){
			$('popupLink').style.display = 'none';
		}
		//添加表情
		function showSmily(){
			$('smily').onclick=function (){
				$('smilies').style.display = 'block';
				ajaxget('post.php?action=smilies&link=1', 'smilies_list');
			};
		}
		function insertLinkSmiley(smilieid) {
			obj = $('talkBox');
			obj.focus();
			var src = $('linksmilie_' + smilieid).src;
			var code = $('linksmilie_' + smilieid).code;
			code += ' ';
			obj.value += code;
			$('smilies').style.display = 'none';
		}
		//快速求链
		function chanTxt(i){
			var f 	= $('fastList'),
				t  	= $('talkBox'),
				l 	= f.getElementsByTagName('li');
				var txt = l[i].innerHTML;
				t.value = txt;
		}
