    $(function(){
        $('.dxtg_url').click(function(){
            var clickfrom = $(this).attr('clickfrom');
            var url = $(this).attr('href') + '?clickfrom=' + clickfrom
            window.open(url);
            return false;
        });
        $('.dxtg .detail').click(function(){
            var dxtg_url_obj = $(this).find('.title .name a');
            var clickfrom = dxtg_url_obj.attr('clickfrom');
            var url = dxtg_url_obj.attr('href') + '?clickfrom=' + clickfrom;
            window.open(url);
        });
    });
</script> 
