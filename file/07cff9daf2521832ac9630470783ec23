      <script type="text/javascript">

    //时间格式化
    Date.prototype.format = function (format) {
        if (!format) {
            //format = "yyyy-MM-dd hh:mm:ss";
            format = "yyyy-MM-dd hh:mm";
        }
        var o = {
            "M+": this.getMonth() + 1, // month
            "d+": this.getDate(), // day
            "h+": this.getHours(), // hour
            "m+": this.getMinutes(), // minute
            //"s+": this.getSeconds(), // second
            "q+": Math.floor((this.getMonth() + 3) / 3)//, // quarter
            //"S": this.getMilliseconds()
            // millisecond
        };
        if (/(y+)/.test(format)) {
            format = format.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
        }
        for (var k in o) {
            if (new RegExp("(" + k + ")").test(format)) {
                format = format.replace(RegExp.$1, RegExp.$1.length == 1 ? o[k] : ("00" + o[k]).substr(("" + o[k]).length));
            }
        }
        return format;
    };
    function fomatDate(str) {
        //return (new Date(parseInt(str.substring(str.indexOf('(') + 1, str.indexOf(')'))))).format("yyyy-MM-dd hh:mm:ss");
        return (new Date(parseInt(str.substring(str.indexOf('(') + 1, str.indexOf(')'))))).format("yyyy-MM-dd hh:mm");
    }

    //分享到新浪微博:
    function opensharewb(sourcehref) {
        var iwidth = 480,
            iheight = 400,
            ileft = (screen.width - iwidth) / 2,
            itop = (screen.height - iheight) / 2,
            url;
        /*count=document.location.href.indexOf("/",8);
        if(count==-1){
        host=document.location.href;
        }
        else{
        host=document.location.href.substring(0,count+1)
        }*/
        if (sourcehref.indexOf("http://") == -1) {
            url = "http://service.weibo.com/share/share.php?url=" + encodeURIComponent("http://") + encodeURIComponent(document.location.host) + encodeURIComponent("/") + encodeURIComponent(sourcehref) + "&type=button&language=zh_cn&appkey=1731680939&searchPic=true&style=simple";
        } else {
            url = "http://service.weibo.com/share/share.php?url=" + encodeURIComponent(sourcehref) + "&type=button&language=zh_cn&appkey=1731680939&searchPic=true&style=simple";
        }
        window.open(url, "_blank", "width=" + iwidth + ",height=" + iheight + ",toolbars=0,resizable=1,left=" + ileft + ",top=" + itop);
        //alert(host+sourcehref);
    }

    //回到顶部的变量：
    var scrollEle = document.documentElement;
    compatMode = document.compatMode;
    isChrome = window.navigator.userAgent.indexOf("Chrome") === -1 ? false : true;
    if (compatMode === "BackCompat" || isChrome) {
        scrollEle = document.body;
    }

    //异步获取数据:
    function getForumInfo(page) {
        $.ajax
        ({
            url: "/Mminfor/DispForumInfor",
            dataType: "text",
            cache: false,
            data: { id: page },
            type: "POST",
            success: function (data) {
                $('#DispForumInfo').html(data);
                scrollEle.scrollTop = 0;
            }
        });
    }

</script> 
