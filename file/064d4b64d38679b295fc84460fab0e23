  <script>
    //忘记密码-页面-提交请求
    $("#j_forgotpwd_submit").click(function () {
        $("#tipsshow_forgotpwd").html();
        var mobile = $("#forgotpwd_mobile").val();
        var smscode = $("#forgotpwd_smscode").val();
        var newpwd = $("#forgotpwd_newpwd").val();
        var newpwd_repeat = $("#forgotpwd_newpwd_repeat").val();

        $('#j_forgotpwd_submit').attr("disabled",true);

        if(mobile){
            var re = /^1\d{10}$/;
            if (re.test(mobile)) {
                if(newpwd != newpwd_repeat){
                    $("#tipsshow_forgotpwd").html('2次输入的密码不一致');
                    $('#j_forgotpwd_submit').attr("disabled",false);
                    return false;
                }
                if(smscode){
                    //都校验过了，提交数据到服务器
                    $.ajax({
                        type: "POST",
                        url: "/index.php?s=/Login/ajax_forgotpwd.html",
                        data: {mobile: mobile,smscode: smscode,newpwd: newpwd,newpwd2:newpwd_repeat},
                        dataType: "json",
                        success: function (data) {
                            if (data.code == 1) {
                                $(".alert").hide();
                                $(".alert_bg").show();
                                $("#dialog_forgotpwd_content").html(data.msg);
                                $("#dialog_forgotpwd").show();
                                alertPosition("#dialog_forgotpwd");
                                $("#forgotpwd_close_dialog").click(function(){
                                    window.location.href = "/index.php?s=/Login/index.html";
                                });
                            }else{
                                $("#tipsshow_forgotpwd").html(data.msg);
                                $('#j_forgotpwd_submit').attr("disabled",false);
                            }
                        }
                    });
                }else{
                    $("#tipsshow_forgotpwd").html('请输入收到的短信验证码');
                    $('#j_forgotpwd_submit').attr("disabled",false);
                    return false;
                }
            } else {
                $("#tipsshow_forgotpwd").html('请输入格式正确的手机号');
                $('#j_forgotpwd_submit').attr("disabled",false);
                return false;
            }
        }else{
            $("#tipsshow_forgotpwd").html('请先输入手机号');
            $('#j_forgotpwd_submit').attr("disabled",false);
            return false;
        }
    });

    //忘记密码-页面-发送验证码
    function j_forgotpwd_countdown(){
        $("#tipsshow_forgotpwd").html();
        var mobile = $("#forgotpwd_mobile").val();
        $('#j_forgotpwd_countdown').attr("disabled",true);

        if(mobile){
            var re = /^1\d{10}$/;
            if (re.test(mobile)) {
                $.ajax({
                    type: "POST",
                    url: "/index.php?s=/Login/ajax_code.html",
                    data: {mobile: mobile},
                    dataType: "json",
                    success: function (data) {
                        if (data.code == 1) {
                            countD('#j_forgotpwd_countdown');
                        }else{
                            $("#tipsshow_forgotpwd").html(data.msg);
                            $('#j_forgotpwd_countdown').attr("disabled",false);
                        }
                    }
                });
            } else {
                $("#tipsshow_forgotpwd").html('请输入格式正确的手机号');
                $('#j_forgotpwd_countdown').attr("disabled",false);
            }
        }else{
            $("#tipsshow_forgotpwd").html('请先输入手机号');
            $('#j_forgotpwd_countdown').attr("disabled",false);
        }
    }

    //点击刷新验证码
    $(".verify").click(function(){
        var src = "/index.php?s=/register/verify.html";
        var random = Math.floor(Math.random()*(1000+1));
        $(this).attr("src",src+"&random="+random);
    });

    //忘记密码-发送验证码
    function forgotpwd_countdown(){
        $("#tipsshow").html();
        var mobile = $("#forgotpwd_mobile").val();
        $('#j_forgotpwd_countdown').attr("disabled",true);

        if(mobile){
            var re = /^1\d{10}$/;
            if (re.test(mobile)) {
                $.ajax({
                    type: "GET",
                    url: "index.php?s=/Login/ajax_code.html",
                    data: {mobile: mobile},
                    dataType: "json",
                    success: function (data) {
                        if (data.code == 1) {
                            countD('#j_forgotpwd_countdown');
                        }else{
                            $("#tipsshow").html(data.msg);
                            $('#j_forgotpwd_countdown').attr("disabled",false);
                        }
                    }
                });
            } else {
                $("#tipsshow").html('请输入格式正确的手机号');
                $('#j_register_countdown').attr("disabled",false);
            }
        }else{
            $("#tipsshow").html('请先输入手机号');
            $('#j_forgotpwd_countdown').attr("disabled",false);
        }
    }

    //注册-必须输入验证码之后才可以倒计时
    function register_countdown(){
        $("#tipsshow").html();
        $('#j_register_countdown').attr("disabled",true);
        var imgcode = $("#imgcode").val();
        var mobile = $("#mobile").val();

        if(mobile){
            var re = /^1\d{10}$/;
            if (re.test(mobile)) {
                if(imgcode){
                    //ajax请求发送验证码
                    $.ajax({
                        type: "GET",
                        url: "index.php?s=/Register/ajax_check_code.html",
                        data: {mobile: mobile, code: imgcode},
                        dataType: "json",
                        success: function (data) {
                            if (data.code == 1) {
                                countD('#j_register_countdown');
                            }else{
                                $("#tipsshow").html(data.msg);
                                $('#j_register_countdown').attr("disabled",false);
                            }
                        }
                    });
                }else{
                    $("#tipsshow").html('请先输入图形验证码');
                    $('#j_register_countdown').attr("disabled",false);
                }
            } else {
                $("#tipsshow").html('请输入格式正确的手机号');
                $('#j_register_countdown').attr("disabled",false);
            }
        }else{
            $("#tipsshow").html('请先输入手机号');
            $('#j_register_countdown').attr("disabled",false);
        }
    }

    //注册提交
    $("#j_submit_register").click(function () {
        var mobile = $("#mobile").val();
        var pwd = $("#pwd").val();
        var smscode = $("#smscode").val();

        if(mobile){
            var re = /^1\d{10}$/;
            if (re.test(mobile)) {
                if(pwd){
                    if(smscode){
                        //提交from表单
                        document.getElementById('myform').submit();
                    }else{
                        $("#tipsshow").html('请输入短信验证码');
                    }
                }else{
                    $("#tipsshow").html('请输入密码');
                }
            } else {
                $("#tipsshow").html('请输入格式正确的手机号');
            }
        }else{
            $("#tipsshow").html('请先输入手机号');
        }
    });

    function showLogin() {
        $(".alert").hide();
        $(".alert_bg").show();
        $("#j_login").show();
        alertPosition("#j_login");
    }
    function showRegister() {
        $(".alert").hide();
        $(".alert_bg").show();
        $("#j_register").show();
        alertPosition("#j_register");
    }
    function showForgotPWD() {
        $(".alert").hide();
        $(".alert_bg").show();
        $("#j_forgotPwd").show();
        alertPosition("#j_forgotPwd");
    }
    function showLoginWX() {
        $(".alert").hide();
        $(".alert_bg").show();
        $("#j_loginbywx").show();
        alertPosition("#j_loginbywx");
    }
</script> 
