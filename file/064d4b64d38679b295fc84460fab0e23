    $(".header_loged_link").mouseenter(function(){
        $(this).find(".header_loged").show();
        $(this).find(".header_loged").animate({
            height:"180px"
        },"fast");
    }).mouseleave(function(){
        $(this).find(".header_loged").animate({
            height:"0"
        },"fast");
        $(this).find(".header_loged").hide();
    });  
    </script> 
