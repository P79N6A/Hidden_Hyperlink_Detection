    <script>
    var pcode = 'wjy_1',
        pack_source = 'sohuvideo',
        downloadLink = 'http://dl.wan.sogoucdn.com/dl0/15022/app48409_default_0009000300000.apk',
        delaytime = '3',
        uiwidth = parseInt('720');

        $.fn.remReSize(uiwidth); 
        $.fn.initSpbVars(pcode, pack_source);

        $(function(){

            //预览时给页面a标签加蒙层
            var pageUrl = window.location.href;
            var pattern = /.\?p=1/;
            if(pattern.test(pageUrl)){
                $('a').addClass('whiteMark');
            }

            $.fn.remReSize(uiwidth); 
            //进页面发pingback
            $.fn.sendPingback('extra', 'onload', 'onload');
            //批量添加下载链接
            $.fn.addDownloadLink(downloadLink);

            //自动下载延时时间 若为空 则视为无自动下载 不能为0 
            if(!!parseInt(delaytime)){
                $.fn.downloadPkg(downloadLink, delaytime);
            }
            //视频点击播放
            $.fn.videoPlay();
        });

        $(window).resize(function(){
            $.fn.remReSize(uiwidth);
        });
 
    </script><script type="text/javascript" async="" src="http://p2.wan.sogoucdn.com/cdn/static/js/pb/pb.3580599.js?t=20160622"></script>


