   <script>
    $(document).ready(function () {
        if ("24" <= 1) {
            $(".Filter_Tr").hide();
        };

        $('#datatable').dataTable({
            searching: true,
            paging: false,
            order: [[1, "asc"]],
            info: false,
            scrollX: true,
            scrollCollapse: true,
            iTabIndex: 0,
            language: {
                url: "/Scripts/dataTables/Localisation/Chinese.json"
            }
        });
    });

    function AddShopCartItemSub(GoodsID) {
        var Qty = $("#GoodsQty_" + GoodsID).val();
        var Btn_Obj = $("#AddCat_" + GoodsID);
        var ImgURL = $("#TS_Img_Val").val();

        var MiniCount = 1;
        var SalesPrice = parseFloat($("#My_Final_Selling_Price_" + GoodsID).text());
        var MiniAmount = SalesPrice * parseInt(Qty);

        $.post("/Index/AddShopCartItem/" + GoodsID, { Qty: Qty },
        function (result) {
            if (result != "") {
                alert(result);
            } else {
                MiniPanel_CountAndAmount(ImgURL, MiniCount, Qty, MiniAmount.toFixed(2));
                ShowMiniCartPanel(Btn_Obj);
            }
        });

        Reload_ShopCartPart_Sim_Count();
    }

    function ChangeQty(NumberID, Act) {
        var ObjNumber = $("#GoodsQty_" + NumberID);
        var Nummber = parseInt($(ObjNumber).val());
        (isNaN(Nummber)) ? Nummber = 0 : Nummber;

        if (Act == "-") {
            Nummber = Nummber - 1;
        } else {
            Nummber = Nummber + 1;
        }
        (Nummber <= 0) ? Nummber = 1 : Nummber;
        $(ObjNumber).val(Nummber);
    }
</script> 
   <!--产品规格过滤筛选--> 
   <script>
    function Set_Filter_KV_List()
    {
        //获取所有已选元素集合
        var ValStr = "";
        var ColIndex = 0;
        $('.Filter_Select').each(function (i) {
            ValStr = $(this).val();
            ColIndex = $(this).attr('ColIndex');
            if (ValStr != "")
            {
                $("#datatable").DataTable().column(ColIndex).search(ValStr).draw();
            } else {
                $("#datatable").DataTable().column(ColIndex).search('').draw();
            }
        })
    }

</script> 
   <!--产品规格过滤筛选--> 
   <!--加入购物车--> 
   <script>
    function ShowMiniCartPanel(Btn_Obj)
    {
        var CartPanel = $("#MiniCartPanel");
        var Top = $(Btn_Obj).offset().top + $(Btn_Obj).innerHeight() + 5;

        //左偏移量 = 按钮左偏移 - 面板宽度 - 按钮宽度
        var left = $(Btn_Obj).offset().left - ($(CartPanel).innerWidth() - $(Btn_Obj).innerWidth());

        $(CartPanel).css("top", Top);
        $(CartPanel).css("left", left);
        $(CartPanel).show(100);
    }

    function HideMiniCartPanel()
    {
        $("#MiniCartPanel").hide(100);
    }

    function MiniPanel_CountAndAmount(ImgURL, MiniCount, MiniQtySum, MiniAmount)
    {
        $("#GroupDefImg").attr("src", ImgURL);
        $(".MiniCount").text(MiniCount);
        $(".MiniQtySum").text(MiniQtySum);
        $(".MiniAmount").text(MiniAmount);
    }

</script> 
   <div id="MiniCartPanel"> 
