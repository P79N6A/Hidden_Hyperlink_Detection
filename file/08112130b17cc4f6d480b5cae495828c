  <script>
    function ChangeSignQty(Act) {
        var ObjNumber = $("#SignQty");
        var Nummber = parseInt($(ObjNumber).val());
        (isNaN(Nummber)) ? Nummber = 1 : Nummber;

        if (Act == "-") {
            Nummber = Nummber - 1;
        } else if (Act == "+") {
            Nummber = Nummber + 1;
        }

        (Nummber <= 0) ? Nummber = 1 : Nummber;
        $(ObjNumber).val(Nummber);
    }

    function ShowMiniCartPanel_Sign(Btn_Obj) {
        var Qty = parseInt($("#SignQty").val());
        var Price = parseFloat('323.730000');
        var Amount = Price * Qty;
        var ImgURL = $("#TS_Img_Val").val();

        MiniPanel_CountAndAmount(ImgURL, 1, Qty, Amount.toFixed(2));
        ShowMiniCartPanel(Btn_Obj);
    }

    function AddShopCartItem(Btn_Obj) {
        var Qty = $("#SignQty").val();
        $.post("/Index/AddShopCartItem/20161104-1620-44b4-ab5a-7b111af31659", { Qty: Qty },
            function (result) {
                if (result != "") {
                    alert(result);
                } else {
                    ShowMiniCartPanel_Sign(Btn_Obj)
                }
            });

        Reload_ShopCartPart_Sim_Count();
    }

    function Reload_MoreDetail_Sub() {
        $("#MoreDetail").text("Loading...");
        $("#MoreDetail").load("/Catalog/MoreDetail_Sub/20161104-1617-493a-b7fa-2ad4cce09947", { GID: "20161104-1620-44b4-ab5a-7b111af31659" });
    }

    $(function () {
        Reload_MoreDetail_Sub();
    });
</script> 
