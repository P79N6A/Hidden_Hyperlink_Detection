  <script>
        function Show_MyService_Sim(Item_Obj) {
            var SimPanel = $("#MyServiceSimPanel");
            var Top = $(Item_Obj).offset().top + $(Item_Obj).innerHeight();

            //左偏移量 = 按钮左偏移 - 面板宽度 - 按钮宽度
            var left = $(Item_Obj).offset().left;

            $(SimPanel).css("top", Top);
            $(SimPanel).css("left", left);
            $(SimPanel).show();
            $("#CartSimPanel").hide();
        }

        $(function () {
            $("#MyServiceSimPanel").hover(function () {
                $("#MyServiceSimPanel").show();
                $("#CartSimPanel").hide();
            },
            function () {
                $("#MyServiceSimPanel").hide();
            });

            $(".G-Head-MyService").hover(function () {
                //Donoting
            },
            function () {
                $("#MyServiceSimPanel").hide();
            });

            $(".G-Head-MyShopCart").hover(function () {
                //Donoting
            },
            function () {
                $("#CartSimPanel").hide();
            });

        });

        

    </script> 
