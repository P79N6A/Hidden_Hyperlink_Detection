         <script>
                                //显示自动完成框
                                function ShowMyMyAutoComplete(KeyObj, Reload, evt)
                                {
                                    var Keyword = $(KeyObj).val();
                                    var CurKC = evt.keyCode || window.event.keyCode;
                                    if (Keyword.length > 1 && CurKC != 38 && CurKC != 40) {
                                        $("#MyAutoComplete").show();
                                        if (Reload == "Reload") {
                                            ReloadMyAutoCompleteContext();
                                        }
                                    } else if (Keyword.length > 1 && (CurKC == 38 || CurKC == 40)) {
                                        UpdownKey_RollingEvent(CurKC);
                                    }
                                    else {
                                        $("#MyAutoComplete").hide();
                                    }
                                }

                                //加载搜索内容
                                function ReloadMyAutoCompleteContext()
                                {
                                    var Keyword = $("#Keyword").val();
                                    var EndKeyword = "";
                                    setTimeout(function () {
                                        EndKeyword = Keyword;
                                    }, 5);


                                    setTimeout(function () {
                                        if (EndKeyword == $("#Keyword").val())
                                        {
                                            $("#MyAutoComplete").load("/Index/IndexQuickSearch", { Keyword: Keyword });
                                            return false;
                                        }
                                    }, 500);
                                }

                                //Hover_MyAutoComplete
                                $(function () {
                                    $("#MyAutoComplete").hover(function () {
                                        $("#MyAutoComplete").show();
                                    },
                                    function () {
                                        $("#MyAutoComplete").hide();
                                    });
                                });

                                /*
                                 *上下键滚动响应
                                 */
                                function UpdownKey_RollingEvent(KeyCode) {
                                    var MaxLength = $(".SearchItems a").length;
                                    var Location = $("#CurIndex").val();
                                    if (KeyCode == 38) {
                                        Location--;
                                    } else if(KeyCode==40){
                                        Location++;
                                    }
                                    if (Location > MaxLength) {
                                        Location = 0;
                                    } else if(Location<0){ //一直按向下导致为负
                                        Location = MaxLength;
                                    }
                                    $("#CurIndex").val(Location);
                                    $(".SearchItems a").removeClass("SearchItemsAct");
                                    $(".SearchItems a").each(function (i, obj) {
                                        if (i == Location - 1) { 
                                            ScrollY(Location, obj);
                                            $(obj).addClass("SearchItemsAct");
                                        }
                                    });
                                }

                                function ScrollY(CIndex, AObj) {
                                    var MinScrollL = 8;
                                    var ScrollHeight = 0;
                                    var RowHeight = $(AObj).height();
                                    var MidNo = MinScrollL / 2; //偏移量
                                    if (CIndex > MinScrollL) {
                                        ScrollHeight = (CIndex * RowHeight) - (RowHeight * (MinScrollL - MidNo));
                                    }
                                    $("#MyAutoComplete").scrollTop(ScrollHeight);
                                }
                            </script> 
