   <!--产品对比--> 
   <script>
    function CheckCompareLab(GoodsID)
    {
        var isCheck = document.getElementById(GoodsID).checked
        if (isCheck == true) {
            var IsCheckLoop = checkBoxCheckLength('CompareBox');
            if(IsCheckLoop > 4)
            {
                alert("最多对比四项！");
                $("#"+GoodsID).prop({ checked: false });
                return false;
            }

            $("#CompareLab_" + GoodsID).addClass('CompareLabAct');
            GoodsCompare_Add(GoodsID);
        } else {
            $("#CompareLab_" + GoodsID).removeClass('CompareLabAct');
            GoodsCompare_Del(GoodsID);
        }

        ReloadGoodsCompareSub();
        ShowCMP_DIV();
    }

    function ReloadGoodsCompareSub()
    {
        $("#CMP").load("/Catalog/GoodsCompareSub/20161104-1617-493a-b7fa-2ad4cce09947");
    }

    function GoodsCompare_Add(GoodsID)
    {
        $.post("/Catalog/GoodsCompare_Add/20161104-1617-493a-b7fa-2ad4cce09947", { GoodsID: GoodsID },
            function (result) {
                if (result != "") {
                    alert(result);
                } else {
                    ReloadGoodsCompareSub();
                }
            });
    }

    function GoodsCompare_Del(GoodsID) {
        $.post("/Catalog/GoodsCompare_Del/20161104-1617-493a-b7fa-2ad4cce09947", { GoodsID: GoodsID },
            function (result) {
                if (result != "") {
                    alert(result);
                } else {
                    ReloadGoodsCompareSub();
                }
            });
    }

    function GoodsCompare_Clean() {
        $.post("/Catalog/GoodsCompare_Clean/20161104-1617-493a-b7fa-2ad4cce09947", 
            function (result) {
                if (result != "") {
                    alert(result);
                } else {
                    ReloadGoodsCompareSub();
                }
            });

        $('.CompareBox').each(function (i) {
            $(this).prop({ checked: false });
        })

        $(".CompareLab").removeClass('CompareLabAct');
    }

    function ShowCMP_DIV()
    {
        $("#CMP_DIV").show();
    }

    function HideCMP_DIV()
    {
        $("#CMP_DIV").hide();
    }

    function JumpToGoodsCompare()
    {
        $("#GoodsCompareForm").submit();
    }

</script> 
   <div id="CMP_DIV" class="CMP_DIV"> 
    <h4>对比栏 <span onclick="HideCMP_DIV()">隐藏</span></h4> 
    <div id="CMP" class="CMP"></div> 
   </div> 
