<script>
	function mmjudge1(suspend){
		var suspendnum = suspend;	
		var storage = window.sessionStorage;
		storage.setItem("suspendnum",suspendnum);
	}
</script>
<script>
$(function(){
  	var storage = window.sessionStorage;
	if(window.sessionStorage){
		var suspend = parseInt(storage.getItem("suspendnum"));
		if (suspend=="1"){
			$(".fixedSuspend").show().css("right","50px");
			$(".suspendsmall").hide().css("right","-45px");
		}
		if(suspend=="0"){
			$(".fixedSuspend").hide().css("right","-200px");
			$(".suspendsmall").show().css("right","0px");
		}
	}
});
</script>
<div class="loudinfo" style="display: none; left: 0px; bottom: 0px; ">
		<!-- <div class="loudshow"><img src="//zuhaowan.zuhaowan.com/v1/images/loud2.gif"/></div> -->
		<div class="loudshow"><span>精品<br>推荐</span></div>
		<span class="loudclose">×</span>
