<script>
var rtimerId = 0;
/** 倒计时 */
function getRTime() {
    var etStr = "";
    var EndTime= new Date(etStr.replace(/-/g, "/")); // 截止时间
    var nowTime = "";
    var NowTime = new Date(nowTime.replace(/-/g, "/"));
    var t =EndTime.getTime() - NowTime.getTime();//alert(t);
    if(t <= 1) {
        clearInterval(rtimerId);
        location.replace(location.href); // 刷新页面
        $("#dd").text("00");
        $("#hh").text("00");
        $("#mm").text("00");
        $("#ss").text("00");
    }

    var d=Math.floor(t/1000/60/60/24);
    var H=Math.floor(t/1000/60/60%24);
    var m=Math.floor(t/1000/60%60);
    var s=Math.floor(t/1000%60);
    
    $("#dd").text(fillTime(d));
    $("#hh").text(fillTime(H));
    $("#mm").text(fillTime(m));
    $("#ss").text(fillTime(s));
}
/** 不足两位补0 */
function fillTime(t) {
    if(t < 10) {
        return "0" + t;
    }
    return t;
}
</script>
