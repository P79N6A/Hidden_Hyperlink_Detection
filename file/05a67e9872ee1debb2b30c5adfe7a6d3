	<script>
$(function(){
	var tt ="30";
	if(tt>0){
		tt="30"*1000;
	}else{
		tt = 30*1000;
	}	
	var offon = "1";
	//var distrump = "";//说明没有数据
	if(offon=='1'){
		timerset(tt,offon);
	}
	
	function timerset(tt,offon)
	{
		var url="/Index/loudinfo.html";
		var data={ajaxtrump:'1'};
		$.post(url,data,function(res){
			var inhtml ="";
			for(var i=0;i<res.length;i++){
				if (typeof(res[i].seller) == "undefined") {
					  window.location.href(true);
				}   
				 inhtml +="<li><div class='loudbg'></div>";
				 inhtml +="<img src=//zuhaowan.zuhaowan.com/v1/images/loud1.gif />";
				 inhtml +="<div class='loudcon'>";
				if(res[i].haotype=='1'){
				 	inhtml+="<a href='/buy/"+res[i].haoid+".html' target='_blank'><span class='loudname'>"+res[i].seller+"<font>说</font>：</span>"+res[i].keywords+"</a>";
				}else{
					 inhtml+="<a href='/zuhao/"+res[i].haoid+".html target='_blank'><span class='loudname'>"+res[i].seller+"<font>说</font>：</span>"+res[i].keywords+"</a>";
				}
				 inhtml+="</div></li>";
			}		

			$('.loudinfo ul').html(inhtml);
			$('.loudinfo').fadeIn();
			if(res.length<1){
				$('.loudinfo').fadeOut();
			}else{
				if(offon=='1'){
					setTimeout(function(){timerset(tt,offon);},tt);
				}
			}
		});
	　 //setTimeout("timerset()", tt)
	}
	
	
	//初始状态运行
	loudint();
	var namec= $.cookie('remarkcookie'); 
	if(namec!=1){
		$(".loudinfo").animate({"left":"0px","bottom":"0px"},"slow");
		$(".loudinfo a img").css("height",$(".loudinfo a font").css("height"));	
	}
	if(namec==1){
		$(".loudinfo a img").css("height",$(".loudinfo a font").css("height"));	
		$(".loudinfo .loudshow").fadeIn();
	}
	$(".loudclose").click(function(){
		$(this).parent().animate({"left":"-700px","bottom":"-200px"},"slow");
		$(".loudinfo a img").css("height",$(".loudinfo a font").css("height"));	
		$(".loudinfo .loudshow").fadeIn();
		clearInterval(loudrecord);
		$.cookie('remarkcookie','1');
	});
	//当点击推荐装备时
	$(".loudinfo .loudshow").click(function(){
		$(this).parent().animate({"left":"0px","bottom":"0px"},"slow");
		$(this).fadeOut();
		$.cookie('remarkcookie','0'); 
		clearInterval(loudrecord);
		//loudint();
	});
	//初始化函数
	var loudrecord = 0;
	function loudint(){
		var loudrecord = setInterval(function(){
			$('.loudinfo ul li:last').css('left','-700px').insertBefore('.loudinfo ul li:first').animate({'left':'0px'}, 'slow', function(){
				$(this).removeAttr('style');
				
			});
		},5000);
		if($('.loudinfo ul li').length<1){
			$('.loudinfo').fadeOut();
		}
	};
})
</script>

