<script type="text/javascript" src="//zuhaowan.zuhaowan.com/v1/plugin/jPaginate/jquery.paginate.zhw.js"></script>
<script type="text/javascript">
	/** 游戏资料库数据/装备 */
	function getGameData() {
		if($("#gameDataWrap div[class='gd_content']").children().length != 0){
			return;
		}
		
		var gameId = "560";
		var url = "/Common/getGameData.html";
		var reqData = {
				gameId : gameId
		};
		$.get(url, reqData, function(rst) {
			var data = $.parseJSON(rst);
			if(data == null) {
				$("#gameDataWrap div[class='gd_content']").html("没有可选信息");
			} 
			else {
				var dtTopList = data.dtTopList;
				if(dtTopList == null || dtTopList.length == 0) {
					$("#gameDataWrap div[class='gd_content']").html("没有可选信息");
				}
				else {
					var dataHtml = '<div>';
					for(var i = 0; i < dtTopList.length; i++) {
						var dt = dtTopList[i];
						var mulchsCls = " single";
						if(dt.multi_choose == "1") {
						    mulchsCls = " multi";
						    dataHtml=dataHtml+'<div class="clear"></div>';
						}
						
						var lolHeroCls = '';
						if(gameId == "17" && dt.dt_name == "英雄") {
						    lolHeroCls = ' lol_hero_menu';
	                    }
						
						dataHtml += '<div class="'+mulchsCls.trim()+'">' 
								+ '<div class="c_head title">'+dt.dt_name+'</div>' 
								+ '<ul class="menu'+lolHeroCls+'">' 
								+ fillDataHtml(dt.dataList)
								+ '</ul>' 
								+ '</div>';
					}
					dataHtml += '</div>';
					$("#gameDataWrap div[class='gd_content']").html(dataHtml);
					
					// 英雄联盟英雄排序
					if(gameId == "17") {
					    sortHero($(".lol_hero_menu"), 0);
					}
					
					$("#gameDataWrap img.gd_lazy").lazyload({
					    effect : "fadeIn",
					    skip_invisible : false
					});
				}
			}
		});
	}
	
	/** 遍历数据列表，构建HTML */
	function fillDataHtml(dataList) {
		if(dataList == null || dataList.length == 0) {
			return '';
		}
		
		var dataHtml = '';
		var gameDataChoose = "";
		for(var i = 0; i <  dataList.length; i++) {
			var gd = dataList[i];
			var clsChs = '';
			var gdChs  = getGameDataChoosed("gd_"+gd.id+"_")
			var gdNum  = ''; // 数量
			//if(gameDataChoose.indexOf("gd_"+gd.id+",") > -1) {
			if(gdChs != null) {
				//clsChs = ' gd_info_slt';
				gdNum  = gdChs[1];
				dataHtml += '<li class="gt" onmouseover="showDiv(\'p'+gd.id+'sub\')" onmouseout="hideDiv(\'p'+gd.id+'sub\')">' 
						+ '<div class="gd_info'+clsChs+'">'
						//+ '<div class="gd_info_img"><img src="'+gd.data_img+'"/>' 
						+ '<div class="gd_info_img"><img class="gd_lazy" data-original="'+gd.data_img+'" src="//zuhaowan.zuhaowan.com/v1/images/grey.gif"/>' 
						;
				var subHtml = '';
				// 下级分类
				if(gd.dataList != null && gd.dataList != 0) {
				    var subHtmlLi = fillDataHtml(gd.dataList);
				    if($(subHtmlLi).length > 1) {
    					// 下级分类名称
    					subHtml += '<div id="p'+gd.id+'sub" class="sub_data" style="display: none;"><div class="skinjiao"></div><div class="sub_title">' + gd.sub_dt_name + '</div>';
    					subHtml += '<ul class="submenu_li">' + subHtmlLi + '</ul></div>';
				    }
				    
				}
				var subNum = $(subHtml).children(".submenu_li").children("li").length-1;
			    if(subNum > 0) {
				    dataHtml += '<div class="sub_num">'+subNum+'</div>';
			    }
			    dataHtml += '</div>'
					+ '<div class="con">' + gd.data_name + (gdNum > 0 ? '×<b style="color:#F00;">'+gdNum+'</b>' : '') + '</div></div>';
				
				if(subHtml != '') {
				    dataHtml += subHtml;
				}
				dataHtml += '</li>';
			}
		}
		dataHtml += '<li class="clear"></li>';
		return dataHtml;
	}
	
	function getGameDataChoosed(gdId) {
		// gdId 格式：gd_33_
		// gameDataChoose 格式：gd_33_0,gd_35_4,...
		var gameDataChoose = "";
		var gdArr = gameDataChoose.split(",");
		var gdChs = null;
		for(var i = 0; i < gdArr.length; i++) {
			var gd = gdArr[i];
			if(gd.indexOf(gdId) > -1) {
				gdChs = new Array();
				gdChs[0] = gdId.substring(3, gdId.length); // id
				gdChs[1] = gd.substring(gdId.length, gd.length); // num
			}
		}
		return gdChs;
	}
	
	function showDiv(id) {
		$("#"+id).css({'z-index': 10}).show();
		if($("#"+id).children(".submenu_li").children("li").length > 4) {
			$("#"+id).css({"width":"300px"});
	    }else{
	    	$("#"+id).css({"width":"160px"});
	    }
	}
	
	function hideDiv(id) {
		$("#"+id).css({'z-index': 0}).hide();
	}
	
	/** LOL英雄排序 **/
	function sortHero(heroUl, index) {
	    var liList = $(heroUl).children(".gt");
	    if(index >= $(liList).length) {
	        return;
	    }
	    
	    var maxLi = $(liList).eq(index);
	    var tempNum = $(maxLi).children(".gd_info").children(".gd_info_img").children(".sub_num").text();
	    var maxNum = tempNum == undefined ? "0" : tempNum;
	    for(var i = index; i < $(liList).length; i++) {
	        var tempLi = $(liList).eq(i);
	        tempNum = $(tempLi).children(".gd_info").children(".gd_info_img").children(".sub_num").text();
	        if(undefined != tempNum && tempNum > maxNum) {
	            maxNum = tempNum;
	            maxLi = tempLi;
	        }
	    }
	    
	    if(maxNum > "0") {
            $(liList).eq(index).before($(maxLi));
	    }
	    sortHero(heroUl, ++index);
	}
	
	/** 添加收藏 */
	function addCollect(actId, obj) {
		var url = "/Account/accountCollectAdd.html";
		var reqData = {
				actId : actId,
		};
		//showLoading();
		 
		$.ajax({
            url: url,
            type: "get",
            data: reqData,
            dataType: "json",
            success: function (data) {
            	if(data.flag == "1") {
    				//alert("收藏成功");
    				$.dialog.alert("收藏成功", function(){
    					var patt = new RegExp(/\d+/);
    					var num = patt.exec($(obj).next().text());
    					num = parseInt(num)+1;
    					$(obj).next().text("（"+num+"人气）")
    				});
    			} else if(data.flag == "2") {
    				//alert(data.msg);
    				$.dialog.alert(data.msg);
    			} else {
    				$.dialog.alert("请登录后操作", function(){
    					location.href="/Login/login.html";
    				});
    			}
            }
        });
		
	}
	
	function changeActTab(name, cursel, n) {
		for(var i = 1; i<=n; i++) {
			var menu = document.getElementById(name+i);
			var con  = document.getElementById("con_"+name+"_"+i);
			menu.className    = i==cursel ? "hover" : "";
			con.style.display = i==cursel ? "block" : "none";
		}
	}
	
	function getAppraiseOfAccount(page) {
		var url = "/Search/appraiseOfAccount.html";
		var reqData = {
				actId : "318596",
				page  : page
		};
		$.get(url, reqData, function(data) {
			$("#a_d_r_c").empty();
			
			if(data == null || data.length == 0) {
				$("#a_d_r_c").append('<div style="text-align: center;">暂无评价</div>');
			}
			else {
				for(var i = 0; i < data.length; i++) {
					var rate = data[i];
					var salerMark = rate.salerMark;
					var rHtml = '';
					if(rate != null) {
						var userLvl = getBuyerLvl(parseInt(rate.buyer_points));
						rHtml += '<div>';
						rHtml += '<div class="infor_a">';
						rHtml += '<span class="sp_a">用户名：' + rate.userid + '</span>';
						rHtml += '<span class="sp_a">用户等级：<a href="/news/111.html" target="_blank">【' + rate.userInfo.buy_level + '】</a></span>';
						rHtml += '<span class="sp_b">用户评价：<img style="padding-top:5px;" src="//zuhaowan.zuhaowan.com/v1/images/rate_' + rate.f + '.png" /></span>';
						rHtml += '<div class="clear">';
						rHtml += '<p>' + rate.n + '</p>';
						if(salerMark != null) {
							rHtml += '<p><span style="color:#df3234;">【卖家解释】</span>' + salerMark.n + '</p>';
						}
						rHtml += '</div>';
						rHtml += '</div>';
						rHtml += '<div class="infor_b">' + rate.t + '</div>';
						rHtml += '</div>';
					}
					
					if(rHtml != '') {
						$("#a_d_r_c").append(rHtml);
					}
				}
			}
		});
		
	}
	
	/** 立即租赁 */
	function rentNow() {
	    /*var rentType = $("input[name='rentType']").val();
	    if(rentType == "8") {
