    	        $.dialog.alert("当前不是包夜时间");
    	        return;
    	    }
	    }*/
		$("form[name='rentForm']").submit();
	}

	$(document).ready(function(){
	    // 图片懒加载
	    $("img.lazy").lazyload({
            effect : "fadeIn",
            skip_invisible : false
        });
		
		$('#slider').flexslider({
			animation: "slide",
			controlNav: false,
			animationLoop: false,
			slideshow: false,
			sync: "#carousel"
		});
		
		/*$(".menu li").hover(function() {
			$(this).find(".d_txt").animate({opacity: "show", top: "-75"}, "slow");
		}, function() {
			$(this).find(".d_txt").animate({opacity: "hide", top: "-85"}, "fast");
		});*/
		
		getGameData(); // 加载游戏资料数据
		
		$("#a_d_r_p").paginate({
			count    : 1,
			start    : 1,
			display  : 5,
			onChange : function(page){
			    window.location.hash=""; // 清空网页定位
			    getAppraiseOfAccount(page);
			    window.location.hash="#aprs"; // 将网页定位到某个位置
			}
		});
		
		//changeActTab('one',1,3);
		getAppraiseOfAccount(); // 加载评论
	});
</script>
<script type="text/javascript">
function report(){
    var haoid=$('#haoid').val();
    var gname=$('#gname').val();
    var lx=$("#lx").val();
    var ly=$("#ly").val();
    if(ly.length>40){
        //alert('举报理由太长啦！')
        $.dialog.alert('举报理由太长啦！', function(){
            $('#ly').focus();
        })
        return false;
    }
    var data={haoid:haoid,gname:gname,lx:lx,ly:ly};
    $.ajax({
        url:"/Search/report.html",
        type:"post",
        dataType:"json",
        data:data,
        success:function(data){
            if(data.state==1){
                //alert(data.msg);
                $.dialog.alert(data.msg, function(){
                    location.reload();
                });
            }else if(data.state==2){
                //alert(data.msg);
                $.dialog.alert(data.msg, function(){
                    location.reload();
                });
            }else if(data.state==3){
                //alert(data.msg);
                $.dialog.alert(data.msg, function(){
                    location.href=data.url;
                });
            }else{
                //alert(data.msg);
                $.dialog.alert(data.msg);
            }
        }
    });

}
</script>
