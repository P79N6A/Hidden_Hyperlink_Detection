  <script type="text/javascript">	
	function openJesongChat(c,tar){
		if("undefined" == typeof jesong){
			var p='';
			if( c == 'c')
			p = 'n='+tar;
			else if(c == 'g'&&tar!=null&&tar!='0')
			p = 'g=' + tar;
			openNoJesongJsChat(p);
		}else{
			jesong.win.openChat(c,tar);
		}
		return false;
	}	
	function openNoJesongJsChat(c){
				var url = "http://live.jswebcall.com/live/" +'chat.dll?c=5241';
				url = url +"&chatUrl="+window.encodeURIComponent(window.location.href);
				if(getCook("im_refer")){
					url = url +"&refer="+window.encodeURIComponent(getCook("im_refer"));
				}
				if(typeof c == 'string' &&c.length !=0 ){
					url += '&'+c;
				}
				var p = "height=500,width=800,directories=no,location=no,menubar=no,resizeable=no,status=no,toolbar=no,top=100,left=200";
				try{
					varcw = window.open(url,'chat_'+1,p);cw.focus();
				}catch(e){
					if(c.force)window.location = url;				
				}
	}
	function getCook(name)
	{ 
		var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");arr=document.cookie.match(reg);if(arr){return unescape(arr[2]);}else{return null;}
	}
	
	function initJesongTags(){
		var mAttach=function(t,e,f){if(t.addEventListener) t.addEventListener(e,f,false);else if(t.attachEvent) t.attachEvent('on'+e,f);}
	        var snifferNode=function(nd){
	             if(nd.nodeType==1 && nd.nodeName=='IMG' && nd.getAttribute("EasyliaoTag") != null){
	                    	var groupId = nd.getAttribute("EasyliaoTag");
	                    	mAttach(nd,'click', function(){
													openJesongChat('g',groupId);
	                    	});
	                    	
	                      nd.style.cursor="pointer";
	             }
	        };
	        var processNode=function(nd){
	            var o =nd.childNodes;
	            snifferNode(nd);
	            if(o&&o.length>0)
	                for(var i=0;i<o.length;i++){
	                   processNode(o[i]);
	                }
	        };
	        
	        processNode(document.body);

	};

initJesongTags();
</script> 
  <!-- IM的JS--> 
  <script>
function getQueryValue(p)
{
var url = location.search; //获取url中"?"符后的字串 
if (url.indexOf("?") != -1) { 
var str = url.substr(1); 
strs = str.split("&"); 
for(var i = 0; i < strs.length; i ++) {

var kandv = strs[i].split("=");
if(kandv[0]==p)
return kandv[1];
} 
}
return null;
}

function setCook(name,value,t)
{
if(typeof t =='undefined' ||t==null) t =60*30*24*60*60*1000;  
var exp  = new Date(); exp.setTime(exp.getTime() + t);
document.cookie = name + "="+ escape (value)+ ";expires=" + exp.toGMTString();
}

function initJesongExtCookie()
{
var ca_kid = getQueryValue("ca_kid");
if(ca_kid==null)
return;

var JESONG_EXT_DATA = "#params:ca_kid," + ca_kid;
setCook('JESONG_EXT_DATA',JESONG_EXT_DATA,10*60*1000);
}

initJesongExtCookie();

</script> 
