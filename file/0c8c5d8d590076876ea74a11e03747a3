			$(".xiao_but").show();
			$(".dibuceshi").hide();
	});
	$(".xiao_but").click(function(){
			$(".dibuceshi").show();
			$(".xiao_but").hide();
	});
	
	$("form[name = 'form_cs']").submit(function(){
		var msg = 1;
		$(".cs_required").each(function() {  
		   if($(this).val() == ''){
              $(this).css("border","1px solid red");
			  msg = 0;		 
		   }
        });
		if(msg == 0){
			return false;
		}
	});
	/*表单验证结束*/	

});
