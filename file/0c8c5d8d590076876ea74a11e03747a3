$(document).ready(function(){	

	$("#addr2").focus(function(){
			$(".db_jgxz").show();
	});
	$(".close-button").click(function(){
			$(".db_jgxz").hide();
	});
	$(".db_city li").click(function(){
			$("#addr2").val($(this).text());
			$(".db_jgxz").hide();
	});

