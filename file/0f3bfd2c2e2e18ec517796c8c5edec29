<script>

(function(){
    function formatTemplate(options){
        return `<div class="row push-down" data-id="${options.commentId}" style="background:${options.bgColor||'transparent'};">
            <div class="col-xs-2 col-sm-2">
                <img src="${options.avatar}" class="img-responsive" style="min-width:50px;">
            </div>
            <div class="col-xs-9 col-sm-10">
                <div class="row">
                    <div class="col-sm-12 ">
                        <a href="/user/profile/${options.playerTag}">${options.playerTag}</a>
                        <span>${options.createdAt}</span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <div class="static-rating">
                            <span data-rating="5" class="star ${ options.rating == 5 ? 'selected' : '' }"></span>
                            <span data-rating="4" class="star ${ options.rating == 4 ? 'selected' : '' }"></span>
                            <span data-rating="3" class="star ${ options.rating == 3 ? 'selected' : '' }"></span>
                            <span data-rating="2" class="star ${ options.rating == 2 ? 'selected' : '' }"></span>
                            <span data-rating="1" class="star ${ options.rating == 1 ? 'selected' : '' }" style="padding-left:0px;"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 ">
                        <p>${options.comment}</p>
                    </div>
                </div>
            </div>
        </div>`;
    }
    $('#commentsModal').on('show.bs.modal', function(){
        $.get('/web/v1/game/comments/recent/endless-mahjong/30', function(data){
            $('#commentsModal .loading').addClass('hidden');
            if(data.comments.length > 0){
                for(var i in data.comments){
                    var comment = data.comments[i];
                    $('#commentsModal .modal-comments').append(formatTemplate({
                        avatar: comment.avatar,
                        playerTag: comment.player_tag,
                        createdAt: comment.readable_date,
                        rating: comment.rating,
                        comment: comment.comment
                    }));
                }
                $('#commentsModal .modal-comments').removeClass('hidden');
                $('#commentsModal .modal-footer').removeClass('hidden');
            }else{
                $('#commentsModal .no-comments').removeClass('hidden');
            }
        }, 'json');
    });
    $('.comment-submit').on('click', function(e){
        e.preventDefault();
        var comment = $('#commentsModal #game-comment').val();
        if(comment.length > 10){
            $('#commentsModal .comment-submit').addClass('hidden');
            $('#commentsModal .inline-dots-container').removeClass('hidden');
            $.post('/web/v1/game/comment', $('#add-comment').serialize(), function(response){
                $('#commentsModal .comment-submit').removeClass('hidden');
                $('#commentsModal .inline-dots-container').addClass('hidden');
                $('#commentsModal #game-comment').val('');
                $('#commentsModal .modal-comments').prepend(formatTemplate({
                    avatar: response.avatar,
                    playerTag: response.player_tag,
                    createdAt: response.readable_date,
                    rating: null,
                    comment: comment,
                    commentId: response.comment_id,
                    bgColor: 'rgba(229,113,9,0.8)'
                }));
                $(`#commentsModal .modal-comments div[data-id=${response.comment_id}]`).animate({
                    backgroundColor: 'transparent'
                }, 2000);
            });
        }
    });
    // gs.comments.show();
})();

</script>
