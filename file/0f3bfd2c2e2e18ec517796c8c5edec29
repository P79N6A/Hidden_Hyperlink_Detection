<script>
    // Rate the game
    gs.gui.rating.set(0);

    $('#ratingModal').on('show.bs.modal', function(e){
        getRatings();
        $('#game').addClass('disable-pointer-events');
    }).on('hidden.bs.modal', function() {
        $('#game').removeClass('disable-pointer-events');
    });

    $(document).on('click', '#ratingModal .rating > a.star', function(e){
        e.preventDefault();
        var rating = $(this).attr('data-rating');
        var $this = $(this);
        gs.game.rate(30, rating, function(err, data){
            if(err){
                console.log('There was an error');
            }else{
                gs.gui.rating.set(rating);
                $('.fa.fa-star').addClass('fa-theme');
                getRatings();
            }
        });
    });

    function getRatings(){
        gs.gui.rating.ratings(30, function(data){
            for(var i = 0; i < data.results.length; i++){
                var rating  = data.results[i].rating;
                var percent = data.results[i].percent;
                var total   = data.results[i].rating_count;
                var average = data.results[i].average;
                if(rating !== null){
                    $('.progress-' + rating + ' > div')
                    .attr('aria-valuenow', percent).css('width', percent + '%').text(total.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
                }else{
                    $('.rating-avg').text(Math.round(average * 10) / 10);
                    $('.total-votes').text(total);
                }
            }
        });
    }
</script>
