  <script>
jQuery(function($){
//点击弹出确认层
$(".loginselecttype").click(function(e) {	
	if ($("#logingenGoodsPanel").css("display") == "none") {
		e.stopPropagation();
		$("#logingenGoodsPanel").show();
		$("#loginshekengmask").show();		
	} else {
		$("#logingenGoodsPanel").hide();
		$("#loginshekengmask").hide();		
	}

});				
$(".loginshekengclose").click(function(e) {
	if ($("#logingenGoodsPanel").css("display") == "none") {
		e.stopPropagation();
		$("#logingenGoodsPanel").show();
		$("#loginshekengmask").show();		
	} else {
		$("#logingenGoodsPanel").hide();
		$("#loginshekengmask").hide();		
	}
});
   }
)
</script>
