  <script>
    function show(obj){
        var img = document.getElementById("showing");

        img.style.display="";
        img.style.position="absolute";
        img.style.top=obj.offsetTop;
        img.style.left = obj.offsetLeft+obj.width;
    }
    function hide(){
         var img = document.getElementById("showing");
        img.style.display="none";
    }
</script> 
