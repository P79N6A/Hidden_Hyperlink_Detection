                     },
                     formatResult: function(row) {//返回值
                         return row.pixname;
                     }
                 }).result(function(event, row, formatted) {//得到值后的事件
                      window.location.href='http://www.guifun.com/house/'+row.hid+'.html';
                 });
	 });
