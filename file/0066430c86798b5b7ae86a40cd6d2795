  <script>
 						$.fn.smartFloatHeader = function() {//顶部LOGO区滚动置顶
					    var position = function(element) {

					        var top = element.position().top, pos = element.css("position");

							var logoObj2 = $("#topHeaderBox").offset();	
					        top2_header = logoObj2.top;
					        //console.debug("这是top2"+top2);
					        


					        //alert(top);

					       // var a = $("#logo").offset();

					        //console.debug("这是info"+a.top);

					        $(window).scroll(function() {

					        	 // var a = $("#logo").offset();	
					        	 // console.debug("这是info"+a.top);

					            var scrolls_header = $(this).scrollTop();
					            if (scrolls_header > top2_header) {
					                if (window.XMLHttpRequest) {
					                    element.css({
					                       
					                        top: 0
					                    });    
					                } else {
					                    element.css({
					                        top: scrolls
					                    });    
					                }
					                 $("#topHeaderBox").css("width","100%").css("background-color","#FFFFFF").css("background-position","0 -80px").css("box-shadow","0px 3px 5px rgba(0, 0, 0, 0.18)").css("z-index","1000");
					            }else {
					                element.css({
					                   position: "relative",
					                    top: top
					                });   
					                 $("#topHeaderBox").css("background-color","#FFFFFF").css("background-position","0 0px").css("box-shadow","0px 0px 0px rgba(0, 0, 0, 0)"); 
					            }
					        });
					    };
					    return $(this).each(function() {
					        position($(this));                         
					    });
					};	
					

					var ie6=!-[1,]&&!window.XMLHttpRequest;
					
					if(!ie6){
						//绑定
						$("#topHeaderBox").smartFloatHeader(); 
					}
</script> 
