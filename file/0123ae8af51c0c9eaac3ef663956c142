    var SetAttentionExhibitionStatue = function () {
        $.ajax({
            url: "http://exh.zhankoo.com/Exhibition/Home/AttentionExhibtiionStatueFind",
            data: { exhibitionIDs: "" },
            async: true,
            type: "POST",
            success: function (data) {
                if (data.success) {
                    if (data.list.length > 0) {
                        for (var i = 0; i < data.list.length; i++) {
                            var attention = data.list[i];
                            var obj = $("a[data-id=Exhibition_" + attention.Text + "]");
                            var html = "";
                            if (attention.Value == "true") {
                                html = '<i class="iconfont red" onclick="return AttentionExhibition(this,' + attention.Text + ');" >&#xe615;</i>';
                                obj.html(html);
                            }
                        }
                    }
                }
            }
        })
    }
    $(document).ready(function () {
        $("#TotalCount").html('共找到结果 <b style="color: #f25618; font-style: normal;" id="searchCount"> 0</b>条');
        contrast.sortList();
        var keyWord = "网上博彩";
        if (keyWord.length > 1) {
            $("#pager").find("a").each(function () {
                var url = $(this).attr("href");
                if (url && url.length > 1) {
                    $(this).attr("href", url + "?keyword=" + keyWord);
                }
            });
        }
    });

    function openCCEUrl(obj) {
        var url = $(obj).attr("linkurl");
        window.open(url);
        var evt = event || window.event;
        event.preventDefault();
        event.stopPropagation();
    }
