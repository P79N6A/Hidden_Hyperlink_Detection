        var AttentionExhibition=function(var1,exhibitionID){
            var  obj=$(var1);
            if(isLogin==1)
            {
                $.ajax({
                    url:"http://exh.zhankoo.com/Exhibition/Home/AttentionExhibitionInsert",
                    data:{ExhibitionID:exhibitionID},
                    type:"POST",
                    success:function(data){
                        if(data.success)
                        {
                            var count=data.count;
                            if(data.type==2){ //取消关注
                                $(obj).html("<i class=\"iconfont\">&#xe615;</i>");
                                $(obj).closest("span").find("span").html(count);
                            }else{ //关注
                                $(obj).html("<i class=\"iconfont red\">&#xe615;</i>");
                                $(obj).closest("span").find("span").html(count);
                            }
                        }else{
                            layer.alert(data.message);
                        }
                    },
                    error:function(){
                        layer.alert("sorry，系统被你玩坏了，稍后再试吧");
                    }
                });

            }else{
                layer.open({
                    type: 2,
                    title: false,
                    area: ['340px;', '430px;'],
                    shade: 0.5,
                    closeBtn: 1,
                    shadeClose: true,
                    content: '//passport.zhankoo.com/account/logindialog?returnUrl=' + window.location.href
                });
            }
        }
        /*----------------关注展会------------------*/

