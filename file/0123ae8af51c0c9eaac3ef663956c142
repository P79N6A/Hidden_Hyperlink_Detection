                    if(province[i].Code=="")
                    {className="thisit";}
                    var searchChar="117-0-20180201-20180228-1-3-0-1?keyword=网上博彩";
                    var split=searchChar.split('-');
                    split=split.map(function(e,index){
                        if(index==1)
                            return "p"+province[i].Code;
                        else
                            return e;
                    });
                    html += '<a href="http://exh.zhankoo.com/zhanhui/search/'+split.join('-')+'"  class="'+className+'" title="' + province[i].Name + '">' + name + ' </a>';
