    <script type="text/javascript">

$(function(){
	$('.emotion').qqFace({
		id : 'facebox', 
		assign:'feedback_msg', 
		path:'/images/statics/arclist/'	//表情存放的路径
	});
	$("#sub_btn").click(function(){
		var str = $("#feedback_msg").val();
		$("#show").html(replace_em(str));
	});
});
//查看结果
function replace_em(str){
	str = str.replace(/\[em_([0-9]*)\]/g,'<img src="/images/statics/arclist/$1.gif" border="0" />');
	return str;
}
</script> 
