   </div> 
   <script>
		var _content = []; //临时存储li循环内容
		var lanren = {
			_default:35, //默认显示图片个数
			_loading:8,  //每次点击按钮后加载的个数
			init:function(){
				var lis = $(".you .wod22");
				$(".list").html("");
				for(var n=0;n<lanren._default;n++){
					lis.eq(n).appendTo(".list");
				}
			
				for(var i=lanren._default;i<lis.length;i++){
					_content.push(lis.eq(i));
				}
				$(".hidden").html("");
			},
			loadMore:function(){
				var mLis = $(".list").length;
				for(var i =0;i<lanren._loading;i++){
					var target = _content.shift();
					if(!target){
						$('.more').html("<p>全部加载完毕...</p>");
						break;
					}
					$(".list").append(target);
				
				}
			}
		}
		lanren.init();
	</script> 
