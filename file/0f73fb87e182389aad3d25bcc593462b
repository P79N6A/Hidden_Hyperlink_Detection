    <script language="javascript">
window.page = 1;
function LoadCommets(page)
{
		var taget_obj = $('#commetcontent');
    var html= "<div style='line-height:50px'><img src='/images/loadinglit.gif' />评论加载中...</div>";
   // taget_obj.html(html);
	  $.ajax({
      type: "GET",
      url: "/plus/feedback_ajax.php",
      data: "dopost=getlist&aid=23511&page="+page,
      dataType: 'html',
      success: function(result)
      {
        if(result == 'error'){
          alert('已经没有了');
          return;          
        }
        window.page++;
        taget_obj.append(result);
      }
    });	
}
function PostComment()
{
		var f = document.feedback;
		var msg =  $("#feedback_msg").val().trim().replace(/\[em_([0-9]*)\]/g,'<img src=/images/statics/arclist/$1.gif border="0" />');  
		
		var nface = '6';
		var nfeedbacktype = 'feedback';
		var nvalidate = '';
		var taget_obj = $('#commetcontentNew');  
		if(msg=='')
		{
			alert("评论内容不能为空！");
			return;
		}
		if(f.validate)
		{
			if(f.validate.value=='') {
				alert("请填写验证码！");
				return;
			}
			else {
				nvalidate = f.validate.value;
			}
		}
		if(msg.length > 500)
		{
			alert("你的评论是不是太长了？请填写500字以内的评论。");
			return;
		}				
	
    taget_obj.html("<div style='line-height:30px'><img src='/images/loadinglit.gif' />正在发送中...</div>");
		$.ajax({
      type: "post",
      url: "/plus/feedback_ajax.php",
      data: "dopost=send&aid=23511&fid="+f.fid.value+"&face="+nface
      +"&feedbacktype="+nfeedbacktype+"&validate="+nvalidate
      +"&msg="+msg,
      dataType: 'html',
      success: function(result)
      {
        taget_obj.html(result);
		 $("#feedback_msg").val("");
		 $(".feed33").val("");
		 $("#feedback_quote").html("");
		 $("#validateimg").click();
		 $("#feedbackfid").val("");
		 $(".nb").val("");
		 $(".nb22").click();
	     $(".ipt-txt").val("");
		 $(".yinyong").html("");
      }
    });			

		f.msg.val('');	
		f.fid.value = 0;
		if(f.validate)
		{
			if($('#validateimg')) $('#validateimg').src = "/include/vdimgck.php?"+f.validate.value;
			f.validate.value = '';
		}		
}

  
function quoteCommet(fid)
{
	    document.feedback.fid.value = fid;
}
  
function ajaxFeedback(aid, fid, type)
{
	
	var taget_obj = $DE('ajaxfeedback_'+fid);
	if(taget_obj.innerHTML == '')
	{
		var myajax = new DedeAjax(taget_obj, true, true, '', 'x');
		myajax.SendGet2("/plus/feedback.php?aid="+aid+"&fid="+fid+"&action=quote&type=ajax");
		eval('var result = typeof CKEDITOR.instances.msg_'+fid);
		if(result != 'undefined')
		{
			// 删除实例
			eval('var edit = CKEDITOR.instances.msg_'+fid);
			CKEDITOR.remove(edit);
		}
		CKEDITOR.replace(document.getElementById('msg_'+fid) , CKEDITOR.instances.msg.config);
		scroll(0, taget_obj.offsetTop - 120);
		var formname = 'f = document.ajaxfeedback_'+fid;
		eval(formname);
		if(f.validate)
		{
			if($DE('vdimg_'+fid)) $DE('vdimg_'+fid).src = "/include/vdimgck.php?"+f.validate.value;
			f.validate.value = '';
		}
		
		DedeXHTTP = null;
	}
}




function postBadGood(ftype,fid)
{
	var taget_obj = $("#"+ftype+fid); 
	var saveid = $.cookie('badgoodid');
	if(saveid != null)
	{
		 var saveids = saveid.split(',');
		 var hasid = false;
		 saveid = '';
		 j = 1;
		 for(i=saveids.length-1;i>=0;i--)
		 {
		 	  if(saveids[i]==fid && hasid) continue;
		 	  else {
		 	  	if(saveids[i]==fid && !hasid) hasid = true;
		 	  	saveid += (saveid=='' ? saveids[i] : ','+saveids[i]);
		 	  	j++;
		 	  	if(j==10 && hasid) break;
		 	  	if(j==9 && !hasid) break;
		 	  }
		 }
     if(hasid) {return false; }
     else saveid += ','+fid;
		 $.cookie('badgoodid',saveid,{expires: 1});
	}
	else
	{
		$.cookie('badgoodid',fid,{expires: 1});
	}
  $.ajax({
      type: "post",
      url: "/plus/feedback.php",
      data: "aid=23511&action="+ftype+"&fid="+fid,
      dataType: 'html',
      success: function(result)
      {      
        taget_obj.html(result);
      }
   });
}
LoadCommets(1);






function ajaxQuotePost(fid)
{
$("#feedbackfid").val(fid);
$("#feedback_msg").val($("#msg_" + fid).val());
$("input[name=validate]:first").val($("#validate_" + fid).val());
PostComment();
$("#feedback_msg").parent("#feedbackfid").hide();

 
}
</script> 
