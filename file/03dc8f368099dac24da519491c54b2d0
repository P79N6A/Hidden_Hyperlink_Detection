        //跑马灯数据在隐藏的div里面，放在数组里    
        var marqueeContent = new Array();
        marqueeContent[0] = "";
        marqueeContent[1] = "";
        $(document).ready(function() {
            $("#JsInitMarquee div").each(function(i) {
                marqueeContent[i] = $(this).html();
            });
            //指定当前导航条选项的样式，通过文件名和导航条li的id名是否一致判断
            var As = $('#globalNav').children(), d = 0;
            for (i = 1; i < As.length; i++) {
                if (window.location.href.indexOf(As[i].id) >= 0) {
                    As[i].className = 'current'; d = 1;
                }
            };
            if (d == 0) {
                As[0].className = 'current';
            };
            
        });
        //收藏夹方法
        function addfavorite() {
            if (document.all) {
                window.external.addFavorite('http://www.yoyoe.com/', '逸程旅行网,让旅程更安逸!');
            }
            else if (window.sidebar) {
                window.sidebar.addPanel('逸程旅行网,让旅程更安逸!', 'http://www.yoyoe.com/', "");
            }
        }
