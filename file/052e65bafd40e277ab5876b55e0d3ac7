			  function search(s){
				  if (s == 1){
					  $('#xiaosou').fadeOut();
					  $('.aw-search-box').stop().css({top:-50}).show().animate({top:43},100);
				  }else{
					  $('#xiaosou').fadeIn();
					  $('.aw-search-box').stop().fadeOut();
				  }
