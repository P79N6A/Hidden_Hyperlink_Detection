	<script>
	$(function(){

		$('form.form-newsletter').submit(function(){

			var email = $(this).find('input[name=email]').val(),
			    re    = /\S+@\S+\.\S+/;

			if (re.test(email)) {
				$.post('https://www.horsesensesolutions.com/360blast/subscribe', $(this).serialize(), function(data) {
					if (data == 'success') {
						$('.newsletter-form .form').hide();
						$('.newsletter-form .form-response').html('<div class="alert alert-success"><span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span> Thank you for subscribing to our newsletter.</div>');
					} else {
						$('.newsletter-form .form-response').html('<div class="alert alert-danger"><span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span> An error occured with your submission.</div>');
					}
				});
			} else {
				$('.newsletter-form .form .btns').removeClass('disabled');
				$('.newsletter-form .form-response').html('<div class="alert alert-danger"><span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span> A valid email address is required.</div>');
			}

			return false;

		});

	});
	</script>


