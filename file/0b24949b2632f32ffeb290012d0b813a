				jQuery(function ()
				{
					// 全媒体文章点击量统计,创建XDomainRequest
					function initXDomainRequest() {
						var xdr;
						if (window.XDomainRequest) {
							xdr = new XDomainRequest();
						} else if (window.XMLHttpRequest) {
							xdr = new XMLHttpRequest();
						}
						if (!xdr) {
							var names = ["msxml", "msxml2", "msxml3", "Microsoft"];
							for (var i = 0; i < names.length; i++) {
								try {
									var name = names[i] + ".XMLHTTP";
									xdr = new ActiveXObject(name);
									break;
								} catch (e) { }
							}
						}
						return xdr;
					}
				
					// 全媒体文章点击量入口函数，在电子报详情页加载完成时通过http pos到全媒体记录点击量。
					function doArticleHits2() {
						try {
							var urlStr = $("#link_hide").find("a").attr("href");
							var id = urlStr.substring((urlStr.indexOf("_")+1),urlStr.indexOf(".htm"));
							
							var pubtime=document.getElementById("date_hide").innerHTML;
								pubtime=pubtime.replace( /^\s+/, "" ).replace( /\s+$/, "" ); 
						 
						 // var pubtime = jQuery("#date_hide").text().trim();// jQuery(".from").first().text().trim().substr(0, 10); // "2014-09-25";
				
							var url = "http://qmt.stcn.com/api/ArticleHits?type=2&webId=" + id + "&pubtime=" + pubtime;
							//alert(url);
							var xdr = initXDomainRequest();
							if (xdr) {
								xdr.open("get", url);
								xdr.send();
							}
						} catch (e) { }
					}
				
				 var publicDate=document.getElementById("date_hide").innerHTML;
				 publicDate=publicDate.replace( /^\s+/, "" ).replace( /\s+$/, "" ); 
				 publicDate=publicDate+" 00:00:00"; //发布时间
				
					var publicDates = publicDate.substring(0, 10).split('-');
					publicDate = publicDates[1] + '/' + publicDates[2] + '/' + publicDates[0] + ' ' + publicDate.substring(10, 19);
				 
				 jQuery.ajax({
							type: 'HEAD', 
							url : window.location.href,
							complete: function( xhr,data )
							{
								var bombay = Date.parse((xhr.getResponseHeader('Date'))) + (60*60* 8);
								var redate = new Date(bombay);
								//alert(redate);
								var a = (redate.getTime() - Date.parse(publicDate)) / 3600 / 1000;
								//alert(a);
									if (a <= 24)
									{ //发布还没有24小时调用统计脚本
										doArticleHits2();
									} 				
							}
					 });
				});
	
	
