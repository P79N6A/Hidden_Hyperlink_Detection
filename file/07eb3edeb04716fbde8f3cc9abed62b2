            document.write(html);
        } else {
            var u = navigator.userAgent;
            var isiOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/); //ios终端
            if (isiOS) {
                //document.getElementById("relateCate").style.height = '1.65rem';
