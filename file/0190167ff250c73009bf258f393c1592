    <script type="text/javascript" language="JavaScript">
function login_switch()
{
	switch(document.forms['loginform'].logintarget.options[document.forms['loginform'].logintarget.selectedIndex].value)
	{
	case "bbs":
		document.forms['loginform'].action          = "http://passport.it.com.cn/phps/login/check_user.php";
		document.forms['loginform'].urlback.value   = "http://bbs.it.com.cn/";
	break;

	case "emailbox":
		document.forms['loginform'].action          = "http://mail.it.com.cn/mail.app";
		document.forms['loginform'].urlback.value   = 'http://mail.it.com.cn';
	break;

	case "passport":
		document.forms['loginform'].action          = "http://passport.it.com.cn/phps/login/check_user.php";
		document.forms['loginform'].urlback.value   = 'http://passport.it.com.cn';
	break;

	default:
		document.forms['loginform'].action          = "http://passport.it.com.cn/phps/login/check_user.php";
		document.forms['loginform'].urlback.value   = 'http://passport.it.com.cn';
	break;
	}
}
function login_submit(){
	var e = document.forms['loginform'].logintarget;
	
	if (e.options[e.selectedIndex].value == "bbs") {
		var user = document.forms['loginform'].username.value;
        if (user=='Guest'){
        window.open("http://bbs.it.com.cn");
        return false;
        } 
		}
	if (e.options[e.selectedIndex].value == "emailbox") {
		var user = document.forms['loginform'].username.value;
		if (user.indexOf("@") == -1) {
			document.forms['loginform'].username.value = user + "@it.com.cn";
		}
	}

	return true;
}
