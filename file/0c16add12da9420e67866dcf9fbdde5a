function isValid()
	{
		if(document.frm1.loupantitle.value==null||document.frm1.loupantitle.value=='')
		{
			alert("请添加楼盘名"); 
			return false; 
		}
		var regu =/(^[1][0-9]{10}$)|(^0[1][0-9]{10}$)/; 
		var re = new RegExp(regu); 
		if(document.frm1.mobile.value==null||document.frm1.mobile.value=='')
		{
			alert("请输入手机号码"); 
			return false; 
		}
		if (!document.frm1.mobile.value.match(regu)) { 
			alert("请输入正确的手机号码"); 
			return false; 
		 } 
		  var myReg = /^([-_A-Za-z0-9\.]+)@([_A-Za-z0-9]+\.)+[A-Za-z0-9]{2,3}$/; 
		 if(!document.frm1.mail.value.match(myReg)&&document.frm1.mail.value!=null&&document.frm1.mail.value!=''){
			 alert("请输入合法的邮箱地址"); 
			 return false; 
		 }
			return true;
	}
