var p_No;
p_No = document.getElementById("Content").getElementsByTagName("p").length;


if(p_No>2){
    $("#Content p:eq(2)").after("<div id=\"ad_incontent\"><iframe width=\"250\" height=\"250\" frameborder=\"0\" scrolling=\"no\" marginwidth=\"0\" marginheight=\"0\" src=\"http://www.tingredian.com/ads/detail.html\" ><\/iframe><\/div>");
}else{
	$("#Content p:eq("+(p_No-1).toString()+")").after("<div id=\"ad_incontent\"><iframe width=\"250\" height=\"250\" frameborder=\"0\" scrolling=\"no\" marginwidth=\"0\" marginheight=\"0\" src=\"http://www.tingredian.com/ads/detail.html\" ><\/iframe><\/div>");
}

</script> 
