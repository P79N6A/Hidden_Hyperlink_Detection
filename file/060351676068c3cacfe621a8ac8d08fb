		    $(".search_left").css("border-bottom","0px");
	    });
        $(".search_left").mouseleave(function(){
		    $("#dis_div").hide();
		    $(".search_left").css("border-bottom","#00a2e9 solid 1px");
        });	
        

    function s_h(strvar)
    {		
	    $('#tp').val($(strvar).attr('name'));
	    var root_name = $('#home_1').attr('name');
	    var root_text = $('#home_1').html();
	    $('#home_1').attr('name',$(strvar).attr('name'));
	    $('#home_1').html($(strvar).html());
	    $(strvar).attr('name',root_name);
	    $(strvar).html(root_text);
	    $('#dis_div').slideUp('normal');
	}

