  <script type="text/javascript">
    $(function () {
        var topsearchtypes = $("#webSearchKeywordn").val();
        //$("#webSearchKeywordn").ComSearchAutoCmpt({ url: "/Ajax/AjaxPage.aspx?t=getsearchintellipromptlist" , searchType : topsearchtypes });

        $("#webSearchKeywordn").keydown(function(e){
            if(e.keyCode==13){
                headerSearch();
            }
        });


        if ($("#webSearchKeywordn").val() == "")
        {
            $("#webSearchKeywordn").val("请输入关键字");
        }
    });

    function headerSearch()
    {
        var topsearchtypes = jQuery(".search_left a :first").attr("name");

        if (topsearchtypes == "1") {
            if(trim($("#webSearchKeywordn").val())==""||trim($("#webSearchKeywordn").val())=="请输入关键字"){
                window.location.href = "http://trade.luosi.com";
            }else{
                window.location.href = "http://trade.luosi.com/offer-0-0-0-0-" + encodeURIComponent($('#webSearchKeywordn').val()) + "-0-0-0-1.html?n=y";
                //window.location.href = "http://trade.luosi.com/offer-0-0-0-0-" + $('#webSearchKeywordn').val() + "-0-0-0-1.html?n=y";
            }
            
        }
        else if (topsearchtypes == "2") {
            if(trim($("#webSearchKeywordn").val())==""||trim($("#webSearchKeywordn").val())=="请输入关键字"){
                window.location.href = "http://yellowpage.luosi.com/companylist.html";
            }else{
                window.location.href = "http://yellowpage.luosi.com/companylist-keyword-"+encodeURIComponent($("#webSearchKeywordn").val())+".html?n=y";
              
            }
        }
        else if (topsearchtypes == "3") {
            if(trim($("#webSearchKeywordn").val())==""||trim($("#webSearchKeywordn").val())=="请输入关键字"){
                window.location.href = "http://standard.luosi.com";
            }else{
                ECommerce.AddStandardSearch("0",$("#webSearchKeywordn").val());
                window.location.href = "http://standard.luosi.com/biaozhun_"+"0"+"st-"+encodeURIComponent($("#webSearchKeywordn").val())+"k-1p-bz.html";
            }
        }
        else {
            if(trim($("#webSearchKeywordn").val())==""||trim($("#webSearchKeywordn").val())=="请输入关键字"){
                window.location.href="http://news.luosi.com";
            }else{
                window.location.href = "http://news.luosi.com/search_list_" + "1" + "_"+encodeURIComponent(filterwords($("#webSearchKeywordn").val()))+".html";
            }
        }
    }


    
    
