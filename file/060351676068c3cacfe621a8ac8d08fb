            jQuery(".nav_content_ul").show();
            jQuery(".b-img").addClass("wiggle");
        });
        
        jQuery(".nav_content_right").mouseleave(function(){
            jQuery(".nav_content_ul").hide();
            jQuery(".b-img").removeClass("wiggle");
