  <script type="text/javascript">

    $(document).click(function (e) {
        e = window.event ? window.event : e;
        var e_tar = e.srcElement ? e.srcElement : e.target;

        if (e_tar.id != "selectionprovince")
            $("#provinceselection").slideUp();

        if (e_tar.id != "selectioncity")
            $("#cityselection").slideUp();
    });

    $(document).ready(function () {

        //
        $(".yellow_Pages .yellow_right .details_left .directory .list").mouseenter(function () {
            $(this).addClass("bg_c");
            $(this).find(".showcomment").show();
        });

        $(".yellow_Pages .yellow_right .details_left .directory .list").mouseleave(function () {
            $(this).removeClass("bg_c");
            $(this).find(".showcomment").hide();
        });
        //

        //
        $(".yellowpagetwo-code img").mouseenter(function () {
            $(this).next().show();
        });

        $(".yellowpagetwo-code").mouseleave(function () {
            $(this).children("div").hide();
        });
        //

    });

    function ShowBaiDuMap(obj, lng, lat, companyname, address, telnumber) {

        $("#mapshowcompanyname").html("&nbsp;&nbsp;" + companyname);
        $position = obj.position();
        $("#mapshow").css("top", $position.top + 210 + "px");
        $("#mapshow").css("left", $position.left - 240 + "px");
        $("#mapshow").show();
        var map = new BMap.Map("baidumap");
        map.addControl(new BMap.NavigationControl());
        var point = new BMap.Point(lng, lat);
        map.centerAndZoom(point, 15);
        var opts = {
            width: 200,     // 信息窗口宽度
            title: "<h3>" + companyname + "</h3>"  // 信息窗口标题
        }
        var infoWindow = new BMap.InfoWindow("地址：" + address + "<br>电话：" + telnumber.replace(",", "，"), opts);  // 创建信息窗口对象
        map.openInfoWindow(infoWindow, point); //开启信息窗口

    }

</script> 
