  <script>

	$(window).bind("scroll", function(){ 

	//当滚动条滚动时

		var scroll_top = $(document).scrollTop();

		if(scroll_top == 0)

		{

			$(".M-top").removeClass("M-top-show");

			$(".M-top").animate({top:'0px'},"slow");

			$(".M-top").css("left","0px");

		}

		if(scroll_top > 0)

		{

			$(".M-top").addClass("M-top-show");

			$(".M-top").css("left","0px");

			$(".M-top").css("top","-165px");

			

		}

		if(scroll_top <500)

		{

			$("#nav").hide();

			

		}

		if(scroll_top >=400)

		{

			$("#nav").show();

			

		}

	});

	

	$(function(){

		$(".M-menu2").click(function(){

			$(".M-top").addClass("M-top-show");

			$(".M-top").animate({top:'0px'},"slow");

			$("#topcontrol").animate({right:'-122px'},"slow");

		})

		

	})

</script> 
