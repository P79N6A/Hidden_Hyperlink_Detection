  <script>

$(window).resize(function(){

	var resolutionList="320,400,640,800,1024,1280,1400,1440,1600,1680,1920,2048,2560,3200,3840,";

	var nowwidth=window.screen.width;

	var H=$(window).width()/2-10

	var T=$(window).width()

	if(resolutionList.indexOf(nowwidth + ",")==-1 ){

		if(H)

		{

			$(".ss-left").css("width",H)	

			$(".ss-right").css("width",H)	

		}

	}

	else{

			if(H)

			{

				$(".ss-left").css("width",H)	

				$(".ss-right").css("width",H)	

			}

		}

	if(resolutionList.indexOf(nowwidth + ",")==-1 ){

				if(T<=3840 & T>=1890 ){

					$("#brand").css("width","1820px")

				}else{

					$("#brand").css("width","1090px")

				}

	}

	else{

			if(T<=3840 & T>=1890 ){

				$("#brand").css("width","1820px")

			}else{

				$("#brand").css("width","1090px")

			}

		}



});

</script> 
