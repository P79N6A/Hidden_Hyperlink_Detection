         <script type="text/javascript">
        (function(S){
            var borders = S.one('.cat-nav').all('.r-border');
            S.use('switchable', function(S){
                var ens = S.one('.entity').all('li');
                var cat = new S.Tabs('#J_CategoryTab',{
                    navCls: 'cat-nav',
                    contentCls: 'cat-con',
                    switchTo: -1,
                    aria: false
                }), content = new S.Node(cat.content),
                    container = new S.Node(cat.container);

                var aNode = cat.nav.children;
                cat.on('switch', function(ev) {
                    if(ev.currentIndex != -1){
                         var offsetTop = cat.nav.children[ev.currentIndex].offsetTop;
                        var height = cat.nav.children[ev.currentIndex].offsetHeight-2;
                        borders.css('height','0');
                        S.Node(borders[ev.currentIndex]).css('height',height+'px');

                        getTop(content,ev.currentIndex,offsetTop,height);

                        if(ev.currentIndex == cat.nav.children.length-1){
                            S.one(cat.nav.children[ev.currentIndex]).css('border-bottom','none');
                        }

                    }
                });

                container.on('mouseleave', function(){
                    S.each(cat.triggers, function(obj) {
                        new S.Node(obj).removeClass('ks-active');
                    });
                    content.hide();
                    cat.switchTo(-1);
                    borders.css('height','0');
                });

            });

            function getTop(obj,index,offsetT,navH){
                var bObj = S.one('.left-border');
                var clientH = document.documentElement.clientHeight,
                    scrollH = document.body.scrollTop||document.documentElement.scrollTop,
                    offsetH = obj.height(),
                    offsetT = offsetT + 60;
                var dis = (clientH + scrollH) - (offsetH + offsetT);
                    dis = dis > 0 ? offsetT-60 : offsetT-60+dis-10;

                /*if(dis < 28){dis = 28;}*/
              /*  if(dis < offsetT-60-offsetH+navH){dis = offsetT-60-offsetH+navH+10;}*/
                obj.css('top',dis+'px').show();
				/*var tObj =KISSY.use('#J_CategoryTab');*/

                isHeight = dis+offsetH-498 > 0 ? dis+offsetH-498 : 0;
                bObj.css('height',isHeight+'px');
            }

        })(KISSY)
    </script> 
         <div class="left-border" style="height: 0px; "></div> 
