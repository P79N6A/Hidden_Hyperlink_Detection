function getSession(wt,tid){
var data = {
'what':wt,
'tsid':tid
}
$.ajax({
type:"POST",
url:"/cmdn/supesite/newdev.ssaa.php",
data: data,
success:function(data){
return;
},
error:function(data){
return;
}
});
}
