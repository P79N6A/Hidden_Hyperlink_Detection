   <script type="text/javascript" src="images/open13b.js"></script> 
   <script type="text/javascript" src="/templets/default/images/open13b.js"></script> 
   <script type="text/javascript" src="/templets/default/images/share.js"></script> 
   <script type="text/javascript" src="/templets/default/images/Common.js"></script> 
   <script type="text/javascript">
(function(){
var topAd = $('#topAd>li');
var topAdPager = $('#topAdPager');
var autoChange=true;
var current=0;

topAd.eq(0).show();
topAdPager.empty();
for(var i=0; i<topAd.size(); i++){
topAdPager.append('<a>'+i+'</a>');
}
topAdPager.children('a').eq(0).addClass('selected');

$('#topAd, #topAdPager').bind({
mouseenter:function(){
autoChange=false;
},
mouseleave:function(){
autoChange=true;
}
})

topAdPager.children('a').each(function(index, element) {
$(this).mouseenter(function(){
change(current=index);
})
});

setInterval(function(){
if(autoChange==true){
change(++current);
};
},5000);
function change(index){
if(index==topAd.size()){
index=0;
current=0;
};
topAd.hide().eq(index).fadeIn(300);
topAdPager.children('a').removeClass().eq(index).addClass('selected');
}
})();

var ability = $('#ability div');
ability.each(function(index, element) {
$(this).bind({
mouseenter:function(){
$(this).children('p').stop().animate({top:0},300);
},
mouseleave:function(){
$(this).children('p').stop().animate({top:140},300);
}
})
});

var fw=$('#fw>a');
var fw2d=$('#fw2d>div');
var fw2dclose = $('#fw2d .close');
fw.each(function(index, element) {
$(this).mouseenter(function(){
fw.removeClass('selected').eq(index).addClass('selected');
fw2d.hide().eq(index).show()
})
$(this).mouseleave(function(){
fw.removeClass('selected');
})
});
fw2dclose.click(function(){
fw.removeClass('selected');
fw2d.hide();
});
</script> 
  </div>
