    function isMobil(obj){ 
    var pattern=/(^[0-9]{3,4}\-[0-9]{3,8}$)|(^[0-9]{3,8}$)|(^\([0-9]{3,4}\)[0-9]{3,8}$)|(^0{0,1}1[0-9]{10}$)/; 
    if(pattern.test(obj)) {
    return true;
    }else{
    return false;
    }
    }
