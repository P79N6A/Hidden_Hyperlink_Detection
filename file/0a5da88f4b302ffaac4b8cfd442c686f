	jQuery(document).ready(function($) {
		$('#map_sidebar').bind('contentchanged',function() {
			var $this = $(this);
			var numItems = $this.children('.results_wrapper').length;
			if (numItems) {
				var $numItemsEl = $this.children('.num_map_results');
				if (!$numItemsEl.length) {
					$numItemsEl = $('<div class="num_map_results"></div>');
					$this.prepend($numItemsEl);
				}
				$numItemsEl.html('<h2>' + numItems + ' results found</h2>');
			}
		});
	});
</script>

