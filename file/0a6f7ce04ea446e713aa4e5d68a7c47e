                required: "请输入留言！"
            }
        },
        errorPlacement: function (error, element) {
            if (element.parent().find("span").size() == 0) {
                element.parent().append("<span></span>");
            }
            error.appendTo(element.parent().find("span").addClass("warn"));
        }
    });
