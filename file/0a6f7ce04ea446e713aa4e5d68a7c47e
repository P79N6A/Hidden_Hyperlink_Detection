    //重新设置产品总数的库存
    function SetTotalResult() {
        var total = 0;
        var totalPrice = 0.00;
        $(".pro_size_item ul").find(".size_input").each(function () {
            total += parseInt($(this).val());
        });
        $(".pro_size_item ul").find(".size_input").each(function () {
            var size_price = parseFloat($(this).closest("ul").find(".size_price").attr("data-id"));
            var mixedJson = eval([{"num":1,"money":118.00}]);
            $.each(mixedJson, function (i, mixedInfo) {
                if (((i == 0 && mixedInfo.money) || (i > 0 && mixedInfo.money > 0 && mixedInfo.num > 0)) && total >= mixedInfo.num) {
                    size_price = parseFloat(mixedInfo.money);
                }
            });
            $(".size_price").text(size_price + "元");
            $(".size_price").attr("date-id", size_price);
            totalPrice += parseInt($(this).val()) * size_price;
        });
        $(".obj-list").find(".amount .value").text(total);
        $(".obj-list").find(".price .value").text(totalPrice);

        $(".table-list tbody").empty();
        var html = "";
        $(".clr-item").each(function () {
            var vid = $(this).attr("vid");
            var name = $(this).attr("title");
            var colorTotal = 0;
            var sizeHtml = "";
            $(".pro_size_item ul[data-id='" + vid + "']").find(".size_input").each(function () {
                if (parseInt($(this).val()) > 0) {
                    colorTotal += parseInt($(this).val());
                    sizeHtml += "<span class=\"name\">" + $(this).closest("ul").find(".size_name").text() + "(" + $(this).val() + ")</span>";
                }
            });
            if (colorTotal > 0 && sizeHtml != "") {
                html += "<tr class=\"last-row\">";
                html += "<td class=\"prop\">" + name + "</td>";
                html += "<td class=\"amount\"><span class=\"value\">" + colorTotal + "</span><span class=\"unit\">件</span></td>";
                html += "<td class=\"desc\">";
                html += "<ul>";
                html += "<li>";
                html += sizeHtml;
                html += "</li>";
                html += "</ul>";
                html += "</td>";
                html += "</tr>";
            }
        });
        $(".table-list tbody").append(html);

    }
    function judgeStock() {
        var re = /^[1-9]\d*|0$/;
        if (!(re.test($(this).val()))) {
            layer.alert("请填入正确的购买数量！", { icon: 0 });
            $(this).val(0);
        }
        else {
            var buyNum = parseInt($(this).val());
            var maxNum = parseInt($(this).attr("maxNum"));
            if (buyNum > maxNum) {
                $(this).val(maxNum);
            }
        }
        SetTotalResult();
    }

    //点击购买加减时触发事件
    $(".pro_size").delegate(".btn,.btn2", "click", function () {
        //尺码全部展开
        $(".pro_size_item").css("height", "auto");

        //隐藏更多按钮
        $("#moresize").hide();

        //------------------------------------------------------------------------加入实际计算逻辑代码开始
        var buyNum = parseInt($(this).parent().find(".size_input").val());
        if ($(this).attr("class") == "btn btn2") {
            $(this).parent().find(".size_input").val(buyNum + 1);
        }
        if ($(this).attr("class") == "btn") {
            if (buyNum > 0) {
                $(this).parent().find(".size_input").val(buyNum - 1);
            }
        }
        var re = /^[1-9]\d*|0$/;
        if (!(re.test($(this).parent().find(".size_input").val()))) {
            layer.alert("请填入正确的购买数量！", { icon: 0 });
            $(this).parent().find(".size_input").val(0);
        }
        else {
            var buyNum = parseInt($(this).parent().find(".size_input").val());
            var maxNum = parseInt($(this).parent().find(".size_input").attr("maxNum"));
            if (buyNum > maxNum) {
                $(this).parent().find(".size_input").val(maxNum);
            }
        }
        SetTotalResult();
        //------------------------------------------------------------------------加入实际计算逻辑代码结束

        //显示详情
        $(".obj-list").show();



    });


</script> 
  <script>
    function ShowRate(page) {
        var rateAjaxUrl = "/GetRateList.html?productid=5674";

        $.ajax({
            type: "GET",
            url: rateAjaxUrl,
            data: "curpage=" + page,
            datatype: "json",
            success: function (data) {
                eval(data);
                RenderRate(data[1]);
                RenderPage(data[0]);
                $(".pro_tab").find(".RateCount").text("成交/评价(" + data[0].RateCount + ")");
            }
        });
    }


    function RenderRate(data) {
        var html = [];
        for (var i = 0, len = data.length; i < len; ++i) {
            if (i % 2 == 0) {
                html.push('<tr class="even">');
            } else {
                html.push('<tr class="odd">');
            }
            html.push('<td class="list list_buyer_sku">');
            html.push('<p class="member">' + data[i].UserName + '</p>');
            var date = new Date(parseInt(data[i].CreateTime.slice(6, 19)));
            html.push('<p class="time">' + date.getFullYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate() + ' ' + date.getHours() + ':' + date.getMinutes() + ':' + date.getSeconds() + '</p>');
            html.push('</td>');
            html.push('<td class="list list_trade_sku">');
            html.push('<div class="item last"><span class="list_spec_sku ">' + data[i].SKUPropNames + '</span><span class="list_price_sku">' + data[i].OnePrice + '</span></div>');
            html.push('</td>');
            html.push('<td class="list list_amount_sku">');
            html.push('<span class="amount">' + data[i].Quantity + ' 双 </span>');
            html.push('</td>');
            html.push('<td class="list list_content_sku ">');
            html.push('<p><span class="sstar sstar' + data[i].ProductQualityScore + '"></span></p>');
            html.push('<p class="content">' + data[i].RateContent + '</p>');
            html.push('</td>');
            html.push('</tr>');
        }
        $("#commentList").html(html.join(""));
    }


    function RenderPage(data) {
        var arr = [],
        total = data.PageCount,
        curr = data.Page,
        pre,
        next;

        arr.push("<tr>");
        arr.push("<td colspan=\"5\">");
        arr.push("<div class=\"pag_p\">");
        arr.push("<div class=\"pag_pages\">");;


        if (curr == 1) {
            arr.push('<a href="javascript:;" class="no-prev">上一页</a>');
            arr.push('<a class="current" href="javascript:;">1</a>');
        } else {
            arr.push('<a href="javascript:;" class="prev" page="' + (curr - 1) + '">上一页</a>');
            arr.push('<a href="javascript:;" page="1">1</a>');
        }
        if (total > 1) {
            if (curr > 4 && total > 7) arr.push('<span>...</span>');
            if (curr < 3) {
                pre = 0;
                next = curr == 1 ? 5 : 4;
                if (curr + next > total) next = total - curr;
            } else if (curr == 3) {
                pre = 1;
                next = 3;
                if (curr + next > total) next = total - curr;
            } else {
                pre = 2;
                next = 2;
                if (curr + next > total) next = total - curr;
                pre = 4 - next;
                if (curr + 3 > total) pre++;
                if (curr - pre < 2) pre = curr - 2;
            }
            for (var i = pre; 0 < i; i--) arr.push('<a href="javascript:;" page="' + (curr - i) + '">' + (curr - i) + '</a>');
            if (curr > 1) arr.push('<a class="current" href="javascript:;">' + curr + '</a>');
            for (var i = 1; i < next + 1; i++) arr.push('<a href="javascript:;" page="' + (curr + i) + '">' + (curr + i) + '</a>');
            if (curr + next < total - 1) {
                arr.push('<span>...</span>');
                arr.push('<a href="javascript:;" page="' + total + '">' + total + '</a>');
            }
            if (curr + next == total - 1) arr.push('<a href="javascript:;" page="' + total + '">' + total + '</a>');
        }
        if (curr == total) arr.push('<a class="next-disabled" href="javascript:;">下一页</a>');
        else {
            arr.push('<a class="next" href="javascript:;" page="' + (curr + 1) + '">下一页</a>');
        }

        arr.push("</div>");
        arr.push("</div>");
        arr.push("</td>");
        arr.push("</tr>");


        $("#commentList").append(arr.join(''));
        $(".pag_pages a").bind("click", function () { ShowRate($(this).attr("page")); });
    }

    ShowRate(1);
</script> 
