    global.init();
    //切换
    var tabli = $(".pro_detail .pro_tab li"),
        tabcon = $(".pro_item"),
        s = false;

    tabli.click(function () {
        if (s) { return; }
        s = true;

        //重置
        tabli.removeClass("active");
        tabcon.hide();

        //开始
        $(this).addClass("active");
        tabcon.eq(tabli.index(this)).show();
