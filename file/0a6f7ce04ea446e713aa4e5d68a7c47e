    var iz = new ImageZoom("qccImage", "qccViewer", { onLoad: function () { } });
    var thumImg = $(".thumb_box a"),
        filter = false;
    thumImg.mouseenter(function () {
        if (filter) { return; }
        filter = true;

        //重置
        thumImg.removeClass("active");

        //读取大图
        var bigSrc = $(this).attr("data-big"),
            photoSrc = $(this).attr("data-photo");
