  <script type="text/javascript">
    function changeProvince() {
        var newProvince = $("#current_area").text().split(' ');
        var oldProvince = $("#hidProvince").val();
        if (newProvince.indexOf(oldProvince) == -1) {
            var productId = $("#productid").val();
            var postData = "productID=" + productId + "&province=" + newProvince[0];
            $.ajax({
                type: "POST",
                data: postData,
                url: "/GetPostFee.html",
                datatype: "json",
                success: function (data) {
                    eval(data);
                    if (data.status == "success") {
                        $(".tatolfree font").text(data.postFee);
                        $("#hidProvince").val(newProvince[0]);
                    }
                }
            });
        }
    }
    function up_taobao() {
        $(".upload_taobao").click();
    }
    function submitNo() {
        var productId = $("#productid").val();

        var total = 0;
        $(".pro_size_item ul").find(".size_input").each(function () {
            if (parseInt($(this).val()) > 0) {
                total += parseInt($(this).val());
            }
        });
        if (total == 0) {
            layer.alert("请填写购买数量数！", { icon: 0 });
            return false;
        }
        var mixedJson = eval([{"num":1,"money":118.00}]);
        if (total < mixedJson[0].num&&((0.00<=0&&0<=0)||0==0))
        {
            layer.alert("无法购买，低于最低门槛！", { icon: 0 });
            return false;
        }
        var color = "";
        var size = "";
        var count = "";
        $(".clr-item").each(function () {
            var name = $(this).attr("title");
            var vid = $(this).attr("vid");
            $(".pro_size_item ul[data-id='" + vid + "']").find(".size_input").each(function () {
                if (parseInt($(this).val()) > 0) {
                    color += name + "ㄉ";
                    size += $(this).closest("ul").find(".size_name").text() + "ㄉ";
                    count += $(this).val() + "ㄉ";
                }
            });
        });
        var postData = "productID=" + productId + "&color=" + color + "&size=" + size + "&count=" + count;
        $.ajax({
            type: "POST",
            data: postData,
            url: "/Shopping/BatchAddToCartConfirm2",
            datatype: "json",
            success: function (data) {
                eval(data);
                if (data.code == "0") {
                    layer.confirm('添加购物车成功，是否立即提交订单？', {
                        btn: ['是', '否'], //按钮
                        shade: false //不显示遮罩
                    }, function () {
                        window.location.href = "/Shopping";
                    }, function () {
                        return;
                    });

                }
                else {
                    layer.alert("添加购物车失败失败！", { icon: 0 });
                }

            }
        });
    }
    function submitEx() {
        var productId = $("#productid").val();
        var total = 0;
        $(".pro_size_item ul").find(".size_input").each(function () {
            if (parseInt($(this).val()) > 0) {
                total += parseInt($(this).val());
            }
        });
        if (total == 0) {
            layer.alert("请填写购买数量数！", { icon: 0 });
            return false;
        }
        var mixedJson = eval([{"num":1,"money":118.00}]);

        if (total < mixedJson[0].num&&((0.00<=0&&0<=0)||0==0))
        {
            layer.alert("无法购买，低于最低门槛！", { icon: 0 });
            return false;
        }
        var color = "";
        var size = "";
        var count = "";
        $(".clr-item").each(function () {
            var name = $(this).attr("title");
            var vid = $(this).attr("vid");
            $(".pro_size_item ul[data-id='" + vid + "']").find(".size_input").each(function () {
                if (parseInt($(this).val()) > 0) {
                    color += name + "ㄉ";
                    size += $(this).closest("ul").find(".size_name").text() + "ㄉ";
                    count += $(this).val() + "ㄉ";
                }
            });
        });
        var postData = "productID=" + productId + "&color=" + color + "&size=" + size + "&count=" + count;
        $.ajax({
            type: "POST",
            data: postData,
            url: "/Shopping/BatchAddToCartConfirm2",
            datatype: "json",
            success: function (data) {
                eval(data);
                if (data.code == "0") {
                    window.location.href = "/Shopping";
                }
                else {
                    layer.alert("立刻购买失败！", { icon: 0 });
                }

            }
        });
    }
</script> 
