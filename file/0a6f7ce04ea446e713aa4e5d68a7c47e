    <script>
    $(function () {

        $('.coy_h').click(function () {
location.href="http://www.qzgoo.cn/login.html?ReturnUrl=http://www.qzgoo.cn/Item.html?productid=5674";return;
            $.ajax({
                url: "/shop/SaveCollectionShop/422",
                type: 'POST',
                dataType: 'json',
                cache: false,
                success: function (result) {
                    if (result.Success) {
                        layer.alert("加入收藏成功！", { icon: 0 });
                    } else {
                        if (result.NeedLogin) {
                            location.href = result.Url;
                        }
                        else {
                            layer.alert("加入收藏失败！" + result.Message, { icon: 0 });
                        }
                    }
                },
                error: function (xmlHttpRequest) {
                    if (xmlHttpRequest.status == 403) {
                        var result = eval("(" + xmlHttpRequest.responseText + ")");
                        if (result.Success) {
                            layer.alert("加入收藏成功", { icon: 0 });
                        } else {
                            if (result.NeedLogin) {
                                location.href = result.Url;
                            }
                            else {
                                layer.alert("加入收藏失败！" + result.Message, { icon: 0 });
                            }
                        }
                    } else {
                        layer.alert("加入收藏失败！" + xmlHttpRequest.responseText, { icon: 0 });
                    }
                }
            });
        });
    });
</script> 
