     <p><script src="/js/CookiesHelper.js"></script> <script type="text/javascript" src="/js/jquery.validate.min.js"></script> <script type="text/javascript" src="/js/jquery.validate.lang_cn.js"></script> <script>
    $(function() {
        var cookieName = "qcc_item_zan_"+5674;
        var cookieValue = cookieHelper.GetCookieValue(cookieName);
        if (cookieValue != null && cookieValue!= "") {
            $('.pro_like').find(".like").find("a span").eq(0).text("已赞");
        }
        $('.pro_like').find(".like").click(function() {
            if(cookieHelper.GetCookieValue(cookieName)=="已赞"){return;}
            $.ajax({
                url: "/TaoBaokuan/AddPraise",
                type: 'POST',
                dataType: 'json',
                data: {productId:5674},
                cache: false,
                success: function (result) {
                    if (result.Success) {
                        $('.pro_like').find(".like").find("span").eq(1).text("("+(parseInt($('.pro_like').find(".like span").eq(1).text().replace("(","").replace(")",""))+1)+")");
                        $('.pro_like').find(".like").find("a span").eq(0).text("已赞");
                        cookieHelper.SetCookiesValue(cookieName, "已赞");
                    } else {
                        layer.alert("点赞失败:" + result.Message, { icon: 0 });
                    }
                },
                error: function (xmlHttpRequest) {
                    layer.alert("点赞失败:" + xmlHttpRequest.responseText, { icon: 0 });
                }
            });
        });
        $('.favourite').click(function() {
            if($(this).find("a").attr("href")=="javascript:;"){
                $.ajax({
                    url: "/TaoBaokuan/AddProductCollection",
                    type: 'POST',
                    dataType: 'json',
                    data: {productId:5674},
                    cache: false,
                    success: function (result) {
                        if (result.Success) {
                            $('.favourite').find("span").eq(1).text("("+result.Message+")");
                            $('.favourite').find("span").eq(0).text("已收藏");
                        } else {
                            layer.alert("收藏失败:" + result.Message, { icon: 0 });
                        }
                    },
                    error: function (xmlHttpRequest) {
                        layer.alert("收藏失败:" + xmlHttpRequest.responseText, { icon: 0 });
                    }
                });
            }
        });
    })
</script></p> 
