    function loadColorPic() {

        var city = remote_ip_info['city'];
        var province = remote_ip_info['province'];
        $(".current_area").text(province + " " + city);
        $(".hidProvince").val(province);

        var productId = $("#productid").val();
        var postData = "productID=" + productId + "&province=" + province;
        $.ajax({
            type: "POST",
            data: postData,
            url: "/GetPostFee.html",
            datatype: "json",
            success: function (data) {
                eval(data);
                if (data.status == "success") {
                    $(".tatolfree font").text(data.postFee);
                    $("#hidProvince").val(newProvince[0]);
                }
            }
        });

        var sizeJons = eval({"9995":{"671":["36","118.00","300"],"29542":["37","118.00","300"],"28388":["38","118.00","300"],"672":["39","118.00","300"],"28389":["40","118.00","300"],"28390":["41","118.00","300"],"28391":["42","118.00","300"],"28392":["43","118.00","300"],"28393":["44","118.00","300"]}});
        $(".pro_size_item").empty();
        var colorLength = 0;
        $(".clr-item").each(function () {
            colorLength++;
            var vid = $(this).attr("vid");
            if (colorLength == 1) {
                $(this).removeClass("J_clr");
                $(this).addClass("on_clr");
            }
            var sizeLength = 0;
            for (var size in sizeJons[vid]) {
                sizeLength++;
            }
            for (var size in sizeJons[vid]) {
                var html = "";
                if (sizeLength != 1) {
                    html += "<ul data-id=\"" + vid + "\" style=\"display:" + (colorLength == 1 ? "" : "none") + "\">";
                } else {
                    html += "<ul class=\"last\" data-id=\"" + vid + "\" style=\"display:" + (colorLength == 1 ? "" : "none") + "\">";
                }
                html += "<li class=\"size_name\">" + sizeJons[vid][size][0] + "</li>";
                html += "<li class=\"size_price\" data-id=\"" + sizeJons[vid][size][1] + "\">" + sizeJons[vid][size][1] + "元</li>";
                html += "<li class=\"size_sale\">" + sizeJons[vid][size][2] + "件可售</li>";
                html += "<li class=\"size_pa\">";
                html += "<div class=\"size_num\">";
                html += "<span class=\"btn\"></span>";
                html += "<span><input type=\"text\" value=\"0\" class=\"size_input\" maxNum=\"" + sizeJons[vid][size][2] + "\"></span>";
                html += "<span class=\"btn btn2\"></span>";
                html += "</div>";
                html += "</li>";
                html += "</ul>";
                $(".pro_size_item").append(html);
                sizeLength--;
            }
        });
        $(".pro_size_item ul input.size_input").bind("blur", judgeStock);
    };
    function changeColorPic(obj) {
        var href = $(obj).attr("data-id");
        if (href != "") {
            $("#qccImage").attr("data-big", href.replace("_40x40.jpg", "").replace("/Thumbnail", ""));
            $("#qccImage").attr("src", href.replace("40x40", "500x500"));
            $(".viewer img").attr("src", href.replace("_40x40.jpg", "").replace("/Thumbnail", ""));
        }
        var vid = $(obj).attr("vid");
        $(".pro_size_item ul").css("display", "none");
        $(".pro_size_item ul[data-id='" + vid + "']").css("display", "");
        $(".clr-item").each(function () {
            $(this).removeClass("on_clr");
            $(this).addClass("J_clr");
        });
        $(obj).removeClass("J_clr");
        $(obj).addClass("on_clr");
    }
</script> 
