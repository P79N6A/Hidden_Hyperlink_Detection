     $(document).ready(function(){
     	$("#zxwztab span:first").addClass("current");
    	$("#zxwztab .tab-bd-con:gt(0)").hide();
	    $("#zxwztab span").mouseover(function(){//mouseover 
	    
           $(this).addClass("current").siblings("span").removeClass("current");
