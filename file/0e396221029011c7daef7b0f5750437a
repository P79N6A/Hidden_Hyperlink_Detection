        if(url.trim().length && url != 'index.php' && url !='index.html')
        {
            jq('.nav').hide();
            jq(function(){
                jq('.all-nav').bind('mouseover',function(){
                    jq('.nav').show();
                });
                jq('.all-nav').bind('mouseout',function(){
                    jq('.nav').hide();
                });
            });

            jq('.header .topbar .topbar-container').css({width:'1225px'});
            jq('.header .header-main').css({width:'1225px'});
            jq('#search').css({marginLeft:'130px'});
            jq('.header .navbar .navbar-container').css({width:'1225px'});
