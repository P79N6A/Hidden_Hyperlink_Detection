  <script type="text/javascript">
    jq(function(){
        //客服咨询添加关闭按钮
        var close_btn='<div class="close_btn">X</div>';
        jq('body').append(close_btn);
        jq('.close_btn').addClass('hide');

        jq('body').delegate('.close_btn', 'click', function(event) {
            jq('#newBridge #nb_icon_wrap').animate({bottom:'-444'},400);
            jq('#newBridge #nb_icon_wrap').animate({left:"-30"},10);
            jq(this).addClass('hide');
        });
        
        function zixun(){
            if(jq('#newBridge #nb_icon_wrap').length){
                // var wrapheight=jq('#newBridge #nb_icon_wrap').height();
                // var bottom=wrapheight-42+"px";
                jq('#newBridge #nb_icon_wrap').css({
                    left: '-30px',
                    bottom:'-444px'
                });
            }else{
                setTimeout(zixun,10);
            }
        }
        zixun();
       
        jq('body').delegate('#newBridge .nb-icon-inner-wrap','click', function() {
            jq('#newBridge #nb_icon_wrap').animate({left:"0"},10);
            jq('#newBridge #nb_icon_wrap').animate({bottom:"20"},400);
            jq('.close_btn').removeClass('hide');
        });
    })
</script>   
