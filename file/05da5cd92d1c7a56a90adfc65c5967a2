<script>
$(function () {
// big-data
    function putTracker() {
        var scene = window.scene;
        if (!scene) return;
        var url = EQX.HOST.TRACK + 'c.gif?';
        var screen = window.screen;
        var screenSize = screen.availWidth + 'x' + screen.availHeight;
        var navigator = window.navigator;
        var data = {
            lag: navigator.userLanguage || navigator.language
        };
        data.event_description = scene.description;
        data.event_id = scene.id;
        data.scene_type = scene.type;
        data.creator_id = scene.userId;
        data.creator_type = scene.userType;

        var args = 'action_name=' + encodeURIComponent(document.title)
            + '&idsite=2&url=' + encodeURIComponent(document.location)
            + '&urlref=' + encodeURIComponent(document.referrer)
            + '&res=' + screenSize + '&data='
            + encodeURIComponent(JSON.stringify(data));

        args += '&cookie=' + navigator.cookieEnabled ? 1 : 0;
        args += '&ct=' + new Date().getTime();
        url += args;

        var img = document.createElement('img');
        img.setAttribute('src', url);
        img.setAttribute('height', '0');
        img.setAttribute('width', '0');
        document.body.appendChild(img);
    }
    putTracker();

})

</script>

