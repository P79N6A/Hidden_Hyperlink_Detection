	var VERIFY_CHECK = "/index.php/Danye-verifyCheck.html";
	var LOGIN_USER = "/index.php/Danye-loadUser.html";
	var LOGOUT = "/index.php/Danye-logout.html";
	var AJAX_LOGIN = "/index.php/index-ajaxLogin.html";
	var AJAX_REG = "/index.php/index-ajaxReg.html";
	var AJAX_FIND_PASS = "/index.php/index-ajaxFind.html";
	var DEL_URL	= "/index.php/Danye-foreverdelete.html";
	var SUCCESS = "/index.php/admin-index-success.html";
	var GET_AJAX_CART = "/index.php/Danye-getAjaxCart.html"
	function addData(url){
		location.href = "/index.php/Danye-"+url+".html";
	}
	
	/**
 * 表单提交内容
 */
$(function(){
    $('#myForm').ajaxForm({
        beforeSubmit:  checkForm,
        success:       complete, 
        dataType: 'json'
    });
    function checkForm(){
		$('button').hide();
		$('.submit_wanning').each(
			function(){
				if($(this).css('color') == '#f00'||$(this).css('color') == "rgb(255, 0, 0)") {
				$('button').show();
					$('#'+this.id.replace('err_','')).focus();
					$('#myForm').attr('title','false');
					return false;
				} else {
					$('#myForm').attr('title','true');
					return true;
				}
			}
		)
		if($('#myForm').attr('title') == "false"){
		$('button').show();
			return false;
		} else {
			return true;
		}
    }
	function complete(data){
		if(data.status == 0){
			openInfoError("/index.php/Admin-index-error.html?msg="+data.info);
		} else {
			openInfo("/index.php/Admin-index-success.html?msg="+data.info);
		}
		$('button').show();
	}
});
function edit(id){
	var keyValue;
	if (id)
	{
		keyValue = id;
	}else {
		keyValue = getSelectCheckboxValue();
	}
	if (!keyValue)
	{
		alert('请选择编辑项!');
		return false;
	}
			location.href = "/index.php/Danye-edit-id-"+keyValue+".html";}
function add(id){
	if (id){
		 location.href  = '/index.php/Danye-add-cid-'+id+'.html';
	}else{
		 location.href  = '/index.php/Danye-add.html';
	}
}
function correctPNG() {
	for(var i=0; i<document.images.length; i++){
	var img = document.images[i];
	var imgName = img.src.toUpperCase();
	if (imgName.substring(imgName.length-3, imgName.length) == "PNG"){
		var imgID = (img.id) ? "id='" + img.id + "' " : "";
		var imgClass = (img.className) ? "class='" + img.className + "' " : "";
		var imgTitle = (img.title) ? "title='" + img.title + "' " : "title='" + img.alt + "' ";
		var imgStyle = "display:inline-block;" + img.style.cssText;
		if (img.align == "left") imgStyle = "float:left;" + imgStyle;
		if (img.align == "right") imgStyle = "float:right;" + imgStyle;
		if (img.parentElement.href) imgStyle = "cursor:hand;" + imgStyle;
		var strNewHTML = "<span "+ imgID + imgClass + imgTitle + "style=\"" + "width:" + img.width + "px; height:" + img.height + "px;" + imgStyle + ";" + "filter:progid:DXImageTransform.Microsoft.AlphaImageLoader" + "(src='" + img.src + "', sizingMethod='scale');\"></span>";
		img.outerHTML = strNewHTML;
		i = i-1;
		}
	}
}
