  <script type="text/javascript">
	$(function(){
		$('.curs').bind('mouseenter',function(){
			$(this).attr('src',$(this).attr('src').replace('.gif','s.gif'));
		}).bind('mouseleave',function(){
			$(this).attr('src',$(this).attr('src').replace('s.gif','.gif'));
		})
		$('#key').focus(function(){
			if($(this).val() == '新闻关键词'){
				$(this).val('')
			}
		}).blur(function(){
			if($(this).val() == ''){
				$(this).val('新闻关键词')
			}
		})
		$('#search').click(function(){
			if($('#key').val() == '新闻关键词'){
				alert('请输入新闻关键词！');
				$('#key').focus();
				return false;
			}
			location.href = "/index.php/xinwen-lanmu.html?key="+$('#key').val();
		})
		$('.nav').find('ul li').bind('mouseenter',function(){
			$(this).find('img').attr('src',$(this).find('img').attr('src').replace('.gif','s.gif'));
			$(this).find('.sonNav').fadeIn(500);
		}).bind('mouseleave',function(){
			$(this).find('img').attr('src',$(this).find('img').attr('src').replace('s.gif','.gif'));
			$(this).find('.sonNav').fadeOut(500);
		})
		$('.closeed').click(function(){
			$('.marsk').hide();
			$('.contents').hide();
		})
	})
	function viewMask(){
		$('.marsk').css('height',document.documentElement.scrollHeight+'px')
		$('.marsk').show();
		$('.contents').css('top',document.documentElement.scrollTop-(-document.body.scrollTop)-(-document.documentElement.clientHeight+396)/2+'px');	
		$('.contents').css('left',((document.documentElement.scrollWidth-953)/2)+'px');
		$('.contents').show();
	}
</script>
