<script src="http://image.bitautoimg.com/appimage/ui/web/yqswap/js/ArticleComments.js"></script>


<script>


    var newCommentsData = {
        newId: '181111',
        hotComments: '',
        newComments: '',
        username: '',
        userid: '0',
        logWeburl: 'http://log.ycapp.yiche.com',
        addViewCountUrl: 'http://h5.ycapp.yiche.com'
    };

    $(document).ready(function () {
        $.ajax({
            url: '/Ajax/ArticleHandler.ashx',
            data: 'action=getnewscommentsall&articleId=' + newCommentsData.newId + '&t=' + Math.random(),
            dataType: 'json',
            success: function (msg) {
                if (msg) {
                    newCommentsData.newComments = JSON.stringify(msg.newComments);
                    newCommentsData.hotComments = JSON.stringify(msg.hotComments);
                    newCommentsData.newComments = newCommentsData.newComments.replace(new RegExp("\n", "g"), "<br>");
                    newCommentsData.hotComments = newCommentsData.hotComments.replace(new RegExp("\n", "g"), "<br>");
                    ArticleComments.init(newCommentsData);
                }
            },
            error: function (e) {
            }
        });
    })

</script>


