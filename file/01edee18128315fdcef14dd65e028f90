            new jBox('Notice', {
                title: "惠九州信息港・广告平台系统提示",
                autoClose: 70000,//窗口停留时间(毫秒)
                position: { x: 'center', y: 'center' },//窗口显示位置
                //animation: { open: 'tada', close: 'zoomIn' },
                animation: 'flip',
                content: '<p style="text-align:center;">' + value + '</p>',
                onInit: function () {
                    this.options.color = 'yellow';
                }
            });
        }
