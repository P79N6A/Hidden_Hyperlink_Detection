         <div id="search-content"> 
          <form method="post" action="https://justwinit.cn/visit.php" onsubmit="return  submitSwitch();" style="padding:0px;margin:0px;"> 
           <input type="hidden" name="job" value="search" /> 
           <input type="text" id="keyword" onfocus="if(this.value=='∷∷∷∷∷ 自定义搜索 ∷∷∷∷∷∷') this.value='';" onblur="if(this.value=='') this.value='∷∷∷∷∷ 自定义搜索 ∷∷∷∷∷∷';" value="∷∷∷∷∷ 自定义搜索 ∷∷∷∷∷∷" size="130" name="keyword" class="search-field" /> 
           <select name="searchmethod" id="searchmethod"><option value="1">日志标题</option><option value="2">日志全文</option><option value="5">页面搜索</option></select> 
           <input type="submit" value="搜索" class="button" id="searchButton" /> 
          </form> 
          <script>
	function run(){
		var key=document.getElementById("keyword").value;
		var doc=document.body.getElementsByTagName("*");
		var i=0;
		while(doc[i]){
			if(doc[i].innerHTML&&doc[i].className!='evtFind'){
				var tmp=doc[i].innerHTML;
				var reg=new RegExp("("+key+")","g");
				doc[i].innerHTML=tmp.replace(reg,'<font color="#ff0000" class="evtFind">$1</font>');
			}
			i++;
		}
		document.getElementById("keyword").value=key;
		return false;
	}
	function submitSwitch()
	{
		var searchmethod = document.getElementById("searchmethod");
		var searchmethodValue = searchmethod.options[searchmethod.selectedIndex].text;
		if(searchmethodValue == "页面搜索")
		{//网页搜索

			for(var i=0;i<searchmethod.options.length;i++)
			{
				if(searchmethod.options[i].text=="页面搜索")
				{
					searchmethod.options[i].selected = true;
				}
			}
			run();
			return false;
		}
	}
</script> 
