  <script type="text/javascript">

        /**右边悬浮编辑**/

        var loadHtml = function (url, renderHtml) { $.ajax({ url: url, dataType: "html", success: function (html) { if ($.isFunction(renderHtml) && (html && html.length > 0)) renderHtml(html); } }); }
        var loadContaienrHtml = function (url, container, lazyLoader) {
            var $container = $(container);
            if ($container.length == 0 || !$.isFunction(lazyLoader)) return;

            loadHtml(url, function (html) {
                $container.children("textarea:first").val(html);
                lazyLoader(container);
            });
        }

        function rightFixed() {
            var height = $(window).height();
            $("#sideFixed").find(".fixedOne").css('height', height);
        }

        function adHeight() {
            var height = $(window).height();
            var width = $(window).width();
            if (height < 500 || width < 1280) {
                $("#sideFixed").find(".fixedOne").animate({ right: "-50px" }, 0);
                $("#sideFixed").find(".fixedSimple").animate({ right: "-10px" }, 0);
            } else {
                $("#sideFixed").find(".fixedOne").animate({ right: "0px" }, 0);
                $("#sideFixed").find(".fixedSimple").animate({ right: "-50px" }, 0);
            }
        }
        $(function () {
            rightFixed();
            adHeight();
            $("#speadAdd").click(function () {
                $(this).parents(".fixedOne").animate({ right: "-50px" }, 500);
                $(this).parents(".fixedOne").next(".fixedSimple").animate({ right: "-10px" }, 500);
            })
            $("#speadSub").click(function () {
                $(this).parents(".fixedSimple").animate({ right: "-50px" }, 500);
                $(this).parents(".fixedSimple").prev(".fixedOne").animate({ right: "0px" }, 500);
            })

            $("#sideFixed .fixedSimple").find(".iconThree").hover(function () {

                $(this).html("0551-62220088<br />0551-62220099").css({ "width": "120px", "font-size": "12px", "color": "#fff", "margin-left": "-72px", "background": "#357be4", "text-align": "center", "line-height": "1.8" });

            }, function () {
                $(this).html("").attr("style", "");

            })

            $("#sideFixed .fixedSimple").find(".iconFour").hover(function () {
                $(this).html("<a href='tencent://message/?uin=1922007149'>在线客服1</a><a href='tencent://message/?uin=1678201941'>在线客服2</a>").css({ "width": "120px", "font-size": "12px", "margin-left": "-72px", "background": "#357be4", "text-align": "center", "line-height": "1.8" });

            }, function () {

                $(this).empty().attr("style", "");
            })


            $(window).resize(function () {
                rightFixed();
                adHeight();
            })

            // 右边声明悬浮
            $(this).width() <= 1450 ? $("#adFixed").addClass("adheight2") : $("#adFixed").removeClass("adheight2");

            $(window).resize(function () {
                $(this).width() <= 1450 ? $("#adFixed").addClass("adheight2") : $("#adFixed").removeClass("adheight2");
            });

            $(window).scroll(function () {
                $(this).scrollTop() > 350
                    ? ($("#adFixed").css({ position: "fixed", top: "0px" }).children("a").css({}))
                    : ($("#adFixed").css({ position: "absolute", top: "350px" }));
            });
        })
    </script> 
