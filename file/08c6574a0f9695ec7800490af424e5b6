  <script type="text/javascript">
    $(document).ready(function () {
        //顶部右侧下拉菜单
        $(".topOneTw .ListRight ul li").mouseover(function () {
            var index = $(".topOneTw .ListRight ul li").index($(this));
            $(this).addClass("current");
            $(this).children(".DownList").css('display', 'block');
            if (index > 0) {
                $(".topOneTw .ListRight ul li").children("a").eq(index - 1).addClass("Border");
            }
        }).mouseout(function () {
            var index = $(".topOneTw .ListRight ul li").index($(this));
            $(this).removeClass("current");
            if (index > 0) {
                $(".topOneTw .ListRight ul li").children("a").eq(index - 1).removeClass("Border");
            }
            $(this).children(".DownList").css('display', 'none');
        });

        //手机浏览器
        var browser = {
            versions: function () {
                var u = navigator.userAgent, app = navigator.appVersion;
                return {     //移动终端浏览器版本信息
                    trident: u.indexOf('Trident') > -1, //IE内核
                    presto: u.indexOf('Presto') > -1, //opera内核
                    webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
                    gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1, //火狐内核
                    mobile: !!u.match(/AppleWebKit.*Mobile.*/), //是否为移动终端
                    ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
                    android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, //android终端或uc浏览器
                    iPhone: u.indexOf('iPhone') > -1, //是否为iPhone或者QQHD浏览器
                    iPad: u.indexOf('iPad') > -1, //是否iPad
                    webApp: u.indexOf('Safari') == -1 //是否web应该程序，没有头部与底部
                };
            }(),
            language: (navigator.browserLanguage || navigator.language).toLowerCase()
        }
        if (browser.versions.mobile) {//判断是否是移动设备打开。browser代码在下面
            if (getCookie("picorphone") != "1") {//picorphone=1强制在手机上打开PC端页面
                var ua = navigator.userAgent.toLowerCase();//获取判断用的对象
                if (ua.match(/MicroMessenger/i) == "micromessenger") {
                    //在微信中打开
                    //setInterval(WeixinJSBridge.call('closeWindow'), 2000);
                    //window.location.href = "http://m.youzhicai.com/Detail/PurcherDetail/76b07d35-98a9-4395-a05e-d4ddb5f2fa18";
                    window.location.href = "http://m.youzhicai.com/Detail/PurcherDetail/821e0e4a-ad6b-4100-aaf0-bbb9daad415f";
                }
                if (ua.match(/WeiBo/i) == "weibo") {
                    //在新浪微博客户端打开
                    window.location.href = "http://m.youzhicai.com/Detail/PurcherDetail/821e0e4a-ad6b-4100-aaf0-bbb9daad415f";
                }
                if (ua.match(/QQ/i) == "qq") {
                    //在QQ空间打开
                    window.location.href = "http://m.youzhicai.com/Detail/PurcherDetail/821e0e4a-ad6b-4100-aaf0-bbb9daad415f";
                }
                if (browser.versions.ios) {
                    //是否在IOS浏览器打开
                    window.location.href = "http://m.youzhicai.com/Detail/PurcherDetail/821e0e4a-ad6b-4100-aaf0-bbb9daad415f";
                }
                if (browser.versions.android) {
                    //是否在安卓浏览器打开
                    window.location.href = "http://m.youzhicai.com/Detail/PurcherDetail/821e0e4a-ad6b-4100-aaf0-bbb9daad415f";
                }
            }
        }
        else {
            //否则就是PC浏览器打开
            //window.close();\
            //$.showWin();
        }

    });

    //下载附件
    function downAttFile(AttViewName, AttPath, TendProjectId) {
        var url = "http://file.youzhicai.com/SingleDown/" + AttPath + "/" + encodeURIComponent(AttViewName) + "/" + TendProjectId + "?token=%24apr1%24HW6trF8l%24ul7G1VBmmh%2fbGW41xCqxa0";
        Download(url);
    }
    function downAttFile1(AttachmentID) {
        var url = "http://file.youzhicai.com/SingleDown/" + AttachmentID + "?token=%24apr1%24HW6trF8l%24ul7G1VBmmh%2fbGW41xCqxa0";
        Download(url);
    }

    ///未审核通过 弹窗提醒
    function AlertOrangeWarn(cont, cont2, fun, type, btnname) {
        var orangeTitle = "<div class='orangeTitle'>" + cont + "</div>";
        var ErrorCont = "<div class='warnFace'><div class='user' style='font-size:14px;'>尊敬的用户：</div><div style='padding:5px 10px;font-size:14px'>" + cont2 + "</div></div>";
        if (type == "1") {
            var D = dialog({
                title: orangeTitle,
                content: ErrorCont,
                Icon: 'IconWrong',
                width: 480,
                height: 200,
                okValue: btnname,
                ok: function () {
                    if (fun) {
                        fun();
                    }
                }
            });
            D.showModal();
        } else {
            var D = dialog({
                title: orangeTitle,
                content: ErrorCont,
                Icon: 'IconWrong',
                width: 480,
                height: 200,
                okValue: "立即完善",
                ok: function () {
                    if (fun) {
                        fun();
                    }
                },
                cancelValue: "稍后完善",
                cancel: true
            });
            D.showModal();
        }
    }

    ///我要申请按钮点击事件
    function ApplyBul(isskiptip) {
        if ($("#bulletinlogId").val() != "") {
            //记录点击日志
            $.post("/Notice/UpdateBulletionViewLog", { "bulletinviewlogid": $("#bulletinlogId").val() }, function () {
            });
        }
        //当前登录人ID
        var CurID = isskiptip == true ? "True" : "False";
        if (CurID != "True") {
            //打开登录窗口登录
            main_login(true);
        } else {
            var ajax = {};
            ajax.type = "POST";
            ajax.url = "/Notice/GetCompanyStatus";
            ajax.data = {};
            ajax.data.EndTime = "2017/3/29 10:00:00";
            ajax.data.type = 2;
            ajax.success = function (result) {
                if (result == "4") {
                    //报名已截止
                    AlertOrangeWarn("温馨提示", "尊敬的用户,您好,该公告的报名时间已截止,不能参加报名！", function () {
                        location.href = location.href;
                    }, "1", "确定");
                }
                else if (result == "5") {
                    //无投标权限
                    AlertOrangeWarn("温馨提示", "哎呀，你竟然没有开通“供应管理”功能，这会错失参与招标、采购项目的机会！赶紧去申请开通吧~！", function () {
                        location.href = "http://member.youzhicai.com/OpenMenu/Index/0f3d3e15-fee4-404e-a375-573ab985d856";
                    }, "1", "去开通");
                }
                else if (result == "1" || result == "6") {
                    $(document).codeWid(isskiptip);
                    //直接跳转到报名页面
                    

                } else if (result == "2") {

                    AlertOrangeWarn("温馨提示", "尊敬的用户，您好，您的会员信息没有审核通过，不能参加报名，请尽快去完善企业信息！", function () {
                        window.location.href = "http://member.youzhicai.com/SubjectMan/Index?topid=b4b967c1-0c5d-46f4-9286-6bf72dc0cf96&currid=0f88af98-bc04-4571-a55e-5dd0fa29cff4";
                    });

                } else if (result == "7") {
                    //已被加入黑名单
                    AlertOrangeWarn("温馨提示", "您好！您已经被平台拉入黑名单，不能参加项目操作！", function () {
                        location.href = location.href;
                    }, "1", "确定");
                } else {
                    //用户信息未获取到 或者是未登录
                    main_login(true);
                }
            };
            $.ajax(ajax);
        }
    }

    (function ($) {
        $.fn.extend({
            "codeWid": function (isskiptip) {
                showMask = function () {
                    var heig = $(window).height();
                    var wid = $(window).width();
                    var htm = "<div style='height:" + heig + "px;width:" + wid + "px;' class='codeWinMask'></div>";
                    $(".codeWinMask") && $(".codeWinMask").remove();
                    $("body").append(htm);
                },
                    showImg = function () {
                        var htm = "<div class='codeWinImgWap'><a class='close' onclick='hideWindow()'><img class='close-img' src='../../img/close.png'></a>";
                        htm += "<img src='../../img/codeImg.png' class='weixin-img' /><a class='signUp'><span class='signUp-text'>我要报价</span><img src='../../img/signUpImg.png' class='signUp-img' /></a></div>";
                        $(".codeWinImgWap") && $(".codeWinImgWap").remove();
                        $("body").append(htm);
                    },
                    hideWindow = function () {
                        $(".codeWinMask,.codeWinImgWap").remove();
                    }
                if (!isskiptip) {
                    showMask();
                    showImg();
                } else {
                    SkipApplyPage();
                }
                $(".signUp").on("click", function () {
                    SkipApplyPage();
                });
                $(window).resize(function () {
                    if ($(".codeWinMask").length > 0 && $(".codeWinImgWap").length > 0) {
                        showMask();
                        showImg();
                    }
                });
            }
        });
    })(jQuery);

    //跳转报名页面
    function SkipApplyPage() {
        // 3 询价(supply.bidder.....)，  4 招募 （supply.bidder....）， 5 竞价（投标人端已剥离开，投标人端域名已不一样biddingbidder...） ，6 企业招标（为了兼容，已不从这里跳转）
        var btype = 3;  //默认是询价
        switch ("1") {
            case "1": //询价
                btype = 3;
                break;
            case "2": //招募
                btype = 4;
                break;
            case "4": //竞价
                btype = 5;
                break;
        }
        if (btype != 5) {
            //询价，招募
            window.location.href = "http://supply.bidder.youzhicai.com/Project/Shared/ToActionCommon?PlatformType=" + btype + "&ProjectId=76b07d35-98a9-4395-a05e-d4ddb5f2fa18&fromType=2";
        }
        else {
            //竞价
            window.location.href = "http://biddingbidder.youzhicai.com/Project/Shared/ToActionCommon?PlatformType=" + btype + "&ProjectId=76b07d35-98a9-4395-a05e-d4ddb5f2fa18&fromType=2";
        }
    }
