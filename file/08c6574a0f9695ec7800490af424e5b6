   <script type="text/javascript" src="/Scripts/Message.js"></script> 
   <script type="text/javascript">
        function login_controller(isapply) {
            var dbs_width = document.body.scrollWidth;//页面的整体宽度，包含不可见的区域
            var dbs_height = document.body.scrollHeight;//页面的整体高度，包含不可见区域
            var dbc_width = document.documentElement.clientWidth;
            var dbc_height = document.documentElement.clientHeight;
            var oMask = document.getElementById("mask");
            var oLogin = document.getElementById("login");
            function login_success() {
                login_close();
                if (isapply) {
                    ApplyBul(true);
                } else {
                    window.location.href = window.location.href;
                }
            }
            function login_close() {
                $("#mask").hide();
                $("#login").hide();
                window.onresize = null;
            }

            function position_adjust() {
                dbs_width = document.body.scrollWidth;//页面的整体宽度，包含不可见的区域
                dbs_height = document.body.scrollHeight;//页面的整体高度，包含不可见区域
                dbc_width = document.documentElement.clientWidth;
                dbc_height = document.documentElement.clientHeight;;

                oMask.style.height = dbs_height + "px";
                oMask.style.width = dbs_width + "px";
                //获取登陆框的宽和高
                var dHeight = oLogin.offsetHeight;
                var dWidth = oLogin.offsetWidth;
                //设置登陆框的left和top
                oLogin.style.left = dbc_width / 2 - dWidth / 2 + "px";
                oLogin.style.top = dbc_height / 2 - dHeight / 2 + "px";
            }

            return {
                show: function () {
                    position_adjust();
                    oMask.style.display = "block";
                    oLogin.style.display = "block";
                    window.onresize = function () {
                        position_adjust();
                    }
                    position_adjust();
                    var oClose = document.getElementById("close");
                    //点击登陆框以外的区域也可以关闭登陆框
                    oClose.onclick = oMask.onclick = function () {
                        document.getElementById("mask").style.display = "none";
                        document.getElementById("login").style.display = "none";
                    };
                },
                init: function () {
                    var html = '<iframe style="width: 360px;height: 350px" id="login_dialog"' +
                            'frameborder="0" scrolling="no" allowtransparency="true" ' +
                            'src="http://passport.youzhicai.com/home/smalllogin">' +
                             '</iframe>';
                    $("#login_div").html(html);
                    var messenger = new Messenger('parent', 'Messenger');
                    var iframe1 = document.getElementById('login_dialog');
                    messenger.listen(function (msg) {
                        login_success();
                    });
                    messenger.addTarget(iframe1.contentWindow, 'login_dialog');
                }
            }
        }

        function main_login(isapply) {
            var loginControl = login_controller(isapply);
            loginControl.init();
            loginControl.show();
        }
    </script> 
