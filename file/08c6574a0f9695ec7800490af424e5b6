  <script src="/Scripts/jquery-ui.js"></script> 
  <script type="text/javascript">
    $(function () {
        //切换搜索标签
        $(".slttab").click(function () {
            //切换效果
            $(".slttab").find(".CommonRed").removeClass("CommonRed");
            $(this).children("a").addClass("CommonRed");
            //切换内容
            var sltval = $.trim($(this).attr("val"));
            sltval = sltval == "" ? "1" : sltval;
            $("#hidslttabval").val(sltval);
            $("#key").val(GetHolderByTab($("#hidslttabval").val()));
        });

        $("#key").bind("keyup", function (e) {
            e = e || event;
            if (e.keyCode == 13) {
                Search();
            }
        })

        $("#key").focus(function () {
            if ($("#key").val() == GetHolderByTab($("#hidslttabval").val())) {
                $("#key").val("");
            }
        })

        $("#key").focusout(function () {
            if ($("#key").val().length == 0) {
                $("#key").val(GetHolderByTab($("#hidslttabval").val()));
            }
        })

        $("#key").keyup(function () {
            //$("#key").val($("#key").val().replace(/[^\a-zA-Z\u4E00-\u9FA50-9]/g, ''));
            if ($("#key").val() == GetHolderByTab($("#hidslttabval").val()) || $.trim($("#key").val()) == "") {
                $("#key").attr("style", "color:#a9a9a9;");
            } else {
                $("#key").attr("style", "color:#333;");
            }
        });

        if ($("#key").val() != GetHolderByTab($("#hidslttabval").val())) {
            $("#key").attr("style", "color:#333;");
        }

        $("#key").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/Default/GetAutoCompleteNoiceNew",
                    dataType: "json",
                    data: {
                        term: $('#key').val(),
                        slttabval: $("#hidslttabval").val()
                    },
                    success: function (data) {
                        response(data);
                    }
                });
            },
            minLength: 2,
            select: function (event, ui) {
            },
            search: function (event, ui) {
                if (!($("#hidslttabval").val() == "1" || $("#hidslttabval").val() == "3")) {
                    return false;
                }
            } 
        });
    });

    //搜索
    function Search() {
        var sltval = $("#hidslttabval").val();
        var sltcontent = $.trim($("#key").val()).replace(GetHolderByTab($("#hidslttabval").val()), "");
        sltcontent = sltcontent.replace(/<[^>]+>/g, "");
        sltcontent = encodeURI(sltcontent); 
        var redirecturl = "";
        switch (sltval) {
            case "1":
                redirecturl = "/bidder/noticesearch?sltval=" + sltval + "&key=" + sltcontent + "";
                break;
                //case "4":
                //    redirecturl = "/bidder/noticesearch?sltval=" + sltval + "&noticecate=2&key=" + sltcontent + "";
                //    break;
            case "2":
                redirecturl = "/product/productsearch?sltval=" + sltval + "&key=" + sltcontent + "";
                break;
            case "3":
                redirecturl = "/company/companysearch?sltval=" + sltval + "&key=" + sltcontent + "";
                break;
            case "5":
                redirecturl = "/product/purchasegood?sltval=" + sltval + "&key=" + sltcontent + "";
                break;
        }
        window.location.href = redirecturl;
    }
</script> 
