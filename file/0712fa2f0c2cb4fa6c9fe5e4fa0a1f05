  <script>
 require.async(["components/jquery/jquery.js"], function($) {
    $(window).scroll(function() {
        var top = 120;
        if (scrollY >= 70) {
            top = 50;
        } else {
            top -= scrollY;
        }
        $('.sidebar').css({'top':top+'px'});
    });
 });
</script> 
  <script>
  require.async(["components/jquery/jquery.js","js/site/feedback_new.js"], function($, Feedback) {
    Feedback.init({ contact: "" });
    
    $("#submit").click(function() {
      if (!Feedback.validateOk()) {
            return false;
      } else {
          var $csrf = $("#csrf-token");
          var content = $('textarea[name=content]').val();
          var data = {
            content:content,
          }
          data[$csrf.attr('name')] = $csrf.val();
          $.ajax({
              type: 'POST',
              url: '/feedback/comment/1751',
              dataType: 'json',
              data: data,
              success: function(res) {
                if (res.success == true) {
                  var comment = res.comment;
                  var num = res.comment_num;
                  var html = '<li class="comment-item"><p class="comment-desc">';
                  html += comment.content;
                  html += '</p><div><span class="time">';
                                    html += comment.created_at;
                  html += '</span>';
                                    html += '</div>';
                  
                  $('.comment-list').prepend(html);
                                    $('.comment-header').text('评论 '+ num);
                  $('textarea[name=content]').val('');
                }
              }
          });
      }
    });
  });
</script> 
  <script>
            require.async(["components/jquery/jquery.js"], function($) {
                $("#nav-download:not('.active')").hover(function() {
                    $('#download-menu').show();
                 },function() {
                    $('#download-menu').fadeOut();
                 });
                $("#download-menu").hover(function() {
                    $('#download-menu').stop(true);
                },function() {
                    $('#download-menu').fadeOut();
                 });

                $("#nav-shop:not('.active')").hover(function() {
                    $('#shop-menu').show();
                 },function() {
                    $('#shop-menu').fadeOut();
                 });
                $("#shop-menu").hover(function() {
                    $('#shop-menu').stop(true);
                },function() {
                    $('#shop-menu').fadeOut();
                 });
            });
        </script> 
