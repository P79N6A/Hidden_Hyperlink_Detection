			{set likeContentsId=(getLikeContentsId(=data))}
			<a href="#" data-sid="ENTERTAIN" data-did="STARCAST" data-cid="{=likeContentsId}" data-likeit="0" class="u_likeit_list_btn u_type_dark"><span class="u_ico"></span><em class="u_cnt"></em></a>
		</div>
		{set fullImageUrl=(getImageDomain(=data.imageDomainNumber) + =data.item.thumbnailPathWithoutDomain)}
		<img src="data:image/gif;base64,R0lGODlhEAAJAJEAAAAAAP///////wAAACH5BAEAAAIALAAAAAAQAAkAAAIKlI+py+0Po5yUFQA7" data-src="{=fullImageUrl}?type=w640" alt="" class="vw_im">
	</div>
