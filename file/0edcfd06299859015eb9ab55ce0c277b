</script>
<script type="text/javascript">
var langCode = "zh";
var imageDomainList = jindo.$H({"2":"http://mimgnews2.naver.net/image/","1":"http://mimgnews1.naver.net/image/"}).values();

function getImageDomain(n){
	return imageDomainList[n-1];
};

// 좋아요
var celebuId = "37";
function getLikeContentsId(data) {
	var likeContentsId = 'ph_' + jindo.$S("%05d").format(celebuId) + '_' + jindo.$S("%010d").format(data.contentsId);
	return likeContentsId;
}
function initLikeItList() {
	nhn.LikeIt.list.util.init({
		sId : "STARCAST"  // displayId
		, sDomain : "http://news.like.naver.com"  // 좋아요 서비스 도메인
		, sLang : "zh-hans"
	});
}
function updateLikeItList() {		
	//nhn.LikeIt.list.instance.update(); // 좋아요 목록 에서 노출 제거
}

(function(oData) {
	window.__oLikeItData = oData;
	var vUrl = ["/static/js/likeIt.list", "js?v="+ (new Date()).toISOString().replace(/\D/g,"").slice(0,8)], s = document.createElement("script");
	s.type = "text/javascript";
	s.charset = "utf-8";
	s.src = oData.sDomain + vUrl.join(".");
	(document.head || document.getElementsByTagName("head")[0]).appendChild(s);
})({
	sDomain : "http://news.like.naver.com"  // 좋아요 서비스 도메인
});


