     <script type="text/javascript">
	function nTabs1(thisObj,Num){
		if(thisObj.className == "zmcon-seltab-active")return;
		var tabObj = thisObj.parentNode.id;
		var tabList = document.getElementById(tabObj).getElementsByTagName("li");
		for(i=0; i <tabList.length; i++){
			if (i == Num){
			   thisObj.className = "zmcon-seltab-active"; 
			   document.getElementById(tabObj+"-con"+i).style.display = "block";
			}
			else{
			   tabList[i].className = "zmcon-seltab-normal"; 
			   document.getElementById(tabObj+"-con"+i).style.display = "none";
			}
		} 
	}
</script> 
