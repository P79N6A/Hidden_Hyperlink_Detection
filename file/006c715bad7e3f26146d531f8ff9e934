  <script type="text/javascript">

    function createAd(adId,divId){
        var ad = document.getElementById(adId);
        var div = document.getElementById(divId);
        if(ad){
            ad.parentNode.removeChild(ad);
            if(div){
                div.appendChild(ad);
            }
        }
    }

