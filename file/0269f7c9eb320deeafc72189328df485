            c.load(function (ele, data) {
                ele = $(ele);
                if (data) {
                    ele.attr("title", "取消收藏");

                    $(ele).closest('a').addClass('active');

                } else {
                    ele.attr("title", "添加收藏");
                    $(ele).closest('a').remove('active');
                }
            });

