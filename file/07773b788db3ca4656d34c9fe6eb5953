      <input type="text" value="" placeholder=" 我也说一句" style="padding:7px 0; width:100%;" color="gray" name="message" id="fastpostmessage" /> 
      <a href="forum.php?mod=post&amp;action=reply&amp;fid=14&amp;tid=497027&amp;reppost=5419620&amp;page=1&amp;mobile=2" class="y" style="height:30px;width:30px;margin-top:7px;background:url(static/image/mobile/images/icon_photo.png) no-repeat"><span class="none">回复</span></a> 
      <input type="button" value="回复" class="uk-button mtopten" style="width:100%" name="replysubmit" id="fastpostsubmit" /> 
     </form>
