    //异步加载收藏房源按钮
    function initShouCangHouse() {
        jQuery.ajax({
            url: "/newsecond/mysoufun.aspx?pagetype=initpage&houseid=" + houseID + "&linkurl=" + window.location.href,
            type: "GET",
            success: function (data) {
                var jsons = eval("(" + data + ")");
                if (jsons.result != "" && jsons.result != "-1")//已经收藏过
                {
                    $(".btnbg .star").addClass("star1");
                    $("#sc").html("已收藏");
                    $("#myselectedid").val(jsons.result);
                }
            }
        });
    }

    function shoucang() {
        var sc = document.getElementById("sc");
        var val = sc.innerHTML;
        if (val == "收藏") {
            jQuery.ajax({
                type: "GET",
                cache: false,
                dataType: "text",
                url: "/newsecond/include/DefaultUserLogin.aspx?method=init",
                success: function (msg) {
                    var Json = eval("(" + msg + ")");
                    if (Json.result == "100") {
                        //ajax保存
                        jQuery.get('/newsecond/mysoufun.aspx?returnid=1&userid=' + Json.userid + '&face=http://cdnsfb.soufunimg.com/3/2017_3/23/M16/15/F6D8A7BA820F4F27A7A69006C2F6ED96.jpg&name=%c1%fa%b3%c7%b4%f3%c7%c5%b1%df%c9%cf%b5%c4%d0%c2%b3%c7%bb%a8%d4%b7&address=%ce%e4%bd%f8%c7%f8%b3%a3%ce%e4%d6%d0%c2%b720%ba%c5&price=89&city=%b3%a3%d6%dd&linkurl=http://esf.cz.fang.com/chushou/3_253366993.htm&type=esf&pricetype=%cd%f2%d4%aa%2f%cc%d7&compareid=253366993&projurl=http%3a%2f%2fxinchenghuayuan0519.fang.com%2f&fangdai=http%3a%2f%2f8.fang.com%2fjsq%2f%3funit_price%3d7120%26buildingarea%3d125%26cshort%3dCZ&room=3&hall=2&floor=4&totalfloor=6&forward=%c4%cf&createtime=2000&area=125&inserttime=2017/3/23 16:15:58&unitprice=7120&propertype=0&comarea=%ba%fe%cc%c1&district=%ce%e4%bd%f8', function (data) {
                            //弹出到我的选房单提示框
                            var jsons = eval("(" + data + ")");
                            if (jsons.result != "房源成功添加到选房单啦" && jsons.result != "你已经添加过该房源了" && isNaN(jsons.result)) {
                                jErpPopup(jsons.result, '/NewSecond/sale_info/boxes/collection.aspx', 240, 120);
                            }
                            else {
                                $(".btnbg .star").addClass("star1");
                                $("#sc").html("已收藏");
                                if (!isNaN(jsons.result)) {
                                    $("#myselectedid").val(jsons.result);
                                }
                            }
                        });
                        //ub 收藏 
                        AddShouCangUB();
                    }
                    else {
                        //弹出登录
                        jErpPopup("您还没有登录，请先登录", '/NewSecond/sale_info/boxes/dboxLogin.aspx?fun=shoucang', 410, 180);
                    }
                }
            });
        }
        else {
            jQuery.ajax({
                url: "/newsecond/mysoufun.aspx?pagetype=cancelshoucang&Myselectid=" + jQuery("#myselectedid").val(),
                type: "GET",
                success: function (data) {
                    var jsons = eval("(" + data + ")");
                    if (jsons.result != "" && jsons.result == "100")//已经收藏过
                    {
                        $(".btnbg .star").removeClass("star1");
                        $("#sc").html("收藏");
                        $("#myselectedid").val("");
                    }
                }
            });
        }
    }
</script>
