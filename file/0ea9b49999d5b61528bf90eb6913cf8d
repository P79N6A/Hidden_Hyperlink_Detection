<script src="http://js.soufunimg.com/upload/webim/newim/scripts/im1.0.1.js" charset="utf-8"></script>
<script type="text/javascript">

    //在线咨询经纪人列表，json格式
    var IMAgentList = [{"AgentID":"164838449","AgentName":"芊芊","ManagerName":"qianqian1124","ShopUrl":"http://esf.cz.fang.com/a/qianqian1124","Avatar":"http://img10.soufunimg.com/m/2016_09/21/M05/0D/A6/wKgHDVfh5baIdor9AACU-wLOYYQAAVU6wG7uQAAAJUT242.png","Tel":"13961151239"}];
    //在线咨询经纪人用户名，多个以","隔开
    var IMManagerName = "qianqian1124";
    //在线图片
    var onlineImg = "http://img.soufun.com/rent/image/rent/FangJsCss/images/talkAgentIcon.gif";
    //不在线图片
    var offlineImg = "http://img.soufun.com/rent/image/rent/FangJsCss/images/btnLixian.gif";
    function getCookie(name) {
        var arr = document.cookie.match(new RegExp('(^| )' + name + '=([^;]*)(;|$)'));
        return arr && arr[2] || '';
    }
    //初始化IM
    jQuery(document).ready(function () {
        var sfutcookievalue = '';
        try
        {
            sfutcookievalue = getCookie('sfut');
        }
        catch(e){
            sfutcookievalue = '';
        }
        if (sfutcookievalue != '') {
            jQuery.ajax({
                type: "get",
                url: "/newsecond/include/DefaultUserLoginNew.aspx?method=init",
                dataType: "json",
                success: function (userData) {
                    var data = eval(userData);
                    window.fangWebIM.init({
                        type: "webesf",
                        userid: userData.userid,//登录用户请传通行证用户ID，非登录不传
                        username: userData.realusername,//登录用户请传通行证用户名，非登录不传
                        showname: userData.realusername,//显示用
                        city: "常州"//城市必填
                    });
                },
                error: function () {
                    window.fangWebIM.init({
                        type: "webesf",
                        userid: '',//登录用户请传通行证用户ID，非登录不传
                        username: '',//登录用户请传通行证用户名，非登录不传
                        showname: '',//显示用
                        city: "常州"//城市必填
                    });
                }
            });
        }
        else
        {
            window.fangWebIM.init({
                type: "webesf",
                userid: '',//登录用户请传通行证用户ID，非登录不传
                username: '',//登录用户请传通行证用户名，非登录不传
                showname: '',//显示用
                city: "常州"//城市必填
            });
        }

        //遍历经纪人列表，绑定事件
        jQuery.each(IMAgentList, function (i, agent) {
            jQuery("[name='" + agent.AgentID + "online']").click(function () {
                window.fangWebIM.openChat({
                    "type": "webesf",//新房为xf，二手房为agent，家居为home
                    "contactId": agent.ManagerName,
                    "showname": agent.AgentName,
                    "city": "常州",
                    "pname": '0',
                    "tip": "我正在浏览您的房源",
                    "shop": agent.ShopUrl,
                    "avatar": agent.Avatar,
                    "tel": agent.Tel
                });
            });
        });

        jQuery.ajax({
            type: "get",
            url: "/EsfHouse/Ajax/IMOnlieStatus.aspx",
            data: "username=" + IMManagerName,
            dataType: "json",
            success: function (onlineData) {
                var data = eval(onlineData);
                jQuery.each(data, function (n, statusValue) {
                    //查找agentid
                    jQuery.each(IMAgentList, function (m, agentValue) {
                        if (agentValue.ManagerName.replace("ae:","") == statusValue.UserName.replace("ae:","")) {
                            //在线/留言图片
                            var talkImg = jQuery("[name='" + agentValue.AgentID + "online']");

                            if (statusValue.Status == 1) {//在线
                                talkImg.attr("src", onlineImg);
                            } else {//不在线
                                talkImg.attr("src", offlineImg);
                            }

                            talkImg.show();
                        }
                    });

                });
            }
        });
    });
</script>
