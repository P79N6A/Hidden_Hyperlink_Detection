<div id="mallHouseDiv" style="display:none;"></div>
<script language="JavaScript" type="text/javascript">
    var mallInfo = imageHits.queryString("mall");
    var mallType = '';
    var mallNum = '';
    var mallSource = '';            //来源:1表示来自列表页楼盘名片处网店房源
    var url = '';
    var purpose = "住宅";
    function mallHouse() {
        try {
            if (mallInfo != null) {
                mallType = mallInfo.split(',')[0];
                mallNum = mallInfo.split(',')[1];
                if (mallInfo.split(',')[2])
                {
                    mallSource = mallInfo.split(',')[2];
                }
                if (mallNum != null && mallType != null) {
                    if (isNorth == "Y") {
                        url = "http://clickstoren.esf.soufun.com/agent_";
                    }
                    else {
                        url = "http://clickstores.esf.soufun.com/agent_";
                    }
                    if (purpose == '住宅' || purpose == '别墅') {
                        url += agentId + "_" + houseID + "_" + shortCity + "_cs_" + mallType + "_" + mallNum + (mallSource == "" ? "" : "_" + mallSource) + ".gif";
                    }
                    else {
                        url += agentId + "_" + houseID + "_" + shortCity + "_bus_" + mallType + "_" + mallNum + ".gif";
                    }
                    var img = document.createElement("img");
                    img.src = url;
                    document.getElementById("mallHouseDiv").appendChild(img);
                }
            }
        }
        catch (err) { }
    }
    mallHouse();
</script>
<!--网店房源点击量end-->
