<script>
    jQuery(document).ready(function () {
        var clickid = 'agantesfxq_'; 
        var json = '';
        
        try {
            var html='<div class="comTitle"><span class="ttc">房天下金融服务</span></div><div class="gfdk_con"><table width="100%" cellspacing="0" cellpadding="0" class="dktable_con"><tr><th colspan="4" ><dl class="dkprice_tit floatl"><dt>房屋总价</dt><dd><input id="dktotalprice" class="dkprice_tit_s" /></dd></dl><dl class="dkprice_tit floatl"><dt>参考首付</dt><dd><input id="dkfirstprice" class="dkprice_tit_s" /><span class="dkprice_span1" id="dkpercentoftotalprice">30%</span></dd></dl></th></tr>';
            if (json) {
                jQuery('#hsLoan').show();
                var List=json.List;
                for (var i = 0; i < List.length; i++) {
                    var Proclickid='';
                    if (clickid=="dsesfxq_") {//电商详情页
                        switch (List[i].ProType) {            
                            case 2:
                                Proclickid=clickid+"B03_11";
                                break;
                            case 1:
                                Proclickid=clickid+"B03_12";
                                break;                             
                            default:
                                Proclickid=clickid+"B03_13";
                                break;
                        }
                    }
                    else if(clickid=="agantesfxq_") {//普通详情页
                        switch (List[i].ProType) {            
                            case 2:
                                Proclickid=clickid+"B03_17";
                                break;
                            case 1:
                                Proclickid=clickid+"B03_18";
                                break;                             
                            default:
                                Proclickid=clickid+"B03_19";
                                break;
                        }
                    }
                    html+=(i==List.length-1?'<tr class="bornone">':'<tr>')

                    html+='<td><div class="jr-type clearfix"><strong class="font14b">'+List[i].ProName+'</strong><div class="jr-tips" id="parentDiv'+List[i].ProType+'"><img src="http://esf.js.soufunimg.com/esf/esf/online/secondhouse/old/secondhouse/image/esfnew/images/detail/jr-ico.gif" width="13" height="13" onmousemove=\'showdiv("'+List[i].ProType+'")\' onmouseout=\'hiddiv("'+List[i].ProType+'")\' /><div class="jr-tipC" id="childrenDiv'+List[i].ProType+'"></div></div></div><p>'+List[i].ProDes+'</p><p>&nbsp;</p></td>';

                    html+='<td><p>'+List[i].QixianTxt+'</p><p>'+List[i].RateTxt+'<strong class="red">'+List[i].RateShow+'</strong></p><p>'+List[i].RepaymentTxt+'</p></td>';

                    html+='<td><p id="LoanMoneyTxt'+List[i].ProType+'">'+List[i].LoanMoneyTxt+'</p><p>'+List[i].RepayMonthTxt+'<strong id="RepayMonth'+List[i].ProType+'">'+List[i].RepayMonth+'</strong></p><p>'+List[i].ProInfoTxt+'</p></td>';

                    html+='<td><a id="'+Proclickid+'" class="dktable_btn" href="javascript:void(0)" onclick="daikuanshenqing('+List[i].ProType+');">立即申请</a></td>';

                    html+='</tr><input type="hidden" id="LoanMoney'+List[i].ProType+'" value="'+List[i].LoanMoney+'" />';
                }
                html+='</table><div class="table_tel"><p>房天下金融服务平台是房天下倾力打造的国内互联网房地产金融平台，根据您的购房需求，为您量身定制贷款服务方案</p><p> <img src="http://esf.js.soufunimg.com/esf/esf/online/secondhouse/old/secondhouse/image/esfnew/styles/Owner/images/tabtel_img.gif"><strong>7X24小时融资热线 ：400&nbsp;850&nbsp;8888</strong></p></div></div>';
      
                jQuery('#hsLoan-pos').html(html);
       
                var percentspan = document.getElementById("dkpercentoftotalprice");//百分比span
                var dktotalpriceinput = document.getElementById("dktotalprice");//总价输入框
                var dkfirstpriceinput = document.getElementById("dkfirstprice");//首付输入框
                //总价赋值
                dktotalpriceinput.value = changeToPrice(financetotalprice);
                //总价获得焦点
                dktotalpriceinput.onfocus = function () {
                    this.value = "";
                }
                //总价失去焦点 赋值 判断值 获取百分比
                dktotalpriceinput.onblur = function () {
                    var p = changeToPrice(this.value);

                    if (p != "0") {
                        if (this.value == "") {//鼠标离开时没有任何输入
                            this.value = changeToPrice(financetotalprice);
                            return;
                        }
                        else {
                            if (parseInt(getobjValue(this)) < parseInt(getobjValue(dkfirstpriceinput))) {
                                this.value = "请大于参考首付";
                                return;
                            }
                            else {
                                this.value = p;
                                if (percentspan) {
                                    getPercent();
                                    var totalp = this.value;
                                    var firstp = document.getElementById("dkfirstprice").value;
                                    changeInfo(changePriceToNum(totalp), changePriceToNum(firstp));
                                }
                            }
                        }
                    }
                    else {
                        this.value = "请输入数字!";
                    }
                }
                //首付赋值
                dkfirstpriceinput.value = changeToPrice(financefirstprice);
                //首付获得焦点
                dkfirstpriceinput.onfocus = function () {
                    this.value = "";
                }
                //首付失去焦点 赋值 判断值 获取百分比
                dkfirstpriceinput.onblur = function () {
                    if (changeToPrice(this.value) != "0") {
                        if (this.value == "") {
                            this.value = changeToPrice(financefirstprice);
                            return;
                        } else {
                            if (parseInt(getobjValue(this)) > parseInt(getobjValue(dktotalpriceinput))) {
                                this.value = "请小于房屋总价";
                                return;
                            } else {
                                this.value = changeToPrice(this.value);
                                if (percentspan) {
                                    getPercent();
                                    var totalp = document.getElementById("dktotalprice").value;
                                    var firstp = this.value;
                                    changeInfo(changePriceToNum(totalp), changePriceToNum(firstp));
                                }
                            }
                        }
                    } else {
                        this.value = "请输入数字!";
                    }
                }
                //获取首付百分比
                function getPercent() {

                    var percent = parseInt(getobjValue(dkfirstpriceinput).replace(/,/g, "")) / parseInt(getobjValue(dktotalpriceinput).replace(/,/g, "")) * 100;
                    var b = percent.toString().indexOf(".");
                    var percent100 = ""
                    if (b > -1) {
                        percent100 = percent.toString().substring(0, b) + "%";
                    }
                    else {
                        percent100 = percent + "%";
                    }
                    percentspan.innerHTML = percent100;
                }
            }
   
        } catch (e) {
    
        }
   

    });
   
</script>
 

