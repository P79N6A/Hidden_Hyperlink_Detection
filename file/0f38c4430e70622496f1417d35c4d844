  <script type="text/javascript">
function Check() {
    if (jQuery("#srchtxt").val() == '' || jQuery("#srchtxt").val() =='搜索话题、问题...') {
        alert('请输入需要搜索的关键词!');
        return false;
    }	
	jQuery("#search").submit();
}
(function($){

	$(function(){
	
		$('#show_table tr').each(function(){
			var tr = this;		
			$(this).hover(function(){
				$(tr).addClass('trhover');
			
			}, function(){
				$(tr).removeClass('trhover');
			});
	
		});
	
	
		var show_name = '搜索话题、问题...';
		//alert('d');
		$("#srchtxt").one('click', function(){
			$(this).val('').css({color:'#000'});
		});
			
		$('#srchtxt').bind('blur', function(){	
			if ('' == $(this).val()) {
				$(this).val(show_name).css({color:'#ccc'});
			}
		}).bind('focus', function(){
			$(this).val('').css({color:'#000'});		
		});

	});
})(jQuery);

</script> 
