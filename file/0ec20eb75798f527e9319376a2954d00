           <textarea class="crayon-plain print-no" data-settings="dblclick" readonly="" style="-moz-tab-size:4; -o-tab-size:4; -webkit-tab-size:4; tab-size:4; font-size: 12px !important; line-height: 15px !important;">
&lt;!DOCTYPE html&gt;
&lt;html&gt;
&lt;head&gt;
&lt;/head&gt;
&lt;body&gt;
&lt;div style=&quot;float:left&quot;&gt;time:&lt;/div&gt;&lt;div id=&quot;div1&quot;&gt;&lt;/div&gt;
&lt;script&gt;
function Change(str){
window.document.getElementById(&quot;div1&quot;).innerText=str;
}var ifrpush;
function onload(){
	ifrpush = new ActiveXObject(&quot;htmlfile&quot;);
	ifrpush.open(); 
	var ifrDiv = ifrpush.createElement(&quot;div&quot;);
	ifrDiv.innerHTML = &quot;&lt;iframe src='Default.aspx'&gt;&lt;/iframe&gt;&quot;;
	ifrpush.appendChild(ifrDiv);
	ifrpush.close();
	ifrpush.parentWindow.Change=Change;
}

onload();
&lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</textarea>
