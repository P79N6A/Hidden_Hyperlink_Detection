          <div class="crayon-plain-wrap">
           <textarea class="crayon-plain print-no" data-settings="dblclick" readonly="" style="-moz-tab-size:4; -o-tab-size:4; -webkit-tab-size:4; tab-size:4; font-size: 12px !important; line-height: 15px !important;">
using System;
using System.Configuration;
using System.Data;
using System.Web;
using System.Web.Security;
using System.Web.UI;
using System.Web.UI.HtmlControls;
using System.Web.UI.WebControls;
using System.Web.UI.WebControls.WebParts;

public partial class _Default : System.Web.UI.Page
{
    protected void Page_Load(object sender, EventArgs e)
    {
        string str;
        while (true)
        {            
           str = &quot;&lt;script &gt;window.parent.Change('&quot; + DateTime.Now.ToLongTimeString() + &quot;')&lt;/script&gt;&quot;;

            Response.Write(str);
            Response.Flush();
            System.Threading.Thread.Sleep(1000);
        }
    }
}</textarea>
