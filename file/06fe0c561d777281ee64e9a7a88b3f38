<script type="text/javascript">
$(function(){
// Start a slideshow
$('body').backstretch([
  'http://www.alpha-grp.co.jp/wp-content/themes/pc_alpha-g/images/top/03.jpg',
  'http://www.alpha-grp.co.jp/wp-content/themes/pc_alpha-g/images/top/03.jpg',
  'http://www.alpha-grp.co.jp/wp-content/themes/pc_alpha-g/images/top/03.jpg'
], {duration: 7000, fade: 750});
$(".slide1Btn a").click(function(e) {
	 e.preventDefault(); 
	 $('body').data('backstretch').show(0), {duration: 5000, fade: 750};
});
$(".slide2Btn a").click(function(e) {
	 e.preventDefault(); 
	 $('body').data('backstretch').show(1), {duration: 5000, fade: 750};
});
$(".slide3Btn a").click(function(e) {
	 e.preventDefault(); 
	 $('body').data('backstretch').show(2), {duration: 5000, fade: 750};
});

// アコーディオン
$(".accordion").on("click", function() {
	$(this).toggleClass("active");//開いた時、ボタンにクラスを追加
	$(this).children("i").toggleClass("fa-plus");
	$(this).children("i").toggleClass("fa-minus");
	$(this).next(".accordion_cont").slideToggle("fast");//”slow”、”normal”、”fast”
});

});
</script>
