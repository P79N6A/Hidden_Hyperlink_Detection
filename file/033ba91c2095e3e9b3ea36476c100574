        var maxwidth = 5;
        var titleLength =$(".text-title").text().length;
        var nicknameLength =$(".text-nickname").text().length;
        if(titleLength>maxwidth){
          var str = $(".text-title").text().substring(0,maxwidth);
          var new_str = str+'...';
          $(".text-title").html(new_str);
        }
        if(nicknameLength>maxwidth){
