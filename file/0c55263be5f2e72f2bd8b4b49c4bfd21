          <div itemprop="logo" itemscope="" itemtype="https://schema.org/ImageObject">
           <meta itemprop="url" content="" />
          </div>
         </div>
         <div style="display: none;" itemprop="aggregateRating" itemscope="" itemtype="http://schema.org/AggregateRating">
