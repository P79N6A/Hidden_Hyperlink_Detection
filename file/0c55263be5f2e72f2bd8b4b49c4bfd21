	if(cf.insertAdjacentHTML){
		cf.insertAdjacentHTML("afterEnd", msghtml);
	}else{/* FireFox */
		var range = cf.ownerDocument.createRange();
		range.setStartAfter(cf);
		var frag = range.createContextualFragment(msghtml);
		cf.parentNode.insertBefore(frag, cf.nextSibling);
	}
}
//--><!]]></script>
    <script type="text/javascript" src="//img3.appinn.com/static/wp-content/plugins/akismet/_inc/form.js?ver=3.3"></script> 
    <script type="text/javascript" src="//img3.appinn.com/static/wp-includes/js/comment-reply.min.js?ver=4.7.3"></script> 
