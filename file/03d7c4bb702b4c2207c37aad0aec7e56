  <script>
$(".list_info .members").hover(
  function () {
	$(this).addClass("membershover");
  },
  function () {
	$(this).removeClass("membershover");
  }
);
$(".list_info .prices").hover(
  function () {
  	$(this).addClass("priceshover");
	$(this).children('.prices_txt').show();
  },
  function () {
  	$(this).removeClass("priceshover");
	$(this).children('.prices_txt').hide();
  }
);
$(".stowthis").attr("title","将这个公司收藏到我的名片夹");
