function checkComment() {
	var uname = THS.user.getUname("escapename");
	if (uname.length > 0) {
		if ($('#addcomment').val()!='' &&　$('#addcomment').val()!='请文明发言') {
			document.getElementById('publish_form').submit();
		} else {
			alert('写点东西吧~');
		}
	} else {
		$('.login_page').removeClass('hidden');
	}
}

function articleCommentRender(){
	this.render = function(json){
		var count = Article.comment.renderCommentLable(json);
		var commentbox = Article.comment.renderCommentBox(json);
		$('#cnum').html(count.replynum);
		$('#pnum').html(count.total);
		$('#commentbox').find('.list-box').prepend(commentbox);
	}
}
function flipCommentPage(page, pagenum){
	var startnum = 0;
	if (page > 0) {
		startnum = (page - 1)*pagenum;
	}
	THS.comment.service.loadComment(newsid, 'common', pagenum, startnum, new articleCommentRender());
}
function showCommentReply (floorid, commentid) {
	var html = THS.reply.commentReply(floorid, commentid);
	if (html) {
		$('#reply-box_' + commentid).find('.input-box').html(html).find('.login').click(function(){
			$('.login_page').removeClass('hidden');
		});
	}
	$('#reply-box_' + commentid).toggle();
}
function showLoginInfo () {
	var uname = THS.user.getUname("escapename");
	if (uname.length > 0) {
		$('.logged_in').html('欢迎您！<a href="http://i.10jqka.com.cn" target="_blank">'+uname+'</a>');
		$("#LoginLabel").hide();
		$("#LoginedLabel").show();
		$("#UserName").html(uname);
	}
}
function showLoginBox () {
	var uname = THS.user.getUname("escapename");
	if (!(uname.length > 0)) {
		$('.login_page').removeClass('hidden');
	}
}
function loginbyjsonp(num)
{
	var username = $('#uname').val();
	var password = $('#passwd').val();
	var href = 'http://upass.10jqka.com.cn/login?act=loginByJsonp&username='+username+'&password='+password+'&inCharset=gbk&callback=?';
	$.getJSON(href,function(jsonp){
		if (jsonp.errorcode == 0) {
			location.reload();
		} else {
			alert('用户名或者密码错误，登录失败！');
		}
	});
}
