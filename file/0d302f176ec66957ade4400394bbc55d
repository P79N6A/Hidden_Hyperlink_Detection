  <script type="text/javascript" language="javascript">

    function AddFavorite(url, title) {
        try {
            window.external.addFavorite(url, title);
        }
        catch (e) {
            try {
                window.sidebar.addPanel(title, url, "");
            }
            catch (e) {
                alert("加入收藏失败，请使用Ctrl+D进行添加");
            }
        }
    }
    function SetHome(obj, url) {
        try {
            obj.style.behavior = 'url(#default#homepage)'; obj.setHomePage(url);
        }
        catch (e) {
            if (window.netscape) {
                try {
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                }
                catch (e) {
                    alert("此操作被浏览器拒绝！\n请在浏览器地址栏输入“about:config”并回车\n然后将[signed.applets.codebase_principal_support]设置为'true'");
                }
                var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
                prefs.setCharPref('browser.startup.homepage', url);
            }
        }
    }

</script> 
  <!-- 页头项 --> 
  <script type="text/javascript">
    var closeTimer_top;
    var closeTarget_top;

    var timeout = 300;

    $(function () {
        //顶部菜单项
        $("#ul_top_bar > li").hover(
            function () {
                if (closeTimer_top) {
                    window.clearTimeout(closeTimer_top);
                }
                $(this).addClass("on").siblings().removeClass("on").find("div").hide();
                closeTarget_top = $(this).find("div");
                $(closeTarget_top).show();
                if ($.trim($('#messageNumDiv').text()) == "0")
                    $('#messageNumDiv').hide();
                if ($.trim($('#itemNumDiv').text()) == "0")
                    $('#itemNumDiv').hide();
            },
            function () {
                closeTimer_top = window.setTimeout(function () {
                    $(closeTarget_top).parent().removeClass("on");
                    $(closeTarget_top).hide();
                    if ($.trim($('#messageNumDiv').text()) != "0")
                        $('#messageNumDiv').show();
                    else
                        $('#messageNumDiv').hide();
                    if ($.trim($('#itemNumDiv').text()) != "0")
                        $('#itemNumDiv').show();
                    else
                        $('#itemNumDiv').hide();
                }, timeout);
            }
        );
        $("#ul_top_bar > li > div").hover(
            function () {
                if (closeTimer_top) {
                    window.clearTimeout(closeTimer_top);
                }
            } //该项的移出会冒泡到父节点li的移出事件 故此处无需处理
        );

        $("#loginInfo_Nav > li").hover(
            function () {
                if (closeTimer_top) {
                    window.clearTimeout(closeTimer_top);
                }
                $(this).addClass("on").siblings().removeClass("on").find("div").hide();
                closeTarget_top = $(this).find("div");
                $(closeTarget_top).show();
                if ($("#userName").val() != "")
                    $('#userNamePrompt').hide();

                if ($("#password").val() != "")
                    $('#passwordPrompt').hide();
            },
            function () {
                closeTimer_top = window.setTimeout(function () {
                    $(closeTarget_top).parent().removeClass("on");
                    $(closeTarget_top).hide();
                }, timeout);
            }
        );
        $("#userName").focus(
            function () {
                $("#loginMessage").hide();
                $('#userNamePrompt').hide();
            });
        $("#userName").blur(
            function () {
                if ($("#userName").val() == "")
                    $('#userNamePrompt').show();
                else
                    $('#userNamePrompt').hide();
            });
        $("#password").blur(
            function () {
                if ($("#password").val() == "")
                    $('#passwordPrompt').show();
                else
                    $('#passwordPrompt').hide();
            });

        $("#password").focus(
            function () {
                $("#loginMessage").hide();
                $('#passwordPrompt').hide();
            });

        $("#loginInfo_Nav > li > div").hover(
            function () {
                if (closeTimer_top) {
                    window.clearTimeout(closeTimer_top);
                }
            } //该项的移出会冒泡到父节点li的移出事件 故此处无需处理
        );
    });
</script> 
  <!--页头滚动信息--> 
  <script type="text/javascript">

    //友情链接=========================
    $("#div_topscroll").jCarouselLite({
        auto: 5000,
        speed: 1000,
        visible: 1,
        scroll: 1,
        vertical: true
    });



    //Remember 用户名和密码
    $(function () {
        var cookie_pwd = "password"; //userName
        var userName = "userName";
        if ($.cookie(cookie_pwd)) {
            $("#password").val($.cookie(cookie_pwd));
            $('#passwordPrompt').hide();
        }

        if ($.cookie(userName)) {
            $("#userName").val($.cookie(userName));
            $('#userNamePrompt').hide();
        }

        $("#remember").click(function () {
            if (this.checked) {
                $.cookie(cookie_pwd, $("#password").val(), { path: '/', expires: 10 })
                $.cookie(userName, $("#userName").val(), { path: '/', expires: 10 });
            } else {
                $.cookie(cookie_pwd, null, { path: '/' });
                $.cookie(userName, null, { path: '/' });
            }

        });
        //注册登录方法
        btnExeSPLoginClick();
        //检查用户状态并显示用户新息
        //wenduedunewobj.checkusersessionandshowuserinfo({ pageclass: 0,checksessionsuccessappendcallback:showuserinfo });
    });

    //注册单点登录
    function btnExeSPLoginClick() {
        $('#btnExeSPLogin').click(function () {
            var optionsjson = {
                usernameObj: $('#userName'),										//用户名
                passwordObj: $('#password'),										//密码
                loginMessageObj: $('#loginMessage'),								//登录信息框
                pageclass: 0,
                cpprocesssuccessappendcallback: showuserinfo
            };
            wenduedunewobj.splogin(optionsjson);
        });
    }

    //by liwenbing 0923
    function showuserinfo(data,options) {
        //$('#userName_Login').html("Hi！" + data.UserName);
       
        $('#loginInfo_Nav').html(" <a href='/user/default.aspx' class='sayhello'>" + data.UserName + "</a><span> 您好！</span><span onclick='wenduedunewobj.loginoutps({lblMsgObj:$(this)})' class='sayhello' style='cursor:pointer'>退出</span>");
        if (parseInt(data.MessageNum) <= 0) {
            $('#messageNumDiv').hide();
        } else {
            $('#messageNumDiv').show();
            $('#messageNumDiv').text(data.MessageNum);
        }

        $('#messageNumDetailsDiv').html(" <p><a href='http://i.wenduedu.com/info/index'>站内信<em>(&nbsp;" + data.MessageNum + "&nbsp;)</em></a></p><p><a href='/newslist.aspx?typeId=337'>最新公告</a></p>");
        if (parseInt(data.ItemNum) <= 0) {
            $('#itemNumDiv').hide();
        } else {
            $('#itemNumDiv').text(data.ItemNum);
            $('#itemNumDiv').show();
        }
        $('#itemNumDetailsDiv').html("您购物车里有<em>(&nbsp;<a href='http://i.wenduedu.com/user/cart'>" + data.ItemNum + "</a>&nbsp;)</em>件未结算课程。</div>");
    }


