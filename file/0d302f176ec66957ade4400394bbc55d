    function btnSearch_Click() {
        var keywords = $.trim($("#txtSearchKeywords").val());
        if (keywords == "") {
            alert("请输入您要找的内容！");
            $("txtSearchKeywords").focus();
            return;
        }

        var currentType = $.trim($("#searchBox").find("li:first-child > span").html());

        if (currentType == "课程") {
            window.open("/search/Lessons.aspx?kw=" + escape(keywords) + "&p="+2);
        }
        else if (currentType == "资讯") {
            window.open("/search/Informations.aspx?kw=" + escape(keywords) + "&p="+2);
        }
}

function txtSearchKeywords_KeyPress() {
    if (event.keyCode == 13) {
        event.keyCode = 0;
        btnSearch_Click();
    }
}
    
