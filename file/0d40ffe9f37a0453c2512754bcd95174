  <script>
ibcommon2 = {
  init : function() {
	this.setEvents();
	this.initArchive();
  },
  setEvents:  function() {
	//popular tab	
	$('.title-header .button-change a').click(function(){
		$('.title-header .button-change').children().removeClass("active");
		$(this).addClass("active");
		var index_num=$(this).index();
		$('.content-left .list-news').hide();
		$('.content-left #list-news'+index_num).show();
		setCookie("archivetype",index_num,"14");
	});
  },
  initArchive:  function() {
	var index_num=getCookie("archivetype");
	if(index_num=="0"||index_num>0){
		$('.title-header .button-change').children().removeClass("active");
		$('.title-header .button-change').children().eq(index_num).addClass("active");
		$('.content-left .list-news').hide();
		$('.content-left #list-news'+index_num).show();
	}
  },
};

$(document).ready(function(){
	ibcommon2.init();
});
</script> 
