       <script>
(function(){
var psum = 386 ;
var p = 1 ;
var ps = 10 ;
var ii = '1' ;

var xing = false ;
$('#i_feedlist_btn_'+ii).bind('click',function(){
if(xing) return false ;
xing = true ;

var _this = this ;

var post = { } ;
post.p = ++p ;
post.ps = ps ;
post.ii = ii ;
post.w = [] ;

$(_this).find('span').html('加载中...') ;
$.ajax({type: 'POST',url:'/feed/jslisthtml',data: post,success: function(html){
xing = false ;
$('#i_feedlist_'+ii).append($(html)) ;

if(post.p == psum){
$(_this).find('span').html('已没有更多内容') ;
}else{
$(_this).find('span').html('加载更多') ;
}
},dataType: 'html',error:function(){
xing = false ;
$(_this).find('span').html('加载更多') ;
}});

}) ;

})() ;
</script> 
