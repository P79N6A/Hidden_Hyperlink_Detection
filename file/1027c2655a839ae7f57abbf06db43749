var fontFamily;
switch('3'-1){
case 0:
fontFamily = '宋体';
break;
case 1:
fontFamily = 'Arial';
break;
case 2:
fontFamily = '微软雅黑';
break;
case 3:
fontFamily = '黑体';
break;
default:
fontFamily = '宋体';

};
fpMessage.style.fontFamily = fontFamily;	
</script> 
