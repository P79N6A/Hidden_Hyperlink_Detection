  <script>
	var Q,timeGo,bro=!window.applicationCache;
	$(function($) {
		var qukedouPublish = $("#publish"),
			qukedouLogoPublish = $("#logoPublish"),
			qukedouDrawers = $("#drawers"),
			qukedouPeople = $("#qukedouPeople");
			qukedouPublish.click(function(){
			  window.location.href = "http://"+window.location.host+"/?/publish/";
			});
			
		var qukedou = {
			down:function(){//下弹系列
				qukedouDrawers.css("left", "-120px");
				qukedouLogoPublish.removeClass("swing");
				qukedouDrawers.removeClass("swingHui swingPop");
				qukedouPeople.text(parseInt(Math.random()*10000+10000)+"人看问题");
				var that = this;
				setTimeout(function(){
						that.swing();
						setTimeout(function(){
							that.swingingPop();
						}, 300);
					}, 900);
					this.swingTop();
					setTimeout(function(){
						that.swing();
						setTimeout(function(){
							that.swingingPop();
						}, 1000);
					}, 900);
			},
			
			
			swingTop: function(){//跳出
				qukedouPublish.removeClass("swingBttom");
				qukedouPublish.addClass("swingTop");
				if(bro){
					qukedouPublish.animate({"top":"20px"}, 1000);
				}else{
					setTimeout(function(){
						qukedouPublish.css("top", "20px");
					}, 900);
				}
				
			},
			
			swing:function(){//旋转
				qukedouLogoPublish.removeClass("swing");
				setTimeout(function(){
					qukedouLogoPublish.addClass("swing");
				}, 0);
				
			},
			
			swingingPop:function(){//打开抽屉
				qukedouDrawers.removeClass("swingHui");
				qukedouDrawers.addClass("swingPop");
				
				if(bro){
					qukedouDrawers.animate({"left":"0px"}, 1000);
				}else{
					setTimeout(function(){
						qukedouDrawers.css("left", "0px");
					}, 700);
				}
				
			},
			
			
			
			
			up:function(){
				var that = this;
				
				qukedouDrawers.css("left", "0px");
				qukedouLogoPublish.removeClass("swing");
				qukedouDrawers.removeClass("swingHui swingPop");
				
				this.swingHui();
				setTimeout(function(){
					that.swing();
					setTimeout(function(){
						that.swingingBottom();
					}, 100);
				}, 600);
			
			},
			
			swingHui:function(){
				qukedouDrawers.removeClass("swingPop");
				qukedouDrawers.addClass("swingHui");
				
				if(bro){
					qukedouDrawers.animate({"left":"-120px"}, 1000);
				}else{
					setTimeout(function(){
							qukedouDrawers.css("left", "-120px");
						}, 700);
				}
					
			},
			//收起
			swingingBottom:function(){
				qukedouPublish.removeClass("swingTop");
				qukedouPublish.addClass("swingBttom");
				
				if(bro){
					qukedouPublish.animate({"top":"-200px"}, 1000);
				}else{
					setTimeout(function(){
						qukedouPublish.css("top", "-200px");
					}, 900);
				}
			},
			
			//旋转
			//回弹
			
			//检测动作
			isShop:false,
			init:function(){
				$(window).scroll(function() {
					var st = $(this).scrollTop();
					if(st> 60 && !qukedou.isShop){
						clearTimeout(timeGo);
						timeGo = setTimeout(function(){
							qukedou.down();
							qukedou.isShop = true;
						}, 500);
					}
					
					if(st<= 60 && qukedou.isShop){
						clearTimeout(timeGo);
						timeGo = setTimeout(function(){
							qukedou.up();
							qukedou.isShop = false;
						}, 500);
					}
				});
			}
		
		} 
		qukedou.init();
		Q = qukedou;
	});
			
	</script> 
