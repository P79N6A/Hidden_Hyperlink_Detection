	readHistory = function() {
		var a = getVrsHistory() || [],
		b = new jq.Buffers;
		if (a.length <= 0) b.append("<li>u60a8u76eeu524du8fd8u6ca1u6709u89c2u770bu8fc7u89c6u9891u8282u76eeuff01</li>");
		else for (var c = 0; c < a.length; c++) {
			var d = a[c].newTime;
			if (d) {
				var e = parseInt(d / 60);
				e = e < 10 ? "0" + e: e;
				var f = parseInt(d % 60);
				f = f < 10 ? "0" + f: f,
				tstr = "u5df2u770b" + e + ":" + f} else tstr = "\u6b63\u5728\u64ad\u653e";
