	<!--用户观看视频获取积分系统-->
	<script>
	$(function(){
		if($.cookie('_uid')){
			var specitalVideoMark = '';	// 区分是否为指定视频的字段

			// 如果是 则在观看30分钟后调用增加积分的方法
			if( specitalVideoMark == 'db' ) {
				setTimeout("storage2()",600000);
			}else{
				setTimeout("update_integral()",30000);
			}
		}
	});
	var name='%E5%8F%A3%E6%B0%B4%E5%B0%81%E7%A5%9E+%E7%AC%AC8%E9%9B%86';
	function update_integral(){
		$.ajax({
	        type:'get',  
	 	    url: _memberUrl+'/Credit/update_integral/vid/'+7833721+'/name/'+name,
	        dataType : 'jsonp',  
	        jsonp:"jsoncallback", 
	        success: function(data) {
	            if(data == 'yes'){
					return false;
				}
	        }
    	});
    }
	
	// 指定视频积分 +10
    function storage2(){
		$.ajax({
	        type:'post',  
	 	    url: _memberUrl+'/Credit/storage2/vid/'+7833721+'/name/'+name,
	        dataType : 'jsonp',  
	        jsonp:"jsoncallback", 
	        success: function(data) {
	            if(data == 'yes'){
					return false;
				}
	        }
    	});
    }
	</script></div>
