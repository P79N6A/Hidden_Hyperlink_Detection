<script type="text/javascript">
function Dluser(){
	var allcookies = $.cookie('_uid');
	if(allcookies){
		MyAutoRun();
		
	}else{	
		_onLogin();	
		}	
}	
</script>
<script type="text/javascript">
function MyAutoRun(){
	var allcookies = $.cookie('_uid');
	if(allcookies){	
		$.ajax({
			type:'GET',
			async:false,
			url:_memberUrl+'/Collect/scjs/id/7833721',
			dataType:"jsonp",
			jsonp:"jsonpCallback",
			success:function(data){
				$('#scjs').html(data);
			}
		});
	};
}
function UserCenterJf(){
	var _uid = $.cookie('_uid');
	var name='口水封神 第8集';
	if(_uid){	
		$.ajax({
			type:'post',
			async:false,
			url:_memberUrl+'/Credit/share/vid/'+7833721+'/name/'+name,
			dataType:"jsonp",
			jsonp:"jsonpCallback",
			success:function(data){
				if(data == 'yes'){
					return false;
				}
			}
		});
	};
}
$(function(){
	MyAutoRun();	
})	
</script>

