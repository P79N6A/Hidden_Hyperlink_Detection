			function changeData(_num,_tag){
				$(window).trigger('scrollstop');
				var _id = '7833721}';
				var _did = '7628625';
				var _trailer = '';
				var _num60 = parseInt('1');
				var _num75 = parseInt('1');
				$.get(
					'/Play/ajax_drama',
					{num : _num, tag : _tag, id : _id, did : _did},
					function(data){
						if(_tag == 60){
							$('#play_tipbox1').html(data);
							$('#play_tipbox1 a').click(djtj); //点击统计
							if(_num == _num60){
								$('#play_tipbox1').append(_trailer);
							}
						}else if(_tag == 75){
							$('#play_tipbox2').html(data);
							$('#play_tipbox2 a').click(djtj); //点击统计
							if(_num == _num75){
								$('#play_tipbox2').append(_trailer);
							}
						}
						scrollbar1.update();
					},'json'
				)
			}
