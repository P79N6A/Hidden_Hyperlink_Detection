<script type="text/javascript">

	$(".username_desc .help_button").hover(function() {
	    $("#help_desc").addClass("hovered");
	    $("#help_desc").show();
    }, function() {
	    var flag = 0;
	    $("#help_desc").mouseover(function() {
		    $("#help_desc").addClass("hovered");
		    $("#help_desc").show();
		    flag = 1;
	    });

	    if (flag == 0) {
		    $("#help_desc").removeClass("hovered");
		    $("#help_desc").hide();
	    }
    });

    $("#help_desc").hover(function() {
	    $("#help_desc").addClass("hovered");
    }, function() {
	    $("#help_desc").removeClass("hovered");
	    $("#help_desc").hide();
    });
</script>




	
