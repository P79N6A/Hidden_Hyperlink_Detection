function htmlDecode(str){
     return str
           .replace(/&#39;/g, '\'')
           .replace(/<br\s*(\/)?\s*>/g, '\n')
           .replace(/&nbsp;/g, ' ')
           .replace(/&amp;nbsp;/g,' ')
           .replace(/&lt;/g, '<')
           .replace(/&gt;/g, '>')
           .replace(/&quot;/g, '"')
           .replace(/&amp;/g, '&');
 }

//服务号appId 分享图片 分享内容 分享标题
var headimg = "static2/user/1665749/article/8a299a295ac80025015b09096567127a/1490505262450.jpg@!square-96";
var imgUrl = "";
var summary = htmlDecode("&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;很多人问");
var title = htmlDecode("云数贸是什么？在这里统一！");

if(headimg=="" || headimg.indexOf("add_box.jpg")!=-1 || headimg.indexOf(".gif")!=-1){
	imgUrl="http://oss.tieqq.com/resource/namecard/article/images/defaultPic.jpg";
}else if(summary==""){
	if(headimg=="" || headimg.indexOf("add_box.jpg")!=-1 || headimg.indexOf(".gif")!=-1){
		imgUrl ="http://oss.tieqq.com/resource/namecard/article/images/defaultPic.jpg";
	}else{
		imgUrl = "http://oss.tieqq.com/static2/user/1665749/article/8a299a295ac80025015b09096567127a/1490505262450.jpg@!square-96";
	}
	summary = title;
}else{
	imgUrl = "http://oss.tieqq.com/static2/user/1665749/article/8a299a295ac80025015b09096567127a/1490505262450.jpg@!square-96";
}

 
