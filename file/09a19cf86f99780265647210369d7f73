   <script type="text/javascript">
    var bigurlCount = 0;
    var imgs = $(".news1_main2").find("img");

    imgs.each(function (i) { if ($(this).attr("bigurl")) { bigurlCount++; } });
    imgs.each(function (i) {
        var img = $(this);
        var bigurl = img.attr("bigurl");
        if (bigurl) {
            var _a = $("<a class='preview' href='" + bigurl + "'></a>");
            img.wrap(_a);
            if (--bigurlCount == 0) { $("a.preview").lightBox(); }
        }
    });

    document.onkeydown = nextpage;
    var pagenow = window.location.pathname;
    var parthref = pagenow.substring(0, pagenow.indexOf(".shtml"));
    var thref = pagenow.substring(0, pagenow.indexOf("_"));
    var m = parseInt(parthref.split('_')[1]);
    //上一页链接
    var prevpage = m > 1 ? thref + "_" + (m - 1) + ".shtml" : pagenow;
    //下一页链接
    var nextpage = m < 1 ? thref + "_" + (m + 1) + ".shtml" : pagenow ;
    function nextpage(event) {
        event = event ? event : (window.event ? window.event : null);
        if (event.keyCode == 39) {
            location = nextpage;
        }
        if (event.keyCode == 37) {
            location = prevpage;
        }
    }
</script> 
   <script type="text/javascript" src="http://gg.hfhome.cn/home/ads/?siteid=2"></script> 
  </div>   
