  <script type="text/javascript">
        var txt = /^[\s\u4e00-\u9fa5\A-Za-z0-9\，\,\.\。\？\?\!\！\;\；\""\”“]*$/; //特许字符串过滤
        //var tel = /^[\d-\.\/\s]+$/;
        var tel = /^((\+86)|(86))?(1)\d{10}$/;
        function UpdateSignup() {
            if ($("#Name").val() == "" || $("#Name").val() == '必填') {
                alert("请填入报名人姓名！");
                $('#Name').val('').focus();
                return false;
            }
            if ($("#Phone").val() == "" || $("#Phone").val() == '必填') {
                alert("请填入联系电话！");
                $('#Phone').val('').focus();
                return false;
            }
            if (!tel.test($("#Phone").val())) {
                alert("您输入的手机号码是非法字符串，请您重新输入！");
                $('#Phone').val('').focus();
                return false;
            }
            if (!txt.test($("#Name").val())) {
                alert("您输入的意向区属是非法字符，请你重新输入！");
                $('#Name').val('').focus();
                return false;
            }
            var name = $("#Name").val();
            var phone = $("#Phone").val();
            var qq = $("#QQ").val();
            var purpose = $("#Purpose").val();
            if (purpose == "意向楼盘或区属") {
                purpose = "";
            }
            $.ajax({
                url: "/UpdateSignup",
                type: "POST",
                data: { name: escape(name), phone: escape(phone), qq: escape(qq), purpose: escape(purpose) },
                success: function (data) {
                    if (data.success) {
                        alert(data.msg);
                        window.location = window.location;
                    } else {
                        alert("报名失败");
                    }
                }
            });
        }
        function Btnsearch() {
            var w = $.trim($('#textfield1').val());
            if (w.length == 0 || w == '请输入关键字') {
                $('#textfield1').val('').focus();
                return false;
            }
            window.location = '/list/?w=' + encodeURI(w);
            return false;
        }
        function test() {
            var event = window.event || arguments.callee.caller.arguments[0];
            if (event.keyCode == 13) {
                Btnsearch();
            }
        } 
    </script> 
