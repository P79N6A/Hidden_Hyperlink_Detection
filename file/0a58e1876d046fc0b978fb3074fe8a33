        <script type="text/javascript">
function dochecked_weixin(){
	if(document.getElementById('sqty_name').value==''){
		alert('姓名不能为空');
		return false;
	}
	if(document.getElementById('sqty_company').value==''){
		alert('公司不能为空');
		return false;
	}	
	if(document.getElementById('sqty_cellphone').value==''){
		alert('手机号码不能为空');
		return false;
	}else{
		if(!checkPhone(document.getElementById('sqty_cellphone').value)){
			alert('手机号码格式错误');
			return false;
		}
	}
	if(document.getElementById('sqty_email').value==''){
		alert('邮件不能为空');
		return false;
	}else{
		if(!isEmail(document.getElementById('sqty_email').value)){
			alert('邮件格式错误');
			return false;
		}
	}
	if(document.getElementById('sqty_content').value==''){
		alert('申请内容不能为空');
		return false;
	}
	return true;
}

//检查电话号码邮箱
function checkPhone(phone)   
{
//验证电话号码手机号码，包含至今所有号段   
var ab=/^1[3|4|5|8][0-9]\d{4,8}$/;
  if(ab.test(phone) == false)
  {
    return false;
  }
 return true;
}

//检查email邮箱
function isEmail(str){
       var reg = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
       return reg.test(str);
}
</script> 
