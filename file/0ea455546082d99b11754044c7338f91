<script type="text/javascript">
(function(){
    $(document).ready(function(){
        $('#left .list').hide();
        var lottery_name = 'qxc';
        var urlParser = new LHC.URLParser(window.location.href);
        if("caipiao.baidu.com" == urlParser.getHost() && "https" == urlParser.getProtocol()){
            var attrHref = 'a[href="/rule/caipiao/' + lottery_name + '/"]';
        }else{
            var attrHref = 'a[href="/caipiao/' + lottery_name + '/"]';
        }
        var menuObj = $('#left .list ' + attrHref).parents('.list');
        menuObj.show();
        $('#left .list ' + attrHref).addClass('show');
    });
    $('.ltitle').click(function(){
        var $ele = $(this);
        var $item = $ele.next('.list');
        var to_hide = $ele.next('.list').is(':visible');

        $ele.parent('li').addClass('on');
        $('.list:visible').slideUp('slow');
        to_hide ? $item.slideUp('slow') : $item.slideDown('slow');
    });
})();
</script>
