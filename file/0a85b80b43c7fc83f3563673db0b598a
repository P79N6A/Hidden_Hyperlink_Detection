  <script type="text/javascript">
            var checkEmpty = function (name) {
                if ($("#" + name).val() == '')
                    $("#" + name).attr("style", "background-color:#FAFFBD");
                else {
                    $("#" + name).removeAttr("style", "background-color:#FAFFBD");
                }
            }
            $(function () {
                $('#NickName').blur(function () {
                    checkEmpty("NickName");
                });
                $('#Captcha').blur(function () {
                    checkEmpty("Captcha");
                });
                $('#PostText').blur(function () {
                    checkEmpty("PostText");
                });
                if ($('#button-post')) {
                    $('#button-post').click(function () {
                        if ($("#NickName").val() == '' || $("#Captcha").val() == '' || $("#PostText").val() == '') {
                            checkEmpty('NickName');
                            checkEmpty('Captcha');
                            checkEmpty('PostText');
                            return;
                        }
                        else
                            $('#frm-post').submit();
                    });
                }
            });
        </script>  
