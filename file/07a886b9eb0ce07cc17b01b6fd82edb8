  <script>
    $(function () {
        //价格区间
        var priceTop = +$("#priceT .priceTop").text();  //最高价格
        var priceDown = +$("#priceT .priceDown").text(); //最低价格
        var priceCurrent = 5485.94;//筛选最高价格

        //筛选下拉框
        $("#searchPlus .dropDl>dt").click(function (e) {
            $("#searchPlus .dropDl").removeClass("current").find("dd").fadeOut(100);
            $(this).parent(".dropDl").find("dd").fadeIn(100);
            $(this).parent(".dropDl").toggleClass("current");
            e.stopPropagation();
        });
        $("#searchPlus .dropDl").click(function (e) {
            e.stopPropagation();
        });
        $("body").click(function () {
            $("#searchPlus .dropDl").removeClass("current").find("dd").fadeOut(100);
        });
        $("#st1 dd :checkbox").change(function () {
            var _l = $("#st1 dd :checkbox:checked").length;
            if (_l == 0) {
                $("#st1 dt").text("设施/条件");
            }
            else {
                $("#st1 dt").text("设施+" + _l);
            }
        });
        $("#st2 dd :checkbox").change(function () {
            var _l = $("#st2 dd :checkbox:checked").length;
            if (_l == 0) {
                $("#st2 dt").text("主题");
            }
            else {
                $("#st2 dt").text("主题+" + _l);
            }
        });
        $("#st3 dd :checkbox").change(function () {
            var _l = $("#st3 dd :checkbox:checked").length;
            if (_l == 0) {
                $("#st3 dt").text("星级");
            }
            else {
                $("#st3 dt").text("星级+" + _l);
            }
        });

        //价格调整
        var _priceS = $("#priceS");
        var _pTol = $("#priceT .line").offset().left;
        $("#priceT .line i").on("mousedown", function (e) {
            var _this = $(this);
            var e = e || event;
            var _m = e.offsetX;
            $("body").on("mousemove.moveP", function (e) {
                var e = e || event;
                moveP(_this, e, _m);
            }).on("mouseup", function () {
                $("body").off("mousemove.moveP");
            });
        }).on("mouseup", function (e) {
            $("body").off("mousemove.moveP");
        }).on("click", function (e) {
            var e = e || event;
            e.stopPropagation();
        });
        $("#priceS").on("mousedown", function (e) {
            var e = e || event;
            e.stopPropagation();
            return;
        });
        function moveP(dom, e, _m) {
            var _l = e.pageX - _pTol - _m;
            if (_l >= 0 && _l <= 175) {
                dom.css("left", e.pageX - _pTol - _m);
                _priceS.text(function () {
                    var _p = parseInt((priceTop - priceDown) * (_l / 175) + priceDown);
                    return ("￥" + priceDown + "~￥" + _p);
                });
            }
            if (_l <= 0) {
                dom.css("left", 0);
                _priceS.text("￥" + priceDown + "~￥" + priceDown);
            }
            if (_l >= 175) {
                dom.css("left", 175);
                _priceS.text("￥" + priceDown + "~￥" + priceTop);
            }
        }
        $("#priceT .line").click(function (e) {
            var e = e || event;
            moveP($("#priceT .line i"), e, 0);
        });
        if (priceCurrent >= priceTop) {
            $("#priceT .line i").css("left", 175);
        }
        else {
            $("#priceT .line i").css("left", priceCurrent / priceTop * 100 + "%");
            _priceS.text("￥" + priceDown + "~￥" + priceCurrent);
        }

        //搜索
        var _searchPlus = $("#searchPlus");
        function jdSearch() {
            var _keyWord = $.trim(_searchPlus.find(".searchKey").val());
            var _st1 = "";
            $("#st1 dd :checkbox").each(function () {
                if ($(this)[0].checked) {
                    _st1 += $(this).attr("name") + "|";
                }
            });
            var _st2 = "";
            $("#st2 dd :checkbox").each(function () {
                if ($(this)[0].checked) {
                    _st2 += $(this).attr("name") + "|";
                }
            });
            var _st3 = "";
            $("#st3 dd :checkbox").each(function () {
                if ($(this)[0].checked) {
                    _st3 += $(this).attr("name") + "|";
                }
            });
            var _ps = $("#priceS").text().replace(/\￥/gi, "");

            if (_keyWord == "") {
                jK.msg("请填写搜索关键词！", 3000);
                _searchPlus.find(".searchKey").focus();
            }
            else {
                $("#searchBtn").off("click").on("click", function () {
                    jK.msg("正在查询，请勿反复点击！", 5000);
                });
                var sUrl = "/hotel-list.aspx?hotelKey=" + encodeURI(_keyWord);
                if (_st1 != "") {
                    sUrl += encodeURI("&amenity=" + _st1.slice(0, -1));
                }
                if (_st2 != "") {
                    sUrl += encodeURI("&theme=" + _st2.slice(0, -1));
                }
                if (_st3 != "") {
                    sUrl += encodeURI("&star=" + _st3.slice(0, -1));
                }
                if (_ps != "") {
                    sUrl += encodeURI("&price=" + _ps);
                }
                window.location.href = "" + sUrl;
            }
        }
        $("#searchBtn").off("click").on("click", function () {
            jdSearch();
        });

        $("#searchPlus input.searchKey").on("focus", function () {
            $(this).on("keydown.enterSearch", function (e) {
                var e = e || event;
                if (e.which == 13) {
                    $("#searchBtn").trigger("click");
                }
            });
        });
        $("searchPlus input.searchKey").on("blur", function () {
            $(this).off("keydown.enterSearch");
        });


        //默认搜索条件设置
        (function () {
            var d = jK.urlSearchObj();
            if (d.amenity) {
                var arr = d.amenity.split("|");
                var n = 0;
                $("#st1 dd :checkbox").each(function () {
                    if ($.inArray($(this).attr("name"), arr) < 0) {
                        $(this)[0].checked = false;
                    }
                    else {
                        $(this)[0].checked = true;
                        n++;
                    }
                });
                n ? $("#st1 dt").text("设施+" + n) : ("#st1 dt").text("设施/条件");
            }
            if (d.theme) {
                var arr = d.theme.split("|");
                var n = 0;
                $("#st2 dd :checkbox").each(function () {
                    if ($.inArray($(this).attr("name"), arr) < 0) {
                        $(this)[0].checked = false;
                    }
                    else {
                        $(this)[0].checked = true;
                        n++;
                    }
                });
                n ? $("#st2 dt").text("主题+" + n) : ("#st2 dt").text("主题");
            }
            if (d.star) {
                var arr = d.star.split("|");
                var n = 0;
                $("#st3 dd :checkbox").each(function () {
                    if ($.inArray($(this).attr("name"), arr) < 0) {
                        $(this)[0].checked = false;
                    }
                    else {
                        $(this)[0].checked = true;
                        n++;
                    }
                });
                n ? $("#st3 dt").text("星级+" + n) : ("#st3 dt").text("星级");
            }
        }());

        //悬浮搜索
        jK.fixScroll($("#searchPlus"), 0, 0);
    });

</script> 
