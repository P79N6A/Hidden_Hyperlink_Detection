   <script type="text/javascript">
    var type = '0';

    $("#moreInfor").click(function (e) {
        $("#showBox").css("display", "block");
        e.stopPropagation();
    });
    if (navigator.userAgent.match(/mobile/i)) {
        $(document).click(function (e) {
            if (e.target.id == "f-menu") {
                $("#tabb").show();
            } else {
                $("#tabb").hide();
            }
            if (e.target.id != "showBox") {
                $("#showBox").hide();
            }
           
        });
        if (type == 4 || type == 3) {
            $(".f-menu-logo").css({ "display": "none" })
        }
        else if( window.screen.width <1024){
           //alert(window.screen.width);
            $(".f-menu-logo").css({ "display": "inline-block" });
        }
       
    } else {
        $(document).click(function (e) {
            if (e.target.id != "showBox") {
                $("#showBox").hide();
            }
        });
    }
    function changtabCss(val) {
        if (val > "-1") {
            $("#tabb").find("a").removeClass("current");
            $("#tabb").find("a").eq(val).addClass("current");
        }
    }
    function checklong() {
        if ($(".dialog").length >= 1) {
            return false;
        } else {
            //requirejs.config({
            //    paths: {
            //        "login": "/Scripts/login"
            //    }
            //});
            //requirejs(["login"], function () {
                $(".g-ft").before(loginDiv);
            //});
        }
    }
    $(".search-btn").click(function () {
        var text = $.trim($(".search-txt").val());
        text = replaceContent(text);
        if (text.length > 30) {
            text = text.substr(0, 30);
        }
        var content = encodeURI(text);
        if (type == 0) {
            if (navigator.userAgent.match(/mobile/i)) {
                window.open("/Article/m_search/" + content, "_blank");
            } else {
                window.open("/Article/search/" + content, "_blank");
            }
        } else if (type == 1) {
            if (navigator.userAgent.match(/mobile/i)) {
                window.open("/Question/m_search/" + content, "_blank");
            } else {
                window.open("/Question/search/" + content, "_blank");
            }
        } else if (type == 2 || type == -2) {
            if (content.length != 0) {
                window.open("/search/" + content, "_blank");
            }
        } else {
            if (content.length != 0) {
                window.open("/User/searchuser/" + content, "_blank");
            }
        }
    });
    $(".search-txt").keydown(function (e) {
        var curKey = e.which;
        if (curKey == 13) {
            var text = $.trim($(".search-txt").val());
            if (text.length > 30) {
                text = text.substr(0, 30);
            }
            text = replaceContent(text);
            if (type == "0") {
                window.open("/Article/search/" + text, "_blank");
            } else if (type == "1") {
                window.open("/Question/search/" + text, "_blank");
            } else if (type == "2" || type == "-2") {
                if (text.length != 0) {
                    window.open("/search/" + text, "_blank");
                } 
            } else {
                if (text.length != 0) {
                    window.open("/User/searchuser/" + text, "_blank");
                }
            }
            return false;
        }
    });
    function replaceContent(str) {
        str = str.replace(/\ /g, '');
        str = str.replace(/\%/g, '');
        str = str.replace(/\:/g, '');
        str = str.replace(/\./g, '');
        str = str.replace(/\?/g, '');
        str = str.replace(/\(/g, '');
        str = str.replace(/\)/g, '');
        str = str.replace(/\'/g, '');
        str = str.replace(/\"/g, '');
        str = str.replace(/\!/g, '');
        str = str.replace(/\~/g, '');
        str = str.replace(/\&/g, '');
        str = str.replace(/\*/g, '');
        return str;
    }
    function isLogin() {
        $.ajax({
            type: "post",
            dataType: "json",
            url: "/Data/IsLogin.ashx",
            success: function (rspText) {
                if (rspText != null && typeof rspText !== "undefined") {
                    $("#divlog").remove();
                    $("#divinfo").show();
                    $("#Head_lblname").text(rspText.uNickName).parent().attr("href", "/" + rspText.uName);
                    $("#Head_imgPhoto").attr("src", "http://user.yszs360.com/UploadFiles/User/Photos/" + rspText.uPhoto).parent().attr("href", "/" + rspText.uName);
                    if (rspText.isEditor == "2") {
                        $("#Head_wrterlink").attr("href", "/Article/writerarticle");
                    }
                    if (rspText.isMsg > 0) {
                        if ($(".alert-email-new").length == 0) {
                            $(".new-user-infor").append('<a class="alert-email-new" href="/User/inbox"><img src="/Images/newemail.gif" /></a>');
                        }
                    } else {
                        if ($(".alert-email-new").length > 0) {
                            $(".alert-email-new").remove();
                        }
                    }
                    $("#showBox").find("a").eq(0).attr("href", "/" + rspText.uNickName);
                }
            }
        });
    }
</script> 
