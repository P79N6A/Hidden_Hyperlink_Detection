    $('body').on('click', '.btn-preorder', function(event) {
        if('undefined' !== typeof $(event.target).attr('data-desc')) $('#preorder-content').val($(event.target).attr('data-desc'));
        $('.preorder-wrap').show();
        $('#preorder-name').focus();
    });
    $('.preorder-close, .btn-close-preorder').on('click', function(event) {
        $('.preorder-wrap').hide();
    });
    window.preorder = function(a){
        if($(a).parsley().validate()){
            $(a).find('button, input[type="submit"]').attr('disabled', 'disabled');
            $.ajax({
                url: $(a).attr('action'),
                type: $(a).attr('method'),
                dataType: 'json',
                data: $(a).serialize(),
                success: function(json) {
                    $(a).find('button, input[type="submit"]').removeAttr('disabled');
                    if(json.head.statusCode == 0){
                        alert('预约成功');
                        $('.preorder-wrap').hide();
                    }else{
                        alert(json.head.note);   
                    }
                },
                error:function() {
                    $(a).find('button, input[type="submit"]').removeAttr('disabled');
                    alert('因网络等原因, 预约失败');
                }
            });
        }
    };
})(window, jQuery);
</script> 
