   function changetab1(index,tabids,showids,style){
            for(i=0;i<tabids.length;i++){
                    if(i==index){
                            $("#"+tabids[i]).removeClass(style[1]);
                            $("#"+tabids[i]).addClass(style[0]);                           
                            $("#"+showids[i]).show();
                        }else{
                            $("#"+tabids[i]).removeClass(style[0]);
                            $("#"+tabids[i]).addClass(style[1]);                               
                            $("#"+showids[i]).hide();
                            }
                }
        }

-->
</script> 
