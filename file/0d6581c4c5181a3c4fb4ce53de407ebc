function check_data(){
	if(document.getElementById("content").value == ""){
		alert("请填写咨询内容");
		document.getElementById("content").focus();
		return false;
	}
	if(document.getElementById("Emailliuyan").value != ''){
		if(document.getElementById("Emailliuyan").value.search(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/ ) == -1){
			alert("请填写正确邮箱");
			document.getElementById("Emailliuyan").focus();
			return false;
		}
	}else{
	        alert("请填写正确邮箱");
			document.getElementById("Emailliuyan").focus();
			return false;
	}
	if(document.getElementById("CheckCode").value == ""){
		alert("请填写验证码");
		document.getElementById("CheckCode").focus();
		return false;
	}
}
