 var count=3
    var i=document.getElementById('hiddennum').value; 
    var  start1="";
    function nowpage(obj)
    {
     document.getElementById('BigImg').src=document.getElementById(obj).src;
    }  
    function nextdown()
    {
    i++;
    if(i==count)
    {
    i=0;
    }
    document.getElementById('BigImg').src=document.getElementById("img"+i).src;
    }
    function nextup()
    {
    i++;
    if(i==0)
    {
    i=count;
    }
    document.getElementById('BigImg').src=document.getElementById("img"+i).src;
    }
    function autonext()
    {
    start1=setInterval("nextdown()",3000);
    }
    autonext();
