         <script>
function showCalendarAjaxNew(route_id,date){

var pars='do=route_ajax_new&route_id='+route_id+'&date='+date+'&flag=2&cache='+Math.random();

Request.sendPOST('/LineAfter.asp', pars, afterShowCalendarAjaxNew);
	
}

function afterShowCalendarAjaxNew(ajax){
	var result = ajax.responseText;
	document.getElementById("xilusy").innerHTML = result;
}

function next_cal(date){
	showCalendarAjaxNew(2655,date);
}

function past_cal(date){
	showCalendarAjaxNew(2655,date);
}

showCalendarAjaxNew(2655,'');


</script> 
