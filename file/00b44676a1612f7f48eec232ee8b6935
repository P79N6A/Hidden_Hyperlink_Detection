var nums = '3';
var clock = '';
function doLoop(){
	nums--;
	if(nums > -1){
		$("#sendcode").html(nums+'秒后进入');
	}else{
		clearInterval(clock); //清除js定时器
		//nums = '3'; //重置时间
		$("#sendcode").hide();
	}
}
setInterval(doLoop, 1000);
