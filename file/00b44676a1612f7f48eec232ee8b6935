    $("#pagetitle-btn").click(function () {
        $(".share-tip").hide();
        $('#sharetitle').val($('#pagetitle').val());

        wx.ready(function () {
            sharedata = {
                title: $('#sharetitle').val(),
                desc: '扩散~让更多人看到',
                link: 'http://wei.yuepai.ren/app/./index.php?i=2&c=entry&id=581&do=miaotie&m=weisrc_quickad',
                imgUrl: 'http://mmbiz.qpic.cn/mmbiz_jpg/P5o9dCgt6sYDODz8CzhudQGcTVMStWe5G30xwnW0rMeOSfomso811A3nquR9881sNDtkBkiaOgMc0zqtgia5cp3g/640?',
                success: function () {
                    var url = "./index.php?i=2&c=entry&id=581&do=share&m=weisrc_quickad";
                    location.href = url;
                },
                cancel: function () {
                }
            };
            wx.onMenuShareQZone(sharedata);
            wx.onMenuShareQQ(sharedata);
            wx.onMenuShareWeibo(sharedata);
            wx.onMenuShareAppMessage(sharedata);
            wx.onMenuShareTimeline(sharedata);
        });
    });
</script> 
