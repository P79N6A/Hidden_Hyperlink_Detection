<script type="text/javascript">
    var h_index = 0
    setInterval(function () {
        if (h_index != 2) {
            $('#horizontalScroll').animate({'top': parseInt($('#horizontalScroll').css('top')) - 40 + 'px'}, 500);
            h_index++
        } else {
            $('#horizontalScroll').animate({'top': '0px'}, 500);
            h_index = 0;
        }
    }, 2400);


    //这段代码在页面你家在完成后执行
    $content = $('#content');  //把ID改成存放内容的容器ID
    $contentWrap = $('#contentWrap');  //把ID改成存放内容的容器ID
    $('span.c333').click(function(){
        $('#page_list').toggle();
    });
    if ($content.height() > 700) {					//大于700启用
        $('#showAll').click(function () {				//绑定点击事件
            $contentWrap.css({'max-height': 'none'}); //显示所有内容

            $('.vertical_scroll_wrap,#verticalScroll').css({'height': window.innerWidth * .33066667 + 'px'});
            var v_index = 0
            setInterval(function () {
                if (v_index < 2) {
                    v_index++
                    $('#verticalScroll').css({'left': -100 * v_index + '%'});
                } else {
                    $('#verticalScroll').css({'left': '0'});
                    v_index = 0;

                }
                $('.scroll_handle li:eq(' + v_index + ')').css('background-color', '#ccc').siblings().css('background-color', '#fff');
            }, 2400);

            $(this).hide();
            $('#pages').show();
            $('#adPagesTop').show();
        });
    } else {
        $('.vertical_scroll_wrap,#verticalScroll').css({'height': window.innerWidth * .33066667 + 'px'});
        var v_index = 0
        setInterval(function () {
            if (v_index < 2) {
                v_index++
                $('#verticalScroll').css({'left': -100 * v_index + '%'});
            } else {
                $('#verticalScroll').css({'left': '0'});
                v_index = 0;
            }
            $('.scroll_handle li:eq(' + v_index + ')').css('background-color', '#ccc').siblings().css('background-color', '#fff');
        }, 2400);
		
		$contentWrap.css({'max-height': 'none'});
        $('#showAll').hide();
		$('#pages').show();
        $('#adPagesTop').show();
    }

    $("#font-big").click(function(){
        $(".content p").css({"font-size":"20px","line-height":"1.8em"});
    });
    $("#font-small").click(function(){
        $(".content p").css({"font-size":"18px","line-height":"1.7em"});
    });
    //需要在页面中曾加【查看全文】按钮，ID为showAll
	
		$.getScript('http://int.dpool.sina.com.cn/iplookup/iplookup.php?format=js',function(){  
		  if(remote_ip_info.city != '北京'){
			$("._beijing").show();
		  }
	});
</script>
