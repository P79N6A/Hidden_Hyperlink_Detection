<script type="text/javascript">
var targetposition = {
    	left:0,
    	top:0,
    	width:0,
    	zindex:99
    }
var mouseFlag  =false;
var bindThirdFunc = function(targtetid,moduleid){
jQuery(targtetid).mouseover(function(){							
if(mouseFlag){
return;
}else{
mouseFlag = true;
}
var that = jQuery(this);
var par =jQuery(that.parent());
var module = null;
if(that.find('.nav-third').length==0){
  	targetposition.left = par.css('left');
targetposition.top = par.css('top');
targetposition.width = par.css('width');
targetposition.zindex = par.css('z-index');
var parheight = par.height();
var parcount = par.children('li').length;
module = jQuery('#third-list>'+moduleid).clone();
jQuery('.nav-third').hide();
var thirdlen = module.find('li').length;
var colcount = Math.ceil(thirdlen/parcount);	
var width =colcount*130;
module.css({
'left':'100%',
'top':'0',
'z-index':-1,
'width':width+'px',
'height':parheight+1+'px'
})	
that.append(module);
module.show();
}else{
jQuery('.nav-third').hide();
module = jQuery(that.find('.nav-third'));
module.show();
}
that.on('mouseout',function(){
if(module === null){
return;
}
   if(!that.find('ul').get(0).contains(window.event.srcElement)){
   	 jQuery('.nav-third').hide(); 
   	 mouseFlag = false;
   	 console.log(1);
   }
})
jQuery(that.find('ul').get(0)).on('mouseout',function(){
                               	jQuery('.nav-third').hide(); 
   	mouseFlag = false;								
})
})
}

    var targetList = [{id:'#mn_N9ec7_562,#mn_N163a_502',module:'#hotellist'},{id:'#mn_N9ec7_561,#mn_Nbecc_506',module:'#airlist'},{id:'#mn_N9ec7_560,#mn_N53cd_375',module:'#banklist'}]
for(var i=0,len = targetList.length;i<len;i++){
var _temp = targetList[i];
bindThirdFunc(_temp.id,_temp.module);
}
 jQuery('.nav-third>li').mouseover(function(){
 	  if (!e){
 	  	var e = window.event;
 	  }else{
 	  	e.cancelBubble = true;
 	  }	        	 
          if (e.stopPropagation){
         	e.stopPropagation();
          }
 })
</script> -->


