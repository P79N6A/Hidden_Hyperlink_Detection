        var readtime_Invoke = 0;
        function let_read()
        { if (g('spn_readtime') != null) { readtime_Invoke = setInterval("readit()", 1000); } }
        function readit() {
            var second = parseInt(g('spn_readtime').innerHTML);
            if ((!isNaN(second)) && (second > 0))
            { second--; g('spn_readtime').innerHTML = second; }
            else { g('btn_submit_end').disabled = false; g('btn_submit_end').src = "http://help.5173cdn.com/dlc2c/images/dl/sqdl.gif"; clearInterval(readtime_Invoke); }
        } //alert(readtime_Invoke);


        // 收藏url by chenjw
        function CopyUrl() {
            var url = document.location.href;
            if (document.all) {
                window.external.addFavorite(url, document.getElementById('lblTitle').innerText);
            }
            else if (window.sidebar) {
                window.sidebar.addPanel(document.getElementById('lblTitle').textContent, url, "");
            }
        }
        function showdiv(id) {
            if (g('div_content').style.display != 'none') { g('div_content').style.display = 'none'; g("li_div_content").className = ''; }
            if (g('div_FrequentlyQuestions').style.display != 'none') { g('div_FrequentlyQuestions').style.display = 'none'; g("li_div_FrequentlyQuestions").className = ''; }
