        function SimpleHeadEnterKeyword(_event)
        {
            var searchbtn_id = 'top1_SearchBarV2_1_btnHeaderSearch';
	        var btn=document.getElementById(searchbtn_id);
	        if(document.all)
	        {
		         if(_event.keyCode == 13)
		         {
			        event.keyCode = 9;
			        event.returnValue = false;
			        btn.click();
		         }
	        }   
	        else
	        {
		        if(_event.which == 13)
		        {
			        var   evt   =   document.createEvent("MouseEvents");   
			        evt.initEvent("click",   true,   true);  
			        if(btn)
			        {
				        btn.dispatchEvent(evt);  
			        }
		        }
	        }
        }
</script> 
