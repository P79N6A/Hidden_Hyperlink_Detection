            $('.fake_select_item').each(function(ind,ele){
                var sel_ele = $(ele).find('.select_lst_cn a.icon_check_on,.select_lst_cn a.select');
                if(sel_ele){
                    $(ele).find('.select_title span').text(sel_ele.text());
                }
            });
