<script type="text/javascript">
    boot.require('util.cookie', function(Frame, cookie){
        if(cookie.get('show_more_filter') == '1'){
            $('.secitem:gt(5)').show();
            $('#filter_slide').addClass('icon_fang_on');
        }
    });

    $(".prifilter input:text:lt(4),.shoufu input:text:lt(2)").click(function() {
        $(this).parents('.prifilter').find('input:button').parent().show();
        return false;
    }).keyup(function(){
        ckdecimal($(this)[0]);
    });

    $(".prifilter input:text:gt(3)").click(function() {
        $(this).parents('.prifilter').find('input:button').parent().show();
        return false;
    }).keyup(function(){
        cknum($(this)[0]);
    });
    $(".prifilter input:button").click(function(){
        return false;
    });
    $(document.body).click(function(){
        $(".prifilter input:button").parent().hide();
    });

    $('#filter_huansuan1').change(function(){
        $('#price_secitem1').hide();
        $('#price_secitem2').show();
        $(this).val(1);
    });
    $('#filter_huansuan2').change(function(){
        $('#price_secitem2').hide();
        $('#price_secitem1').show();
        $(this).val(2);
    });


    var chanquanDesc = {
        w1:'商品房指在市场经济条件下，具有经营资格的房地产开发公司通过出让方式取得土地使用权后经营的住宅。从法律角度来分析，商品房一般产权年限为70年，可以按法律、法规及有关规定在市场上自由交易，目前，有部分城市已经对商品房开始限购。',
        w5:'是居家办公住宅观念的一种延伸。它属于住宅，但同时又融入写字楼的诸多硬件设施。商住两用房目前无限购政策，一般产权年限为50年，需缴纳较高的商业水电费价格，且部分商住两用房内没有天然气等设施。',
        w2:'经济适用房指已经列入国家计划，由城市政府组织房地产开发企业或者集资建房单位建造，以微利价向城镇中低收入家庭出售的住房。经济适用房未满5年时不能上市交易，满5年后，补交土地出让金把经适房转为商品房后就可以合法销售。未满5年交易，签署的买卖合同无效，不受法律保护。',
        w4:'使用权房是计划经济和住房分配体制下的产物，该房屋的产权属于国家或集体。使用权是从所有权分离出来的一项独立的财产权，允许使用权人在一定范围内通过一定方式转让或交换房屋使用权，所获收益扣除应缴税费后归使用权人所有。',
        w3:'公房指由国家以及国有企业、事业单位投资兴建、销售的住宅，在住宅未出售之前，住宅的产权（拥有权、占有权、处分权、收益权）归国家所有。目前居民租用的公有住房，按房改政策分为两大类：一类是可售公有住房，一类是不可售公有住房。上述两类房均为使用权房。'
    };

    $('#secitem_cq a').hover(function(){
        var _this = $(this);
        if(chanquanDesc[_this.attr('data-para-value')])
            _this.addClass('chanquan_a').append('<span style="z-index: 10;" class="local_new_list"><em></em>' + chanquanDesc[_this.attr('data-para-value')] + '</span>');
    }, function(){
        $(this).find('.local_new_list').remove();
    });

    //筛选条件-checkbox
    $('#buildingera a[data-para-value],#floor a[data-para-value],#fittype a[data-para-value]').click(function(){
        var key = $(this).parents('.secitem').attr('data-para-key');
        var val = $(this).attr('data-para-value');
        var url_params = $.getUrlParams();
        var url_value = url_params[key] ? url_params[key] + '|' : '';
        //删除，重新赋值
        delete url_params[key];
        if(val){
            val += '|';
        }
        //如果已经在筛选条件中则删除 | hack of '1|'->-4_-1|
        if(val && ('|' + url_value).indexOf('|' + val) > -1){
            url_value = url_value.replace(val, '');
        }
        //否则添加到最后
        else{
            url_value += val;
        }
        url_value = url_value.substring(0, url_value.length - 1);
        if(url_value){
            url_params[key] = url_value;
        }
        var url = location.href.replace(/\?.*/, '');
        if(!$.isEmptyObject(url_params)){
            var arr = [];
            for(var o in url_params){
                arr.push(o + '=' + url_params[o]);
            }
            url += '?' + arr.join('&');
        }
        location.href = url.replace(/%7C/g, '|').replace(/\/pn\d+/, '');
        return false;
    });

    //筛选条件-总价、面积、厅室按钮
    $('#minprice_filter1 :button,#minprice_filter2 :button,#minprice_filter3 :button,#area_filter :button,#tingshi_filter :button').click(function(){
        var inputs = $(this).parents('.prifilter').find('input[para]');
        if($(this).val() == "查找")
        {
            inputs = $(this).parents('.shoufu').find('input[para]');
        }

        var url = window.location.href.substring(7);
        var url_pre = location.hostname;
        var url_middle = location.pathname;
        //点击价格、面积筛选的时候需要将url中的h1、k1替换成空；
        var replace_reg = {
            //huansuan:/i\d+(\/)?/,
            huansuanyue:/i\d+(\/)?/,
            area:/k\d+(\/)?/,
            huxingshi:/e\d+(\/)?/
        };
        //如果点击排序之后需要加此默认的筛选条件
        var default_cond = {
            huansuan:'0.05_20',
            huansuanyue:'2_*',
            area:'2_*'
        };

        var _paras = {};
        $.each(inputs, function(i, item){
            var para = $(item).attr('para');
            if(replace_reg[para] && '' != $(item).val()){
                url_middle = url_middle.replace(replace_reg[para], '');
            }
            if(!_paras[para])
            {
                _paras[para] = [];
            }
            if($(item).val() == "输入首付")
            {
                $(item).attr("value","0");
            };
            _paras[para].push($(item).val());
        });
        var paras = $.getUrlParams();
        var huansuanyueOffset = 1, huansuanOffset = 10000;
        if($("#minprice_filter3 :text").val() != "输入首付")
        {
            if(!_paras["bunengdaikuan"])
            {
                _paras["bunengdaikuan"] = [];
            }
            _paras["bunengdaikuan"].push(0);
        };

        for(var o in _paras){
            var sort_hack = paras['sort_hack'];
            if(2 == _paras[o].length){
                var offset = 1;
                if(o == 'huansuanyue'){
                    offset = huansuanyueOffset;
                }
                else if(o == 'huansuan'){
                    offset = huansuanOffset;
                }

                var val0 = 0;
                if(_paras[o][0]){
                    val0 = _paras[o][0] - 0;
                }
                if(sort_hack && default_cond[o]){
                    var values = default_cond[o].split('_');
                    values[0] = values[0] - 0;
                    if(values[0] > val0)
                        val0 = values[0];
                }
                if(offset != 1)
                    _paras[o][0] = Math.round(val0 * offset);
                else
                    _paras[o][0] = val0;

                var val1 = '*';
                if(_paras[o][1]){
                    val1 = _paras[o][1];
                }
                if('*' != val1 && sort_hack && default_cond[o]){
                    val1 = val1 - 0;
                    var values = default_cond[o].split('_');
                    if(values[1] < val1)
                        val1 = values[1];
                }
                if('*' != val1 && offset != 1)
                    val1 = Math.round(val1 * offset);
                _paras[o][1] = val1;

                paras[o] = _paras[o].join('_');
            }
            else{
                if(sort_hack && default_cond[o]){
                    var val = paras[o];
                    if(val)
                        val = val - 0;

                    var values = default_cond[o].split('_');
                    values[0] = values[0] - 0;
                    if(values[0] > val)
                        val = values[0];
                    if('*' != values[1]){
                        values[1] = values[1] - 0;
                        if(values[1] < val)
                            val = values[1];
                    }
                    paras[o] = val;
                }
                else{
                    paras[o] = _paras[o][0];
                    if('' == _paras[o][0]){
                        delete paras[o];
                    }
                }
            }
            paras[o] = _paras[o].join('_');
        }

        var url = 'http://' + url_pre + url_middle;
        if(!$.isEmptyObject(paras)){
            url += '?' + $.param(paras);
        }
        location.href = url.replace(/%7C/g, '|').replace(/\/pn\d+/, '');
    });

</script><!--------------------------------------------------------页面的主体部分，包括智能广告和信息列表等-------------------------------->
