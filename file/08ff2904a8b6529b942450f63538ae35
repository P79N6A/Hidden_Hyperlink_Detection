<html style="height: 100%"><head><style>body {margin: 0;padding: 0;}</style><meta name="robots" content="noindex, nofollow"><script>function refreshIframe(){document.getElementById("bodytag").innerHTML=document.getElementById("bodytag").innerHTML}function escapeHTMLEncode(e){var r={"&":"&amp;","<":"&lt;",">":"&gt;",'"':"&quot;","'":"&#039;"};return e.replace(/[&<>"']/g,function(e){return r[e]})}function parseQuery(e){for(var r={},n=e.split("&"),t=0;t<n.length;t++){var a=n[t].split("=");r[decodeURIComponent(a[0])]=decodeURIComponent(a[1]||"")}return r}function buildQuery(e){var r=[];for(var n in e)e.hasOwnProperty(n)&&r.push(encodeURI(n)+"="+encodeURI(e[n]));return r.join("&")}function createIframe(){var e=window.location.hash.substring(1);m=e.match(/refresh=(\d+)&refresh_times=(\d+)&/),m&&m[1]&&(e=e.replace(/refresh=(\d+)&/,""),refresh=m[1],refreshTimes=m[2]);var r=parseQuery(e);e=buildQuery(r);var n="<iframe src='//ads.trafficjunky.net/ads?"+escapeHTMLEncode(e)+"' frameborder='0' border='0' scroll='no' scrolling='no' width='100%' height='100%' style='border: 0px; overflow: hidden !important;' />";if(document.getElementById("bodytag").innerHTML=n,refresh>0){var t=setInterval("refreshIframe()",1e3*refresh);setTimeout(function(){clearInterval(t)},1e3*refresh*refreshTimes+.5)}}var refresh=0,refreshTimes=1;window.addEventListener?window.addEventListener("load",createIframe,!1):window.attachEvent?window.attachEvent("onload",createIframe):window.onload=createIframe;</script></head><body style="height: 100%" id="bodytag"><iframe src="//ads.trafficjunky.net/ads?zone_id=" frameborder="0" border="0" scroll="no" scrolling="no" width="100%" height="100%" style="border: 0px; overflow: hidden !important;"></iframe></body></html>