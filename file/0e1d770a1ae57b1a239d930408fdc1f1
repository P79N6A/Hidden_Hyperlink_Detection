    function windowOpen(url){

    var dwId = 22843;
    var dwcId = getCookieLogin("did");
    var cookieArt = getCookie("did");
    var patt = new RegExp(dwId);

    if(!patt.test(cookieArt)){
        var date=new Date();//获取当前时间
        var expiresDays=1;//将date设置为1天以后的时间
        date.setTime(date.getTime()+expiresDays*2*3600*1000);
        var setTid = dwcId+','+dwId;
        document.cookie="did="+setTid+"; expires="+date.toGMTString();
        var clicked = 0;
    }else{
        var clicked = 1;
    };

    $.ajax({
        type: "POST",
	async:true,
        url: "http://app.pchome.net/index.php?c=Ajax&a=GetPcCount&clicked="+clicked,
        data: "dwId="+dwId,
	dataType : "jsonp",
        jsonp: "callback",
        jsonpCallback:"jsonpCallback",
        success: function(msg){
            url = url.indexOf('google.com')<0 ? url + '?key='+msg.key+'&tmp=' + new Date().getTime() : url;
            //self.location = url;
            //setTimeout(function(){window.close();},300);
            window.location.href = url;

        },
        error: function(XMLHttpRequest, textStatus, errorThrown){
           window.close();
        }
    });
}

window.onload=function(){
