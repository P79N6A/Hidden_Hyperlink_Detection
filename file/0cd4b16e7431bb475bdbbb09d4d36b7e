   <script>
	$(function () {
		var btn = $("#aj-random-access-btn");
		btn.on("click", function () {
			var min = parseInt($(this).attr("min"));
			var max = parseInt($(this).attr("max"));
			
			var random = Math.floor(min + max * Math.random());
			location.href = "?id=" + random;
		})
	})
</script> 
