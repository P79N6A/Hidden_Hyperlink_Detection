<script type="text/javascript" language="javascript">
    var userIdN = 0;
    (function (win, doc, undefined) {
        var Tool = {
            GetCookie: function (name) {
                var acookie = doc.cookie.split("; "),
    				i = 0,
    				len = acookie.length,
    				arr;
                for (; i < len;) {
                    arr = acookie[i++].split("=");
                    if (name == arr[0] && arr.length > 1) return arr
                }
                return null
            },
            loadScript: function (b, c) {
                var a = doc.createElement('script');
                a.type = 'text/javascript';
                if (a.readyState) {
                    a.onreadystatechange = function () {
                        if (a.readyState == 'loaded' || a.readyState == 'complete') {
                            a.onreadystatechange = null;
                            typeof c === 'function' && c();
                        }
                    }
                } else {
                    a.onload = function () {
                        typeof c === 'function' && c();
                    }
                }
                a.src = b;
                doc.getElementsByTagName('head')[0].appendChild(a)
            }
        };

        (function () {
            function GetUserId() {
                var reg = /[1-9]{1,}\d{0,}?$/,
                    userCookie = Tool.GetCookie("clubUserShow");
                if (!!userCookie) {
                    userCookie = reg.exec(userCookie[1].split('|')[0]);
                    return userCookie.length && userCookie[0];
                }
                return 0;
            }
            var loginDom = doc.getElementById('header_user'),
                userId = GetUserId();
            userIdN = userId;
            if (userId == 0) {
                loginDom.href = "//account.m.autohome.com.cn/?backurl=" + location.href + "#pvareaid=105246";
            } else {
                Tool.loadScript('//i.api.autohome.com.cn/apiajax/AjaxNewsMsg/GetMessageByHeadImage?_appid=car&userid=' + userId + '&callback=GetUser&timestamp=' + (new Date()).getTime(), function () {
                    var timer = null;
                    (function () {
                       var callback = arguments.callee;

                        if (typeof ($a) != 'undefined') {
                            $a.ajax({
                                url: "//msg.autoimg.cn/clubalert",
                                type: "POST",
                                data: { uid: userId },
                                dataType: "jsonp",
                                callback: "callback",

                                success: function (data) {
                                    clearTimeout(timer);
                                    timer = setTimeout(function () {
                                        callback();
                                        GetMessage(data)
                                    }, 30000);
                                }
                            });
                        }
                    }());
                });
            }
            win.GetUser = function (obj) {
                if (!obj || obj.returncode != 0) {
                    loginDom.href = "//account.m.autohome.com.cn/?backurl=" + location.href + "#pvareaid=105246";
                } else {
                    loginDom.href = "//i.m.autohome.com.cn/#pvareaid=105245";
                    loginDom.className = 'user';
                    loginDom.innerHTML = '<img src="' + obj.headimageurl30 + '" /><i style="display:none;">0</i>';
                    win.GetMessage(obj);
                }
            }
            win.GetMessage = function (obj) {
                if (!!obj) {
                    var icount = loginDom.getElementsByTagName('i')[0],
                        count = obj.total > 99 ? 99 : obj.total;
                    if (!icount) return;
                    icount.innerHTML = count;
                    icount.style.display = count > 0 ? 'block' : 'none';
                }
            }
        }());
    })(window, document)
</script>
 
