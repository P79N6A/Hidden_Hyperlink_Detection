                            $("#MorePicA").addClass("fn-hide");
                        }
                        else {
                            $("#MorePicA").removeClass("fn-hide");
                        }
                    }
                });
            },
            Loading:function(n)
            {
                switch(n)
                {
                    case 0:
                        $("#LoadLayer").hide();
                        break;
                    case 1:
                        $("#LoadLayer").show();
                        break;
                }
            },
            GetUrl: function () {
                var self = this;
                var url = self.ajaxUrl + "?delsize=" + self.initsize + "&seriesId=" + self.tempData["seriesId"] + "&specid=" + self.tempData["specid"] + "&colorid=" + self.tempData["colorid"] + "&innercolorid=" + self.tempData["innerColorid"] + "&classid=" + self.tempData["classid"] + "&size=" + self.tempData["size"] + "&index=" + self.tempData["index"] + "&lastPicId=" + self.tempData["lastPicId"];
                return url;
            },
            GetViewUrl: function (picClassId,picid) {
                var self = this;
                if(self.specId!=0)
                {
                    if (self.colorId > 0) {
                        if( self.classId==1 ||  self.classId==12 ||  self.classId==0)
                        {
                            return "#!/spec/" + self.specId + "/t" + self.classId +"/c" + self.colorId + "/i" + picid;
                        }
                        else
                        {
                            return "#!/spec/" + self.specId + "/t" + self.classId +"/i" + picid;
                        }
                    
                    }
                    else if(self.innerColorId>0)
                    {
                        if( self.classId==3 ||  self.classId==10 ||  self.classId==0)
                        {
                            return "#!/spec/" + self.specId + "/t" + self.classId+ "/n" + self.innerColorId + "/i" + picid;
                        }
                        else
                        {
                            return "#!/spec/" + self.specId + "/t" + self.classId+ "/i" + picid;
                        }
                    }
                    else {
                        return "#!/spec/" + self.specId + "/t" + self.classId+ "/i" + picid;
                    }
                }
                else
                {
                    if (self.colorId > 0) {
                        if( picClassId==1 ||  picClassId==12 ||  self.classId==0)
                        {
                            return "#!/series/" + self.seriesId + "/t" + self.classId+ "/c" + self.colorId + "/i" + picid;
                        }else
                        {
                            return "#!/series/" + self.seriesId + "/t" + self.classId+ "/i" + picid;
                        }
                    }
                    else if(self.innerColorId>0)
                    {
                        if( self.classId==3 ||  self.classId==10 ||  self.classId==0)
                        {
                            return "#!/series/" + self.seriesId + "/t" + self.classId+ "/n" + self.innerColorId + "/i" + picid;
                        }
                        else
                        {
                            return "#!/series/" + self.seriesId + "/t" + self.classId+ "/i" + picid;
                        }
                    }
                    else {
                        return "#!/series/" + self.seriesId + "/t" + self.classId+ "/i" + picid;
                    }
                }
            },
            AddMorePic: function (picdata) {
                var picObj = $("#listPic");
                var pichtml = new Array();
                var pichtmly = new Array();
                var self = this;
                var m=1;//第一次
                var n=0;
                $.each(picdata, function (i, item) {
                    
                    if (item.picClassId == self.lastPicClassId) {
                        $.each(item.picClassPicItems, function (i, itemo) {
                            pichtmly.push("<li><a href=\"/pic/view.html"+self.GetViewUrl(itemo.picClassId,itemo.picId)+"\"><img src=\"" + itemo.picUrl + "\"/></a></li>");
                            n++;
                        })
                    }
                    else {
                        self.lastPicClassId = item.picClassId;
                        pichtml.push("<h3>" + item.picClassName + "<small>" + item.picClassCount + "张</small></h3>");
                        pichtml.push("<ul class=\"list-picture\">");
                        $.each(item.picClassPicItems, function (i, itemo) {
                            pichtml.push("<li><a href=\"/pic/view.html"+self.GetViewUrl(itemo.picClassId,itemo.picId)+"\"><img src=\"" + itemo.picUrl + "\"/></a></li>");
                            n++;
                        })
                        pichtml.push("</ul>");
                    }
                    
                });
                if(n!=20)
                {
                    self.PicTotalCount++;
                }
                picObj.find("ul").last().append(pichtmly.join(''));
                picObj.append(pichtml.join(''));
                self.Loading(0);
            },
