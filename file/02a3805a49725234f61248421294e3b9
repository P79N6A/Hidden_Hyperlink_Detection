   <script type="text/javascript">
function check(){
var result='4414,';
var fid = document.getElementById('boxOne');
var box = fid.getElementsByTagName('input');
//alert(box.length);
if(box.length==3){
for(var i = 0; i < box.length; i++){
           if(box[i].type == 'checkbox' && box[i].checked  || box[i].type == 'radio' && box[i].checked){
                result = result + box[i].value + ',';
           }
        }
addToCartNums(result);
}else{
    var result1=false;
      for(var i=0;i<box.length;i++){
        if(box[i].type=="radio"&& box[i].checked){
        result1=true;
      }
}
if(!result1){
	alert("请选择套餐商品规格");
	return false;
	}
for(var i = 0; i < box.length; i++){
           if(box[i].type == 'checkbox' && box[i].checked  || box[i].type == 'radio' && box[i].checked){
                result = result + box[i].value + ',';
           }
        }
//alert(result);
//return false;
        //在Common.js文件中添加了addToCartNums方法
        addToCartNums(result);
document.documentElement.scrollTop=0;
}
}
function checks(){
var result='4414,';
var fid = document.getElementById('boxtwo');
var box = fid.getElementsByTagName('input');
if(box.length== 0){
addToCartNums(result);
}else{
    var result1=false;
    for(var i=0;i<box.length;i++){
       if(box[i].type=="radio"&& box[i].checked){
        result1=true;
       }
    }
      if(!result1){
	         alert("请选择套餐商品规格");
	         return false;
	        }
for(var i = 0; i < box.length; i++){
           if(box[i].type == 'checkbox' && box[i].checked  || box[i].type == 'radio' && box[i].checked){
                result = result + box[i].value + ',';
           }
        }
//alert(result);
//return false;
        //在Common.js文件中添加了addToCartNums方法
        addToCartNums(result);
document.documentElement.scrollTop=0;
}
}
/*
   计算合计金额，by 517ld.com Joe
*/
function totalPrice(){
//var result=Math.round(249.00);
var re = /[￥元]/g;
  var result=249.00;
//alert(result);
var fid = document.getElementById('boxOne');
var box = fid.getElementsByTagName('input');
var j=1;
for(var i = 0; i < box.length; i++){
    if(box[i].type == 'checkbox' && box[i].checked){
  var a = box[i].name;
     result =  Number(result) +  Number(a.replace(re, ''));
     j+=1;
    }
}
result=Math.round(result*100)/100;
result_format='￥%s';
re_re=/\%s/g;
result_format=result_format.replace(re_re, result);
document.getElementById("totalPrice").innerHTML=result_format;
document.getElementById("totalNum").innerHTML=j;
}
function totalPrices(){
//var result=Math.round(249.00);
var re = /[￥元]/g;
  var result=249.00;
//alert(result);
var fid = document.getElementById('boxtwo');
var box = fid.getElementsByTagName('input');
var j=1;
for(var i = 0; i < box.length; i++){
    if(box[i].type == 'checkbox' && box[i].checked){
  var a = box[i].name;
  //alert();
     result =  Number(result) +  Number(a.replace(re, ''));
     j+=1;
    }
}
result_format='￥%s';
re_re=/\%s/g;
result_format=result_format.replace(re_re, result);
document.getElementById("totalPrices").innerHTML=result_format;
document.getElementById("totalNum").innerHTML=j;
}
</script> 
