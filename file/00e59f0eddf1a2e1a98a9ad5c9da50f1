   <script type="text/javascript">
    $.ajax({
            type:"get",
            url:"http://www.yibu777.com/dataserver/1986",
            success:function(data){
                var starttime=$(data).find(".starttime").html();
                console.log(starttime);
                $(".shijian").html(starttime);
                var qishu =$(".fb1").html();
                var haoma=qishu*1+starttime*1;
                console.log(haoma)
                $(".quyu").html(haoma);

            },
