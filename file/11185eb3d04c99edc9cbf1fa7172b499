        <div class="crayon-plain-wrap">
         <textarea wrap="soft" class="crayon-plain print-no" data-settings="dblclick" readonly="" style="-moz-tab-size:4; -o-tab-size:4; -webkit-tab-size:4; tab-size:4; font-size: 12px !important; line-height: 15px !important;">
/************* WordPress jQuery 添加AJAXo刷新之u分功能_始 *************/
/**** 教W：http://www.arefly.com/wordpress-ajax-comment-pages/ ****/
jQuery(document).ready(function($) {
	$body = (window.opera) ? (document.compatMode == &quot;CSS1Compat&quot; ? $('html') : $('body')) : $('html,body');
	// ⑾路降摹#comments-navi」改槟愕脑uШID/CLASS！
	$(document).on('click', '#comments-navi a', function(e) {
		e.preventDefault();
		$.ajax({
			type: &quot;GET&quot;,
			url: $(this).attr('href'),
			beforeSend: function() {
				// ⑾路降摹#comments_content」改槟愕脑u^K部任意ID/CLASS！
				$body.animate({scrollTop: $('#comments_content').offset().top - 65}, 1500);
				setTimeout(function (){
					$('#loading-comments').slideDown();
					// ⑾路降摹.navigation」改槟愕ШID/CLASS！
					$('.navigation').remove();
					// ⑾路降摹.commentlist」改槟愕恼wu热葜ID/CLASS！
					$('.commentlist').fadeOut(800);
				}, 1700);
			},
			dataType: &quot;html&quot;,
			success: function(out) {
				// ⑾路降摹.commentlist」改槟愕恼wu热葜ID/CLASS！
				result = $(out).find('.commentlist');
				// ⑾路降摹.navigation」改槟愕ШID/CLASS！
				belownav = $(out).find('.navigation');
				$('#loading-comments').slideUp(550);
				$('#loading-comments').after(result.fadeIn(800));
				result.after(belownav);
			}
		});
	});
});
/************* WordPress jQuery 添加AJAXo刷新之u分功能Y束 *************/</textarea>
        </div> 
