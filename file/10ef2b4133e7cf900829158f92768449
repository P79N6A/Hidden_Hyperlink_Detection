    var topHeiht = $("#nav").position().top; //div离顶部多少距离
    $(window).scroll(function () {
        if (jQuery(document).scrollTop() < topHeiht) {
            jQuery("#nav").removeClass("fixTop");
        }
        else {
            jQuery("#nav").addClass("fixTop");
        }
    });

    $(function () {
        try {
            if ($("#zbwindows").length > 0) {
                var tHeiht = $("#zbwindows").position().top; //div离顶部多少距离
                $(window).scroll(function () {
                    if (jQuery(document).scrollTop() < tHeiht) {
                        jQuery("#zbwindows").removeClass("zbfixTop");
                    }
                    else {
                        jQuery("#zbwindows").addClass("zbfixTop")
                    }
                });
