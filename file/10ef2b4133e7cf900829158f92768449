    //selecttype(1);
    function sclick() {
        $(".zk_search_bar").show();
        $("#spic").addClass("upspic");
        $("#spic").removeClass("downspic");
    }

    function selecttype(num) {
        $(".zk_search_bar").hide();
        $("#spic").removeClass("upspic");
        $("#spic").addClass("downspic");
        $("#isselect").val(num);

        switch (num) {
            case 1:
                $("#key").attr("placeholder", "公司名称");
                $("#shtml").html("公司");
                break;
            case 2:
                $("#key").attr("placeholder", "小区名称");
                $("#shtml").html("案例");
                break;
            case 3:
                $("#key").attr("placeholder", "报价关键词");
                $("#shtml").html("报价");
                break;
            case 4:
                $("#key").attr("placeholder", "关键词");
                $("#shtml").html("文章");
                break;
        }
    }
    $("#key").keydown(function (e) {
        e.stopPropagation();
        if (e.keyCode == 13) {
            $("form").submit(function () {
                return false;
            });
            so();
        }
    });
    function so() {
        var sd = $("#isselect").val();
        switch (sd) {
            case "1":
                window.location.href = "http://www.zhuangku.com/gongsi?&keyword=" + $("#key").val();
                break;
            case "2":
                window.location.href = "http://www.zhuangku.com/anli?sd=2&keyword=" + $("#key").val();
                break;
            case "3":
                window.location.href = "http://www.zhuangku.com/baojia?sd=3&key=" + $("#key").val();
                break;
            case "4":
                window.location.href = "http://www.zhuangku.com/xuexi/class/0?sd=4&keyword=" + $("#key").val();
                break;
        }
    }

    $(function () {
        //导航移动上去样式
        $(".nav ul li").hover(function () {
            $(this).find(".menu_nav").show();
        }, function () {
            $(this).find(".menu_nav").hide();
        });
        //导航下拉高度
        $("#nav ul li .menu_nav").each(function () {
            $(this).css("width", $(this).parent("li").width());
            $(this).find("a").css("width", $(this).parent("li").width());
        })
        //二维码
        $("#erwima").unbind().bind("click", function () {
            if ($("#downbox").is(":hidden")) {
                $("#wdownbox").hide();
                $("#downbox").show(200);
            }
            else {
                $("#downbox").hide(200);
            }

        });
    })

    //导航选中处理
    var pathname = window.location.pathname.toLowerCase();
