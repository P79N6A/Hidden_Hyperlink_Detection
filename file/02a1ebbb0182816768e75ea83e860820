  <script>
var inputWidth=$(".sch_panel").width()-52;	
$('#scbar_txt').autocomplete('http://s.feng.com/api/autocomplateAjax.php',{
max: 12,
minChars: 1,
width: inputWidth,
scrollHeight: 300,
matchContains: true,
autoFill: false,
dataType: 'jsonp',
jsonp: "callback", 
extraParams: {
searchtype: 'thread' 
},           
parse: function(rdata){ 
var rows = [];
for(var i=0; i<rdata.length; i++){
rows[i] = { 
data:rdata, 
value:rdata[i], 
result:rdata[i], 
}; 
}
return rows;    
}
}).result(function(event, data, formatted) {
$("#scbar_form").submit();
});

