var obj = $("#manage_"+pid+" ul li")
var width = $("#manage_"+pid+" ul").width();
var len = obj.length;
(len==1)?obj.css('width',(width/len)+'%'):obj.css('width',((width/len-2)+'%'))
$("#manage_"+pid).toggle();
});
