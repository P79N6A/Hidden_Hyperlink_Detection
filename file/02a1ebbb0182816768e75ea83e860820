            adsImgArr = ['http://yes1.feng.com/images/58d39e93e4249.jpg'];

        function _setCookie(name, value, expireHours) {
            var expires = "";

            if (expireHours) {
                var exdate=new Date();
                exdate.setTime(exdate.getTime()+(expireHours*60*60*1000));

                expires = "; expires="+exdate.toGMTString();
            }

            document.cookie = name+ "=" + encodeURIComponent(value) + expires + ';path=/';
        }

        document.addEventListener('DOMContentLoaded', function(){
            // 检查是否已弹出安装窗口
            var popedFlag = '_ADS_POPED_DHXYv37_FLAG',
                cookies = document.cookie;
           
            if (cookies.indexOf(popedFlag) > -1) {
                // 已弹窗
                return;
            } else {
                var adsShell = document.createElement('div'),
                    closeBtn = document.createElement('span'),
                    logoShell = document.createElement('div'),
                    randomPos = Math.floor(Math.random() * adsImgArr.length),
                    head = document.head || document.getElementsByTagName('head')[0],
                    styleNode = document.createElement('style'),
                    realImg = adsImgArr[randomPos];

                styleNode.type = "text/css";
                styleNode.appendChild(document.createTextNode('@keyframes breath {from { opacity: 0.6; }  50%  { opacity:   1; }  to   {opacity: 0.6; } };'));
                head.appendChild(styleNode);

                adsShell.style.cssText = 'position: fixed; width: 100%; bottom: 0; left: 0; background: transparent; z-index: 9999; text-align:center;';
                adsShell.innerHTML = '<a href="'+ DOWNLOAD_URL +'" style="display: block;"><img src="'+ realImg +'" style="max-width:100%;"/></a>';
                closeBtn.style.cssText = 'position: absolute; right: 0px; top: 0px; padding: 12px; display: inline-block; width: 16px; height:16px; background: url('+ closeBtnURL +') no-repeat;background-size: 16px auto; background-position: 50% 60%';
                adsShell.appendChild(closeBtn);
                logoShell.style.cssText = 'position: fixed; width: 60px; height: 60px; left: 10px; bottom: 30px; display: none; animation:breath 3s ease-in-out infinite;';
               // logoShell.innerHTML = '<img src="'+ logoURL +'" style="width: 60px; height: 60px;">';

                document.body.appendChild(adsShell);
               document.body.appendChild(logoShell);

                closeBtn.addEventListener('click', function(e){
                    e.stopPropagation();

                    // 点击关闭按钮 - 隐藏1小时
                   //_setCookie(popedFlag, 1, 1);

                    adsShell.style.display = 'none';
                    logoShell.style.display = 'block';
                }, false);

                logoShell.addEventListener('click', function(e){
                    adsShell.style.display = 'block';
