      <script type="text/javascript">
    var changeListDefaultText = function(){
        var objSelect = Sizzle('#list_select_language')[0];
        if (objSelect && objSelect.selectedIndex != 0) {
            objSelect.options[0].text = "全部语言";
        }
        objSelect = Sizzle('#list_select_country')[0];
        if (objSelect && objSelect.selectedIndex != 0) {
            objSelect.options[0].text = "全部地区";
        }
        objSelect = Sizzle('#list_select_kind')[0];
        if (objSelect && objSelect.selectedIndex != 0) {
            objSelect.options[0].text = "全部类型";
        }
        objSelect = Sizzle('#list_select_year')[0];
        if (objSelect && objSelect.selectedIndex != 0) {
            objSelect.options[0].text = "全部年代";
        }
    }
    changeListDefaultText();
</script> 
