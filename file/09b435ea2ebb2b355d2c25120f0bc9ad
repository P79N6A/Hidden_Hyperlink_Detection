function logintype()
{
//alert(document.from1.login[0].checked);
//window.from1.action="deleteTrader.asp";
//window.from1.submit();
	if(!(document.from1.login[0].checked))
	{
		window.from1.action="userlogin.asp";
		window.from1.submit();

	}else
	{
		window.from1.action="http://trader.sanhaostreet.com/Admin/GoTo_Login.asp";
		window.from1.submit();
	}
	
}
