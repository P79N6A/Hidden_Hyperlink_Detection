  <script>
var isnight = "";
var tpl_style = "";
$(function(){

//涓瑁绀
$(".browsers_ad_close_gray").on("click", function() {
$(".cc-down-tip-box").toggle();
});



    $('.list-group .active').on('tap',function(){
        if($(this).attr('status') == '1'){
            $(this).addClass('list-group-radius').attr('status','0').nextAll('li').hide();
$(this).find("span").removeClass('glyphicon-minus-sign');
$(this).find("span").addClass('glyphicon-plus-sign')
        }else{
            $(this).removeClass('list-group-radius').attr('status','1').nextAll('li').show();
$(this).find("span").removeClass('glyphicon-plus-sign');
$(this).find("span").addClass('glyphicon-minus-sign')
        }
    });

    $('#myTab a').click(function (e) {
        e.preventDefault()
        $(this).tab('show')
    });

    $('.pg label').remove();

    if(isnight == "1"){
        $.each($("a:not(.carousel-control,.tabs)"),function(i,v){
            var href = $(v).attr("href");
            $(v).attr("href", href + "&isnight=1");
        });

        var form_url = $(".form-horizontal").attr("action");
        $(".form-horizontal").attr("action", form_url + "&isnight=1");
    }

    $('#isnight').bootstrapSwitch();
    $('#isnight').bootstrapSwitch('setSizeClass', 'switch-small');
    if(isnight == "1"){
        $('#isnight').bootstrapSwitch('setState', true);
    }else{
        $('#isnight').bootstrapSwitch('setState', false);
    }

    $('#isnight').on('switch-change', function (e, data) {
        if(data.value == true){
            loadjscssfile("./template/default/resources/style/night/style.css","css");
            $.each($("a:not(.carousel-control,.tabs)"),function(i,v){
                var href = $(v).attr("href");
                $(v).attr("href",href + "&isnight=1");
            });
        }else{
            loadjscssfile("./template/default/resources/style/"+ tpl_style +"/style.css","css");
            removejscssfile("./template/default/resources/style/night/style.css","css");
            $.each($("a"),function(i,v){
                var href = $(v).attr("href");
                $(v).attr("href", parse_url(href));
            });
        }
    });

$('.bs-navbar-collapse').on('shown.bs.collapse', function () {
        $('#wrap').click(function(){
            $('.bs-navbar-collapse').collapse('hide');
        });
    });

});

function loadjscssfile(filename, filetype){
    if (filetype=="js"){
        var fileref=document.createElement('script')
        fileref.setAttribute("type","text/javascript")
        fileref.setAttribute("src", filename)
    }
    else if (filetype=="css"){
        var fileref=document.createElement("link")
        fileref.setAttribute("rel", "stylesheet")
        fileref.setAttribute("type", "text/css")
        fileref.setAttribute("href", filename)
    }
    if (typeof fileref!="undefined")
        document.getElementsByTagName("head")[0].appendChild(fileref)
}

function removejscssfile(filename, filetype){
    var targetelement=(filetype=="js")? "script" : (filetype=="css")? "link" : "none"
    var targetattr=(filetype=="js")? "src" : (filetype=="css")? "href" : "none"
    var allsuspects=document.getElementsByTagName(targetelement)
    for (var i=allsuspects.length; i>=0; i--){
        if (allsuspects[i] && allsuspects[i].getAttribute(targetattr)!=null && allsuspects[i].getAttribute(targetattr).indexOf(filename)!=-1)
            allsuspects[i].parentNode.removeChild(allsuspects[i])
    }
}

function parse_url(url){
    var pattern = /&isnight=(\w+)/ig;
    s = url.replace(pattern,'');
    return s;
}
</script> 
