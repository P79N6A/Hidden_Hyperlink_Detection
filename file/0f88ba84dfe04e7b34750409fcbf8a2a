    <script>
function sharenv(action) {
if (action == 'share_qt') {
$('#share_other').fadeIn(350);
$(".share_fx").slideDown();
} else {
$(".share_fx").slideUp();
$('#share_other').fadeOut(350);
}
}

</script>
