  <script type="text/javascript"> 
$(document).ready(function(){ 
var headHeight=$("#ECS_FORMBUY").height()+$(".position").height()+$(".head").height()+$(".body_top").height()+20;
var height1 =  $("#qz01").height()+headHeight+20;
var height2 =  $("#qz02").height()+height1+40;
var height3 =  $("#qz03").height()+height2+40;
var height4 =  $("#qz04").height()+height3+40;
var height5 =  $("#qz05").height()+height4+60;
var nav=$("#secondnav"); 
$("#qzmidnav06 span").html($("#asknumber").html());



var bestnum = $("#two2").val();
var goodnum = $("#two3").val();
var badnum  = $("#two4").val();

var d = bestnum+goodnum+badnum;
if(d != 0){
var s = bestnum*5/d + "";
var bs = Math.round(bestnum/d*10000)/100.00;
var gs = Math.round(goodnum/d*10000)/100.00;
var bas =Math.round(badnum/d*10000)/100.00;
}else{
var s = 5.00 + "";
var bs = 100;
var gs = 0;
var bas =0;
}
$("#manyidu").html(s.substring(0,s.indexOf(".") + 2));
$("#bestnum").html("（" + bs + "%）");
$("#bestnump").css("width",bs + "%");
$("#goodnum").html("（" + gs + "%）");
$("#goodnump").css("width",gs + "%");
$("#badnum").html("（" + bas + "%）");
$("#badnump").css("width",bas + "%");


$("#qzmidnav05 span").html(d);



$(window).scroll(function(){ 
	if($(this).scrollTop()>headHeight){ 
		nav.addClass("navFix"); 
		
		if($(this).scrollTop()>height5){ 
			$("#navul a").removeClass("active");
			$("#qzmidnav06").addClass("active");			
		}else if($(this).scrollTop()>height4){ 
			$("#navul a").removeClass("active");
			$("#qzmidnav05").addClass("active");
		}else if($(this).scrollTop()>height3){ 
			$("#navul a").removeClass("active");
			$("#qzmidnav04").addClass("active");
		}else if($(this).scrollTop()>height2){ 
			$("#navul a").removeClass("active");
			$("#qzmidnav03").addClass("active");
		}else if($(this).scrollTop()>height1){ 
			$("#navul a").removeClass("active");
			$("#qzmidnav02").addClass("active");
		}
		
	}else{ 
		$("#navul a").removeClass("active");
		$("#qzmidnav01").addClass("active");
		nav.removeClass("navFix"); 
	} 
})

$(".lf").click(function(){
	if($("#vnumber").val()>0){
		$("#vnumber").val(parseInt($("#vnumber").val())-1);
	}
})

$(".lr").click(function(){
	$("#vnumber").val(parseInt($("#vnumber").val())+1);
})

