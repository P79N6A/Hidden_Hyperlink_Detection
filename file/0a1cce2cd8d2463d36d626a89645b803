	$('.tm-sale-prop li').bind("click",function(){

		 $.getJSON("/goods.php", { act: "price", id: 316, attr: $(this).val(), number: 1 },function(data){
  changePriceResponse(data);
});
		 $('.tm-sale-prop li').removeClass('tb-selected');
		 $(".tm-sale-prop li i").remove();
