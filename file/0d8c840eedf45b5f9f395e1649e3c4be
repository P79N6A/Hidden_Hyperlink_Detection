     <script type="text/javascript">
    /*
     * 滚动<li>
     * @o    '#id'
     * @time 停留时间
     */
    function liSroll(o, time) {
        var ul = $(o);
        var f = function () {
            var li = ul.find('li:first');
            li.animate({marginTop: -li.height() + 'px'}, 600, function () {
                li.css('marginTop', 0).appendTo(ul); // 还原li的marginTop值，把li移到最后。
            });
        };
        var t;
        ul
            .hover(function () {
                clearInterval(t);
            }, function () {
                t = setInterval(f, time);
            })
            .trigger('mouseleave'); // 模拟鼠标离开时的时间，让此动画一开始就执行。
    }
    liSroll('#fzScroll', 2000);
</script> 
