  <script>
function loginAgainComplete(){
return;
}
function acceptLoginUrl(urllist) {
    //进行同步登录请求
var _length = urllist.length,
down = 0;
for(var i = 0; i < _length; i++){
try{
var img = new Image();
img.onerror = img.onload = function(){
down++;
if(down >= _length){
;
}
}
img.src = urllist[i];
}catch(err){
down++ ;
if( down >= _length){
location.reload();
}
}
}
}

;(function(){
try{
window.shopClient.isAtCurApk(); //判断是否在apk中
//调用apk的接口以便获取同步登录地址
window.shopClient.doLoginAgain();
} catch (e) {
;
}
 })();
</script>  
