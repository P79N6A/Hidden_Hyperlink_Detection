<script type="text/javascript">
var requiredCaptcha = 'true' || false;
var mibaoQues = '' || '';
if (mibaoQues != '') {
	requiredCaptcha = false;
} else {
	requiredCaptcha = true;
}

function updateImage() {
    $('#otherRegist').html('<img  id="identifyImage" style="text-align:right;cursor:pointer;width:100px;height: 40px;" onclick="updateImage()"/>');
    var appname = navigator.appName.toLowerCase();
    if (appname.indexOf("netscape") != -1)
    {
        $('#identifyImage')[0].onload = function () {
            if ($('#identifyImage')[0].complete == true)
            {
                $('#identifyImage').show();
            }
        }
    }
    $('#identifyImage').attr("src","https://captcha.qbao.com/captcha/create/img/static?sessionId=1589D3D71C751E0EA0922C2BF6936E9C.i37j84is&time="+Math.random());

}

//$(document).ready(function() {
	/*文本框提示判断，无内容时显示，记住密码后有内容的情况下yin'ca*/
	/* $(window).bind("load",function(){
		if( $("#username").val().length<=0 ){
			 $("#username").parent().find(".tip").show();
			 $("#password").parent().find(".tip").show();
		}
	}); */

if ($("#errorCodeMsg").html()) {
    $("#usernameTipsLab").hide();
    $("#common_errors").html("<span></span>验证码不正确，请重新输入");
}

if (mibaoQues != '') {
	$("#unamePwdCaptcha").hide();
	$("#mibaoCheck").show();
	$("#password").val('');
	$("#thirdLoginBtn").hide();
	if ('' != "") {
		$("#mibao_errors").html("<span></span>密保答案不正确");
	}
} else {
	$("#unamePwdCaptcha").show();
	$("#mibaoCheck").hide();
	$("#thirdLoginBtn").show();
	if ($("#errtip").html()) {
	    $("#usernameTipsLab").hide();
	    $("#common_errors").html("<span></span>"+$("#errtip").html());
	}else {
	    if ($('#status').html() && $('#status').html().length > 0) {
	        $("#usernameTipsLab").hide();
	        var errorMsgs = $('#status').html().split(/<br\/?>/i);
	        $("#common_errors").html("<span></span>会员账号或密码错误，请重新输入");
	        if (errorMsgs.length == 2) {
	            $("#common_errors").html("<span></span>会员账号或密码错误，请重新输入");
	        }
	    }
	}
}
	$("#password").keyboard({
	    openOn : null,
	    stayOpen : true,
	    layout : 'qwerty',
	    display:{'lock':'Caps'}
	});
	$("#loginKey").bind("click",function(){
	    var _this = $(this);
	    if($(".ui-keyboard").length){
	      return false;
	    }
	    $("#password").getkeyboard().reveal();
	    setKeyPos(_this);
	    $(window).resize(function(){
	        setKeyPos(_this);
	    })
	});
//});

function setKeyPos(o){
    $(".ui-keyboard").css({
        "left":o.offset().left-269.5
    })
}
/**
 * 控制输入框中的文字显示与隐藏
 */
function validateData(){
	$("#usernameTipsLab").html('');
	$("#usernameTipsLab").hide();
	if(isBlank($('#username').val())){
		$("#usernameTipsLab").html("<span></span>请输入手机号/邮箱/用户名");
		$("#usernameTipsLab").show();
		$("#common_errors").html("<span></span>会员账号不能为空");
		return false;
	}
	$("#passwordTip").html('');
	$("#passwordTip").hide();
	if(isBlank($('#password').val())){
		$("#passwordTip").html("<span></span>请输入密码");
		$("#passwordTip").show();
		$("#common_errors").html("<span></span>密码不能为空");
		return false;
	}

	$("#captchaTip").html('');
	$("#captchaTip").hide();
	if(requiredCaptcha && isBlank($('#captcha').val())){
		$("#captchaTip").html("<span></span>请输入验证码");
		$("#captchaTip").show();
		$("#common_errors").html("<span></span>验证码不能为空");
		return false;
	} 
	if (!requiredCaptcha) {
		if ($("#mibaoText").val() == "") {
			$("#mibao_errors").html("<span></span>请输入密保答案");
			return false;
		}
		$("#mibaoText").val(encodeURIComponent($("#mibaoText").val()));
	}
	$('#loginFormEle').append("<input type='hidden' name='cas_signed' value='8d8e1417-afbc-42db-b70f-83dbcc8b8b3b' />");
	$('#submitForm').attr("disabled", true).css('background',"#ccc").val('提交中');
	$('#submitForm2').attr("disabled", true).css('background',"#ccc").val('提交中');
}

function isBlank(text){
	return /^\s*$/.test(text);
}

function trimTextValue(id) {
	  $("#" + id).val($.trim($("#" + id).val()));
}

$(function(){
	$("input").bind({
	focus:function(){ $(this).siblings(".defaultTips").hide(); },
	blur:function(){
		if($(this).val() == "")
		$(this).siblings(".defaultTips").show();
	}
	});
	$(".defaultTips").click(function(){ $(this).hide();$(this).siblings("input").focus();});
	var locationAddr = window.location.href;
	if (locationAddr.indexOf("/aucLoginEntry/toAuctioninfoByNum.html") != -1) {
		locationAddr = locationAddr.substring(locationAddr.indexOf("?service=") + 9);
		var regLink = $("#regLinkBtn").attr("href");
		var channelId =  locationAddr.substring(locationAddr.indexOf(",") + 1);
		if (channelId != "") {
			regLink += "?channelId=" + channelId + "&backUrl=" + locationAddr;
		} else {
			regLink += "?backUrl=" + locationAddr;
		}
		$("#regLinkBtn").attr("href", regLink);
		$("#regHeaderBtn").attr("href", regLink);
	}
})
</script>


<!-- 新版登录注册，动态banner 2014-11-5 -->
<script type="text/javascript">
    String.prototype.trim = function(){
        this.replace(/(^\s+)|(\s+$)/g,"");
        return this;
    }

    function inputInformation(o){

        if(!o || o.length==0) return false;

        var len = o.length;

        var c = document.getElementById( o[1] ).getElementsByTagName("input")[0];
        c.oninput = function(i){
            if( this.value != "" ){
                document.getElementById( o[1] ).getElementsByTagName("span")[0].style.display="none";
            }
        }

        c.onpropertychange = function(i){
            if( this.value != "" ){
                document.getElementById( o[1] ).getElementsByTagName("span")[0].style.display="none";
            }

        }

        document.getElementById( o[1] ).getElementsByTagName("input")[0].oninput();
        document.getElementById( o[1] ).getElementsByTagName("input")[0].onpropertychange();


        for( var i=0;i<len;i++ ){

            var obj = document.getElementById( o[i] );
            obj.getElementsByTagName("input")[0].onfocus = (function(i){
                return function(){
                    var c = document.getElementById( o[i] );
                    c.getElementsByTagName("span")[0].style.display="none";
                }
            })(i);

            obj.getElementsByTagName("span")[0].onmousedown = (function(i){
                return function(){
                    var c = document.getElementById( o[i] );
                    c.getElementsByTagName("input")[0].focus();
                }
            })(i);

            obj.getElementsByTagName("input")[0].onblur = (function(i){
                return function(){
                    var c = document.getElementById( o[i] );
                    if( this.value.trim().length==0 ){
                        c.getElementsByTagName("span")[0].style.display="inline";
                    }
                }

            })(i);

        }
    }

    // keyboard
    (function($){
      function setKeyPos(o){
          $(".ui-keyboard").css({
              "left":o.offset().left-285,
              "top":o.offset().top+24
          })
      }
      $("#password").keyboard({
          openOn : null,
          stayOpen : true,
          layout : 'qwerty',
          display:{'lock':'Caps'}
      })

      $("#loginKey").bind("click",function(){
          var _this = $(this);
          if($(".ui-keyboard").length){
            return false;
          }
          $("#password").getkeyboard().reveal();
          setKeyPos(_this);
          $(window).resize(function(){
              setKeyPos(_this);
          })
      })
    })(jQuery);

    inputInformation(["login-name","login-pass","login-code"]);

//============= banner切换 ===============

    function addCircle(){
        var len = $("#images-list a").length,
        	circle = new Array();
	    if( len>1 ){
	        circle.push("<dl id='check-list'>");
	        for( var i=0;i<len;i++ ){
	           if( i==0 ){
	                circle.push("<dd class='hover'></dd>");
	           }else{
	                circle.push("<dd></dd>")
	           }
	        }
	        circle.push("</dl>");
	    }
	    return circle.join("");
	}

    $(".banner").append( addCircle() );
    $("#images-list a").banner_thaw({
        thumbObj:"#check-list dd"
    });
</script>
<!-- end 新版登录注册，动态banner 2014-11-5 -->

